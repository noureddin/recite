"بِسۡمِ ٱللَّهِ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ ٱX<ل>R<رَّ>حِJ<ی>مِ A<۝>D<١>",
"ٱلۡحَمۡدُ لِلَّهِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٢>",
"ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ ٱX<ل>R<رَّ>حِJ<ی>مِ A<۝>D<٣>",
"مَT<ـٰ>لِكِ یَوۡمِ ٱX<ل>دِّJ<ی>نِ A<۝>D<٤>",
"إِیَّاكَ نَعۡبُدُ وَإِیَّاكَ نَسۡتَعِJ<ی>نُ A<۝>D<٥>",
"ٱهۡدِنَا ٱX<ل>صِّR<رَ>T< ٰ>طَ ٱلۡمُسۡتَقِJ<ی>مَ A<۝>D<٦>",
"صِR<رَ>T< ٰ>طَ ٱلَّذِینَ أَنۡعَمۡتَ عَلَیۡهِمۡ غَیۡرِ ٱلۡمَغۡضُوبِ عَلَیۡهِمۡ وَلَا ٱX<ل>ضَّL<اۤ>لِّJ<ی>نَ A<۝>D<٧>",
"#اL<لۤ>L<مۤ> A<۝>D<١>",
"ذَT< ٰ>لِكَ ٱلۡكِتَT<ـٰ>بُ لَا R<رَ>یۡبَۛ فِیهِۛ هُدX<ࣰ>ى لِّلۡمُتَّقِJ<ی>نَ A<۝>D<٢>",
"ٱلَّذِینَ یُؤۡمِنُونَ بِX<ٱ>لۡغَیۡبِ وَیُقِیمُونَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَمِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡ یُN<ن>فِقُJ<و>نَ A<۝>D<٣>",
"وَX<ٱ>لَّذِینَ یُؤۡمِنُونَ بِمَW<اۤ> أُN<ن>زِلَ إِلَیۡكَ وَمَW<اۤ> أُN<ن>زِلَ مِN<ن> قَQ<بۡ>لِكَ وَبِX<ٱ>لۡءَاخِR<رَ>ةِ هُمۡ یُوقِنُJ<و>نَ A<۝>D<٤>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ عَلَىٰ هُدX<ࣰ>ى N<مِّ>X<ن> R<رَّ>بِّهِمۡۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<٥>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> سَوَW<اۤ>ءٌ عَلَیۡهِمۡ ءَأَN<ن>ذَR<رۡ>تَهُمۡ أَمۡ لَمۡ تُN<ن>ذِرۡهُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٦>",
"خَتَمَ ٱللَّهُ عَلَىٰ قُلُوبِهِمۡ وَعَلَىٰ سَمۡعِهِمۡۖ وَعَلَىW<ٰۤ> أَQ<بۡ>صَT<ـٰ>رِهِمۡ غِشَT<ـٰ>وَةX<ࣱ>ۖ N<وَ>لَهُمۡ عَذَابٌ عَظِJ<ی>مࣱ A<۝>D<٧>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یَ>قُولُ ءَامَN<نَّ>ا بِٱللَّهِ وَبِX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَمَا هُN<م> بِمُؤۡمِنِJ<ی>نَ A<۝>D<٨>",
"یُخَT<ـٰ>دِعُونَ ٱللَّهَ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَمَا یَخۡدَعُونَ إِلَّW<اۤ> أَN<ن>فُسَهُمۡ وَمَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٩>",
"فِی قُلُوبِهِم N<مَّ>R<رَ>ضN<ࣱ> فَزَادَهُمُ ٱللَّهُ مَR<رَ>ضX<ࣰ>اۖ N<وَ>لَهُمۡ عَذَابٌ أَلِیمُN<ۢ> بِمَا كَانُوX<ا۟> یَكۡذِبُJ<و>نَ A<۝>D<١٠>",
"وَإِذَا قِیلَ لَهُمۡ لَا تُفۡسِدُوX<ا۟> فِی ٱلۡأَR<رۡ>ضِ قَالُW<وۤ>X<ا۟> إِN<نَّ>مَا نَحۡنُ مُصۡلِحُJ<و>نَ A<۝>D<١١>",
"أَلَW<اۤ> إِN<نَّ>هُمۡ هُمُ ٱلۡمُفۡسِدُونَ وَلَT<ـٰ>كِX<ن> لَّا یَشۡعُR<رُ>J<و>نَ A<۝>D<١٢>",
"وَإِذَا قِیلَ لَهُمۡ ءَامِنُوX<ا۟> كَمَW<اۤ> ءَامَنَ ٱX<ل>N<نَّ>اسُ قَالُW<وۤ>X<ا۟> أَنُؤۡمِنُ كَمَW<اۤ> ءَامَنَ ٱX<ل>سُّفَهَW<اۤ>ءُۗ أَلَW<اۤ> إِN<نَّ>هُمۡ هُمُ ٱX<ل>سُّفَهَW<اۤ>ءُ وَلَT<ـٰ>كِX<ن> لَّا یَعۡلَمُJ<و>نَ A<۝>D<١٣>",
"وَإِذَا لَقُوX<ا۟> ٱلَّذِینَ ءَامَنُوX<ا۟> قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا وَإِذَا خَلَوۡX<ا۟> إِلَىٰ شَیَT<ـٰ>طِینِهِمۡ قَالُW<وۤ>X<ا۟> إِN<نَّ>ا مَعَكُمۡ إِN<نَّ>مَا نَحۡنُ مُسۡتَهۡزِءُJ<و>نَ A<۝>D<١٤>",
"ٱللَّهُ یَسۡتَهۡزِئُ بِهِمۡ وَیَمُدُّهُمۡ فِی طُغۡیَT<ـٰ>نِهِمۡ یَعۡمَهُJ<و>نَ A<۝>D<١٥>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ ٱشۡتَR<رَ>وُX<ا۟> ٱX<ل>ضَّلَT<ـٰ>لَةَ بِX<ٱ>لۡهُدَىٰ فَمَا R<رَ>بِحَت تِّجَT<ـٰ>R<رَ>تُهُمۡ وَمَا كَانُوX<ا۟> مُهۡتَدِJ<ی>نَ A<۝>D<١٦>",
"مَثَلُهُمۡ كَمَثَلِ ٱلَّذِی ٱسۡتَوۡقَدَ نَاR<ر>N<ࣰ>ا فَلَN<مَّ>W<اۤ> أَضَW<اۤ>ءَتۡ مَا حَوۡلَهُT<ۥ> ذَهَبَ ٱللَّهُ بِنُورِهِمۡ وَتَR<رَ>كَهُمۡ فِی ظُلُمَT<ـٰ>تX<ࣲ> لَّا یُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<١٧>",
"صُN<مُّ>N<ۢ> بُكۡمٌ عُمۡیN<ࣱ> فَهُمۡ لَا یَR<رۡ>جِعُJ<و>نَ A<۝>D<١٨>",
"أَوۡ كَصَیِّبX<ࣲ> N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ فِیهِ ظُلُمَT<ـٰ>تX<ࣱ> N<وَ>R<رَ>عۡدX<ࣱ> N<وَ>بَR<رۡ>قX<ࣱ> N<یَ>Q<جۡ>عَلُونَ أَصَT<ـٰ>بِعَهُمۡ فِW<یۤ> ءَاذَانِهِم N<مِّ>نَ ٱX<ل>صَّوَT< ٰ>عِقِ حَذَR<رَ> ٱلۡمَوۡتِۚ وَٱللَّهُ مُحِیطُN<ۢ> بِX<ٱ>لۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٩>",
"یَكَادُ ٱلۡبَR<رۡ>قُ یَخۡطَفُ أَQ<بۡ>صَT<ـٰ>R<رَ>هُمۡۖ كُلَّمَW<اۤ> أَضَW<اۤ>ءَ لَهُم N<مَّ>شَوۡX<ا۟> فِیهِ وَإِذَW<اۤ> أَظۡلَمَ عَلَیۡهِمۡ قَامُوX<ا۟>ۚ وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ لَذَهَبَ بِسَمۡعِهِمۡ وَأَQ<بۡ>صَT<ـٰ>رِهِمۡۚ إِN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٢٠>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ ٱعۡبُدُوX<ا۟> R<رَ>بَّكُمُ ٱلَّذِی خَلَقَكُمۡ وَX<ٱ>لَّذِینَ مِN<ن> قَQ<بۡ>لِكُمۡ لَعَلَّكُمۡ تَتَّقُJ<و>نَ A<۝>D<٢١>",
"ٱلَّذِی جَعَلَ لَكُمُ ٱلۡأَR<رۡ>ضَ فِR<رَ>T< ٰ>شX<ࣰ>ا N<وَ>X<ٱ>X<ل>سَّمَW<اۤ>ءَ بِنَW<اۤ>ءX<ࣰ> N<وَ>أَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَخۡR<رَ>جَ بِهِT<ۦ> مِنَ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِ رِزۡقX<ࣰ>ا لَّكُمۡۖ فَلَا تَQ<جۡ>عَلُوX<ا۟> لِلَّهِ أَN<ن>دَادX<ࣰ>ا N<وَ>أَN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٢٢>",
"وَإِN<ن> كُN<ن>تُمۡ فِی R<رَ>یۡبX<ࣲ> N<مِّ>N<مَّ>ا نَزَّلۡنَا عَلَىٰ عَQ<بۡ>دِنَا فَأۡتُوX<ا۟> بِسُوR<رَ>ةX<ࣲ> N<مِّ>X<ن> N<مِّ>ثۡلِهِT<ۦ> وَX<ٱ>Q<دۡ>عُوX<ا۟> شُهَدَW<اۤ>ءَكُم N<مِّ>N<ن> دُونِ ٱللَّهِ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٢٣>",
"فَإِX<ن> لَّمۡ تَفۡعَلُوX<ا۟> وَلَN<ن> تَفۡعَلُوX<ا۟> فَX<ٱ>تَّقُوX<ا۟> ٱX<ل>N<نَّ>اR<رَ> ٱلَّتِی وَقُودُهَا ٱX<ل>N<نَّ>اسُ وَX<ٱ>لۡحِجَاR<رَ>ةُۖ أُعِدَّتۡ لِلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٢٤>",
"وَبَشِّرِ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ أَN<نَّ> لَهُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ>ۖ كُلَّمَا R<رُ>زِقُوX<ا۟> مِنۡهَا مِN<ن> ثَمَR<رَ>ةX<ࣲ> رِّزۡقN<ࣰ>ا قَالُوX<ا۟> هَT<ـٰ>ذَا ٱلَّذِی R<رُ>زِQ<قۡ>نَا مِN<ن> قَQ<بۡ>لُۖ وَأُتُوX<ا۟> بِهِT<ۦ> مُتَشَT<ـٰ>بِهX<ࣰ>اۖ N<وَ>لَهُمۡ فِیهَW<اۤ> أَزۡوَT< ٰ>جX<ࣱ> N<مُّ>طَهَّR<رَ>ةX<ࣱ>ۖ N<وَ>هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٢٥>",
"۞ إِN<نَّ> ٱللَّهَ لَا یَسۡتَحۡیِW<ۦۤ> أَX<ن> N<یَ>ضۡرِبَ مَثَلX<ࣰ>ا N<مَّ>ا بَعُوضَةN<ࣰ> فَمَا فَوۡقَهَاۚ فَأَN<مَّ>ا ٱلَّذِینَ ءَامَنُوX<ا۟> فَیَعۡلَمُونَ أَN<نَّ>هُ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّهِمۡۖ وَأَN<مَّ>ا ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فَیَقُولُونَ مَاذَW<اۤ> أَR<رَ>ادَ ٱللَّهُ بِهَT<ـٰ>ذَا مَثَلX<ࣰ>اۘ N<یُ>ضِلُّ بِهِT<ۦ> كَثِیR<ر>X<ࣰ>ا N<وَ>یَهۡدِی بِهِT<ۦ> كَثِیR<ر>X<ࣰ>اۚ N<وَ>مَا یُضِلُّ بِهِW<ۦۤ> إِلَّا ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٢٦>",
"ٱلَّذِینَ یَN<ن>قُضُونَ عَهۡدَ ٱللَّهِ مِX<ن>N<ۢ> بَعۡدِ مِیثَT<ـٰ>قِهِT<ۦ> وَیَQ<قۡ>طَعُونَ مَW<اۤ> أَمَR<رَ> ٱللَّهُ بِهِW<ۦۤ> أَX<ن> N<یُ>وصَلَ وَیُفۡسِدُونَ فِی ٱلۡأَR<رۡ>ضِۚ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<٢٧>",
"كَیۡفَ تَكۡفُR<رُ>ونَ بِٱللَّهِ وَكُN<ن>تُمۡ أَمۡوَT< ٰ>تN<ࣰ>ا فَأَحۡیَT<ـٰ>كُمۡۖ ثُN<مَّ> یُمِیتُكُمۡ ثُN<مَّ> یُحۡیِیكُمۡ ثُN<مَّ> إِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٢٨>",
"هُوَ ٱلَّذِی خَلَقَ لَكُم N<مَّ>ا فِی ٱلۡأَR<رۡ>ضِ جَمِیعN<ࣰ>ا ثُN<مَّ> ٱسۡتَوَىW<ٰۤ> إِلَى ٱX<ل>سَّمَW<اۤ>ءِ فَسَوَّX<ى>T<ٰ>هُN<نَّ> سَQ<بۡ>عَ سَمَT<ـٰ>وَT< ٰ>تX<ࣲ>ۚ N<وَ>هُوَ بِكُلِّ شَیۡءٍ عَلِJ<ی>مࣱ A<۝>D<٢٩>",
"وَإِذۡ قَالَ R<رَ>بُّكَ لِلۡمَلَW<ـٰۤ>ئِكَةِ إِN<نِّ>ی جَاعِلN<ࣱ> فِی ٱلۡأَR<رۡ>ضِ خَلِیفَةN<ࣰ>ۖ قَالُW<وۤ>X<ا۟> أَتَQ<جۡ>عَلُ فِیهَا مَX<ن> N<یُ>فۡسِدُ فِیهَا وَیَسۡفِكُ ٱX<ل>دِّمَW<اۤ>ءَ وَنَحۡنُ نُسَبِّحُ بِحَمۡدِكَ وَنُقَدِّسُ لَكَۖ قَالَ إِN<نِّ>W<یۤ> أَعۡلَمُ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٣٠>",
"وَعَلَّمَ ءَادَمَ ٱلۡأَسۡمَW<اۤ>ءَ كُلَّهَا ثُN<مَّ> عَR<رَ>ضَهُمۡ عَلَى ٱلۡمَلَW<ـٰۤ>ئِكَةِ فَقَالَ أَX<ن>N<ۢ>بِءُونِی بِأَسۡمَW<اۤ>ءِ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٣١>",
"قَالُوX<ا۟> سُQ<بۡ>حَT<ـٰ>نَكَ لَا عِلۡمَ لَنَW<اۤ> إِلَّا مَا عَلَّمۡتَنَW<اۤ>ۖ إِN<نَّ>كَ أَN<ن>تَ ٱلۡعَلِیمُ ٱلۡحَكِJ<ی>مُ A<۝>D<٣٢>",
"قَالَ یَW<ـٰۤ>ءَادَمُ أَX<ن>N<ۢ>بِئۡهُN<م> بِأَسۡمَW<اۤ>ئِهِمۡۖ فَلَN<مَّ>W<اۤ> أَX<ن>N<ۢ>بَأَهُN<م> بِأَسۡمَW<اۤ>ئِهِمۡ قَالَ أَلَمۡ أَقُل لَّكُمۡ إِN<نِّ>W<یۤ> أَعۡلَمُ غَیۡبَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَأَعۡلَمُ مَا تُQ<بۡ>دُونَ وَمَا كُN<ن>تُمۡ تَكۡتُمُJ<و>نَ A<۝>D<٣٣>",
"وَإِذۡ قُلۡنَا لِلۡمَلَW<ـٰۤ>ئِكَةِ ٱسۡجُدُوX<ا۟> لِءَادَمَ فَسَجَدُW<وۤ>X<ا۟> إِلَّW<اۤ> إِQ<بۡ>لِیسَ أَبَىٰ وَX<ٱ>سۡتَكۡبَR<رَ> وَكَانَ مِنَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٣٤>",
"وَقُلۡنَا یَW<ـٰۤ>ءَادَمُ ٱسۡكُنۡ أَN<ن>تَ وَزَوۡجُكَ ٱلۡجَN<نَّ>ةَ وَكُلَا مِنۡهَا R<رَ>غَدًا حَیۡثُ شِئۡتُمَا وَلَا تَQ<قۡ>R<رَ>بَا هَT<ـٰ>ذِهِ ٱX<ل>شَّجَR<رَ>ةَ فَتَكُونَا مِنَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٣٥>",
"فَأَزَلَّهُمَا ٱX<ل>شَّیۡطَT<ـٰ>نُ عَنۡهَا فَأَخۡR<رَ>جَهُمَا مِN<مَّ>ا كَانَا فِیهِۖ وَقُلۡنَا ٱهۡبِطُوX<ا۟> بَعۡضُكُمۡ لِبَعۡضٍ عَدُوّX<ࣱ>ۖ N<وَ>لَكُمۡ فِی ٱلۡأَR<رۡ>ضِ مُسۡتَقَR<رّ>X<ࣱ> N<وَ>مَتَT<ـٰ>عٌ إِلَىٰ حِJ<ی>نࣲ A<۝>D<٣٦>",
"فَتَلَقَّىW<ٰۤ> ءَادَمُ مِX<ن> R<رَّ>بِّهِT<ۦ> كَلِمَT<ـٰ>تN<ࣲ> فَتَابَ عَلَیۡهِۚ إِN<نَّ>هُT<ۥ> هُوَ ٱX<ل>تَّوَّابُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٣٧>",
"قُلۡنَا ٱهۡبِطُوX<ا۟> مِنۡهَا جَمِیعN<ࣰ>اۖ فَإِN<مَّ>ا یَأۡتِیَN<نَّ>كُم N<مِّ>N<نِّ>ی هُدN<ࣰ>ى فَمَN<ن> تَبِعَ هُدَایَ فَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٣٨>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> وَكَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَW<اۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٣٩>",
"یَT<ـٰ>بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ ٱذۡكُR<رُ>وX<ا۟> نِعۡمَتِیَ ٱلَّتِW<یۤ> أَنۡعَمۡتُ عَلَیۡكُمۡ وَأَوۡفُوX<ا۟> بِعَهۡدِW<یۤ> أُوفِ بِعَهۡدِكُمۡ وَإِیَّT<ـٰ>یَ فَX<ٱ>R<رۡ>هَبُJ<و>نِ A<۝>D<٤٠>",
"وَءَامِنُوX<ا۟> بِمَW<اۤ> أَN<ن>زَلۡتُ مُصَدِّقX<ࣰ>ا لِّمَا مَعَكُمۡ وَلَا تَكُونُW<وۤ>X<ا۟> أَوَّلَ كَافِرِN<ۭ> بِهِT<ۦ>ۖ وَلَا تَشۡتَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِی ثَمَنN<ࣰ>ا قَلِیلX<ࣰ>ا N<وَ>إِیَّT<ـٰ>یَ فَX<ٱ>تَّقُJ<و>نِ A<۝>D<٤١>",
"وَلَا تَلۡبِسُوX<ا۟> ٱلۡحَقَّ بِX<ٱ>لۡبَT<ـٰ>طِلِ وَتَكۡتُمُوX<ا۟> ٱلۡحَقَّ وَأَN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٤٢>",
"وَأَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتُوX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَX<ٱ>R<رۡ>كَعُوX<ا۟> مَعَ ٱX<ل>R<رَّ>T< ٰ>كِعِJ<ی>نَ A<۝>D<٤٣>",
"۞ أَتَأۡمُR<رُ>ونَ ٱX<ل>N<نَّ>اسَ بِX<ٱ>لۡبِرِّ وَتَN<ن>سَوۡنَ أَN<ن>فُسَكُمۡ وَأَN<ن>تُمۡ تَتۡلُونَ ٱلۡكِتَT<ـٰ>بَۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<٤٤>",
"وَX<ٱ>سۡتَعِینُوX<ا۟> بِX<ٱ>X<ل>صَّQ<بۡ>رِ وَX<ٱ>X<ل>صَّلَX<و>T<ٰ>ةِۚ وَإِN<نَّ>هَا لَكَبِیR<رَ>ةٌ إِلَّا عَلَى ٱلۡخَT<ـٰ>شِعِJ<ی>نَ A<۝>D<٤٥>",
"ٱلَّذِینَ یَظُN<نُّ>ونَ أَN<نَّ>هُم N<مُّ>لَT<ـٰ>قُوX<ا۟> R<رَ>بِّهِمۡ وَأَN<نَّ>هُمۡ إِلَیۡهِ R<رَ>T< ٰ>جِعُJ<و>نَ A<۝>D<٤٦>",
"یَT<ـٰ>بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ ٱذۡكُR<رُ>وX<ا۟> نِعۡمَتِیَ ٱلَّتِW<یۤ> أَنۡعَمۡتُ عَلَیۡكُمۡ وَأَN<نِّ>ی فَضَّلۡتُكُمۡ عَلَى ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٤٧>",
"وَX<ٱ>تَّقُوX<ا۟> یَوۡمX<ࣰ>ا لَّا تَQ<جۡ>زِی نَفۡسٌ عَن N<نَّ>فۡسN<ࣲ> شَیۡءX<ࣰ>ا N<وَ>لَا یُQ<قۡ>بَلُ مِنۡهَا شَفَT<ـٰ>عَةX<ࣱ> N<وَ>لَا یُؤۡخَذُ مِنۡهَا عَQ<دۡ>لX<ࣱ> N<وَ>لَا هُمۡ یُN<ن>صَR<رُ>J<و>نَ A<۝>D<٤٨>",
"وَإِذۡ نَجَّیۡنَT<ـٰ>كُم N<مِّ>نۡ ءَالِ فِرۡعَوۡنَ یَسُومُونَكُمۡ سُW<وۤ>ءَ ٱلۡعَذَابِ یُذَبِّحُونَ أَQ<بۡ>نَW<اۤ>ءَكُمۡ وَیَسۡتَحۡیُونَ نِسَW<اۤ>ءَكُمۡۚ وَفِی ذَT< ٰ>لِكُN<م> بَلَW<اۤ>ءX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ عَظِJ<ی>مࣱ A<۝>D<٤٩>",
"وَإِذۡ فَR<رَ>Q<قۡ>نَا بِكُمُ ٱلۡبَحۡR<رَ> فَأَN<ن>جَیۡنَT<ـٰ>كُمۡ وَأَغۡR<رَ>Q<قۡ>نَW<اۤ> ءَالَ فِرۡعَوۡنَ وَأَN<ن>تُمۡ تَN<ن>ظُR<رُ>J<و>نَ A<۝>D<٥٠>",
"وَإِذۡ وَT< ٰ>عَQ<دۡ>نَا مُوسَىW<ٰۤ> أَR<رۡ>بَعِینَ لَیۡلَةN<ࣰ> ثُN<مَّ> ٱتَّخَذۡتُمُ ٱلۡعِQ<جۡ>لَ مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> وَأَN<ن>تُمۡ ظَT<ـٰ>لِمُJ<و>نَ A<۝>D<٥١>",
"ثُN<مَّ> عَفَوۡنَا عَN<ن>كُم N<مِّ>X<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَ لَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<٥٢>",
"وَإِذۡ ءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡفُR<رۡ>قَانَ لَعَلَّكُمۡ تَهۡتَدُJ<و>نَ A<۝>D<٥٣>",
"وَإِذۡ قَالَ مُوسَىٰ لِقَوۡمِهِT<ۦ> یَT<ـٰ>قَوۡمِ إِN<نَّ>كُمۡ ظَلَمۡتُمۡ أَN<ن>فُسَكُN<م> بِX<ٱ>تِّخَاذِكُمُ ٱلۡعِQ<جۡ>لَ فَتُوبُW<وۤ>X<ا۟> إِلَىٰ بَارِئِكُمۡ فَX<ٱ>Q<قۡ>تُلُW<وۤ>X<ا۟> أَN<ن>فُسَكُمۡ ذَT< ٰ>لِكُمۡ خَیۡR<ر>X<ࣱ> لَّكُمۡ عِN<ن>دَ بَارِئِكُمۡ فَتَابَ عَلَیۡكُمۡۚ إِN<نَّ>هُT<ۥ> هُوَ ٱX<ل>تَّوَّابُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٥٤>",
"وَإِذۡ قُلۡتُمۡ یَT<ـٰ>مُوسَىٰ لَن N<نُّ>ؤۡمِنَ لَكَ حَتَّىٰ نَR<رَ>ى ٱللَّهَ جَهۡR<رَ>ةN<ࣰ> فَأَخَذَتۡكُمُ ٱX<ل>صَّT<ـٰ>عِقَةُ وَأَN<ن>تُمۡ تَN<ن>ظُR<رُ>J<و>نَ A<۝>D<٥٥>",
"ثُN<مَّ> بَعَثۡنَT<ـٰ>كُم N<مِّ>X<ن>N<ۢ> بَعۡدِ مَوۡتِكُمۡ لَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<٥٦>",
"وَظَلَّلۡنَا عَلَیۡكُمُ ٱلۡغَمَامَ وَأَN<ن>زَلۡنَا عَلَیۡكُمُ ٱلۡمَN<نَّ> وَX<ٱ>X<ل>سَّلۡوَىٰۖ كُلُوX<ا۟> مِN<ن> طَیِّبَT<ـٰ>تِ مَا R<رَ>زَQ<قۡ>نَT<ـٰ>كُمۡۚ وَمَا ظَلَمُونَا وَلَT<ـٰ>كِN<ن> كَانُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ یَظۡلِمُJ<و>نَ A<۝>D<٥٧>",
"وَإِذۡ قُلۡنَا ٱQ<دۡ>خُلُوX<ا۟> هَT<ـٰ>ذِهِ ٱلۡقَR<رۡ>یَةَ فَكُلُوX<ا۟> مِنۡهَا حَیۡثُ شِئۡتُمۡ R<رَ>غَدX<ࣰ>ا N<وَ>X<ٱ>Q<دۡ>خُلُوX<ا۟> ٱلۡبَابَ سُجَّدX<ࣰ>ا N<وَ>قُولُوX<ا۟> حِطَّةX<ࣱ> N<نَّ>غۡفِرۡ لَكُمۡ خَطَT<ـٰ>یَT<ـٰ>كُمۡۚ وَسَنَزِیدُ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٥٨>",
"فَبَدَّلَ ٱلَّذِینَ ظَلَمُوX<ا۟> قَوۡلًا غَیۡR<رَ> ٱلَّذِی قِیلَ لَهُمۡ فَأَN<ن>زَلۡنَا عَلَى ٱلَّذِینَ ظَلَمُوX<ا۟> رِQ<جۡ>زX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ بِمَا كَانُوX<ا۟> یَفۡسُقُJ<و>نَ A<۝>D<٥٩>",
"۞ وَإِذِ ٱسۡتَسۡقَىٰ مُوسَىٰ لِقَوۡمِهِT<ۦ> فَقُلۡنَا ٱضۡرِب بِّعَصَاكَ ٱلۡحَجَR<رَ>ۖ فَX<ٱ>N<ن>فَجَR<رَ>تۡ مِنۡهُ ٱثۡنَتَا عَشۡR<رَ>ةَ عَیۡنN<ࣰ>اۖ قَQ<دۡ> عَلِمَ كُلُّ أُنَاسX<ࣲ> N<مَّ>شۡR<رَ>بَهُمۡۖ كُلُوX<ا۟> وَX<ٱ>شۡR<رَ>بُوX<ا۟> مِX<ن> رِّزۡقِ ٱللَّهِ وَلَا تَعۡثَوۡX<ا۟> فِی ٱلۡأَR<رۡ>ضِ مُفۡسِدِJ<ی>نَ A<۝>D<٦٠>",
"وَإِذۡ قُلۡتُمۡ یَT<ـٰ>مُوسَىٰ لَن N<نَّ>صۡبِR<رَ> عَلَىٰ طَعَامX<ࣲ> N<وَ>T< ٰ>حِدN<ࣲ> فَX<ٱ>Q<دۡ>عُ لَنَا R<رَ>بَّكَ یُخۡرِQ<جۡ> لَنَا مِN<مَّ>ا تُX<ن>N<ۢ>بِتُ ٱلۡأَR<رۡ>ضُ مِX<ن>N<ۢ> بَQ<قۡ>لِهَا وَقِثَّW<اۤ>ئِهَا وَفُومِهَا وَعَدَسِهَا وَبَصَلِهَاۖ قَالَ أَتَسۡتَQ<بۡ>دِلُونَ ٱلَّذِی هُوَ أَQ<دۡ>نَىٰ بِX<ٱ>لَّذِی هُوَ خَیۡR<ر>ٌۚ ٱهۡبِطُوX<ا۟> مِصۡR<ر>N<ࣰ>ا فَإِN<نَّ> لَكُم N<مَّ>ا سَأَلۡتُمۡۗ وَضُرِبَتۡ عَلَیۡهِمُ ٱX<ل>ذِّلَّةُ وَX<ٱ>لۡمَسۡكَنَةُ وَبَW<اۤ>ءُو بِغَضَبX<ࣲ> N<مِّ>نَ ٱللَّهِۗ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ كَانُوX<ا۟> یَكۡفُR<رُ>ونَ بِءَایَT<ـٰ>تِ ٱللَّهِ وَیَQ<قۡ>تُلُونَ ٱX<ل>N<نَّ>بِیِّT<ـۧ>نَ بِغَیۡرِ ٱلۡحَقِّۗ ذَT< ٰ>لِكَ بِمَا عَصَوX<ا۟> وَّكَانُوX<ا۟> یَعۡتَدُJ<و>نَ A<۝>D<٦١>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَX<ٱ>لَّذِینَ هَادُوX<ا۟> وَX<ٱ>X<ل>N<نَّ>صَT<ـٰ>R<رَ>ىٰ وَX<ٱ>X<ل>صَّT<ـٰ>بِءِینَ مَنۡ ءَامَنَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَعَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَلَهُمۡ أَQ<جۡ>R<رُ>هُمۡ عِN<ن>دَ R<رَ>بِّهِمۡ وَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٦٢>",
"وَإِذۡ أَخَذۡنَا مِیثَT<ـٰ>قَكُمۡ وَR<رَ>فَعۡنَا فَوۡقَكُمُ ٱX<ل>طُّوR<رَ> خُذُوX<ا۟> مَW<اۤ> ءَاتَیۡنَT<ـٰ>كُN<م> بِقُوَّةX<ࣲ> N<وَ>X<ٱ>ذۡكُR<رُ>وX<ا۟> مَا فِیهِ لَعَلَّكُمۡ تَتَّقُJ<و>نَ A<۝>D<٦٣>",
"ثُN<مَّ> تَوَلَّیۡتُم N<مِّ>X<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَۖ فَلَوۡلَا فَضۡلُ ٱللَّهِ عَلَیۡكُمۡ وَR<رَ>حۡمَتُهُT<ۥ> لَكُN<ن>تُم N<مِّ>نَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٦٤>",
"وَلَقَQ<دۡ> عَلِمۡتُمُ ٱلَّذِینَ ٱعۡتَدَوۡX<ا۟> مِN<ن>كُمۡ فِی ٱX<ل>سَّQ<بۡ>تِ فَقُلۡنَا لَهُمۡ كُونُوX<ا۟> قِR<رَ>دَةً خَT<ـٰ>سِءِJ<ی>نَ A<۝>D<٦٥>",
"فَجَعَلۡنَT<ـٰ>هَا نَكَT<ـٰ>لX<ࣰ>ا لِّمَا بَیۡنَ یَدَیۡهَا وَمَا خَلۡفَهَا وَمَوۡعِظَةX<ࣰ> لِّلۡمُتَّقِJ<ی>نَ A<۝>D<٦٦>",
"وَإِذۡ قَالَ مُوسَىٰ لِقَوۡمِهِW<ۦۤ> إِN<نَّ> ٱللَّهَ یَأۡمُR<رُ>كُمۡ أَN<ن> تَذۡبَحُوX<ا۟> بَقَR<رَ>ةN<ࣰ>ۖ قَالُW<وۤ>X<ا۟> أَتَتَّخِذُنَا هُزُوN<ࣰ>اۖ قَالَ أَعُوذُ بِٱللَّهِ أَنۡ أَكُونَ مِنَ ٱلۡجَT<ـٰ>هِلِJ<ی>نَ A<۝>D<٦٧>",
"قَالُوX<ا۟> ٱQ<دۡ>عُ لَنَا R<رَ>بَّكَ یُبَیِّX<ن> لَّنَا مَا هِیَۚ قَالَ إِN<نَّ>هُT<ۥ> یَقُولُ إِN<نَّ>هَا بَقَR<رَ>ةX<ࣱ> لَّا فَارِضX<ࣱ> N<وَ>لَا بِكۡR<ر>ٌ عَوَانُN<ۢ> بَیۡنَ ذَT< ٰ>لِكَۖ فَX<ٱ>فۡعَلُوX<ا۟> مَا تُؤۡمَR<رُ>J<و>نَ A<۝>D<٦٨>",
"قَالُوX<ا۟> ٱQ<دۡ>عُ لَنَا R<رَ>بَّكَ یُبَیِّX<ن> لَّنَا مَا لَوۡنُهَاۚ قَالَ إِN<نَّ>هُT<ۥ> یَقُولُ إِN<نَّ>هَا بَقَR<رَ>ةN<ࣱ> صَفۡR<رَ>W<اۤ>ءُ فَاقِعX<ࣱ> لَّوۡنُهَا تَسُR<رُّ> ٱX<ل>N<نَّ>T<ـٰ>ظِرِJ<ی>نَ A<۝>D<٦٩>",
"قَالُوX<ا۟> ٱQ<دۡ>عُ لَنَا R<رَ>بَّكَ یُبَیِّX<ن> لَّنَا مَا هِیَ إِN<نَّ> ٱلۡبَقَR<رَ> تَشَT<ـٰ>بَهَ عَلَیۡنَا وَإِN<نَّ>W<اۤ> إِN<ن> شَW<اۤ>ءَ ٱللَّهُ لَمُهۡتَدُJ<و>نَ A<۝>D<٧٠>",
"قَالَ إِN<نَّ>هُT<ۥ> یَقُولُ إِN<نَّ>هَا بَقَR<رَ>ةX<ࣱ> لَّا ذَلُولN<ࣱ> تُثِیR<رُ> ٱلۡأَR<رۡ>ضَ وَلَا تَسۡقِی ٱلۡحَR<رۡ>ثَ مُسَلَّمَةX<ࣱ> لَّا شِیَةَ فِیهَاۚ قَالُوX<ا۟> ٱلۡءَT<ـٰ>نَ جِئۡتَ بِX<ٱ>لۡحَقِّۚ فَذَبَحُوهَا وَمَا كَادُوX<ا۟> یَفۡعَلُJ<و>نَ A<۝>D<٧١>",
"وَإِذۡ قَتَلۡتُمۡ نَفۡسN<ࣰ>ا فَX<ٱ>دَّT< ٰ>R<رَ>ءۡتُمۡ فِیهَاۖ وَٱللَّهُ مُخۡرِجX<ࣱ> N<مَّ>ا كُN<ن>تُمۡ تَكۡتُمُJ<و>نَ A<۝>D<٧٢>",
"فَقُلۡنَا ٱضۡرِبُوهُ بِبَعۡضِهَاۚ كَذَT< ٰ>لِكَ یُحۡیِ ٱللَّهُ ٱلۡمَوۡتَىٰ وَیُرِیكُمۡ ءَایَT<ـٰ>تِهِT<ۦ> لَعَلَّكُمۡ تَعۡقِلُJ<و>نَ A<۝>D<٧٣>",
"ثُN<مَّ> قَسَتۡ قُلُوبُكُم N<مِّ>X<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَ فَهِیَ كَX<ٱ>لۡحِجَاR<رَ>ةِ أَوۡ أَشَدُّ قَسۡوَةX<ࣰ>ۚ N<وَ>إِN<نَّ> مِنَ ٱلۡحِجَاR<رَ>ةِ لَمَا یَتَفَجَّR<رُ> مِنۡهُ ٱلۡأَنۡهَT<ـٰ>R<رُ>ۚ وَإِN<نَّ> مِنۡهَا لَمَا یَشَّقَّقُ فَیَخۡR<رُ>جُ مِنۡهُ ٱلۡمَW<اۤ>ءُۚ وَإِN<نَّ> مِنۡهَا لَمَا یَهۡبِطُ مِنۡ خَشۡیَةِ ٱللَّهِۗ وَمَا ٱللَّهُ بِغَT<ـٰ>فِلٍ عَN<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<٧٤>",
"۞ أَفَتَQ<طۡ>مَعُونَ أَX<ن> N<یُ>ؤۡمِنُوX<ا۟> لَكُمۡ وَقَQ<دۡ> كَانَ فَرِیقX<ࣱ> N<مِّ>نۡهُمۡ یَسۡمَعُونَ كَلَT<ـٰ>مَ ٱللَّهِ ثُN<مَّ> یُحَرِّفُونَهُT<ۥ> مِX<ن>N<ۢ> بَعۡدِ مَا عَقَلُوهُ وَهُمۡ یَعۡلَمُJ<و>نَ A<۝>D<٧٥>",
"وَإِذَا لَقُوX<ا۟> ٱلَّذِینَ ءَامَنُوX<ا۟> قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا وَإِذَا خَلَا بَعۡضُهُمۡ إِلَىٰ بَعۡضN<ࣲ> قَالُW<وۤ>X<ا۟> أَتُحَدِّثُونَهُN<م> بِمَا فَتَحَ ٱللَّهُ عَلَیۡكُمۡ لِیُحَL<اۤ>جُّوكُN<م> بِهِT<ۦ> عِN<ن>دَ R<رَ>بِّكُمۡۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<٧٦>",
"أَوَ لَا یَعۡلَمُونَ أَN<نَّ> ٱللَّهَ یَعۡلَمُ مَا یُسِR<رُّ>ونَ وَمَا یُعۡلِنُJ<و>نَ A<۝>D<٧٧>",
"وَمِنۡهُمۡ أُN<مِّ>یُّونَ لَا یَعۡلَمُونَ ٱلۡكِتَT<ـٰ>بَ إِلَّW<اۤ> أَمَانِیَّ وَإِنۡ هُمۡ إِلَّا یَظُN<نُّ>J<و>نَ A<۝>D<٧٨>",
"فَوَیۡلX<ࣱ> لِّلَّذِینَ یَكۡتُبُونَ ٱلۡكِتَT<ـٰ>بَ بِأَیۡدِیهِمۡ ثُN<مَّ> یَقُولُونَ هَT<ـٰ>ذَا مِنۡ عِN<ن>دِ ٱللَّهِ لِیَشۡتَR<رُ>وX<ا۟> بِهِT<ۦ> ثَمَنN<ࣰ>ا قَلِیلN<ࣰ>اۖ فَوَیۡلX<ࣱ> لَّهُم N<مِّ>N<مَّ>ا كَتَبَتۡ أَیۡدِیهِمۡ وَوَیۡلX<ࣱ> لَّهُم N<مِّ>N<مَّ>ا یَكۡسِبُJ<و>نَ A<۝>D<٧٩>",
"وَقَالُوX<ا۟> لَN<ن> تَمَسَّنَا ٱX<ل>N<نَّ>اR<رُ> إِلَّW<اۤ> أَیَّامX<ࣰ>ا N<مَّ>عۡدُودَةN<ࣰ>ۚ قُلۡ أَتَّخَذۡتُمۡ عِN<ن>دَ ٱللَّهِ عَهۡدN<ࣰ>ا فَلَX<ن> N<یُ>خۡلِفَ ٱللَّهُ عَهۡدَهُW<ۥۤ>ۖ أَمۡ تَقُولُونَ عَلَى ٱللَّهِ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٨٠>",
"بَلَىٰۚ مَN<ن> كَسَبَ سَیِّئَةX<ࣰ> N<وَ>أَحَT<ـٰ>طَتۡ بِهِT<ۦ> خَطِW<یۤ>ءَتُهُT<ۥ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٨١>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡجَN<نَّ>ةِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٨٢>",
"وَإِذۡ أَخَذۡنَا مِیثَT<ـٰ>قَ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ لَا تَعۡبُدُونَ إِلَّا ٱللَّهَ وَبِX<ٱ>لۡوَT< ٰ>لِدَیۡنِ إِحۡسَانX<ࣰ>ا N<وَ>ذِی ٱلۡقُR<رۡ>بَىٰ وَX<ٱ>لۡیَتَT<ـٰ>مَىٰ وَX<ٱ>لۡمَسَT<ـٰ>كِینِ وَقُولُوX<ا۟> لِX<ل>N<نَّ>اسِ حُسۡنX<ࣰ>ا N<وَ>أَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتُوX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ ثُN<مَّ> تَوَلَّیۡتُمۡ إِلَّا قَلِیلX<ࣰ>ا N<مِّ>N<ن>كُمۡ وَأَN<ن>تُم N<مُّ>عۡرِضُJ<و>نَ A<۝>D<٨٣>",
"وَإِذۡ أَخَذۡنَا مِیثَT<ـٰ>قَكُمۡ لَا تَسۡفِكُونَ دِمَW<اۤ>ءَكُمۡ وَلَا تُخۡرِجُونَ أَN<ن>فُسَكُم N<مِّ>N<ن> دِیَT<ـٰ>رِكُمۡ ثُN<مَّ> أَQ<قۡ>R<رَ>R<رۡ>تُمۡ وَأَN<ن>تُمۡ تَشۡهَدُJ<و>نَ A<۝>D<٨٤>",
"ثُN<مَّ> أَN<ن>تُمۡ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ تَQ<قۡ>تُلُونَ أَN<ن>فُسَكُمۡ وَتُخۡرِجُونَ فَرِیقX<ࣰ>ا N<مِّ>N<ن>كُم N<مِّ>N<ن> دِیَT<ـٰ>رِهِمۡ تَظَT<ـٰ>هَR<رُ>ونَ عَلَیۡهِN<م> بِX<ٱ>لۡإِثۡمِ وَX<ٱ>لۡعُQ<دۡ>وَT< ٰ>نِ وَإِX<ن> N<یَ>أۡتُوكُمۡ أُسَT<ـٰ>R<رَ>ىٰ تُفَT<ـٰ>دُوهُمۡ وَهُوَ مُحَR<رَّ>مٌ عَلَیۡكُمۡ إِخۡR<رَ>اجُهُمۡۚ أَفَتُؤۡمِنُونَ بِبَعۡضِ ٱلۡكِتَT<ـٰ>بِ وَتَكۡفُR<رُ>ونَ بِبَعۡضN<ࣲ>ۚ فَمَا جَزَW<اۤ>ءُ مَX<ن> N<یَ>فۡعَلُ ذَT< ٰ>لِكَ مِN<ن>كُمۡ إِلَّا خِزۡیN<ࣱ> فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۖ وَیَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ یُR<رَ>دُّونَ إِلَىW<ٰۤ> أَشَدِّ ٱلۡعَذَابِۗ وَمَا ٱللَّهُ بِغَT<ـٰ>فِلٍ عَN<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<٨٥>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ ٱشۡتَR<رَ>وُX<ا۟> ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا بِX<ٱ>لۡءَاخِR<رَ>ةِۖ فَلَا یُخَفَّفُ عَنۡهُمُ ٱلۡعَذَابُ وَلَا هُمۡ یُN<ن>صَR<رُ>J<و>نَ A<۝>D<٨٦>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ وَقَفَّیۡنَا مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> بِX<ٱ>X<ل>R<رُّ>سُلِۖ وَءَاتَیۡنَا عِیسَى ٱQ<بۡ>نَ مَR<رۡ>یَمَ ٱلۡبَیِّنَT<ـٰ>تِ وَأَیَّQ<دۡ>نَT<ـٰ>هُ بِR<رُ>وحِ ٱلۡقُدُسِۗ أَفَكُلَّمَا جَW<اۤ>ءَكُمۡ R<رَ>سُولُN<ۢ> بِمَا لَا تَهۡوَىW<ٰۤ> أَN<ن>فُسُكُمُ ٱسۡتَكۡبَR<رۡ>تُمۡ فَفَرِیقN<ࣰ>ا كَذَّQ<بۡ>تُمۡ وَفَرِیقN<ࣰ>ا تَQ<قۡ>تُلُJ<و>نَ A<۝>D<٨٧>",
"وَقَالُوX<ا۟> قُلُوبُنَا غُلۡفُN<ۢ>ۚ بَل لَّعَنَهُمُ ٱللَّهُ بِكُفۡرِهِمۡ فَقَلِیلX<ࣰ>ا N<مَّ>ا یُؤۡمِنُJ<و>نَ A<۝>D<٨٨>",
"وَلَN<مَّ>ا جَW<اۤ>ءَهُمۡ كِتَT<ـٰ>بX<ࣱ> N<مِّ>نۡ عِN<ن>دِ ٱللَّهِ مُصَدِّقX<ࣱ> لِّمَا مَعَهُمۡ وَكَانُوX<ا۟> مِN<ن> قَQ<بۡ>لُ یَسۡتَفۡتِحُونَ عَلَى ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فَلَN<مَّ>ا جَW<اۤ>ءَهُم N<مَّ>ا عَR<رَ>فُوX<ا۟> كَفَR<رُ>وX<ا۟> بِهِT<ۦ>ۚ فَلَعۡنَةُ ٱللَّهِ عَلَى ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٨٩>",
"بِئۡسَمَا ٱشۡتَR<رَ>وۡX<ا۟> بِهِW<ۦۤ> أَN<ن>فُسَهُمۡ أَX<ن> N<یَ>كۡفُR<رُ>وX<ا۟> بِمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ بَغۡیًا أَX<ن> N<یُ>نَزِّلَ ٱللَّهُ مِN<ن> فَضۡلِهِT<ۦ> عَلَىٰ مَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِT<ۦ>ۖ فَبَW<اۤ>ءُو بِغَضَبٍ عَلَىٰ غَضَبX<ࣲ>ۚ N<وَ>لِلۡكَT<ـٰ>فِرِینَ عَذَابX<ࣱ> N<مُّ>هِJ<ی>نࣱ A<۝>D<٩٠>",
"وَإِذَا قِیلَ لَهُمۡ ءَامِنُوX<ا۟> بِمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ قَالُوX<ا۟> نُؤۡمِنُ بِمَW<اۤ> أُN<ن>زِلَ عَلَیۡنَا وَیَكۡفُR<رُ>ونَ بِمَا وَR<رَ>W<اۤ>ءَهُT<ۥ> وَهُوَ ٱلۡحَقُّ مُصَدِّقX<ࣰ>ا لِّمَا مَعَهُمۡۗ قُلۡ فَلِمَ تَQ<قۡ>تُلُونَ أَX<ن>N<ۢ>بِیَW<اۤ>ءَ ٱللَّهِ مِN<ن> قَQ<بۡ>لُ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٩١>",
"۞ وَلَقَQ<دۡ> جَW<اۤ>ءَكُم N<مُّ>وسَىٰ بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ ثُN<مَّ> ٱتَّخَذۡتُمُ ٱلۡعِQ<جۡ>لَ مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> وَأَN<ن>تُمۡ ظَT<ـٰ>لِمُJ<و>نَ A<۝>D<٩٢>",
"وَإِذۡ أَخَذۡنَا مِیثَT<ـٰ>قَكُمۡ وَR<رَ>فَعۡنَا فَوۡقَكُمُ ٱX<ل>طُّوR<رَ> خُذُوX<ا۟> مَW<اۤ> ءَاتَیۡنَT<ـٰ>كُN<م> بِقُوَّةX<ࣲ> N<وَ>X<ٱ>سۡمَعُوX<ا۟>ۖ قَالُوX<ا۟> سَمِعۡنَا وَعَصَیۡنَا وَأُشۡرِبُوX<ا۟> فِی قُلُوبِهِمُ ٱلۡعِQ<جۡ>لَ بِكُفۡرِهِمۡۚ قُلۡ بِئۡسَمَا یَأۡمُR<رُ>كُN<م> بِهِW<ۦۤ> إِیمَT<ـٰ>نُكُمۡ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٩٣>",
"قُلۡ إِN<ن> كَانَتۡ لَكُمُ ٱX<ل>دَّاR<رُ> ٱلۡءَاخِR<رَ>ةُ عِN<ن>دَ ٱللَّهِ خَالِصَةX<ࣰ> N<مِّ>N<ن> دُونِ ٱX<ل>N<نَّ>اسِ فَتَمَN<نَّ>وُX<ا۟> ٱلۡمَوۡتَ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٩٤>",
"وَلَX<ن> N<یَ>تَمَN<نَّ>وۡهُ أَبَدَN<ۢ>ا بِمَا قَدَّمَتۡ أَیۡدِیهِمۡۚ وَٱللَّهُ عَلِیمُN<ۢ> بِX<ٱ>X<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٩٥>",
"وَلَتَجِدَN<نَّ>هُمۡ أَحۡR<رَ>صَ ٱX<ل>N<نَّ>اسِ عَلَىٰ حَیَX<و>T<ٰ>ةX<ࣲ> N<وَ>مِنَ ٱلَّذِینَ أَشۡR<رَ>كُوX<ا۟>ۚ یَوَدُّ أَحَدُهُمۡ لَوۡ یُعَN<مَّ>R<رُ> أَلۡفَ سَنَةX<ࣲ> N<وَ>مَا هُوَ بِمُزَحۡزِحِهِT<ۦ> مِنَ ٱلۡعَذَابِ أَX<ن> N<یُ>عَN<مَّ>R<رَ>ۗ وَٱللَّهُ بَصِیR<رُ>N<ۢ> بِمَا یَعۡمَلُJ<و>نَ A<۝>D<٩٦>",
"قُلۡ مَN<ن> كَانَ عَدُوّX<ࣰ>ا لِّجِQ<بۡ>رِیلَ فَإِN<نَّ>هُT<ۥ> نَزَّلَهُT<ۥ> عَلَىٰ قَلۡبِكَ بِإِذۡنِ ٱللَّهِ مُصَدِّقX<ࣰ>ا لِّمَا بَیۡنَ یَدَیۡهِ وَهُدX<ࣰ>ى N<وَ>بُشۡR<رَ>ىٰ لِلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٩٧>",
"مَN<ن> كَانَ عَدُوّX<ࣰ>ا لِّلَّهِ وَمَلَW<ـٰۤ>ئِكَتِهِT<ۦ> وَR<رُ>سُلِهِT<ۦ> وَجِQ<بۡ>رِیلَ وَمِیكَX<ى>T<ٰ>لَ فَإِN<نَّ> ٱللَّهَ عَدُوّX<ࣱ> لِّلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٩٨>",
"وَلَقَQ<دۡ> أَN<ن>زَلۡنَW<اۤ> إِلَیۡكَ ءَایَT<ـٰ>تِN<ۭ> بَیِّنَT<ـٰ>تX<ࣲ>ۖ N<وَ>مَا یَكۡفُR<رُ> بِهَW<اۤ> إِلَّا ٱلۡفَT<ـٰ>سِقُJ<و>نَ A<۝>D<٩٩>",
"أَوَ كُلَّمَا عَT<ـٰ>هَدُوX<ا۟> عَهۡدX<ࣰ>ا N<نَّ>بَذَهُT<ۥ> فَرِیقX<ࣱ> N<مِّ>نۡهُN<م>ۚ بَلۡ أَكۡثَR<رُ>هُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<١٠٠>",
"وَلَN<مَّ>ا جَW<اۤ>ءَهُمۡ R<رَ>سُولX<ࣱ> N<مِّ>نۡ عِN<ن>دِ ٱللَّهِ مُصَدِّقX<ࣱ> لِّمَا مَعَهُمۡ نَبَذَ فَرِیقX<ࣱ> N<مِّ>نَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ كِتَT<ـٰ>بَ ٱللَّهِ وَR<رَ>W<اۤ>ءَ ظُهُورِهِمۡ كَأَN<نَّ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<١٠١>",
"وَX<ٱ>تَّبَعُوX<ا۟> مَا تَتۡلُوX<ا۟> ٱX<ل>شَّیَT<ـٰ>طِینُ عَلَىٰ مُلۡكِ سُلَیۡمَT<ـٰ>نَۖ وَمَا كَفَR<رَ> سُلَیۡمَT<ـٰ>نُ وَلَT<ـٰ>كِN<نَّ> ٱX<ل>شَّیَT<ـٰ>طِینَ كَفَR<رُ>وX<ا۟> یُعَلِّمُونَ ٱX<ل>N<نَّ>اسَ ٱX<ل>سِّحۡR<رَ> وَمَW<اۤ> أُN<ن>زِلَ عَلَى ٱلۡمَلَكَیۡنِ بِبَابِلَ هَT<ـٰ>R<رُ>وتَ وَمَT<ـٰ>R<رُ>وتَۚ وَمَا یُعَلِّمَانِ مِنۡ أَحَدٍ حَتَّىٰ یَقُولَW<اۤ> إِN<نَّ>مَا نَحۡنُ فِتۡنَةN<ࣱ> فَلَا تَكۡفُR<رۡ>ۖ فَیَتَعَلَّمُونَ مِنۡهُمَا مَا یُفَرِّقُونَ بِهِT<ۦ> بَیۡنَ ٱلۡمَR<رۡ>ءِ وَزَوۡجِهِT<ۦ>ۚ وَمَا هُN<م> بِضَL<اۤ>رِّینَ بِهِT<ۦ> مِنۡ أَحَدٍ إِلَّا بِإِذۡنِ ٱللَّهِۚ وَیَتَعَلَّمُونَ مَا یَضُR<رُّ>هُمۡ وَلَا یَN<ن>فَعُهُمۡۚ وَلَقَQ<دۡ> عَلِمُوX<ا۟> لَمَنِ ٱشۡتَR<رَ>X<ى>T<ٰ>هُ مَا لَهُT<ۥ> فِی ٱلۡءَاخِR<رَ>ةِ مِنۡ خَلَT<ـٰ>قX<ࣲ>ۚ N<وَ>لَبِئۡسَ مَا شَR<رَ>وۡX<ا۟> بِهِW<ۦۤ> أَN<ن>فُسَهُمۡۚ لَوۡ كَانُوX<ا۟> یَعۡلَمُJ<و>نَ A<۝>D<١٠٢>",
"وَلَوۡ أَN<نَّ>هُمۡ ءَامَنُوX<ا۟> وَX<ٱ>تَّقَوۡX<ا۟> لَمَثُوبَةX<ࣱ> N<مِّ>نۡ عِN<ن>دِ ٱللَّهِ خَیۡR<ر>X<ࣱ>ۚ لَّوۡ كَانُوX<ا۟> یَعۡلَمُJ<و>نَ A<۝>D<١٠٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَقُولُوX<ا۟> R<رَ>T< ٰ>عِنَا وَقُولُوX<ا۟> ٱN<ن>ظُR<رۡ>نَا وَX<ٱ>سۡمَعُوX<ا۟>ۗ وَلِلۡكَT<ـٰ>فِرِینَ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<١٠٤>",
"مَّا یَوَدُّ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِنۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ وَلَا ٱلۡمُشۡرِكِینَ أَX<ن> N<یُ>نَزَّلَ عَلَیۡكُم N<مِّ>نۡ خَیۡرX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّكُمۡۚ وَٱللَّهُ یَخۡتَصُّ بِR<رَ>حۡمَتِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَٱللَّهُ ذُو ٱلۡفَضۡلِ ٱلۡعَظِJ<ی>مِ A<۝>D<١٠٥>",
"۞ مَا نَN<ن>سَخۡ مِنۡ ءَایَةٍ أَوۡ نُN<ن>سِهَا نَأۡتِ بِخَیۡرX<ࣲ> N<مِّ>نۡهَW<اۤ> أَوۡ مِثۡلِهَW<اۤ>ۗ أَلَمۡ تَعۡلَمۡ أَN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<١٠٦>",
"أَلَمۡ تَعۡلَمۡ أَN<نَّ> ٱللَّهَ لَهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۗ وَمَا لَكُم N<مِّ>N<ن> دُونِ ٱللَّهِ مِX<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا نَصِJ<ی>رٍ A<۝>D<١٠٧>",
"أَمۡ تُرِیدُونَ أَN<ن> تَسۡءَلُوX<ا۟> R<رَ>سُولَكُمۡ كَمَا سُئِلَ مُوسَىٰ مِN<ن> قَQ<بۡ>لُۗ وَمَX<ن> N<یَ>تَبَدَّلِ ٱلۡكُفۡR<رَ> بِX<ٱ>لۡإِیمَT<ـٰ>نِ فَقَQ<دۡ> ضَلَّ سَوَW<اۤ>ءَ ٱX<ل>سَّبِJ<ی>لِ A<۝>D<١٠٨>",
"وَدَّ كَثِیR<ر>X<ࣱ> N<مِّ>نۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ لَوۡ یَR<رُ>دُّونَكُم N<مِّ>X<ن>N<ۢ> بَعۡدِ إِیمَT<ـٰ>نِكُمۡ كُفَّاR<ر>ًا حَسَدX<ࣰ>ا N<مِّ>نۡ عِN<ن>دِ أَN<ن>فُسِهِم N<مِّ>X<ن>N<ۢ> بَعۡدِ مَا تَبَیَّنَ لَهُمُ ٱلۡحَقُّۖ فَX<ٱ>عۡفُوX<ا۟> وَX<ٱ>صۡفَحُوX<ا۟> حَتَّىٰ یَأۡتِیَ ٱللَّهُ بِأَمۡرِهِW<ۦۤ>ۗ إِN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<١٠٩>",
"وَأَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتُوX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَۚ وَمَا تُقَدِّمُوX<ا۟> لِأَN<ن>فُسِكُم N<مِّ>نۡ خَیۡرN<ࣲ> تَجِدُوهُ عِN<ن>دَ ٱللَّهِۗ إِN<نَّ> ٱللَّهَ بِمَا تَعۡمَلُونَ بَصِJ<ی>رࣱ A<۝>D<١١٠>",
"وَقَالُوX<ا۟> لَX<ن> N<یَ>Q<دۡ>خُلَ ٱلۡجَN<نَّ>ةَ إِلَّا مَN<ن> كَانَ هُودًا أَوۡ نَصَT<ـٰ>R<رَ>ىٰۗ تِلۡكَ أَمَانِیُّهُمۡۗ قُلۡ هَاتُوX<ا۟> بُR<رۡ>هَT<ـٰ>نَكُمۡ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<١١١>",
"بَلَىٰۚ مَنۡ أَسۡلَمَ وَQ<جۡ>هَهُT<ۥ> لِلَّهِ وَهُوَ مُحۡسِنN<ࣱ> فَلَهُW<ۥۤ> أَQ<جۡ>R<رُ>هُT<ۥ> عِN<ن>دَ R<رَ>بِّهِT<ۦ> وَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<١١٢>",
"وَقَالَتِ ٱلۡیَهُودُ لَیۡسَتِ ٱX<ل>N<نَّ>صَT<ـٰ>R<رَ>ىٰ عَلَىٰ شَیۡءX<ࣲ> N<وَ>قَالَتِ ٱX<ل>N<نَّ>صَT<ـٰ>R<رَ>ىٰ لَیۡسَتِ ٱلۡیَهُودُ عَلَىٰ شَیۡءX<ࣲ> N<وَ>هُمۡ یَتۡلُونَ ٱلۡكِتَT<ـٰ>بَۗ كَذَT< ٰ>لِكَ قَالَ ٱلَّذِینَ لَا یَعۡلَمُونَ مِثۡلَ قَوۡلِهِمۡۚ فَٱللَّهُ یَحۡكُمُ بَیۡنَهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ فِیمَا كَانُوX<ا۟> فِیهِ یَخۡتَلِفُJ<و>نَ A<۝>D<١١٣>",
"وَمَنۡ أَظۡلَمُ مِN<مَّ>X<ن> N<مَّ>نَعَ مَسَT<ـٰ>جِدَ ٱللَّهِ أَX<ن> N<یُ>ذۡكَR<رَ> فِیهَا ٱسۡمُهُT<ۥ> وَسَعَىٰ فِی خَR<رَ>ابِهَW<اۤ>ۚ أُX<و۟>لَW<ـٰۤ>ئِكَ مَا كَانَ لَهُمۡ أَX<ن> N<یَ>Q<دۡ>خُلُوهَW<اۤ> إِلَّا خَW<اۤ>ئِفِینَۚ لَهُمۡ فِی ٱX<ل>دُّنۡیَا خِزۡیX<ࣱ> N<وَ>لَهُمۡ فِی ٱلۡءَاخِR<رَ>ةِ عَذَابٌ عَظِJ<ی>مࣱ A<۝>D<١١٤>",
"وَلِلَّهِ ٱلۡمَشۡرِقُ وَX<ٱ>لۡمَغۡرِبُۚ فَأَیۡنَمَا تُوَلُّوX<ا۟> فَثَN<مَّ> وَQ<جۡ>هُ ٱللَّهِۚ إِN<نَّ> ٱللَّهَ وَT< ٰ>سِعٌ عَلِJ<ی>مࣱ A<۝>D<١١٥>",
"وَقَالُوX<ا۟> ٱتَّخَذَ ٱللَّهُ وَلَدN<ࣰ>اۗ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ>ۖ بَل لَّهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ كُلّX<ࣱ> لَّهُT<ۥ> قَT<ـٰ>نِتُJ<و>نَ A<۝>D<١١٦>",
"بَدِیعُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ وَإِذَا قَضَىW<ٰۤ> أَمۡR<ر>N<ࣰ>ا فَإِN<نَّ>مَا یَقُولُ لَهُT<ۥ> كُN<ن> فَیَكُJ<و>نُ A<۝>D<١١٧>",
"وَقَالَ ٱلَّذِینَ لَا یَعۡلَمُونَ لَوۡلَا یُكَلِّمُنَا ٱللَّهُ أَوۡ تَأۡتِینَW<اۤ> ءَایَةN<ࣱ>ۗ كَذَT< ٰ>لِكَ قَالَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِم N<مِّ>ثۡلَ قَوۡلِهِمۡۘ تَشَT<ـٰ>بَهَتۡ قُلُوبُهُمۡۗ قَQ<دۡ> بَیَّN<نَّ>ا ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یُ>وقِنُJ<و>نَ A<۝>D<١١٨>",
"إِN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ بِX<ٱ>لۡحَقِّ بَشِیR<ر>X<ࣰ>ا N<وَ>نَذِیR<ر>X<ࣰ>اۖ N<وَ>لَا تُسۡءَلُ عَنۡ أَصۡحَT<ـٰ>بِ ٱلۡجَحِJ<ی>مِ A<۝>D<١١٩>",
"وَلَN<ن> تَR<رۡ>ضَىٰ عَN<ن>كَ ٱلۡیَهُودُ وَلَا ٱX<ل>N<نَّ>صَT<ـٰ>R<رَ>ىٰ حَتَّىٰ تَتَّبِعَ مِلَّتَهُمۡۗ قُلۡ إِN<نَّ> هُدَى ٱللَّهِ هُوَ ٱلۡهُدَىٰۗ وَلَئِنِ ٱتَّبَعۡتَ أَهۡوَW<اۤ>ءَهُN<م> بَعۡدَ ٱلَّذِی جَW<اۤ>ءَكَ مِنَ ٱلۡعِلۡمِ مَا لَكَ مِنَ ٱللَّهِ مِX<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا نَصِJ<ی>رٍ A<۝>D<١٢٠>",
"ٱلَّذِینَ ءَاتَیۡنَT<ـٰ>هُمُ ٱلۡكِتَT<ـٰ>بَ یَتۡلُونَهُT<ۥ> حَقَّ تِلَاوَتِهِW<ۦۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ یُؤۡمِنُونَ بِهِT<ۦ>ۗ وَمَX<ن> N<یَ>كۡفُR<رۡ> بِهِT<ۦ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<١٢١>",
"یَT<ـٰ>بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ ٱذۡكُR<رُ>وX<ا۟> نِعۡمَتِیَ ٱلَّتِW<یۤ> أَنۡعَمۡتُ عَلَیۡكُمۡ وَأَN<نِّ>ی فَضَّلۡتُكُمۡ عَلَى ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٢٢>",
"وَX<ٱ>تَّقُوX<ا۟> یَوۡمX<ࣰ>ا لَّا تَQ<جۡ>زِی نَفۡسٌ عَن N<نَّ>فۡسN<ࣲ> شَیۡءX<ࣰ>ا N<وَ>لَا یُQ<قۡ>بَلُ مِنۡهَا عَQ<دۡ>لX<ࣱ> N<وَ>لَا تَN<ن>فَعُهَا شَفَT<ـٰ>عَةX<ࣱ> N<وَ>لَا هُمۡ یُN<ن>صَR<رُ>J<و>نَ A<۝>D<١٢٣>",
"۞ وَإِذِ ٱQ<بۡ>تَلَىW<ٰۤ> إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مَ R<رَ>بُّهُT<ۥ> بِكَلِمَT<ـٰ>تN<ࣲ> فَأَتَN<مَّ>هُN<نَّ>ۖ قَالَ إِN<نِّ>ی جَاعِلُكَ لِX<ل>N<نَّ>اسِ إِمَامN<ࣰ>اۖ قَالَ وَمِN<ن> ذُرِّیَّتِیۖ قَالَ لَا یَنَالُ عَهۡدِی ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٢٤>",
"وَإِذۡ جَعَلۡنَا ٱلۡبَیۡتَ مَثَابَةX<ࣰ> لِّX<ل>N<نَّ>اسِ وَأَمۡنX<ࣰ>ا N<وَ>X<ٱ>تَّخِذُوX<ا۟> مِX<ن> N<مَّ>قَامِ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مَ مُصَلّX<ࣰ>ىۖ N<وَ>عَهِQ<دۡ>نَW<اۤ> إِلَىW<ٰۤ> إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مَ وَإِسۡمَT<ـٰ>عِیلَ أَN<ن> طَهِّR<رَ>ا بَیۡتِیَ لِX<ل>طَّW<اۤ>ئِفِینَ وَX<ٱ>لۡعَT<ـٰ>كِفِینَ وَX<ٱ>X<ل>R<رُّ>كَّعِ ٱX<ل>سُّجُJ<و>Q<د>ِ A<۝>D<١٢٥>",
"وَإِذۡ قَالَ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مُ R<رَ>بِّ ٱQ<جۡ>عَلۡ هَT<ـٰ>ذَا بَلَدًا ءَامِنX<ࣰ>ا N<وَ>X<ٱ>R<رۡ>زُQ<قۡ> أَهۡلَهُT<ۥ> مِنَ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِ مَنۡ ءَامَنَ مِنۡهُN<م> بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِۚ قَالَ وَمَN<ن> كَفَR<رَ> فَأُمَتِّعُهُT<ۥ> قَلِیلN<ࣰ>ا ثُN<مَّ> أَضۡطَR<رُّ>هُW<ۥۤ> إِلَىٰ عَذَابِ ٱX<ل>N<نَّ>ارِۖ وَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<١٢٦>",
"وَإِذۡ یَR<رۡ>فَعُ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مُ ٱلۡقَوَاعِدَ مِنَ ٱلۡبَیۡتِ وَإِسۡمَT<ـٰ>عِیلُ R<رَ>بَّنَا تَقَبَّلۡ مِN<نَّ>W<اۤ>ۖ إِN<نَّ>كَ أَN<ن>تَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<١٢٧>",
"R<رَ>بَّنَا وَX<ٱ>Q<جۡ>عَلۡنَا مُسۡلِمَیۡنِ لَكَ وَمِN<ن> ذُرِّیَّتِنَW<اۤ> أُN<مَّ>ةX<ࣰ> N<مُّ>سۡلِمَةX<ࣰ> لَّكَ وَأَرِنَا مَنَاسِكَنَا وَتُQ<بۡ> عَلَیۡنَW<اۤ>ۖ إِN<نَّ>كَ أَN<ن>تَ ٱX<ل>تَّوَّابُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٢٨>",
"R<رَ>بَّنَا وَX<ٱ>Q<بۡ>عَثۡ فِیهِمۡ R<رَ>سُولX<ࣰ>ا N<مِّ>نۡهُمۡ یَتۡلُوX<ا۟> عَلَیۡهِمۡ ءَایَT<ـٰ>تِكَ وَیُعَلِّمُهُمُ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحِكۡمَةَ وَیُزَكِّیهِمۡۖ إِN<نَّ>كَ أَN<ن>تَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<١٢٩>",
"وَمَX<ن> N<یَ>R<رۡ>غَبُ عَX<ن> N<مِّ>لَّةِ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مَ إِلَّا مَN<ن> سَفِهَ نَفۡسَهُT<ۥ>ۚ وَلَقَدِ ٱصۡطَفَیۡنَT<ـٰ>هُ فِی ٱX<ل>دُّنۡیَاۖ وَإِN<نَّ>هُT<ۥ> فِی ٱلۡءَاخِR<رَ>ةِ لَمِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<١٣٠>",
"إِذۡ قَالَ لَهُT<ۥ> R<رَ>بُّهُW<ۥۤ> أَسۡلِمۡۖ قَالَ أَسۡلَمۡتُ لِR<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٣١>",
"وَوَصَّىٰ بِهَW<اۤ> إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مُ بَنِیهِ وَیَعۡقُوبُ یَT<ـٰ>بَنِیَّ إِN<نَّ> ٱللَّهَ ٱصۡطَفَىٰ لَكُمُ ٱX<ل>دِّینَ فَلَا تَمُوتُN<نَّ> إِلَّا وَأَN<ن>تُم N<مُّ>سۡلِمُJ<و>نَ A<۝>D<١٣٢>",
"أَمۡ كُN<ن>تُمۡ شُهَدَW<اۤ>ءَ إِذۡ حَضَR<رَ> یَعۡقُوبَ ٱلۡمَوۡتُ إِذۡ قَالَ لِبَنِیهِ مَا تَعۡبُدُونَ مِX<ن>N<ۢ> بَعۡدِیۖ قَالُوX<ا۟> نَعۡبُدُ إِلَT<ـٰ>هَكَ وَإِلَT<ـٰ>هَ ءَابَW<اۤ>ئِكَ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مَ وَإِسۡمَT<ـٰ>عِیلَ وَإِسۡحَT<ـٰ>قَ إِلَT<ـٰ>هX<ࣰ>ا N<وَ>T< ٰ>حِدX<ࣰ>ا N<وَ>نَحۡنُ لَهُT<ۥ> مُسۡلِمُJ<و>نَ A<۝>D<١٣٣>",
"تِلۡكَ أُN<مَّ>ةN<ࣱ> قَQ<دۡ> خَلَتۡۖ لَهَا مَا كَسَبَتۡ وَلَكُم N<مَّ>ا كَسَQ<بۡ>تُمۡۖ وَلَا تُسۡءَلُونَ عَN<مَّ>ا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٣٤>",
"وَقَالُوX<ا۟> كُونُوX<ا۟> هُودًا أَوۡ نَصَT<ـٰ>R<رَ>ىٰ تَهۡتَدُوX<ا۟>ۗ قُلۡ بَلۡ مِلَّةَ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مَ حَنِیفX<ࣰ>اۖ N<وَ>مَا كَانَ مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<١٣٥>",
"قُولُW<وۤ>X<ا۟> ءَامَN<نَّ>ا بِٱللَّهِ وَمَW<اۤ> أُN<ن>زِلَ إِلَیۡنَا وَمَW<اۤ> أُN<ن>زِلَ إِلَىW<ٰۤ> إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مَ وَإِسۡمَT<ـٰ>عِیلَ وَإِسۡحَT<ـٰ>قَ وَیَعۡقُوبَ وَX<ٱ>لۡأَسۡبَاطِ وَمَW<اۤ> أُوتِیَ مُوسَىٰ وَعِیسَىٰ وَمَW<اۤ> أُوتِیَ ٱX<ل>N<نَّ>بِیُّونَ مِX<ن> R<رَّ>بِّهِمۡ لَا نُفَرِّقُ بَیۡنَ أَحَدX<ࣲ> N<مِّ>نۡهُمۡ وَنَحۡنُ لَهُT<ۥ> مُسۡلِمُJ<و>نَ A<۝>D<١٣٦>",
"فَإِنۡ ءَامَنُوX<ا۟> بِمِثۡلِ مَW<اۤ> ءَامَN<ن>تُN<م> بِهِT<ۦ> فَقَدِ ٱهۡتَدَوX<ا۟>ۖ وَّإِN<ن> تَوَلَّوۡX<ا۟> فَإِN<نَّ>مَا هُمۡ فِی شِقَاقN<ࣲ>ۖ فَسَیَكۡفِیكَهُمُ ٱللَّهُۚ وَهُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<١٣٧>",
"صِQ<بۡ>غَةَ ٱللَّهِ وَمَنۡ أَحۡسَنُ مِنَ ٱللَّهِ صِQ<بۡ>غَةX<ࣰ>ۖ N<وَ>نَحۡنُ لَهُT<ۥ> عَT<ـٰ>بِدُJ<و>نَ A<۝>D<١٣٨>",
"قُلۡ أَتُحَL<اۤ>جُّونَنَا فِی ٱللَّهِ وَهُوَ R<رَ>بُّنَا وَR<رَ>بُّكُمۡ وَلَنَW<اۤ> أَعۡمَT<ـٰ>لُنَا وَلَكُمۡ أَعۡمَT<ـٰ>لُكُمۡ وَنَحۡنُ لَهُT<ۥ> مُخۡلِصُJ<و>نَ A<۝>D<١٣٩>",
"أَمۡ تَقُولُونَ إِN<نَّ> إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مَ وَإِسۡمَT<ـٰ>عِیلَ وَإِسۡحَT<ـٰ>قَ وَیَعۡقُوبَ وَX<ٱ>لۡأَسۡبَاطَ كَانُوX<ا۟> هُودًا أَوۡ نَصَT<ـٰ>R<رَ>ىٰۗ قُلۡ ءَأَN<ن>تُمۡ أَعۡلَمُ أَمِ ٱللَّهُۗ وَمَنۡ أَظۡلَمُ مِN<مَّ>N<ن> كَتَمَ شَهَT<ـٰ>دَةً عِN<ن>دَهُT<ۥ> مِنَ ٱللَّهِۗ وَمَا ٱللَّهُ بِغَT<ـٰ>فِلٍ عَN<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<١٤٠>",
"تِلۡكَ أُN<مَّ>ةN<ࣱ> قَQ<دۡ> خَلَتۡۖ لَهَا مَا كَسَبَتۡ وَلَكُم N<مَّ>ا كَسَQ<بۡ>تُمۡۖ وَلَا تُسۡءَلُونَ عَN<مَّ>ا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٤١>",
"۞ سَیَقُولُ ٱX<ل>سُّفَهَW<اۤ>ءُ مِنَ ٱX<ل>N<نَّ>اسِ مَا وَلَّX<ى>T<ٰ>هُمۡ عَN<ن> قِQ<بۡ>لَتِهِمُ ٱلَّتِی كَانُوX<ا۟> عَلَیۡهَاۚ قُل لِّلَّهِ ٱلۡمَشۡرِقُ وَX<ٱ>لۡمَغۡرِبُۚ یَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُ إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<١٤٢>",
"وَكَذَT< ٰ>لِكَ جَعَلۡنَT<ـٰ>كُمۡ أُN<مَّ>ةX<ࣰ> N<وَ>سَطX<ࣰ>ا لِّتَكُونُوX<ا۟> شُهَدَW<اۤ>ءَ عَلَى ٱX<ل>N<نَّ>اسِ وَیَكُونَ ٱX<ل>R<رَّ>سُولُ عَلَیۡكُمۡ شَهِیدX<ࣰ>اۗ N<وَ>مَا جَعَلۡنَا ٱلۡقِQ<بۡ>لَةَ ٱلَّتِی كُN<ن>تَ عَلَیۡهَW<اۤ> إِلَّا لِنَعۡلَمَ مَX<ن> N<یَ>تَّبِعُ ٱX<ل>R<رَّ>سُولَ مِN<مَّ>X<ن> N<یَ>N<ن>قَلِبُ عَلَىٰ عَقِبَیۡهِۚ وَإِN<ن> كَانَتۡ لَكَبِیR<رَ>ةً إِلَّا عَلَى ٱلَّذِینَ هَدَى ٱللَّهُۗ وَمَا كَانَ ٱللَّهُ لِیُضِیعَ إِیمَT<ـٰ>نَكُمۡۚ إِN<نَّ> ٱللَّهَ بِX<ٱ>X<ل>N<نَّ>اسِ لَR<رَ>ءُوفX<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٤٣>",
"قَQ<دۡ> نَR<رَ>ىٰ تَقَلُّبَ وَQ<جۡ>هِكَ فِی ٱX<ل>سَّمَW<اۤ>ءِۖ فَلَنُوَلِّیَN<نَّ>كَ قِQ<بۡ>لَةN<ࣰ> تَR<رۡ>ضَX<ى>T<ٰ>هَاۚ فَوَلِّ وَQ<جۡ>هَكَ شَQ<طۡ>R<رَ> ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِۚ وَحَیۡثُ مَا كُN<ن>تُمۡ فَوَلُّوX<ا۟> وُجُوهَكُمۡ شَQ<طۡ>R<رَ>هُT<ۥ>ۗ وَإِN<نَّ> ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ لَیَعۡلَمُونَ أَN<نَّ>هُ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّهِمۡۗ وَمَا ٱللَّهُ بِغَT<ـٰ>فِلٍ عَN<مَّ>ا یَعۡمَلُJ<و>نَ A<۝>D<١٤٤>",
"وَلَئِنۡ أَتَیۡتَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ بِكُلِّ ءَایَةX<ࣲ> N<مَّ>ا تَبِعُوX<ا۟> قِQ<بۡ>لَتَكَۚ وَمَW<اۤ> أَN<ن>تَ بِتَابِعN<ࣲ> قِQ<بۡ>لَتَهُمۡۚ وَمَا بَعۡضُهُN<م> بِتَابِعN<ࣲ> قِQ<بۡ>لَةَ بَعۡضX<ࣲ>ۚ N<وَ>لَئِنِ ٱتَّبَعۡتَ أَهۡوَW<اۤ>ءَهُم N<مِّ>X<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَكَ مِنَ ٱلۡعِلۡمِ إِN<نَّ>كَ إِذX<ࣰ>ا لَّمِنَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٤٥>",
"ٱلَّذِینَ ءَاتَیۡنَT<ـٰ>هُمُ ٱلۡكِتَT<ـٰ>بَ یَعۡرِفُونَهُT<ۥ> كَمَا یَعۡرِفُونَ أَQ<بۡ>نَW<اۤ>ءَهُمۡۖ وَإِN<نَّ> فَرِیقX<ࣰ>ا N<مِّ>نۡهُمۡ لَیَكۡتُمُونَ ٱلۡحَقَّ وَهُمۡ یَعۡلَمُJ<و>نَ A<۝>D<١٤٦>",
"ٱلۡحَقُّ مِX<ن> R<رَّ>بِّكَ فَلَا تَكُونَN<نَّ> مِنَ ٱلۡمُمۡتَرِJ<ی>نَ A<۝>D<١٤٧>",
"وَلِكُلّX<ࣲ> N<وِ>Q<جۡ>هَةٌ هُوَ مُوَلِّیهَاۖ فَX<ٱ>سۡتَبِقُوX<ا۟> ٱلۡخَیۡR<رَ>T< ٰ>تِۚ أَیۡنَ مَا تَكُونُوX<ا۟> یَأۡتِ بِكُمُ ٱللَّهُ جَمِیعًاۚ إِN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<١٤٨>",
"وَمِنۡ حَیۡثُ خَR<رَ>Q<جۡ>تَ فَوَلِّ وَQ<جۡ>هَكَ شَQ<طۡ>R<رَ> ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِۖ وَإِN<نَّ>هُT<ۥ> لَلۡحَقُّ مِX<ن> R<رَّ>بِّكَۗ وَمَا ٱللَّهُ بِغَT<ـٰ>فِلٍ عَN<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<١٤٩>",
"وَمِنۡ حَیۡثُ خَR<رَ>Q<جۡ>تَ فَوَلِّ وَQ<جۡ>هَكَ شَQ<طۡ>R<رَ> ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِۚ وَحَیۡثُ مَا كُN<ن>تُمۡ فَوَلُّوX<ا۟> وُجُوهَكُمۡ شَQ<طۡ>R<رَ>هُT<ۥ> لِئَلَّا یَكُونَ لِX<ل>N<نَّ>اسِ عَلَیۡكُمۡ حُجَّةٌ إِلَّا ٱلَّذِینَ ظَلَمُوX<ا۟> مِنۡهُمۡ فَلَا تَخۡشَوۡهُمۡ وَX<ٱ>خۡشَوۡنِی وَلِأُتِN<مَّ> نِعۡمَتِی عَلَیۡكُمۡ وَلَعَلَّكُمۡ تَهۡتَدُJ<و>نَ A<۝>D<١٥٠>",
"كَمَW<اۤ> أَR<رۡ>سَلۡنَا فِیكُمۡ R<رَ>سُولX<ࣰ>ا N<مِّ>N<ن>كُمۡ یَتۡلُوX<ا۟> عَلَیۡكُمۡ ءَایَT<ـٰ>تِنَا وَیُزَكِّیكُمۡ وَیُعَلِّمُكُمُ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحِكۡمَةَ وَیُعَلِّمُكُم N<مَّ>ا لَمۡ تَكُونُوX<ا۟> تَعۡلَمُJ<و>نَ A<۝>D<١٥١>",
"فَX<ٱ>ذۡكُR<رُ>ونِW<یۤ> أَذۡكُR<رۡ>كُمۡ وَX<ٱ>شۡكُR<رُ>وX<ا۟> لِی وَلَا تَكۡفُR<رُ>J<و>نِ A<۝>D<١٥٢>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱسۡتَعِینُوX<ا۟> بِX<ٱ>X<ل>صَّQ<بۡ>رِ وَX<ٱ>X<ل>صَّلَX<و>T<ٰ>ةِۚ إِN<نَّ> ٱللَّهَ مَعَ ٱX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<١٥٣>",
"وَلَا تَقُولُوX<ا۟> لِمَX<ن> N<یُ>Q<قۡ>تَلُ فِی سَبِیلِ ٱللَّهِ أَمۡوَT< ٰ>تُN<ۢ>ۚ بَلۡ أَحۡیَW<اۤ>ءX<ࣱ> N<وَ>لَT<ـٰ>كِX<ن> لَّا تَشۡعُR<رُ>J<و>نَ A<۝>D<١٥٤>",
"وَلَنَQ<بۡ>لُوَN<نَّ>كُN<م> بِشَیۡءX<ࣲ> N<مِّ>نَ ٱلۡخَوۡفِ وَX<ٱ>لۡجُوعِ وَنَQ<قۡ>صX<ࣲ> N<مِّ>نَ ٱلۡأَمۡوَT< ٰ>لِ وَX<ٱ>لۡأَN<ن>فُسِ وَX<ٱ>X<ل>ثَّمَR<رَ>T< ٰ>تِۗ وَبَشِّرِ ٱX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<١٥٥>",
"ٱلَّذِینَ إِذَW<اۤ> أَصَT<ـٰ>بَتۡهُم N<مُّ>صِیبَةN<ࣱ> قَالُW<وۤ>X<ا۟> إِN<نَّ>ا لِلَّهِ وَإِN<نَّ>W<اۤ> إِلَیۡهِ R<رَ>T< ٰ>جِعُJ<و>نَ A<۝>D<١٥٦>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ عَلَیۡهِمۡ صَلَوَT< ٰ>تX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِمۡ وَR<رَ>حۡمَةX<ࣱ>ۖ N<وَ>أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُهۡتَدُJ<و>نَ A<۝>D<١٥٧>",
"۞ إِN<نَّ> ٱX<ل>صَّفَا وَX<ٱ>لۡمَR<رۡ>وَةَ مِN<ن> شَعَW<اۤ>ئِرِ ٱللَّهِۖ فَمَنۡ حَجَّ ٱلۡبَیۡتَ أَوِ ٱعۡتَمَR<رَ> فَلَا جُنَاحَ عَلَیۡهِ أَX<ن> N<یَ>طَّوَّفَ بِهِمَاۚ وَمَN<ن> تَطَوَّعَ خَیۡR<ر>N<ࣰ>ا فَإِN<نَّ> ٱللَّهَ شَاكِR<ر>ٌ عَلِJ<ی>مٌ A<۝>D<١٥٨>",
"إِN<نَّ> ٱلَّذِینَ یَكۡتُمُونَ مَW<اۤ> أَN<ن>زَلۡنَا مِنَ ٱلۡبَیِّنَT<ـٰ>تِ وَX<ٱ>لۡهُدَىٰ مِX<ن>N<ۢ> بَعۡدِ مَا بَیَّN<نَّ>T<ـٰ>هُ لِX<ل>N<نَّ>اسِ فِی ٱلۡكِتَT<ـٰ>بِ أُX<و۟>لَW<ـٰۤ>ئِكَ یَلۡعَنُهُمُ ٱللَّهُ وَیَلۡعَنُهُمُ ٱX<ل>لَّT<ـٰ>عِنُJ<و>نَ A<۝>D<١٥٩>",
"إِلَّا ٱلَّذِینَ تَابُوX<ا۟> وَأَصۡلَحُوX<ا۟> وَبَیَّنُوX<ا۟> فَأُX<و۟>لَW<ـٰۤ>ئِكَ أَتُوبُ عَلَیۡهِمۡ وَأَنَا ٱX<ل>تَّوَّابُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٦٠>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَمَاتُوX<ا۟> وَهُمۡ كُفَّاR<ر>ٌ أُX<و۟>لَW<ـٰۤ>ئِكَ عَلَیۡهِمۡ لَعۡنَةُ ٱللَّهِ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةِ وَX<ٱ>X<ل>N<نَّ>اسِ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<١٦١>",
"خَT<ـٰ>لِدِینَ فِیهَا لَا یُخَفَّفُ عَنۡهُمُ ٱلۡعَذَابُ وَلَا هُمۡ یُN<ن>ظَR<رُ>J<و>نَ A<۝>D<١٦٢>",
"وَإِلَT<ـٰ>هُكُمۡ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدX<ࣱ>ۖ لَّW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٦٣>",
"إِN<نَّ> فِی خَلۡقِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَX<ٱ>خۡتِلَT<ـٰ>فِ ٱلَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِ وَX<ٱ>لۡفُلۡكِ ٱلَّتِی تَQ<جۡ>رِی فِی ٱلۡبَحۡرِ بِمَا یَN<ن>فَعُ ٱX<ل>N<نَّ>اسَ وَمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مِX<ن> N<مَّ>W<اۤ>ءN<ࣲ> فَأَحۡیَا بِهِ ٱلۡأَR<رۡ>ضَ بَعۡدَ مَوۡتِهَا وَبَثَّ فِیهَا مِN<ن> كُلِّ دَL<اۤ>بَّةX<ࣲ> N<وَ>تَصۡرِیفِ ٱX<ل>رِّیَT<ـٰ>حِ وَX<ٱ>X<ل>سَّحَابِ ٱلۡمُسَخَّرِ بَیۡنَ ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>عۡقِلُJ<و>نَ A<۝>D<١٦٤>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یَ>تَّخِذُ مِN<ن> دُونِ ٱللَّهِ أَN<ن>دَادX<ࣰ>ا N<یُ>حِبُّونَهُمۡ كَحُبِّ ٱللَّهِۖ وَX<ٱ>لَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَشَدُّ حُبّX<ࣰ>ا لِّلَّهِۗ وَلَوۡ یَR<رَ>ى ٱلَّذِینَ ظَلَمُW<وۤ>X<ا۟> إِذۡ یَR<رَ>وۡنَ ٱلۡعَذَابَ أَN<نَّ> ٱلۡقُوَّةَ لِلَّهِ جَمِیعX<ࣰ>ا N<وَ>أَN<نَّ> ٱللَّهَ شَدِیدُ ٱلۡعَذَJ<ا>Q<ب>ِ A<۝>D<١٦٥>",
"إِذۡ تَبَR<رَّ>أَ ٱلَّذِینَ ٱتُّبِعُوX<ا۟> مِنَ ٱلَّذِینَ ٱتَّبَعُوX<ا۟> وَR<رَ>أَوُX<ا۟> ٱلۡعَذَابَ وَتَقَطَّعَتۡ بِهِمُ ٱلۡأَسۡبَJ<ا>Q<ب>ُ A<۝>D<١٦٦>",
"وَقَالَ ٱلَّذِینَ ٱتَّبَعُوX<ا۟> لَوۡ أَN<نَّ> لَنَا كَR<رَّ>ةN<ࣰ> فَنَتَبَR<رَّ>أَ مِنۡهُمۡ كَمَا تَبَR<رَّ>ءُوX<ا۟> مِN<نَّ>اۗ كَذَT< ٰ>لِكَ یُرِیهِمُ ٱللَّهُ أَعۡمَT<ـٰ>لَهُمۡ حَسَR<رَ>T< ٰ>تٍ عَلَیۡهِمۡۖ وَمَا هُN<م> بِخَT<ـٰ>رِجِینَ مِنَ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<١٦٧>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ كُلُوX<ا۟> مِN<مَّ>ا فِی ٱلۡأَR<رۡ>ضِ حَلَT<ـٰ>لN<ࣰ>ا طَیِّبX<ࣰ>ا N<وَ>لَا تَتَّبِعُوX<ا۟> خُطُوَT< ٰ>تِ ٱX<ل>شَّیۡطَT<ـٰ>نِۚ إِN<نَّ>هُT<ۥ> لَكُمۡ عَدُوّX<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<١٦٨>",
"إِN<نَّ>مَا یَأۡمُR<رُ>كُN<م> بِX<ٱ>X<ل>سُّW<وۤ>ءِ وَX<ٱ>لۡفَحۡشَW<اۤ>ءِ وَأَN<ن> تَقُولُوX<ا۟> عَلَى ٱللَّهِ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<١٦٩>",
"وَإِذَا قِیلَ لَهُمُ ٱتَّبِعُوX<ا۟> مَW<اۤ> أَN<ن>زَلَ ٱللَّهُ قَالُوX<ا۟> بَلۡ نَتَّبِعُ مَW<اۤ> أَلۡفَیۡنَا عَلَیۡهِ ءَابَW<اۤ>ءَنَW<اۤ>ۚ أَوَ لَوۡ كَانَ ءَابَW<اۤ>ؤُهُمۡ لَا یَعۡقِلُونَ شَیۡءX<ࣰ>ا N<وَ>لَا یَهۡتَدُJ<و>نَ A<۝>D<١٧٠>",
"وَمَثَلُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> كَمَثَلِ ٱلَّذِی یَنۡعِقُ بِمَا لَا یَسۡمَعُ إِلَّا دُعَW<اۤ>ءX<ࣰ> N<وَ>نِدَW<اۤ>ءN<ࣰ>ۚ صُN<مُّ>N<ۢ> بُكۡمٌ عُمۡیN<ࣱ> فَهُمۡ لَا یَعۡقِلُJ<و>نَ A<۝>D<١٧١>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> كُلُوX<ا۟> مِN<ن> طَیِّبَT<ـٰ>تِ مَا R<رَ>زَQ<قۡ>نَT<ـٰ>كُمۡ وَX<ٱ>شۡكُR<رُ>وX<ا۟> لِلَّهِ إِN<ن> كُN<ن>تُمۡ إِیَّاهُ تَعۡبُدُJ<و>نَ A<۝>D<١٧٢>",
"إِN<نَّ>مَا حَR<رَّ>مَ عَلَیۡكُمُ ٱلۡمَیۡتَةَ وَX<ٱ>X<ل>دَّمَ وَلَحۡمَ ٱلۡخِN<ن>زِیرِ وَمَW<اۤ> أُهِلَّ بِهِT<ۦ> لِغَیۡرِ ٱللَّهِۖ فَمَنِ ٱضۡطُR<رَّ> غَیۡR<رَ> بَاغX<ࣲ> N<وَ>لَا عَادN<ࣲ> فَلَW<اۤ> إِثۡمَ عَلَیۡهِۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<١٧٣>",
"إِN<نَّ> ٱلَّذِینَ یَكۡتُمُونَ مَW<اۤ> أَN<ن>زَلَ ٱللَّهُ مِنَ ٱلۡكِتَT<ـٰ>بِ وَیَشۡتَR<رُ>ونَ بِهِT<ۦ> ثَمَنN<ࣰ>ا قَلِیلًا أُX<و۟>لَW<ـٰۤ>ئِكَ مَا یَأۡكُلُونَ فِی بُطُونِهِمۡ إِلَّا ٱX<ل>N<نَّ>اR<رَ> وَلَا یُكَلِّمُهُمُ ٱللَّهُ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ وَلَا یُزَكِّیهِمۡ وَلَهُمۡ عَذَابٌ أَلِJ<ی>مٌ A<۝>D<١٧٤>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ ٱشۡتَR<رَ>وُX<ا۟> ٱX<ل>ضَّلَT<ـٰ>لَةَ بِX<ٱ>لۡهُدَىٰ وَX<ٱ>لۡعَذَابَ بِX<ٱ>لۡمَغۡفِR<رَ>ةِۚ فَمَW<اۤ> أَصۡبَR<رَ>هُمۡ عَلَى ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<١٧٥>",
"ذَT< ٰ>لِكَ بِأَN<نَّ> ٱللَّهَ نَزَّلَ ٱلۡكِتَT<ـٰ>بَ بِX<ٱ>لۡحَقِّۗ وَإِN<نَّ> ٱلَّذِینَ ٱخۡتَلَفُوX<ا۟> فِی ٱلۡكِتَT<ـٰ>بِ لَفِی شِقَاقِN<ۭ> بَعِJ<ی>Q<د>ࣲ A<۝>D<١٧٦>",
"۞ لَّیۡسَ ٱلۡبِR<رَّ> أَN<ن> تُوَلُّوX<ا۟> وُجُوهَكُمۡ قِبَلَ ٱلۡمَشۡرِقِ وَX<ٱ>لۡمَغۡرِبِ وَلَT<ـٰ>كِN<نَّ> ٱلۡبِR<رَّ> مَنۡ ءَامَنَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةِ وَX<ٱ>لۡكِتَT<ـٰ>بِ وَX<ٱ>X<ل>N<نَّ>بِیِّT<ـۧ>نَ وَءَاتَى ٱلۡمَالَ عَلَىٰ حُبِّهِT<ۦ> ذَوِی ٱلۡقُR<رۡ>بَىٰ وَX<ٱ>لۡیَتَT<ـٰ>مَىٰ وَX<ٱ>لۡمَسَT<ـٰ>كِینَ وَX<ٱ>Q<بۡ>نَ ٱX<ل>سَّبِیلِ وَX<ٱ>X<ل>سَّW<اۤ>ئِلِینَ وَفِی ٱX<ل>رِّقَابِ وَأَقَامَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتَى ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَX<ٱ>لۡمُوفُونَ بِعَهۡدِهِمۡ إِذَا عَT<ـٰ>هَدُوX<ا۟>ۖ وَX<ٱ>X<ل>صَّT<ـٰ>بِرِینَ فِی ٱلۡبَأۡسَW<اۤ>ءِ وَX<ٱ>X<ل>ضَّR<رَّ>W<اۤ>ءِ وَحِینَ ٱلۡبَأۡسِۗ أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ صَدَقُوX<ا۟>ۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُتَّقُJ<و>نَ A<۝>D<١٧٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> كُتِبَ عَلَیۡكُمُ ٱلۡقِصَاصُ فِی ٱلۡقَتۡلَىۖ ٱلۡحُR<رُّ> بِX<ٱ>لۡحُرِّ وَX<ٱ>لۡعَQ<بۡ>دُ بِX<ٱ>لۡعَQ<بۡ>دِ وَX<ٱ>لۡأُN<ن>ثَىٰ بِX<ٱ>لۡأُN<ن>ثَىٰۚ فَمَنۡ عُفِیَ لَهُT<ۥ> مِنۡ أَخِیهِ شَیۡءN<ࣱ> فَX<ٱ>تِّبَاعُN<ۢ> بِX<ٱ>لۡمَعۡR<رُ>وفِ وَأَدَW<اۤ>ءٌ إِلَیۡهِ بِإِحۡسَT<ـٰ>نN<ࣲ>ۗ ذَT< ٰ>لِكَ تَخۡفِیفX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ وَR<رَ>حۡمَةN<ࣱ>ۗ فَمَنِ ٱعۡتَدَىٰ بَعۡدَ ذَT< ٰ>لِكَ فَلَهُT<ۥ> عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<١٧٨>",
"وَلَكُمۡ فِی ٱلۡقِصَاصِ حَیَX<و>T<ٰ>ةX<ࣱ> N<یَ>W<ـٰۤ>أُX<و۟>لِی ٱلۡأَلۡبَT<ـٰ>بِ لَعَلَّكُمۡ تَتَّقُJ<و>نَ A<۝>D<١٧٩>",
"كُتِبَ عَلَیۡكُمۡ إِذَا حَضَR<رَ> أَحَدَكُمُ ٱلۡمَوۡتُ إِN<ن> تَR<رَ>كَ خَیۡR<ر>ًا ٱلۡوَصِیَّةُ لِلۡوَT< ٰ>لِدَیۡنِ وَX<ٱ>لۡأَQ<قۡ>R<رَ>بِینَ بِX<ٱ>لۡمَعۡR<رُ>وفِۖ حَقًّا عَلَى ٱلۡمُتَّقِJ<ی>نَ A<۝>D<١٨٠>",
"فَمَX<ن>N<ۢ> بَدَّلَهُT<ۥ> بَعۡدَ مَا سَمِعَهُT<ۥ> فَإِN<نَّ>مَW<اۤ> إِثۡمُهُT<ۥ> عَلَى ٱلَّذِینَ یُبَدِّلُونَهُW<ۥۤ>ۚ إِN<نَّ> ٱللَّهَ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<١٨١>",
"فَمَنۡ خَافَ مِX<ن> N<مُّ>وصN<ࣲ> جَنَفًا أَوۡ إِثۡمN<ࣰ>ا فَأَصۡلَحَ بَیۡنَهُمۡ فَلَW<اۤ> إِثۡمَ عَلَیۡهِۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٨٢>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> كُتِبَ عَلَیۡكُمُ ٱX<ل>صِّیَامُ كَمَا كُتِبَ عَلَى ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِكُمۡ لَعَلَّكُمۡ تَتَّقُJ<و>نَ A<۝>D<١٨٣>",
"أَیَّامX<ࣰ>ا N<مَّ>عۡدُودَT< ٰ>تN<ࣲ>ۚ فَمَN<ن> كَانَ مِN<ن>كُم N<مَّ>رِیضًا أَوۡ عَلَىٰ سَفَرN<ࣲ> فَعِدَّةX<ࣱ> N<مِّ>نۡ أَیَّامٍ أُخَR<رَ>ۚ وَعَلَى ٱلَّذِینَ یُطِیقُونَهُT<ۥ> فِQ<دۡ>یَةN<ࣱ> طَعَامُ مِسۡكِینN<ࣲ>ۖ فَمَN<ن> تَطَوَّعَ خَیۡR<ر>N<ࣰ>ا فَهُوَ خَیۡR<ر>X<ࣱ> لَّهُT<ۥ>ۚ وَأَN<ن> تَصُومُوX<ا۟> خَیۡR<ر>X<ࣱ> لَّكُمۡ إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<١٨٤>",
"شَهۡR<رُ> R<رَ>مَضَانَ ٱلَّذِW<یۤ> أُN<ن>زِلَ فِیهِ ٱلۡقُR<رۡ>ءَانُ هُدX<ࣰ>ى لِّX<ل>N<نَّ>اسِ وَبَیِّنَT<ـٰ>تX<ࣲ> N<مِّ>نَ ٱلۡهُدَىٰ وَX<ٱ>لۡفُR<رۡ>قَانِۚ فَمَN<ن> شَهِدَ مِN<ن>كُمُ ٱX<ل>شَّهۡR<رَ> فَلۡیَصُمۡهُۖ وَمَN<ن> كَانَ مَرِیضًا أَوۡ عَلَىٰ سَفَرN<ࣲ> فَعِدَّةX<ࣱ> N<مِّ>نۡ أَیَّامٍ أُخَR<رَ>ۗ یُرِیدُ ٱللَّهُ بِكُمُ ٱلۡیُسۡR<رَ> وَلَا یُرِیدُ بِكُمُ ٱلۡعُسۡR<رَ> وَلِتُكۡمِلُوX<ا۟> ٱلۡعِدَّةَ وَلِتُكَبِّR<رُ>وX<ا۟> ٱللَّهَ عَلَىٰ مَا هَدَX<ى>T<ٰ>كُمۡ وَلَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<١٨٥>",
"وَإِذَا سَأَلَكَ عِبَادِی عَN<نِّ>ی فَإِN<نِّ>ی قَرِیبٌۖ أُجِیبُ دَعۡوَةَ ٱX<ل>دَّاعِ إِذَا دَعَانِۖ فَلۡیَسۡتَجِیبُوX<ا۟> لِی وَلۡیُؤۡمِنُوX<ا۟> بِی لَعَلَّهُمۡ یَR<رۡ>شُدُJ<و>نَ A<۝>D<١٨٦>",
"أُحِلَّ لَكُمۡ لَیۡلَةَ ٱX<ل>صِّیَامِ ٱX<ل>R<رَّ>فَثُ إِلَىٰ نِسَW<اۤ>ئِكُمۡۚ هُN<نَّ> لِبَاسX<ࣱ> لَّكُمۡ وَأَN<ن>تُمۡ لِبَاسX<ࣱ> لَّهُN<نَّ>ۗ عَلِمَ ٱللَّهُ أَN<نَّ>كُمۡ كُN<ن>تُمۡ تَخۡتَانُونَ أَN<ن>فُسَكُمۡ فَتَابَ عَلَیۡكُمۡ وَعَفَا عَN<ن>كُمۡۖ فَX<ٱ>لۡءَT<ـٰ>نَ بَT<ـٰ>شِR<رُ>وهُN<نَّ> وَX<ٱ>Q<بۡ>تَغُوX<ا۟> مَا كَتَبَ ٱللَّهُ لَكُمۡۚ وَكُلُوX<ا۟> وَX<ٱ>شۡR<رَ>بُوX<ا۟> حَتَّىٰ یَتَبَیَّنَ لَكُمُ ٱلۡخَیۡطُ ٱلۡأَQ<بۡ>یَضُ مِنَ ٱلۡخَیۡطِ ٱلۡأَسۡوَدِ مِنَ ٱلۡفَQ<جۡ>رِۖ ثُN<مَّ> أَتِN<مُّ>وX<ا۟> ٱX<ل>صِّیَامَ إِلَى ٱلَّیۡلِۚ وَلَا تُبَT<ـٰ>شِR<رُ>وهُN<نَّ> وَأَN<ن>تُمۡ عَT<ـٰ>كِفُونَ فِی ٱلۡمَسَT<ـٰ>جِدِۗ تِلۡكَ حُدُودُ ٱللَّهِ فَلَا تَQ<قۡ>R<رَ>بُوهَاۗ كَذَT< ٰ>لِكَ یُبَیِّنُ ٱللَّهُ ءَایَT<ـٰ>تِهِT<ۦ> لِX<ل>N<نَّ>اسِ لَعَلَّهُمۡ یَتَّقُJ<و>نَ A<۝>D<١٨٧>",
"وَلَا تَأۡكُلُW<وۤ>X<ا۟> أَمۡوَT< ٰ>لَكُN<م> بَیۡنَكُN<م> بِX<ٱ>لۡبَT<ـٰ>طِلِ وَتُQ<دۡ>لُوX<ا۟> بِهَW<اۤ> إِلَى ٱلۡحُكَّامِ لِتَأۡكُلُوX<ا۟> فَرِیقX<ࣰ>ا N<مِّ>نۡ أَمۡوَT< ٰ>لِ ٱX<ل>N<نَّ>اسِ بِX<ٱ>لۡإِثۡمِ وَأَN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<١٨٨>",
"۞ یَسۡءَلُونَكَ عَنِ ٱلۡأَهِلَّةِۖ قُلۡ هِیَ مَوَT< ٰ>قِیتُ لِX<ل>N<نَّ>اسِ وَX<ٱ>لۡحَجِّۗ وَلَیۡسَ ٱلۡبِR<رُّ> بِأَN<ن> تَأۡتُوX<ا۟> ٱلۡبُیُوتَ مِN<ن> ظُهُورِهَا وَلَT<ـٰ>كِN<نَّ> ٱلۡبِR<رَّ> مَنِ ٱتَّقَىٰۗ وَأۡتُوX<ا۟> ٱلۡبُیُوتَ مِنۡ أَQ<بۡ>وَT< ٰ>بِهَاۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ لَعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<١٨٩>",
"وَقَT<ـٰ>تِلُوX<ا۟> فِی سَبِیلِ ٱللَّهِ ٱلَّذِینَ یُقَT<ـٰ>تِلُونَكُمۡ وَلَا تَعۡتَدُW<وۤ>X<ا۟>ۚ إِN<نَّ> ٱللَّهَ لَا یُحِبُّ ٱلۡمُعۡتَدِJ<ی>نَ A<۝>D<١٩٠>",
"وَX<ٱ>Q<قۡ>تُلُوهُمۡ حَیۡثُ ثَقِفۡتُمُوهُمۡ وَأَخۡرِجُوهُم N<مِّ>نۡ حَیۡثُ أَخۡR<رَ>جُوكُمۡۚ وَX<ٱ>لۡفِتۡنَةُ أَشَدُّ مِنَ ٱلۡقَتۡلِۚ وَلَا تُقَT<ـٰ>تِلُوهُمۡ عِN<ن>دَ ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِ حَتَّىٰ یُقَT<ـٰ>تِلُوكُمۡ فِیهِۖ فَإِN<ن> قَT<ـٰ>تَلُوكُمۡ فَX<ٱ>Q<قۡ>تُلُوهُمۡۗ كَذَT< ٰ>لِكَ جَزَW<اۤ>ءُ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٩١>",
"فَإِنِ ٱN<ن>تَهَوۡX<ا۟> فَإِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٩٢>",
"وَقَT<ـٰ>تِلُوهُمۡ حَتَّىٰ لَا تَكُونَ فِتۡنَةX<ࣱ> N<وَ>یَكُونَ ٱX<ل>دِّینُ لِلَّهِۖ فَإِنِ ٱN<ن>تَهَوۡX<ا۟> فَلَا عُQ<دۡ>وَT< ٰ>نَ إِلَّا عَلَى ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٩٣>",
"ٱX<ل>شَّهۡR<رُ> ٱلۡحَR<رَ>امُ بِX<ٱ>X<ل>شَّهۡرِ ٱلۡحَR<رَ>امِ وَX<ٱ>لۡحُR<رُ>مَT<ـٰ>تُ قِصَاصN<ࣱ>ۚ فَمَنِ ٱعۡتَدَىٰ عَلَیۡكُمۡ فَX<ٱ>عۡتَدُوX<ا۟> عَلَیۡهِ بِمِثۡلِ مَا ٱعۡتَدَىٰ عَلَیۡكُمۡۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ مَعَ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<١٩٤>",
"وَأَN<ن>فِقُوX<ا۟> فِی سَبِیلِ ٱللَّهِ وَلَا تُلۡقُوX<ا۟> بِأَیۡدِیكُمۡ إِلَى ٱX<ل>تَّهۡلُكَةِ وَأَحۡسِنُW<وۤ>X<ا۟>ۚ إِN<نَّ> ٱللَّهَ یُحِبُّ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٩٥>",
"وَأَتِN<مُّ>وX<ا۟> ٱلۡحَجَّ وَX<ٱ>لۡعُمۡR<رَ>ةَ لِلَّهِۚ فَإِنۡ أُحۡصِرۡتُمۡ فَمَا ٱسۡتَیۡسَR<رَ> مِنَ ٱلۡهَQ<دۡ>یِۖ وَلَا تَحۡلِقُوX<ا۟> R<رُ>ءُوسَكُمۡ حَتَّىٰ یَQ<بۡ>لُغَ ٱلۡهَQ<دۡ>یُ مَحِلَّهُT<ۥ>ۚ فَمَN<ن> كَانَ مِN<ن>كُم N<مَّ>رِیضًا أَوۡ بِهِW<ۦۤ> أَذX<ࣰ>ى N<مِّ>X<ن> R<رَّ>أۡسِهِT<ۦ> فَفِQ<دۡ>یَةX<ࣱ> N<مِّ>N<ن> صِیَامٍ أَوۡ صَدَقَةٍ أَوۡ نُسُكN<ࣲ>ۚ فَإِذَW<اۤ> أَمِN<ن>تُمۡ فَمَN<ن> تَمَتَّعَ بِX<ٱ>لۡعُمۡR<رَ>ةِ إِلَى ٱلۡحَجِّ فَمَا ٱسۡتَیۡسَR<رَ> مِنَ ٱلۡهَQ<دۡ>یِۚ فَمَX<ن> لَّمۡ یَجِQ<دۡ> فَصِیَامُ ثَلَT<ـٰ>ثَةِ أَیَّامN<ࣲ> فِی ٱلۡحَجِّ وَسَQ<بۡ>عَةٍ إِذَا R<رَ>جَعۡتُمۡۗ تِلۡكَ عَشَR<رَ>ةN<ࣱ> كَامِلَةN<ࣱ>ۗ ذَT< ٰ>لِكَ لِمَX<ن> لَّمۡ یَكُنۡ أَهۡلُهُT<ۥ> حَاضِرِی ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<١٩٦>",
"ٱلۡحَجُّ أَشۡهُR<ر>X<ࣱ> N<مَّ>عۡلُومَT<ـٰ>تN<ࣱ>ۚ فَمَN<ن> فَR<رَ>ضَ فِیهِN<نَّ> ٱلۡحَجَّ فَلَا R<رَ>فَثَ وَلَا فُسُوقَ وَلَا جِدَالَ فِی ٱلۡحَجِّۗ وَمَا تَفۡعَلُوX<ا۟> مِنۡ خَیۡرX<ࣲ> N<یَ>عۡلَمۡهُ ٱللَّهُۗ وَتَزَوَّدُوX<ا۟> فَإِN<نَّ> خَیۡR<رَ> ٱX<ل>زَّادِ ٱX<ل>تَّQ<قۡ>وَىٰۖ وَX<ٱ>تَّقُونِ یَW<ـٰۤ>أُX<و۟>لِی ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<١٩٧>",
"لَیۡسَ عَلَیۡكُمۡ جُنَاحٌ أَN<ن> تَQ<بۡ>تَغُوX<ا۟> فَضۡلX<ࣰ>ا N<مِّ>X<ن> R<رَّ>بِّكُمۡۚ فَإِذَW<اۤ> أَفَضۡتُم N<مِّ>نۡ عَR<رَ>فَT<ـٰ>تN<ࣲ> فَX<ٱ>ذۡكُR<رُ>وX<ا۟> ٱللَّهَ عِN<ن>دَ ٱلۡمَشۡعَرِ ٱلۡحَR<رَ>امِۖ وَX<ٱ>ذۡكُR<رُ>وهُ كَمَا هَدَX<ى>T<ٰ>كُمۡ وَإِN<ن> كُN<ن>تُم N<مِّ>N<ن> قَQ<بۡ>لِهِT<ۦ> لَمِنَ ٱX<ل>ضَّL<اۤ>لِّJ<ی>نَ A<۝>D<١٩٨>",
"ثُN<مَّ> أَفِیضُوX<ا۟> مِنۡ حَیۡثُ أَفَاضَ ٱX<ل>N<نَّ>اسُ وَX<ٱ>سۡتَغۡفِR<رُ>وX<ا۟> ٱللَّهَۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٩٩>",
"فَإِذَا قَضَیۡتُم N<مَّ>نَT<ـٰ>سِكَكُمۡ فَX<ٱ>ذۡكُR<رُ>وX<ا۟> ٱللَّهَ كَذِكۡرِكُمۡ ءَابَW<اۤ>ءَكُمۡ أَوۡ أَشَدَّ ذِكۡR<ر>N<ࣰ>اۗ فَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یَ>قُولُ R<رَ>بَّنَW<اۤ> ءَاتِنَا فِی ٱX<ل>دُّنۡیَا وَمَا لَهُT<ۥ> فِی ٱلۡءَاخِR<رَ>ةِ مِنۡ خَلَT<ـٰ>Q<ق>ࣲ A<۝>D<٢٠٠>",
"وَمِنۡهُم N<مَّ>X<ن> N<یَ>قُولُ R<رَ>بَّنَW<اۤ> ءَاتِنَا فِی ٱX<ل>دُّنۡیَا حَسَنَةX<ࣰ> N<وَ>فِی ٱلۡءَاخِR<رَ>ةِ حَسَنَةX<ࣰ> N<وَ>قِنَا عَذَابَ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٢٠١>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ نَصِیبX<ࣱ> N<مِّ>N<مَّ>ا كَسَبُوX<ا۟>ۚ وَٱللَّهُ سَرِیعُ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<٢٠٢>",
"۞ وَX<ٱ>ذۡكُR<رُ>وX<ا۟> ٱللَّهَ فِW<یۤ> أَیَّامX<ࣲ> N<مَّ>عۡدُودَT< ٰ>تN<ࣲ>ۚ فَمَN<ن> تَعَجَّلَ فِی یَوۡمَیۡنِ فَلَW<اۤ> إِثۡمَ عَلَیۡهِ وَمَN<ن> تَأَخَّR<رَ> فَلَW<اۤ> إِثۡمَ عَلَیۡهِۖ لِمَنِ ٱتَّقَىٰۗ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ>كُمۡ إِلَیۡهِ تُحۡشَR<رُ>J<و>نَ A<۝>D<٢٠٣>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یُ>عۡجِبُكَ قَوۡلُهُT<ۥ> فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَیُشۡهِدُ ٱللَّهَ عَلَىٰ مَا فِی قَلۡبِهِT<ۦ> وَهُوَ أَلَدُّ ٱلۡخِصَJ<ا>مِ A<۝>D<٢٠٤>",
"وَإِذَا تَوَلَّىٰ سَعَىٰ فِی ٱلۡأَR<رۡ>ضِ لِیُفۡسِدَ فِیهَا وَیُهۡلِكَ ٱلۡحَR<رۡ>ثَ وَX<ٱ>X<ل>N<نَّ>سۡلَۗ وَٱللَّهُ لَا یُحِبُّ ٱلۡفَسَJ<ا>Q<د>َ A<۝>D<٢٠٥>",
"وَإِذَا قِیلَ لَهُ ٱتَّقِ ٱللَّهَ أَخَذَتۡهُ ٱلۡعِزَّةُ بِX<ٱ>لۡإِثۡمِۚ فَحَسۡبُهُT<ۥ> جَهَN<نَّ>مُۖ وَلَبِئۡسَ ٱلۡمِهَJ<ا>Q<د>ُ A<۝>D<٢٠٦>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یَ>شۡرِی نَفۡسَهُ ٱQ<بۡ>تِغَW<اۤ>ءَ مَR<رۡ>ضَاتِ ٱللَّهِۗ وَٱللَّهُ R<رَ>ءُوفُN<ۢ> بِX<ٱ>لۡعِبَJ<ا>Q<د>ِ A<۝>D<٢٠٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱQ<دۡ>خُلُوX<ا۟> فِی ٱX<ل>سِّلۡمِ كَL<اۤ>فَّةX<ࣰ> N<وَ>لَا تَتَّبِعُوX<ا۟> خُطُوَT< ٰ>تِ ٱX<ل>شَّیۡطَT<ـٰ>نِۚ إِN<نَّ>هُT<ۥ> لَكُمۡ عَدُوّX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٢٠٨>",
"فَإِN<ن> زَلَلۡتُم N<مِّ>X<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَتۡكُمُ ٱلۡبَیِّنَT<ـٰ>تُ فَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ عَزِیزٌ حَكِJ<ی>مٌ A<۝>D<٢٠٩>",
"هَلۡ یَN<ن>ظُR<رُ>ونَ إِلَّW<اۤ> أَX<ن> N<یَ>أۡتِیَهُمُ ٱللَّهُ فِی ظُلَلX<ࣲ> N<مِّ>نَ ٱلۡغَمَامِ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ وَقُضِیَ ٱلۡأَمۡR<رُ>ۚ وَإِلَى ٱللَّهِ تُR<رۡ>جَعُ ٱلۡأُمُJ<و>R<ر>ُ A<۝>D<٢١٠>",
"سَلۡ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ كَمۡ ءَاتَیۡنَT<ـٰ>هُم N<مِّ>نۡ ءَایَةِN<ۭ> بَیِّنَةX<ࣲ>ۗ N<وَ>مَX<ن> N<یُ>بَدِّلۡ نِعۡمَةَ ٱللَّهِ مِX<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَتۡهُ فَإِN<نَّ> ٱللَّهَ شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<٢١١>",
"زُیِّنَ لِلَّذِینَ كَفَR<رُ>وX<ا۟> ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَا وَیَسۡخَR<رُ>ونَ مِنَ ٱلَّذِینَ ءَامَنُوX<ا۟>ۘ وَX<ٱ>لَّذِینَ ٱتَّقَوۡX<ا۟> فَوۡقَهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۗ وَٱللَّهُ یَR<رۡ>زُقُ مَX<ن> N<یَ>شَW<اۤ>ءُ بِغَیۡرِ حِسَJ<ا>Q<ب>ࣲ A<۝>D<٢١٢>",
"كَانَ ٱX<ل>N<نَّ>اسُ أُN<مَّ>ةX<ࣰ> N<وَ>T< ٰ>حِدَةN<ࣰ> فَبَعَثَ ٱللَّهُ ٱX<ل>N<نَّ>بِیِّT<ـۧ>نَ مُبَشِّرِینَ وَمُN<ن>ذِرِینَ وَأَN<ن>زَلَ مَعَهُمُ ٱلۡكِتَT<ـٰ>بَ بِX<ٱ>لۡحَقِّ لِیَحۡكُمَ بَیۡنَ ٱX<ل>N<نَّ>اسِ فِیمَا ٱخۡتَلَفُوX<ا۟> فِیهِۚ وَمَا ٱخۡتَلَفَ فِیهِ إِلَّا ٱلَّذِینَ أُوتُوهُ مِX<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَتۡهُمُ ٱلۡبَیِّنَT<ـٰ>تُ بَغۡیَN<ۢ>ا بَیۡنَهُمۡۖ فَهَدَى ٱللَّهُ ٱلَّذِینَ ءَامَنُوX<ا۟> لِمَا ٱخۡتَلَفُوX<ا۟> فِیهِ مِنَ ٱلۡحَقِّ بِإِذۡنِهِT<ۦ>ۗ وَٱللَّهُ یَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُ إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مٍ A<۝>D<٢١٣>",
"أَمۡ حَسِQ<بۡ>تُمۡ أَN<ن> تَQ<دۡ>خُلُوX<ا۟> ٱلۡجَN<نَّ>ةَ وَلَN<مَّ>ا یَأۡتِكُم N<مَّ>ثَلُ ٱلَّذِینَ خَلَوۡX<ا۟> مِN<ن> قَQ<بۡ>لِكُمۖ N<مَّ>سَّتۡهُمُ ٱلۡبَأۡسَW<اۤ>ءُ وَX<ٱ>X<ل>ضَّR<رَّ>W<اۤ>ءُ وَزُلۡزِلُوX<ا۟> حَتَّىٰ یَقُولَ ٱX<ل>R<رَّ>سُولُ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مَعَهُT<ۥ> مَتَىٰ نَصۡR<رُ> ٱللَّهِۗ أَلَW<اۤ> إِN<نَّ> نَصۡR<رَ> ٱللَّهِ قَرِJ<ی>Q<ب>ࣱ A<۝>D<٢١٤>",
"یَسۡءَلُونَكَ مَاذَا یُN<ن>فِقُونَۖ قُلۡ مَW<اۤ> أَN<ن>فَQ<قۡ>تُم N<مِّ>نۡ خَیۡرN<ࣲ> فَلِلۡوَT< ٰ>لِدَیۡنِ وَX<ٱ>لۡأَQ<قۡ>R<رَ>بِینَ وَX<ٱ>لۡیَتَT<ـٰ>مَىٰ وَX<ٱ>لۡمَسَT<ـٰ>كِینِ وَX<ٱ>Q<بۡ>نِ ٱX<ل>سَّبِیلِۗ وَمَا تَفۡعَلُوX<ا۟> مِنۡ خَیۡرN<ࣲ> فَإِN<نَّ> ٱللَّهَ بِهِT<ۦ> عَلِJ<ی>مࣱ A<۝>D<٢١٥>",
"كُتِبَ عَلَیۡكُمُ ٱلۡقِتَالُ وَهُوَ كُR<رۡ>هX<ࣱ> لَّكُمۡۖ وَعَسَىW<ٰۤ> أَN<ن> تَكۡR<رَ>هُوX<ا۟> شَیۡءX<ࣰ>ا N<وَ>هُوَ خَیۡR<ر>X<ࣱ> لَّكُمۡۖ وَعَسَىW<ٰۤ> أَN<ن> تُحِبُّوX<ا۟> شَیۡءX<ࣰ>ا N<وَ>هُوَ شَR<رّ>X<ࣱ> لَّكُمۡۗ وَٱللَّهُ یَعۡلَمُ وَأَN<ن>تُمۡ لَا تَعۡلَمُJ<و>نَ A<۝>D<٢١٦>",
"یَسۡءَلُونَكَ عَنِ ٱX<ل>شَّهۡرِ ٱلۡحَR<رَ>امِ قِتَالN<ࣲ> فِیهِۖ قُلۡ قِتَالN<ࣱ> فِیهِ كَبِیR<ر>X<ࣱ>ۚ N<وَ>صَدٌّ عَN<ن> سَبِیلِ ٱللَّهِ وَكُفۡR<رُ>N<ۢ> بِهِT<ۦ> وَX<ٱ>لۡمَسۡجِدِ ٱلۡحَR<رَ>امِ وَإِخۡR<رَ>اجُ أَهۡلِهِT<ۦ> مِنۡهُ أَكۡبَR<رُ> عِN<ن>دَ ٱللَّهِۚ وَX<ٱ>لۡفِتۡنَةُ أَكۡبَR<رُ> مِنَ ٱلۡقَتۡلِۗ وَلَا یَزَالُونَ یُقَT<ـٰ>تِلُونَكُمۡ حَتَّىٰ یَR<رُ>دُّوكُمۡ عَN<ن> دِینِكُمۡ إِنِ ٱسۡتَطَT<ـٰ>عُوX<ا۟>ۚ وَمَX<ن> N<یَ>R<رۡ>تَدِQ<دۡ> مِN<ن>كُمۡ عَN<ن> دِینِهِT<ۦ> فَیَمُتۡ وَهُوَ كَافِR<ر>N<ࣱ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ حَبِطَتۡ أَعۡمَT<ـٰ>لُهُمۡ فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٢١٧>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَX<ٱ>لَّذِینَ هَاجَR<رُ>وX<ا۟> وَجَT<ـٰ>هَدُوX<ا۟> فِی سَبِیلِ ٱللَّهِ أُX<و۟>لَW<ـٰۤ>ئِكَ یَR<رۡ>جُونَ R<رَ>حۡمَتَ ٱللَّهِۚ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٢١٨>",
"۞ یَسۡءَلُونَكَ عَنِ ٱلۡخَمۡرِ وَX<ٱ>لۡمَیۡسِرِۖ قُلۡ فِیهِمَW<اۤ> إِثۡمN<ࣱ> كَبِیR<ر>X<ࣱ> N<وَ>مَنَT<ـٰ>فِعُ لِX<ل>N<نَّ>اسِ وَإِثۡمُهُمَW<اۤ> أَكۡبَR<رُ> مِن N<نَّ>فۡعِهِمَاۗ وَیَسۡءَلُونَكَ مَاذَا یُN<ن>فِقُونَۖ قُلِ ٱلۡعَفۡوَۗ كَذَT< ٰ>لِكَ یُبَیِّنُ ٱللَّهُ لَكُمُ ٱلۡءَایَT<ـٰ>تِ لَعَلَّكُمۡ تَتَفَكَّR<رُ>J<و>نَ A<۝>D<٢١٩>",
"فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِۗ وَیَسۡءَلُونَكَ عَنِ ٱلۡیَتَT<ـٰ>مَىٰۖ قُلۡ إِصۡلَاحX<ࣱ> لَّهُمۡ خَیۡR<ر>X<ࣱ>ۖ N<وَ>إِN<ن> تُخَالِطُوهُمۡ فَإِخۡوَT< ٰ>نُكُمۡۚ وَٱللَّهُ یَعۡلَمُ ٱلۡمُفۡسِدَ مِنَ ٱلۡمُصۡلِحِۚ وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ لَأَعۡنَتَكُمۡۚ إِN<نَّ> ٱللَّهَ عَزِیزٌ حَكِJ<ی>مࣱ A<۝>D<٢٢٠>",
"وَلَا تَN<ن>كِحُوX<ا۟> ٱلۡمُشۡرِكَT<ـٰ>تِ حَتَّىٰ یُؤۡمِN<نَّ>ۚ وَلَأَمَةX<ࣱ> N<مُّ>ؤۡمِنَةٌ خَیۡR<ر>X<ࣱ> N<مِّ>X<ن> N<مُّ>شۡرِكَةX<ࣲ> N<وَ>لَوۡ أَعۡجَبَتۡكُمۡۗ وَلَا تُN<ن>كِحُوX<ا۟> ٱلۡمُشۡرِكِینَ حَتَّىٰ یُؤۡمِنُوX<ا۟>ۚ وَلَعَQ<بۡ>دX<ࣱ> N<مُّ>ؤۡمِنٌ خَیۡR<ر>X<ࣱ> N<مِّ>X<ن> N<مُّ>شۡرِكX<ࣲ> N<وَ>لَوۡ أَعۡجَبَكُمۡۗ أُX<و۟>لَW<ـٰۤ>ئِكَ یَQ<دۡ>عُونَ إِلَى ٱX<ل>N<نَّ>ارِۖ وَٱللَّهُ یَQ<دۡ>عُW<وۤ>X<ا۟> إِلَى ٱلۡجَN<نَّ>ةِ وَX<ٱ>لۡمَغۡفِR<رَ>ةِ بِإِذۡنِهِT<ۦ>ۖ وَیُبَیِّنُ ءَایَT<ـٰ>تِهِT<ۦ> لِX<ل>N<نَّ>اسِ لَعَلَّهُمۡ یَتَذَكَّR<رُ>J<و>نَ A<۝>D<٢٢١>",
"وَیَسۡءَلُونَكَ عَنِ ٱلۡمَحِیضِۖ قُلۡ هُوَ أَذN<ࣰ>ى فَX<ٱ>عۡتَزِلُوX<ا۟> ٱX<ل>N<نِّ>سَW<اۤ>ءَ فِی ٱلۡمَحِیضِ وَلَا تَQ<قۡ>R<رَ>بُوهُN<نَّ> حَتَّىٰ یَQ<طۡ>هُR<رۡ>نَۖ فَإِذَا تَطَهَّR<رۡ>نَ فَأۡتُوهُN<نَّ> مِنۡ حَیۡثُ أَمَR<رَ>كُمُ ٱللَّهُۚ إِN<نَّ> ٱللَّهَ یُحِبُّ ٱX<ل>تَّوَّT< ٰ>بِینَ وَیُحِبُّ ٱلۡمُتَطَهِّرِJ<ی>نَ A<۝>D<٢٢٢>",
"نِسَW<اۤ>ؤُكُمۡ حَR<رۡ>ثX<ࣱ> لَّكُمۡ فَأۡتُوX<ا۟> حَR<رۡ>ثَكُمۡ أَN<نَّ>ىٰ شِئۡتُمۡۖ وَقَدِّمُوX<ا۟> لِأَN<ن>فُسِكُمۡۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ>كُم N<مُّ>لَT<ـٰ>قُوهُۗ وَبَشِّرِ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٢٢٣>",
"وَلَا تَQ<جۡ>عَلُوX<ا۟> ٱللَّهَ عُR<رۡ>ضَةX<ࣰ> لِّأَیۡمَT<ـٰ>نِكُمۡ أَN<ن> تَبَR<رُّ>وX<ا۟> وَتَتَّقُوX<ا۟> وَتُصۡلِحُوX<ا۟> بَیۡنَ ٱX<ل>N<نَّ>اسِۚ وَٱللَّهُ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<٢٢٤>",
"لَّا یُؤَاخِذُكُمُ ٱللَّهُ بِX<ٱ>X<ل>لَّغۡوِ فِW<یۤ> أَیۡمَT<ـٰ>نِكُمۡ وَلَT<ـٰ>كِX<ن> N<یُ>ؤَاخِذُكُN<م> بِمَا كَسَبَتۡ قُلُوبُكُمۡۗ وَٱللَّهُ غَفُوR<ر>ٌ حَلِJ<ی>مࣱ A<۝>D<٢٢٥>",
"لِّلَّذِینَ یُؤۡلُونَ مِن N<نِّ>سَW<اۤ>ئِهِمۡ تَR<رَ>بُّصُ أَR<رۡ>بَعَةِ أَشۡهُرN<ࣲ>ۖ فَإِN<ن> فَW<اۤ>ءُو فَإِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٢٢٦>",
"وَإِنۡ عَزَمُوX<ا۟> ٱX<ل>طَّلَT<ـٰ>قَ فَإِN<نَّ> ٱللَّهَ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<٢٢٧>",
"وَX<ٱ>لۡمُطَلَّقَT<ـٰ>تُ یَتَR<رَ>بَّصۡنَ بِأَN<ن>فُسِهِN<نَّ> ثَلَT<ـٰ>ثَةَ قُR<رُ>W<وۤ>ءX<ࣲ>ۚ N<وَ>لَا یَحِلُّ لَهُN<نَّ> أَX<ن> N<یَ>كۡتُمۡنَ مَا خَلَقَ ٱللَّهُ فِW<یۤ> أَR<رۡ>حَامِهِN<نَّ> إِN<ن> كُN<نَّ> یُؤۡمِN<نَّ> بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِۚ وَبُعُولَتُهُN<نَّ> أَحَقُّ بِR<رَ>دِّهِN<نَّ> فِی ذَT< ٰ>لِكَ إِنۡ أَR<رَ>ادُW<وۤ>X<ا۟> إِصۡلَT<ـٰ>حX<ࣰ>اۚ N<وَ>لَهُN<نَّ> مِثۡلُ ٱلَّذِی عَلَیۡهِN<نَّ> بِX<ٱ>لۡمَعۡR<رُ>وفِۚ وَلِX<ل>رِّجَالِ عَلَیۡهِN<نَّ> دَR<رَ>جَةX<ࣱ>ۗ N<وَ>ٱللَّهُ عَزِیزٌ حَكِJ<ی>مٌ A<۝>D<٢٢٨>",
"ٱX<ل>طَّلَT<ـٰ>قُ مَR<رَّ>تَانِۖ فَإِمۡسَاكُN<ۢ> بِمَعۡR<رُ>وفٍ أَوۡ تَسۡرِیحُN<ۢ> بِإِحۡسَT<ـٰ>نX<ࣲ>ۗ N<وَ>لَا یَحِلُّ لَكُمۡ أَN<ن> تَأۡخُذُوX<ا۟> مِN<مَّ>W<اۤ> ءَاتَیۡتُمُوهُN<نَّ> شَیۡءًا إِلَّW<اۤ> أَX<ن> N<یَ>خَافَW<اۤ> أَلَّا یُقِیمَا حُدُودَ ٱللَّهِۖ فَإِنۡ خِفۡتُمۡ أَلَّا یُقِیمَا حُدُودَ ٱللَّهِ فَلَا جُنَاحَ عَلَیۡهِمَا فِیمَا ٱفۡتَدَتۡ بِهِT<ۦ>ۗ تِلۡكَ حُدُودُ ٱللَّهِ فَلَا تَعۡتَدُوهَاۚ وَمَX<ن> N<یَ>تَعَدَّ حُدُودَ ٱللَّهِ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٢٢٩>",
"فَإِN<ن> طَلَّقَهَا فَلَا تَحِلُّ لَهُT<ۥ> مِX<ن>N<ۢ> بَعۡدُ حَتَّىٰ تَN<ن>كِحَ زَوۡجًا غَیۡR<رَ>هُT<ۥ>ۗ فَإِN<ن> طَلَّقَهَا فَلَا جُنَاحَ عَلَیۡهِمَW<اۤ> أَX<ن> N<یَ>تَR<رَ>اجَعَW<اۤ> إِN<ن> ظَN<نَّ>W<اۤ> أَX<ن> N<یُ>قِیمَا حُدُودَ ٱللَّهِۗ وَتِلۡكَ حُدُودُ ٱللَّهِ یُبَیِّنُهَا لِقَوۡمX<ࣲ> N<یَ>عۡلَمُJ<و>نَ A<۝>D<٢٣٠>",
"وَإِذَا طَلَّQ<قۡ>تُمُ ٱX<ل>N<نِّ>سَW<اۤ>ءَ فَبَلَغۡنَ أَجَلَهُN<نَّ> فَأَمۡسِكُوهُN<نَّ> بِمَعۡR<رُ>وفٍ أَوۡ سَرِّحُوهُN<نَّ> بِمَعۡR<رُ>وفX<ࣲ>ۚ N<وَ>لَا تُمۡسِكُوهُN<نَّ> ضِR<رَ>اR<ر>X<ࣰ>ا لِّتَعۡتَدُوX<ا۟>ۚ وَمَX<ن> N<یَ>فۡعَلۡ ذَT< ٰ>لِكَ فَقَQ<دۡ> ظَلَمَ نَفۡسَهُT<ۥ>ۚ وَلَا تَتَّخِذُW<وۤ>X<ا۟> ءَایَT<ـٰ>تِ ٱللَّهِ هُزُوX<ࣰ>اۚ N<وَ>X<ٱ>ذۡكُR<رُ>وX<ا۟> نِعۡمَتَ ٱللَّهِ عَلَیۡكُمۡ وَمَW<اۤ> أَN<ن>زَلَ عَلَیۡكُم N<مِّ>نَ ٱلۡكِتَT<ـٰ>بِ وَX<ٱ>لۡحِكۡمَةِ یَعِظُكُN<م> بِهِT<ۦ>ۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ بِكُلِّ شَیۡءٍ عَلِJ<ی>مࣱ A<۝>D<٢٣١>",
"وَإِذَا طَلَّQ<قۡ>تُمُ ٱX<ل>N<نِّ>سَW<اۤ>ءَ فَبَلَغۡنَ أَجَلَهُN<نَّ> فَلَا تَعۡضُلُوهُN<نَّ> أَX<ن> N<یَ>N<ن>كِحۡنَ أَزۡوَT< ٰ>جَهُN<نَّ> إِذَا تَR<رَ>T< ٰ>ضَوۡX<ا۟> بَیۡنَهُN<م> بِX<ٱ>لۡمَعۡR<رُ>وفِۗ ذَT< ٰ>لِكَ یُوعَظُ بِهِT<ۦ> مَN<ن> كَانَ مِN<ن>كُمۡ یُؤۡمِنُ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِۗ ذَT< ٰ>لِكُمۡ أَزۡكَىٰ لَكُمۡ وَأَQ<طۡ>هَR<رُ>ۚ وَٱللَّهُ یَعۡلَمُ وَأَN<ن>تُمۡ لَا تَعۡلَمُJ<و>نَ A<۝>D<٢٣٢>",
"۞ وَX<ٱ>لۡوَT< ٰ>لِدَT< ٰ>تُ یُR<رۡ>ضِعۡنَ أَوۡلَT<ـٰ>دَهُN<نَّ> حَوۡلَیۡنِ كَامِلَیۡنِۖ لِمَنۡ أَR<رَ>ادَ أَX<ن> N<یُ>تِN<مَّ> ٱX<ل>R<رَّ>ضَاعَةَۚ وَعَلَى ٱلۡمَوۡلُودِ لَهُT<ۥ> رِزۡقُهُN<نَّ> وَكِسۡوَتُهُN<نَّ> بِX<ٱ>لۡمَعۡR<رُ>وفِۚ لَا تُكَلَّفُ نَفۡسٌ إِلَّا وُسۡعَهَاۚ لَا تُضَL<اۤ>R<رَّ> وَT< ٰ>لِدَةُN<ۢ> بِوَلَدِهَا وَلَا مَوۡلُودX<ࣱ> لَّهُT<ۥ> بِوَلَدِهِT<ۦ>ۚ وَعَلَى ٱلۡوَارِثِ مِثۡلُ ذَT< ٰ>لِكَۗ فَإِنۡ أَR<رَ>ادَا فِصَالًا عَN<ن> تَR<رَ>اضX<ࣲ> N<مِّ>نۡهُمَا وَتَشَاوُرN<ࣲ> فَلَا جُنَاحَ عَلَیۡهِمَاۗ وَإِنۡ أَR<رَ>X<د>تُّمۡ أَN<ن> تَسۡتَR<رۡ>ضِعُW<وۤ>X<ا۟> أَوۡلَT<ـٰ>دَكُمۡ فَلَا جُنَاحَ عَلَیۡكُمۡ إِذَا سَلَّمۡتُم N<مَّ>W<اۤ> ءَاتَیۡتُN<م> بِX<ٱ>لۡمَعۡR<رُ>وفِۗ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ بِمَا تَعۡمَلُونَ بَصِJ<ی>رࣱ A<۝>D<٢٣٣>",
"وَX<ٱ>لَّذِینَ یُتَوَفَّوۡنَ مِN<ن>كُمۡ وَیَذَR<رُ>ونَ أَزۡوَT< ٰ>جX<ࣰ>ا N<یَ>تَR<رَ>بَّصۡنَ بِأَN<ن>فُسِهِN<نَّ> أَR<رۡ>بَعَةَ أَشۡهُرX<ࣲ> N<وَ>عَشۡR<ر>N<ࣰ>اۖ فَإِذَا بَلَغۡنَ أَجَلَهُN<نَّ> فَلَا جُنَاحَ عَلَیۡكُمۡ فِیمَا فَعَلۡنَ فِW<یۤ> أَN<ن>فُسِهِN<نَّ> بِX<ٱ>لۡمَعۡR<رُ>وفِۗ وَٱللَّهُ بِمَا تَعۡمَلُونَ خَبِJ<ی>رࣱ A<۝>D<٢٣٤>",
"وَلَا جُنَاحَ عَلَیۡكُمۡ فِیمَا عَR<رَّ>ضۡتُN<م> بِهِT<ۦ> مِنۡ خِQ<طۡ>بَةِ ٱX<ل>N<نِّ>سَW<اۤ>ءِ أَوۡ أَكۡنَN<ن>تُمۡ فِW<یۤ> أَN<ن>فُسِكُمۡۚ عَلِمَ ٱللَّهُ أَN<نَّ>كُمۡ سَتَذۡكُR<رُ>ونَهُN<نَّ> وَلَT<ـٰ>كِX<ن> لَّا تُوَاعِدُوهُN<نَّ> سِR<رّ>ًا إِلَّW<اۤ> أَN<ن> تَقُولُوX<ا۟> قَوۡلX<ࣰ>ا N<مَّ>عۡR<رُ>وفX<ࣰ>اۚ N<وَ>لَا تَعۡزِمُوX<ا۟> عُQ<قۡ>دَةَ ٱX<ل>N<نِّ>كَاحِ حَتَّىٰ یَQ<بۡ>لُغَ ٱلۡكِتَT<ـٰ>بُ أَجَلَهُT<ۥ>ۚ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ یَعۡلَمُ مَا فِW<یۤ> أَN<ن>فُسِكُمۡ فَX<ٱ>حۡذَR<رُ>وهُۚ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ غَفُوR<ر>ٌ حَلِJ<ی>مࣱ A<۝>D<٢٣٥>",
"لَّا جُنَاحَ عَلَیۡكُمۡ إِN<ن> طَلَّQ<قۡ>تُمُ ٱX<ل>N<نِّ>سَW<اۤ>ءَ مَا لَمۡ تَمَسُّوهُN<نَّ> أَوۡ تَفۡرِضُوX<ا۟> لَهُN<نَّ> فَرِیضَةX<ࣰ>ۚ N<وَ>مَتِّعُوهُN<نَّ> عَلَى ٱلۡمُوسِعِ قَدَR<رُ>هُT<ۥ> وَعَلَى ٱلۡمُQ<قۡ>تِرِ قَدَR<رُ>هُT<ۥ> مَتَT<ـٰ>عَN<ۢ>ا بِX<ٱ>لۡمَعۡR<رُ>وفِۖ حَقًّا عَلَى ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٢٣٦>",
"وَإِN<ن> طَلَّQ<قۡ>تُمُوهُN<نَّ> مِN<ن> قَQ<بۡ>لِ أَN<ن> تَمَسُّوهُN<نَّ> وَقَQ<دۡ> فَR<رَ>ضۡتُمۡ لَهُN<نَّ> فَرِیضَةN<ࣰ> فَنِصۡفُ مَا فَR<رَ>ضۡتُمۡ إِلَّW<اۤ> أَX<ن> N<یَ>عۡفُونَ أَوۡ یَعۡفُوَX<ا۟> ٱلَّذِی بِیَدِهِT<ۦ> عُQ<قۡ>دَةُ ٱX<ل>N<نِّ>كَاحِۚ وَأَN<ن> تَعۡفُW<وۤ>X<ا۟> أَQ<قۡ>R<رَ>بُ لِX<ل>تَّQ<قۡ>وَىٰۚ وَلَا تَN<ن>سَوُX<ا۟> ٱلۡفَضۡلَ بَیۡنَكُمۡۚ إِN<نَّ> ٱللَّهَ بِمَا تَعۡمَلُونَ بَصِJ<ی>رٌ A<۝>D<٢٣٧>",
"حَT<ـٰ>فِظُوX<ا۟> عَلَى ٱX<ل>صَّلَوَT< ٰ>تِ وَX<ٱ>X<ل>صَّلَX<و>T<ٰ>ةِ ٱلۡوُسۡطَىٰ وَقُومُوX<ا۟> لِلَّهِ قَT<ـٰ>نِتِJ<ی>نَ A<۝>D<٢٣٨>",
"فَإِنۡ خِفۡتُمۡ فَرِجَالًا أَوۡ R<رُ>كۡبَانN<ࣰ>اۖ فَإِذَW<اۤ> أَمِN<ن>تُمۡ فَX<ٱ>ذۡكُR<رُ>وX<ا۟> ٱللَّهَ كَمَا عَلَّمَكُم N<مَّ>ا لَمۡ تَكُونُوX<ا۟> تَعۡلَمُJ<و>نَ A<۝>D<٢٣٩>",
"وَX<ٱ>لَّذِینَ یُتَوَفَّوۡنَ مِN<ن>كُمۡ وَیَذَR<رُ>ونَ أَزۡوَT< ٰ>جX<ࣰ>ا N<وَ>صِیَّةX<ࣰ> لِّأَزۡوَT< ٰ>جِهِم N<مَّ>تَT<ـٰ>عًا إِلَى ٱلۡحَوۡلِ غَیۡR<رَ> إِخۡR<رَ>اجN<ࣲ>ۚ فَإِنۡ خَR<رَ>Q<جۡ>نَ فَلَا جُنَاحَ عَلَیۡكُمۡ فِی مَا فَعَلۡنَ فِW<یۤ> أَN<ن>فُسِهِN<نَّ> مِX<ن> N<مَّ>عۡR<رُ>وفX<ࣲ>ۗ N<وَ>ٱللَّهُ عَزِیزٌ حَكِJ<ی>مࣱ A<۝>D<٢٤٠>",
"وَلِلۡمُطَلَّقَT<ـٰ>تِ مَتَT<ـٰ>عُN<ۢ> بِX<ٱ>لۡمَعۡR<رُ>وفِۖ حَقًّا عَلَى ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٢٤١>",
"كَذَT< ٰ>لِكَ یُبَیِّنُ ٱللَّهُ لَكُمۡ ءَایَT<ـٰ>تِهِT<ۦ> لَعَلَّكُمۡ تَعۡقِلُJ<و>نَ A<۝>D<٢٤٢>",
"۞ أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ خَR<رَ>جُوX<ا۟> مِN<ن> دِیَT<ـٰ>رِهِمۡ وَهُمۡ أُلُوفٌ حَذَR<رَ> ٱلۡمَوۡتِ فَقَالَ لَهُمُ ٱللَّهُ مُوتُوX<ا۟> ثُN<مَّ> أَحۡیَT<ـٰ>هُمۡۚ إِN<نَّ> ٱللَّهَ لَذُو فَضۡلٍ عَلَى ٱX<ل>N<نَّ>اسِ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَشۡكُR<رُ>J<و>نَ A<۝>D<٢٤٣>",
"وَقَT<ـٰ>تِلُوX<ا۟> فِی سَبِیلِ ٱللَّهِ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<٢٤٤>",
"مَّN<ن> ذَا ٱلَّذِی یُQ<قۡ>رِضُ ٱللَّهَ قَR<رۡ>ضًا حَسَنN<ࣰ>ا فَیُضَT<ـٰ>عِفَهُT<ۥ> لَهُW<ۥۤ> أَضۡعَافN<ࣰ>ا كَثِیR<رَ>ةX<ࣰ>ۚ N<وَ>ٱللَّهُ یَQ<قۡ>بِضُ وَیَQ<بۡ>X<ص>ُۜطُ وَإِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٢٤٥>",
"أَلَمۡ تَR<رَ> إِلَى ٱلۡمَلَإِ مِX<ن>N<ۢ> بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ مِX<ن>N<ۢ> بَعۡدِ مُوسَىW<ٰۤ> إِذۡ قَالُوX<ا۟> لِنَبِیّX<ࣲ> لَّهُمُ ٱQ<بۡ>عَثۡ لَنَا مَلِكX<ࣰ>ا N<نُّ>قَT<ـٰ>تِلۡ فِی سَبِیلِ ٱللَّهِۖ قَالَ هَلۡ عَسَیۡتُمۡ إِN<ن> كُتِبَ عَلَیۡكُمُ ٱلۡقِتَالُ أَلَّا تُقَT<ـٰ>تِلُوX<ا۟>ۖ قَالُوX<ا۟> وَمَا لَنَW<اۤ> أَلَّا نُقَT<ـٰ>تِلَ فِی سَبِیلِ ٱللَّهِ وَقَQ<دۡ> أُخۡرِQ<جۡ>نَا مِN<ن> دِیَT<ـٰ>رِنَا وَأَQ<بۡ>نَW<اۤ>ئِنَاۖ فَلَN<مَّ>ا كُتِبَ عَلَیۡهِمُ ٱلۡقِتَالُ تَوَلَّوۡX<ا۟> إِلَّا قَلِیلX<ࣰ>ا N<مِّ>نۡهُمۡۚ وَٱللَّهُ عَلِیمُN<ۢ> بِX<ٱ>X<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢٤٦>",
"وَقَالَ لَهُمۡ نَبِیُّهُمۡ إِN<نَّ> ٱللَّهَ قَQ<دۡ> بَعَثَ لَكُمۡ طَالُوتَ مَلِكN<ࣰ>اۚ قَالُW<وۤ>X<ا۟> أَN<نَّ>ىٰ یَكُونُ لَهُ ٱلۡمُلۡكُ عَلَیۡنَا وَنَحۡنُ أَحَقُّ بِX<ٱ>لۡمُلۡكِ مِنۡهُ وَلَمۡ یُؤۡتَ سَعَةX<ࣰ> N<مِّ>نَ ٱلۡمَالِۚ قَالَ إِN<نَّ> ٱللَّهَ ٱصۡطَفَX<ى>T<ٰ>هُ عَلَیۡكُمۡ وَزَادَهُT<ۥ> بَسۡطَةN<ࣰ> فِی ٱلۡعِلۡمِ وَX<ٱ>لۡجِسۡمِۖ وَٱللَّهُ یُؤۡتِی مُلۡكَهُT<ۥ> مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَٱللَّهُ وَT< ٰ>سِعٌ عَلِJ<ی>مࣱ A<۝>D<٢٤٧>",
"وَقَالَ لَهُمۡ نَبِیُّهُمۡ إِN<نَّ> ءَایَةَ مُلۡكِهِW<ۦۤ> أَX<ن> N<یَ>أۡتِیَكُمُ ٱX<ل>تَّابُوتُ فِیهِ سَكِینَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ وَبَقِیَّةX<ࣱ> N<مِّ>N<مَّ>ا تَR<رَ>كَ ءَالُ مُوسَىٰ وَءَالُ هَT<ـٰ>R<رُ>ونَ تَحۡمِلُهُ ٱلۡمَلَW<ـٰۤ>ئِكَةُۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لَّكُمۡ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٢٤٨>",
"فَلَN<مَّ>ا فَصَلَ طَالُوتُ بِX<ٱ>لۡجُنُودِ قَالَ إِN<نَّ> ٱللَّهَ مُQ<بۡ>تَلِیكُN<م> بِنَهَرN<ࣲ> فَمَN<ن> شَرِبَ مِنۡهُ فَلَیۡسَ مِN<نِّ>ی وَمَX<ن> لَّمۡ یَQ<طۡ>عَمۡهُ فَإِN<نَّ>هُT<ۥ> مِN<نِّ>W<یۤ> إِلَّا مَنِ ٱغۡتَR<رَ>فَ غُR<رۡ>فَةَN<ۢ> بِیَدِهِT<ۦ>ۚ فَشَرِبُوX<ا۟> مِنۡهُ إِلَّا قَلِیلX<ࣰ>ا N<مِّ>نۡهُمۡۚ فَلَN<مَّ>ا جَاوَزَهُT<ۥ> هُوَ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مَعَهُT<ۥ> قَالُوX<ا۟> لَا طَاقَةَ لَنَا ٱلۡیَوۡمَ بِجَالُوتَ وَجُنُودِهِT<ۦ>ۚ قَالَ ٱلَّذِینَ یَظُN<نُّ>ونَ أَN<نَّ>هُم N<مُّ>لَT<ـٰ>قُوX<ا۟> ٱللَّهِ كَم N<مِّ>N<ن> فِئَةN<ࣲ> قَلِیلَةٍ غَلَبَتۡ فِئَةN<ࣰ> كَثِیR<رَ>ةَN<ۢ> بِإِذۡنِ ٱللَّهِۗ وَٱللَّهُ مَعَ ٱX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<٢٤٩>",
"وَلَN<مَّ>ا بَR<رَ>زُوX<ا۟> لِجَالُوتَ وَجُنُودِهِT<ۦ> قَالُوX<ا۟> R<رَ>بَّنَW<اۤ> أَفۡرِغۡ عَلَیۡنَا صَQ<بۡ>R<ر>X<ࣰ>ا N<وَ>ثَبِّتۡ أَQ<قۡ>دَامَنَا وَX<ٱ>N<ن>صُR<رۡ>نَا عَلَى ٱلۡقَوۡمِ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٢٥٠>",
"فَهَزَمُوهُN<م> بِإِذۡنِ ٱللَّهِ وَقَتَلَ دَاوُT<ۥ>دُ جَالُوتَ وَءَاتَX<ى>T<ٰ>هُ ٱللَّهُ ٱلۡمُلۡكَ وَX<ٱ>لۡحِكۡمَةَ وَعَلَّمَهُT<ۥ> مِN<مَّ>ا یَشَW<اۤ>ءُۗ وَلَوۡلَا دَفۡعُ ٱللَّهِ ٱX<ل>N<نَّ>اسَ بَعۡضَهُN<م> بِبَعۡضX<ࣲ> لَّفَسَدَتِ ٱلۡأَR<رۡ>ضُ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ ذُو فَضۡلٍ عَلَى ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٢٥١>",
"تِلۡكَ ءَایَT<ـٰ>تُ ٱللَّهِ نَتۡلُوهَا عَلَیۡكَ بِX<ٱ>لۡحَقِّۚ وَإِN<نَّ>كَ لَمِنَ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٢٥٢>",
"۞ تِلۡكَ ٱX<ل>R<رُّ>سُلُ فَضَّلۡنَا بَعۡضَهُمۡ عَلَىٰ بَعۡضX<ࣲ>ۘ N<مِّ>نۡهُم N<مَّ>N<ن> كَلَّمَ ٱللَّهُۖ وَR<رَ>فَعَ بَعۡضَهُمۡ دَR<رَ>جَT<ـٰ>تX<ࣲ>ۚ N<وَ>ءَاتَیۡنَا عِیسَى ٱQ<بۡ>نَ مَR<رۡ>یَمَ ٱلۡبَیِّنَT<ـٰ>تِ وَأَیَّQ<دۡ>نَT<ـٰ>هُ بِR<رُ>وحِ ٱلۡقُدُسِۗ وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ مَا ٱQ<قۡ>تَتَلَ ٱلَّذِینَ مِX<ن>N<ۢ> بَعۡدِهِم N<مِّ>X<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَتۡهُمُ ٱلۡبَیِّنَT<ـٰ>تُ وَلَT<ـٰ>كِنِ ٱخۡتَلَفُوX<ا۟> فَمِنۡهُم N<مَّ>نۡ ءَامَنَ وَمِنۡهُم N<مَّ>N<ن> كَفَR<رَ>ۚ وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ مَا ٱQ<قۡ>تَتَلُوX<ا۟> وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ یَفۡعَلُ مَا یُرِJ<ی>Q<د>ُ A<۝>D<٢٥٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَN<ن>فِقُوX<ا۟> مِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>كُم N<مِّ>N<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>أۡتِیَ یَوۡمX<ࣱ> لَّا بَیۡعN<ࣱ> فِیهِ وَلَا خُلَّةX<ࣱ> N<وَ>لَا شَفَT<ـٰ>عَةX<ࣱ>ۗ N<وَ>X<ٱ>لۡكَT<ـٰ>فِR<رُ>ونَ هُمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٢٥٤>",
"ٱللَّهُ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ ٱلۡحَیُّ ٱلۡقَیُّومُۚ لَا تَأۡخُذُهُT<ۥ> سِنَةX<ࣱ> N<وَ>لَا نَوۡمX<ࣱ>ۚ لَّهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۗ مَN<ن> ذَا ٱلَّذِی یَشۡفَعُ عِN<ن>دَهُW<ۥۤ> إِلَّا بِإِذۡنِهِT<ۦ>ۚ یَعۡلَمُ مَا بَیۡنَ أَیۡدِیهِمۡ وَمَا خَلۡفَهُمۡۖ وَلَا یُحِیطُونَ بِشَیۡءX<ࣲ> N<مِّ>نۡ عِلۡمِهِW<ۦۤ> إِلَّا بِمَا شَW<اۤ>ءَۚ وَسِعَ كُR<رۡ>سِیُّهُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَۖ وَلَا یَءُودُهُT<ۥ> حِفۡظُهُمَاۚ وَهُوَ ٱلۡعَلِیُّ ٱلۡعَظِJ<ی>مُ A<۝>D<٢٥٥>",
"لَW<اۤ> إِكۡR<رَ>اهَ فِی ٱX<ل>دِّینِۖ قَد تَّبَیَّنَ ٱX<ل>R<رُّ>شۡدُ مِنَ ٱلۡغَیِّۚ فَمَX<ن> N<یَ>كۡفُR<رۡ> بِX<ٱ>X<ل>طَّT<ـٰ>غُوتِ وَیُؤۡمِX<ن>N<ۢ> بِٱللَّهِ فَقَدِ ٱسۡتَمۡسَكَ بِX<ٱ>لۡعُR<رۡ>وَةِ ٱلۡوُثۡقَىٰ لَا ٱN<ن>فِصَامَ لَهَاۗ وَٱللَّهُ سَمِیعٌ عَلِJ<ی>مٌ A<۝>D<٢٥٦>",
"ٱللَّهُ وَلِیُّ ٱلَّذِینَ ءَامَنُوX<ا۟> یُخۡرِجُهُم N<مِّ>نَ ٱX<ل>ظُّلُمَT<ـٰ>تِ إِلَى ٱX<ل>N<نُّ>ورِۖ وَX<ٱ>لَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَوۡلِیَW<اۤ>ؤُهُمُ ٱX<ل>طَّT<ـٰ>غُوتُ یُخۡرِجُونَهُم N<مِّ>نَ ٱX<ل>N<نُّ>ورِ إِلَى ٱX<ل>ظُّلُمَT<ـٰ>تِۗ أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٢٥٧>",
"أَلَمۡ تَR<رَ> إِلَى ٱلَّذِی حَL<اۤ>جَّ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مَ فِی R<رَ>بِّهِW<ۦۤ> أَنۡ ءَاتَX<ى>T<ٰ>هُ ٱللَّهُ ٱلۡمُلۡكَ إِذۡ قَالَ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مُ R<رَ>بِّیَ ٱلَّذِی یُحۡیِT<ۦ> وَیُمِیتُ قَالَ أَنَا۠ أُحۡیِT<ۦ> وَأُمِیتُۖ قَالَ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مُ فَإِN<نَّ> ٱللَّهَ یَأۡتِی بِX<ٱ>X<ل>شَّمۡسِ مِنَ ٱلۡمَشۡرِقِ فَأۡتِ بِهَا مِنَ ٱلۡمَغۡرِبِ فَبُهِتَ ٱلَّذِی كَفَR<رَ>ۗ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢٥٨>",
"أَوۡ كَX<ٱ>لَّذِی مَR<رَّ> عَلَىٰ قَR<رۡ>یَةX<ࣲ> N<وَ>هِیَ خَاوِیَةٌ عَلَىٰ عُR<رُ>وشِهَا قَالَ أَN<نَّ>ىٰ یُحۡیِT<ۦ> هَT<ـٰ>ذِهِ ٱللَّهُ بَعۡدَ مَوۡتِهَاۖ فَأَمَاتَهُ ٱللَّهُ مِX<ا۟>ئَةَ عَامN<ࣲ> ثُN<مَّ> بَعَثَهُT<ۥ>ۖ قَالَ كَمۡ لَبِثۡتَۖ قَالَ لَبِثۡتُ یَوۡمًا أَوۡ بَعۡضَ یَوۡمN<ࣲ>ۖ قَالَ بَل لَّبِثۡتَ مِX<ا۟>ئَةَ عَامN<ࣲ> فَX<ٱ>N<ن>ظُR<رۡ> إِلَىٰ طَعَامِكَ وَشَR<رَ>ابِكَ لَمۡ یَتَسَN<نَّ>هۡۖ وَX<ٱ>N<ن>ظُR<رۡ> إِلَىٰ حِمَارِكَ وَلِنَQ<جۡ>عَلَكَ ءَایَةX<ࣰ> لِّX<ل>N<نَّ>اسِۖ وَX<ٱ>N<ن>ظُR<رۡ> إِلَى ٱلۡعِظَامِ كَیۡفَ نُN<ن>شِزُهَا ثُN<مَّ> نَكۡسُوهَا لَحۡمN<ࣰ>اۚ فَلَN<مَّ>ا تَبَیَّنَ لَهُT<ۥ> قَالَ أَعۡلَمُ أَN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٢٥٩>",
"وَإِذۡ قَالَ إِQ<بۡ>R<رَ>T< ٰ>هِT<ـۧ>مُ R<رَ>بِّ أَرِنِی كَیۡفَ تُحۡیِ ٱلۡمَوۡتَىٰۖ قَالَ أَوَ لَمۡ تُؤۡمِN<ن>ۖ قَالَ بَلَىٰ وَلَT<ـٰ>كِX<ن> لِّیَQ<طۡ>مَئِN<نَّ> قَلۡبِیۖ قَالَ فَخُذۡ أَR<رۡ>بَعَةX<ࣰ> N<مِّ>نَ ٱX<ل>طَّیۡرِ فَصُR<رۡ>هُN<نَّ> إِلَیۡكَ ثُN<مَّ> ٱQ<جۡ>عَلۡ عَلَىٰ كُلِّ جَبَلX<ࣲ> N<مِّ>نۡهُN<نَّ> جُزۡءN<ࣰ>ا ثُN<مَّ> ٱQ<دۡ>عُهُN<نَّ> یَأۡتِینَكَ سَعۡیX<ࣰ>اۚ N<وَ>X<ٱ>عۡلَمۡ أَN<نَّ> ٱللَّهَ عَزِیزٌ حَكِJ<ی>مࣱ A<۝>D<٢٦٠>",
"مَّثَلُ ٱلَّذِینَ یُN<ن>فِقُونَ أَمۡوَT< ٰ>لَهُمۡ فِی سَبِیلِ ٱللَّهِ كَمَثَلِ حَبَّةٍ أَX<ن>N<ۢ>بَتَتۡ سَQ<بۡ>عَ سَنَابِلَ فِی كُلِّ سُX<ن>N<ۢ>بُلَةX<ࣲ> N<مِّ>X<ا۟>ئَةُ حَبَّةX<ࣲ>ۗ N<وَ>ٱللَّهُ یُضَT<ـٰ>عِفُ لِمَX<ن> N<یَ>شَW<اۤ>ءُۚ وَٱللَّهُ وَT< ٰ>سِعٌ عَلِJ<ی>مٌ A<۝>D<٢٦١>",
"ٱلَّذِینَ یُN<ن>فِقُونَ أَمۡوَT< ٰ>لَهُمۡ فِی سَبِیلِ ٱللَّهِ ثُN<مَّ> لَا یُتۡبِعُونَ مَW<اۤ> أَN<ن>فَقُوX<ا۟> مَN<نّ>X<ࣰ>ا N<وَ>لَW<اۤ> أَذX<ࣰ>ى لَّهُمۡ أَQ<جۡ>R<رُ>هُمۡ عِN<ن>دَ R<رَ>بِّهِمۡ وَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٢٦٢>",
"۞ قَوۡلX<ࣱ> N<مَّ>عۡR<رُ>وفX<ࣱ> N<وَ>مَغۡفِR<رَ>ةٌ خَیۡR<ر>X<ࣱ> N<مِّ>N<ن> صَدَقَةX<ࣲ> N<یَ>تۡبَعُهَW<اۤ> أَذX<ࣰ>ىۗ N<وَ>ٱللَّهُ غَنِیٌّ حَلِJ<ی>مࣱ A<۝>D<٢٦٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تُQ<بۡ>طِلُوX<ا۟> صَدَقَT<ـٰ>تِكُN<م> بِX<ٱ>لۡمَN<نِّ> وَX<ٱ>لۡأَذَىٰ كَX<ٱ>لَّذِی یُN<ن>فِقُ مَالَهُT<ۥ> رِئَW<اۤ>ءَ ٱX<ل>N<نَّ>اسِ وَلَا یُؤۡمِنُ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِۖ فَمَثَلُهُT<ۥ> كَمَثَلِ صَفۡوَانٍ عَلَیۡهِ تُR<رَ>ابN<ࣱ> فَأَصَابَهُT<ۥ> وَابِلN<ࣱ> فَتَR<رَ>كَهُT<ۥ> صَلۡدX<ࣰ>اۖ لَّا یَQ<قۡ>دِR<رُ>ونَ عَلَىٰ شَیۡءX<ࣲ> N<مِّ>N<مَّ>ا كَسَبُوX<ا۟>ۗ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٢٦٤>",
"وَمَثَلُ ٱلَّذِینَ یُN<ن>فِقُونَ أَمۡوَT< ٰ>لَهُمُ ٱQ<بۡ>تِغَW<اۤ>ءَ مَR<رۡ>ضَاتِ ٱللَّهِ وَتَثۡبِیتX<ࣰ>ا N<مِّ>نۡ أَN<ن>فُسِهِمۡ كَمَثَلِ جَN<نَّ>ةِN<ۭ> بِR<رَ>Q<بۡ>وَةٍ أَصَابَهَا وَابِلN<ࣱ> فَءَاتَتۡ أُكُلَهَا ضِعۡفَیۡنِ فَإِX<ن> لَّمۡ یُصِQ<بۡ>هَا وَابِلN<ࣱ> فَطَلّX<ࣱ>ۗ N<وَ>ٱللَّهُ بِمَا تَعۡمَلُونَ بَصِJ<ی>رٌ A<۝>D<٢٦٥>",
"أَیَوَدُّ أَحَدُكُمۡ أَN<ن> تَكُونَ لَهُT<ۥ> جَN<نَّ>ةX<ࣱ> N<مِّ>ن N<نَّ>خِیلX<ࣲ> N<وَ>أَعۡنَابN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> لَهُT<ۥ> فِیهَا مِN<ن> كُلِّ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِ وَأَصَابَهُ ٱلۡكِبَR<رُ> وَلَهُT<ۥ> ذُرِّیَّةN<ࣱ> ضُعَفَW<اۤ>ءُ فَأَصَابَهَW<اۤ> إِعۡصَاR<ر>N<ࣱ> فِیهِ نَاR<ر>N<ࣱ> فَX<ٱ>حۡتَR<رَ>قَتۡۗ كَذَT< ٰ>لِكَ یُبَیِّنُ ٱللَّهُ لَكُمُ ٱلۡءَایَT<ـٰ>تِ لَعَلَّكُمۡ تَتَفَكَّR<رُ>J<و>نَ A<۝>D<٢٦٦>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَN<ن>فِقُوX<ا۟> مِN<ن> طَیِّبَT<ـٰ>تِ مَا كَسَQ<بۡ>تُمۡ وَمِN<مَّ>W<اۤ> أَخۡR<رَ>Q<جۡ>نَا لَكُم N<مِّ>نَ ٱلۡأَR<رۡ>ضِۖ وَلَا تَیَN<مَّ>مُوX<ا۟> ٱلۡخَبِیثَ مِنۡهُ تُN<ن>فِقُونَ وَلَسۡتُN<م> بِءَاخِذِیهِ إِلَّW<اۤ> أَN<ن> تُغۡمِضُوX<ا۟> فِیهِۚ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ غَنِیٌّ حَمِJ<ی>Q<د>ٌ A<۝>D<٢٦٧>",
"ٱX<ل>شَّیۡطَT<ـٰ>نُ یَعِدُكُمُ ٱلۡفَQ<قۡ>R<رَ> وَیَأۡمُR<رُ>كُN<م> بِX<ٱ>لۡفَحۡشَW<اۤ>ءِۖ وَٱللَّهُ یَعِدُكُم N<مَّ>غۡفِR<رَ>ةX<ࣰ> N<مِّ>نۡهُ وَفَضۡلX<ࣰ>اۗ N<وَ>ٱللَّهُ وَT< ٰ>سِعٌ عَلِJ<ی>مࣱ A<۝>D<٢٦٨>",
"یُؤۡتِی ٱلۡحِكۡمَةَ مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَمَX<ن> N<یُ>ؤۡتَ ٱلۡحِكۡمَةَ فَقَQ<دۡ> أُوتِیَ خَیۡR<ر>N<ࣰ>ا كَثِیR<ر>X<ࣰ>اۗ N<وَ>مَا یَذَّكَّR<رُ> إِلَّW<اۤ> أُX<و۟>لُوX<ا۟> ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<٢٦٩>",
"وَمَW<اۤ> أَN<ن>فَQ<قۡ>تُم N<مِّ>ن N<نَّ>فَقَةٍ أَوۡ نَذَR<رۡ>تُم N<مِّ>ن N<نَّ>ذۡرN<ࣲ> فَإِN<نَّ> ٱللَّهَ یَعۡلَمُهُT<ۥ>ۗ وَمَا لِX<ل>ظَّT<ـٰ>لِمِینَ مِنۡ أَN<ن>صَJ<ا>R<ر>ٍ A<۝>D<٢٧٠>",
"إِN<ن> تُQ<بۡ>دُوX<ا۟> ٱX<ل>صَّدَقَT<ـٰ>تِ فَنِعِN<مَّ>ا هِیَۖ وَإِN<ن> تُخۡفُوهَا وَتُؤۡتُوهَا ٱلۡفُقَR<رَ>W<اۤ>ءَ فَهُوَ خَیۡR<ر>X<ࣱ> لَّكُمۡۚ وَیُكَفِّR<رُ> عَN<ن>كُم N<مِّ>N<ن> سَیِّءَاتِكُمۡۗ وَٱللَّهُ بِمَا تَعۡمَلُونَ خَبِJ<ی>رࣱ A<۝>D<٢٧١>",
"۞ لَّیۡسَ عَلَیۡكَ هُدَX<ى>T<ٰ>هُمۡ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ یَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُۗ وَمَا تُN<ن>فِقُوX<ا۟> مِنۡ خَیۡرN<ࣲ> فَلِأَN<ن>فُسِكُمۡۚ وَمَا تُN<ن>فِقُونَ إِلَّا ٱQ<بۡ>تِغَW<اۤ>ءَ وَQ<جۡ>هِ ٱللَّهِۚ وَمَا تُN<ن>فِقُوX<ا۟> مِنۡ خَیۡرX<ࣲ> N<یُ>وَفَّ إِلَیۡكُمۡ وَأَN<ن>تُمۡ لَا تُظۡلَمُJ<و>نَ A<۝>D<٢٧٢>",
"لِلۡفُقَR<رَ>W<اۤ>ءِ ٱلَّذِینَ أُحۡصِR<رُ>وX<ا۟> فِی سَبِیلِ ٱللَّهِ لَا یَسۡتَطِیعُونَ ضَR<رۡ>بN<ࣰ>ا فِی ٱلۡأَR<رۡ>ضِ یَحۡسَبُهُمُ ٱلۡجَاهِلُ أَغۡنِیَW<اۤ>ءَ مِنَ ٱX<ل>تَّعَفُّفِ تَعۡرِفُهُN<م> بِسِیمَT<ـٰ>هُمۡ لَا یَسۡءَلُونَ ٱX<ل>N<نَّ>اسَ إِلۡحَافX<ࣰ>اۗ N<وَ>مَا تُN<ن>فِقُوX<ا۟> مِنۡ خَیۡرN<ࣲ> فَإِN<نَّ> ٱللَّهَ بِهِT<ۦ> عَلِJ<ی>مٌ A<۝>D<٢٧٣>",
"ٱلَّذِینَ یُN<ن>فِقُونَ أَمۡوَT< ٰ>لَهُN<م> بِX<ٱ>لَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِ سِR<رّ>X<ࣰ>ا N<وَ>عَلَانِیَةN<ࣰ> فَلَهُمۡ أَQ<جۡ>R<رُ>هُمۡ عِN<ن>دَ R<رَ>بِّهِمۡ وَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٢٧٤>",
"ٱلَّذِینَ یَأۡكُلُونَ ٱX<ل>رِّبَX<و>T<ٰ>X<ا۟> لَا یَقُومُونَ إِلَّا كَمَا یَقُومُ ٱلَّذِی یَتَخَبَّطُهُ ٱX<ل>شَّیۡطَT<ـٰ>نُ مِنَ ٱلۡمَسِّۚ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ قَالُW<وۤ>X<ا۟> إِN<نَّ>مَا ٱلۡبَیۡعُ مِثۡلُ ٱX<ل>رِّبَX<و>T<ٰ>X<ا۟>ۗ وَأَحَلَّ ٱللَّهُ ٱلۡبَیۡعَ وَحَR<رَّ>مَ ٱX<ل>رِّبَX<و>T<ٰ>X<ا۟>ۚ فَمَN<ن> جَW<اۤ>ءَهُT<ۥ> مَوۡعِظَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ> فَX<ٱ>N<ن>تَهَىٰ فَلَهُT<ۥ> مَا سَلَفَ وَأَمۡR<رُ>هُW<ۥۤ> إِلَى ٱللَّهِۖ وَمَنۡ عَادَ فَأُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٢٧٥>",
"یَمۡحَقُ ٱللَّهُ ٱX<ل>رِّبَX<و>T<ٰ>X<ا۟> وَیُR<رۡ>بِی ٱX<ل>صَّدَقَT<ـٰ>تِۗ وَٱللَّهُ لَا یُحِبُّ كُلَّ كَفَّارٍ أَثِJ<ی>مٍ A<۝>D<٢٧٦>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَأَقَامُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتَوُX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ لَهُمۡ أَQ<جۡ>R<رُ>هُمۡ عِN<ن>دَ R<رَ>بِّهِمۡ وَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٢٧٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱتَّقُوX<ا۟> ٱللَّهَ وَذَR<رُ>وX<ا۟> مَا بَقِیَ مِنَ ٱX<ل>رِّبَX<و>W<ٰۤ>X<ا۟> إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٢٧٨>",
"فَإِX<ن> لَّمۡ تَفۡعَلُوX<ا۟> فَأۡذَنُوX<ا۟> بِحَR<رۡ>بX<ࣲ> N<مِّ>نَ ٱللَّهِ وَR<رَ>سُولِهِT<ۦ>ۖ وَإِN<ن> تُQ<بۡ>تُمۡ فَلَكُمۡ R<رُ>ءُوسُ أَمۡوَT< ٰ>لِكُمۡ لَا تَظۡلِمُونَ وَلَا تُظۡلَمُJ<و>نَ A<۝>D<٢٧٩>",
"وَإِN<ن> كَانَ ذُو عُسۡR<رَ>ةN<ࣲ> فَنَظِR<رَ>ةٌ إِلَىٰ مَیۡسَR<رَ>ةX<ࣲ>ۚ N<وَ>أَN<ن> تَصَدَّقُوX<ا۟> خَیۡR<ر>X<ࣱ> لَّكُمۡ إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٢٨٠>",
"وَX<ٱ>تَّقُوX<ا۟> یَوۡمN<ࣰ>ا تُR<رۡ>جَعُونَ فِیهِ إِلَى ٱللَّهِۖ ثُN<مَّ> تُوَفَّىٰ كُلُّ نَفۡسX<ࣲ> N<مَّ>ا كَسَبَتۡ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<٢٨١>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا تَدَایَN<ن>تُN<م> بِدَیۡنٍ إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>N<ࣰ>ى فَX<ٱ>كۡتُبُوهُۚ وَلۡیَكۡتُب بَّیۡنَكُمۡ كَاتِبُN<ۢ> بِX<ٱ>لۡعَQ<دۡ>لِۚ وَلَا یَأۡبَ كَاتِبٌ أَX<ن> N<یَ>كۡتُبَ كَمَا عَلَّمَهُ ٱللَّهُۚ فَلۡیَكۡتُQ<بۡ> وَلۡیُمۡلِلِ ٱلَّذِی عَلَیۡهِ ٱلۡحَقُّ وَلۡیَتَّقِ ٱللَّهَ R<رَ>بَّهُT<ۥ> وَلَا یَQ<بۡ>خَسۡ مِنۡهُ شَیۡءN<ࣰ>اۚ فَإِN<ن> كَانَ ٱلَّذِی عَلَیۡهِ ٱلۡحَقُّ سَفِیهًا أَوۡ ضَعِیفًا أَوۡ لَا یَسۡتَطِیعُ أَX<ن> N<یُ>مِلَّ هُوَ فَلۡیُمۡلِلۡ وَلِیُّهُT<ۥ> بِX<ٱ>لۡعَQ<دۡ>لِۚ وَX<ٱ>سۡتَشۡهِدُوX<ا۟> شَهِیدَیۡنِ مِX<ن> رِّجَالِكُمۡۖ فَإِX<ن> لَّمۡ یَكُونَا R<رَ>جُلَیۡنِ فَR<رَ>جُلX<ࣱ> N<وَ>X<ٱ>مۡR<رَ>أَتَانِ مِN<مَّ>N<ن> تَR<رۡ>ضَوۡنَ مِنَ ٱX<ل>شُّهَدَW<اۤ>ءِ أَN<ن> تَضِلَّ إِحۡدَX<ى>T<ٰ>هُمَا فَتُذَكِّR<رَ> إِحۡدَX<ى>T<ٰ>هُمَا ٱلۡأُخۡR<رَ>ىٰۚ وَلَا یَأۡبَ ٱX<ل>شُّهَدَW<اۤ>ءُ إِذَا مَا دُعُوX<ا۟>ۚ وَلَا تَسۡءَمُW<وۤ>X<ا۟> أَN<ن> تَكۡتُبُوهُ صَغِیR<ر>ًا أَوۡ كَبِیR<ر>ًا إِلَىW<ٰۤ> أَجَلِهِT<ۦ>ۚ ذَT< ٰ>لِكُمۡ أَQ<قۡ>سَطُ عِN<ن>دَ ٱللَّهِ وَأَQ<قۡ>وَمُ لِX<ل>شَّهَT<ـٰ>دَةِ وَأَQ<دۡ>نَىW<ٰۤ> أَلَّا تَR<رۡ>تَابُW<وۤ>X<ا۟> إِلَّW<اۤ> أَN<ن> تَكُونَ تِجَT<ـٰ>R<رَ>ةً حَاضِR<رَ>ةN<ࣰ> تُدِیR<رُ>ونَهَا بَیۡنَكُمۡ فَلَیۡسَ عَلَیۡكُمۡ جُنَاحٌ أَلَّا تَكۡتُبُوهَاۗ وَأَشۡهِدُW<وۤ>X<ا۟> إِذَا تَبَایَعۡتُمۡۚ وَلَا یُضَL<اۤ>R<رَّ> كَاتِبX<ࣱ> N<وَ>لَا شَهِیدX<ࣱ>ۚ N<وَ>إِN<ن> تَفۡعَلُوX<ا۟> فَإِN<نَّ>هُT<ۥ> فُسُوقُN<ۢ> بِكُمۡۗ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَۖ وَیُعَلِّمُكُمُ ٱللَّهُۗ وَٱللَّهُ بِكُلِّ شَیۡءٍ عَلِJ<ی>مࣱ A<۝>D<٢٨٢>",
"۞ وَإِN<ن> كُN<ن>تُمۡ عَلَىٰ سَفَرX<ࣲ> N<وَ>لَمۡ تَجِدُوX<ا۟> كَاتِبN<ࣰ>ا فَرِهَT<ـٰ>نX<ࣱ> N<مَّ>Q<قۡ>بُوضَةN<ࣱ>ۖ فَإِنۡ أَمِنَ بَعۡضُكُN<م> بَعۡضN<ࣰ>ا فَلۡیُؤَدِّ ٱلَّذِی ٱؤۡتُمِنَ أَمَT<ـٰ>نَتَهُT<ۥ> وَلۡیَتَّقِ ٱللَّهَ R<رَ>بَّهُT<ۥ>ۗ وَلَا تَكۡتُمُوX<ا۟> ٱX<ل>شَّهَT<ـٰ>دَةَۚ وَمَX<ن> N<یَ>كۡتُمۡهَا فَإِN<نَّ>هُW<ۥۤ> ءَاثِمN<ࣱ> قَلۡبُهُT<ۥ>ۗ وَٱللَّهُ بِمَا تَعۡمَلُونَ عَلِJ<ی>مࣱ A<۝>D<٢٨٣>",
"لِّلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۗ وَإِN<ن> تُQ<بۡ>دُوX<ا۟> مَا فِW<یۤ> أَN<ن>فُسِكُمۡ أَوۡ تُخۡفُوهُ یُحَاسِQ<بۡ>كُN<م> بِهِ ٱللَّهُۖ فَیَغۡفِR<رُ> لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیُعَذِّبُ مَX<ن> N<یَ>شَW<اۤ>ءُۗ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<٢٨٤>",
"ءَامَنَ ٱX<ل>R<رَّ>سُولُ بِمَW<اۤ> أُN<ن>زِلَ إِلَیۡهِ مِX<ن> R<رَّ>بِّهِT<ۦ> وَX<ٱ>لۡمُؤۡمِنُونَۚ كُلٌّ ءَامَنَ بِٱللَّهِ وَمَلَW<ـٰۤ>ئِكَتِهِT<ۦ> وَكُتُبِهِT<ۦ> وَR<رُ>سُلِهِT<ۦ> لَا نُفَرِّقُ بَیۡنَ أَحَدX<ࣲ> N<مِّ>X<ن> R<رُّ>سُلِهِT<ۦ>ۚ وَقَالُوX<ا۟> سَمِعۡنَا وَأَطَعۡنَاۖ غُفۡR<رَ>انَكَ R<رَ>بَّنَا وَإِلَیۡكَ ٱلۡمَصِJ<ی>رُ A<۝>D<٢٨٥>",
"لَا یُكَلِّفُ ٱللَّهُ نَفۡسًا إِلَّا وُسۡعَهَاۚ لَهَا مَا كَسَبَتۡ وَعَلَیۡهَا مَا ٱكۡتَسَبَتۡۗ R<رَ>بَّنَا لَا تُؤَاخِذۡنَW<اۤ> إِن N<نَّ>سِینَW<اۤ> أَوۡ أَخۡطَأۡنَاۚ R<رَ>بَّنَا وَلَا تَحۡمِلۡ عَلَیۡنَW<اۤ> إِصۡR<ر>N<ࣰ>ا كَمَا حَمَلۡتَهُT<ۥ> عَلَى ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِنَاۚ R<رَ>بَّنَا وَلَا تُحَN<مِّ>لۡنَا مَا لَا طَاقَةَ لَنَا بِهِT<ۦ>ۖ وَX<ٱ>عۡفُ عَN<نَّ>ا وَX<ٱ>غۡفِرۡ لَنَا وَX<ٱ>R<رۡ>حَمۡنَW<اۤ>ۚ أَN<ن>تَ مَوۡلَX<ى>T<ٰ>نَا فَX<ٱ>N<ن>صُR<رۡ>نَا عَلَى ٱلۡقَوۡمِ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٢٨٦>",
"#اL<لۤ>L<مۤ> A<۝>D<١>",
"ٱللَّهُ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ ٱلۡحَیُّ ٱلۡقَیُّJ<و>مُ A<۝>D<٢>",
"نَزَّلَ عَلَیۡكَ ٱلۡكِتَT<ـٰ>بَ بِX<ٱ>لۡحَقِّ مُصَدِّقX<ࣰ>ا لِّمَا بَیۡنَ یَدَیۡهِ وَأَN<ن>زَلَ ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةَ وَX<ٱ>لۡإِN<ن>جِJ<ی>لَ A<۝>D<٣>",
"مِN<ن> قَQ<بۡ>لُ هُدX<ࣰ>ى لِّX<ل>N<نَّ>اسِ وَأَN<ن>زَلَ ٱلۡفُR<رۡ>قَانَۗ إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِ ٱللَّهِ لَهُمۡ عَذَابN<ࣱ> شَدِیدX<ࣱ>ۗ N<وَ>ٱللَّهُ عَزِیزN<ࣱ> ذُو ٱN<ن>تِقَJ<ا>مٍ A<۝>D<٤>",
"إِN<نَّ> ٱللَّهَ لَا یَخۡفَىٰ عَلَیۡهِ شَیۡءN<ࣱ> فِی ٱلۡأَR<رۡ>ضِ وَلَا فِی ٱX<ل>سَّمَW<اۤ>ءِ A<۝>D<٥>",
"هُوَ ٱلَّذِی یُصَوِّR<رُ>كُمۡ فِی ٱلۡأَR<رۡ>حَامِ كَیۡفَ یَشَW<اۤ>ءُۚ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٦>",
"هُوَ ٱلَّذِW<یۤ> أَN<ن>زَلَ عَلَیۡكَ ٱلۡكِتَT<ـٰ>بَ مِنۡهُ ءَایَT<ـٰ>تX<ࣱ> N<مُّ>حۡكَمَT<ـٰ>تٌ هُN<نَّ> أُN<مُّ> ٱلۡكِتَT<ـٰ>بِ وَأُخَR<رُ> مُتَشَT<ـٰ>بِهَT<ـٰ>تN<ࣱ>ۖ فَأَN<مَّ>ا ٱلَّذِینَ فِی قُلُوبِهِمۡ زَیۡغN<ࣱ> فَیَتَّبِعُونَ مَا تَشَT<ـٰ>بَهَ مِنۡهُ ٱQ<بۡ>تِغَW<اۤ>ءَ ٱلۡفِتۡنَةِ وَX<ٱ>Q<بۡ>تِغَW<اۤ>ءَ تَأۡوِیلِهِT<ۦ>ۖ وَمَا یَعۡلَمُ تَأۡوِیلَهُW<ۥۤ> إِلَّا ٱللَّهُۗ وَX<ٱ>X<ل>R<رَّ>T< ٰ>سِخُونَ فِی ٱلۡعِلۡمِ یَقُولُونَ ءَامَN<نَّ>ا بِهِT<ۦ> كُلّX<ࣱ> N<مِّ>نۡ عِN<ن>دِ R<رَ>بِّنَاۗ وَمَا یَذَّكَّR<رُ> إِلَّW<اۤ> أُX<و۟>لُوX<ا۟> ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<٧>",
"R<رَ>بَّنَا لَا تُزِغۡ قُلُوبَنَا بَعۡدَ إِذۡ هَدَیۡتَنَا وَهَQ<بۡ> لَنَا مِX<ن> لَّدُN<ن>كَ R<رَ>حۡمَةًۚ إِN<نَّ>كَ أَN<ن>تَ ٱلۡوَهَّJ<ا>Q<ب>ُ A<۝>D<٨>",
"R<رَ>بَّنَW<اۤ> إِN<نَّ>كَ جَامِعُ ٱX<ل>N<نَّ>اسِ لِیَوۡمX<ࣲ> لَّا R<رَ>یۡبَ فِیهِۚ إِN<نَّ> ٱللَّهَ لَا یُخۡلِفُ ٱلۡمِیعَJ<ا>Q<د>َ A<۝>D<٩>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَN<ن> تُغۡنِیَ عَنۡهُمۡ أَمۡوَT< ٰ>لُهُمۡ وَلَW<اۤ> أَوۡلَT<ـٰ>دُهُم N<مِّ>نَ ٱللَّهِ شَیۡءX<ࣰ>اۖ N<وَ>أُX<و۟>لَW<ـٰۤ>ئِكَ هُمۡ وَقُودُ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<١٠>",
"كَدَأۡبِ ءَالِ فِرۡعَوۡنَ وَX<ٱ>لَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۚ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا فَأَخَذَهُمُ ٱللَّهُ بِذُنُوبِهِمۡۗ وَٱللَّهُ شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<١١>",
"قُل لِّلَّذِینَ كَفَR<رُ>وX<ا۟> سَتُغۡلَبُونَ وَتُحۡشَR<رُ>ونَ إِلَىٰ جَهَN<نَّ>مَۖ وَبِئۡسَ ٱلۡمِهَJ<ا>Q<د>ُ A<۝>D<١٢>",
"قَQ<دۡ> كَانَ لَكُمۡ ءَایَةN<ࣱ> فِی فِئَتَیۡنِ ٱلۡتَقَتَاۖ فِئَةN<ࣱ> تُقَT<ـٰ>تِلُ فِی سَبِیلِ ٱللَّهِ وَأُخۡR<رَ>ىٰ كَافِR<رَ>ةX<ࣱ> N<یَ>R<رَ>وۡنَهُم N<مِّ>ثۡلَیۡهِمۡ R<رَ>أۡیَ ٱلۡعَیۡنِۚ وَٱللَّهُ یُؤَیِّدُ بِنَصۡرِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَعِQ<بۡ>R<رَ>ةX<ࣰ> لِّأُX<و۟>لِی ٱلۡأَQ<بۡ>صَT<ـٰ>R<ر>ِ A<۝>D<١٣>",
"زُیِّنَ لِX<ل>N<نَّ>اسِ حُبُّ ٱX<ل>شَّهَوَT< ٰ>تِ مِنَ ٱX<ل>N<نِّ>سَW<اۤ>ءِ وَX<ٱ>لۡبَنِینَ وَX<ٱ>لۡقَنَT<ـٰ>طِیرِ ٱلۡمُقَN<ن>طَR<رَ>ةِ مِنَ ٱX<ل>ذَّهَبِ وَX<ٱ>لۡفِضَّةِ وَX<ٱ>لۡخَیۡلِ ٱلۡمُسَوَّمَةِ وَX<ٱ>لۡأَنۡعَT<ـٰ>مِ وَX<ٱ>لۡحَR<رۡ>ثِۗ ذَT< ٰ>لِكَ مَتَT<ـٰ>عُ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۖ وَٱللَّهُ عِN<ن>دَهُT<ۥ> حُسۡنُ ٱلۡمَءَJ<ا>Q<ب>ِ A<۝>D<١٤>",
"۞ قُلۡ أَؤُنَبِّئُكُN<م> بِخَیۡرX<ࣲ> N<مِّ>N<ن> ذَT< ٰ>لِكُمۡۖ لِلَّذِینَ ٱتَّقَوۡX<ا۟> عِN<ن>دَ R<رَ>بِّهِمۡ جَN<نَّ>T<ـٰ>تN<ࣱ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَا وَأَزۡوَT< ٰ>جX<ࣱ> N<مُّ>طَهَّR<رَ>ةX<ࣱ> N<وَ>رِضۡوَT< ٰ>نX<ࣱ> N<مِّ>نَ ٱللَّهِۗ وَٱللَّهُ بَصِیR<رُ>N<ۢ> بِX<ٱ>لۡعِبَJ<ا>Q<د>ِ A<۝>D<١٥>",
"ٱلَّذِینَ یَقُولُونَ R<رَ>بَّنَW<اۤ> إِN<نَّ>نَW<اۤ> ءَامَN<نَّ>ا فَX<ٱ>غۡفِرۡ لَنَا ذُنُوبَنَا وَقِنَا عَذَابَ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<١٦>",
"ٱX<ل>صَّT<ـٰ>بِرِینَ وَX<ٱ>X<ل>صَّT<ـٰ>دِقِینَ وَX<ٱ>لۡقَT<ـٰ>نِتِینَ وَX<ٱ>لۡمُN<ن>فِقِینَ وَX<ٱ>لۡمُسۡتَغۡفِرِینَ بِX<ٱ>لۡأَسۡحَJ<ا>R<ر>ِ A<۝>D<١٧>",
"شَهِدَ ٱللَّهُ أَN<نَّ>هُT<ۥ> لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ وَأُX<و۟>لُوX<ا۟> ٱلۡعِلۡمِ قَW<اۤ>ئِمَN<ۢ>ا بِX<ٱ>لۡقِسۡطِۚ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<١٨>",
"إِN<نَّ> ٱX<ل>دِّینَ عِN<ن>دَ ٱللَّهِ ٱلۡإِسۡلَT<ـٰ>مُۗ وَمَا ٱخۡتَلَفَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ إِلَّا مِX<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَهُمُ ٱلۡعِلۡمُ بَغۡیَN<ۢ>ا بَیۡنَهُمۡۗ وَمَX<ن> N<یَ>كۡفُR<رۡ> بِءَایَT<ـٰ>تِ ٱللَّهِ فَإِN<نَّ> ٱللَّهَ سَرِیعُ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<١٩>",
"فَإِنۡ حَL<اۤ>جُّوكَ فَقُلۡ أَسۡلَمۡتُ وَQ<جۡ>هِیَ لِلَّهِ وَمَنِ ٱتَّبَعَنِۗ وَقُل لِّلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡأُN<مِّ>یِّT<ـۧ>نَ ءَأَسۡلَمۡتُمۡۚ فَإِنۡ أَسۡلَمُوX<ا۟> فَقَدِ ٱهۡتَدَوX<ا۟>ۖ وَّإِN<ن> تَوَلَّوۡX<ا۟> فَإِN<نَّ>مَا عَلَیۡكَ ٱلۡبَلَT<ـٰ>غُۗ وَٱللَّهُ بَصِیR<رُ>N<ۢ> بِX<ٱ>لۡعِبَJ<ا>Q<د>ِ A<۝>D<٢٠>",
"إِN<نَّ> ٱلَّذِینَ یَكۡفُR<رُ>ونَ بِءَایَT<ـٰ>تِ ٱللَّهِ وَیَQ<قۡ>تُلُونَ ٱX<ل>N<نَّ>بِیِّT<ـۧ>نَ بِغَیۡرِ حَقّX<ࣲ> N<وَ>یَQ<قۡ>تُلُونَ ٱلَّذِینَ یَأۡمُR<رُ>ونَ بِX<ٱ>لۡقِسۡطِ مِنَ ٱX<ل>N<نَّ>اسِ فَبَشِّرۡهُN<م> بِعَذَابٍ أَلِJ<ی>مٍ A<۝>D<٢١>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ حَبِطَتۡ أَعۡمَT<ـٰ>لُهُمۡ فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِ وَمَا لَهُم N<مِّ>ن N<نَّ>T<ـٰ>صِرِJ<ی>نَ A<۝>D<٢٢>",
"أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ أُوتُوX<ا۟> نَصِیبX<ࣰ>ا N<مِّ>نَ ٱلۡكِتَT<ـٰ>بِ یُQ<دۡ>عَوۡنَ إِلَىٰ كِتَT<ـٰ>بِ ٱللَّهِ لِیَحۡكُمَ بَیۡنَهُمۡ ثُN<مَّ> یَتَوَلَّىٰ فَرِیقX<ࣱ> N<مِّ>نۡهُمۡ وَهُم N<مُّ>عۡرِضُJ<و>نَ A<۝>D<٢٣>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ قَالُوX<ا۟> لَN<ن> تَمَسَّنَا ٱX<ل>N<نَّ>اR<رُ> إِلَّW<اۤ> أَیَّامX<ࣰ>ا N<مَّ>عۡدُودَT< ٰ>تX<ࣲ>ۖ N<وَ>غَR<رَّ>هُمۡ فِی دِینِهِم N<مَّ>ا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<٢٤>",
"فَكَیۡفَ إِذَا جَمَعۡنَT<ـٰ>هُمۡ لِیَوۡمX<ࣲ> لَّا R<رَ>یۡبَ فِیهِ وَوُفِّیَتۡ كُلُّ نَفۡسX<ࣲ> N<مَّ>ا كَسَبَتۡ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<٢٥>",
"قُلِ ٱللَّهُN<مَّ> مَT<ـٰ>لِكَ ٱلۡمُلۡكِ تُؤۡتِی ٱلۡمُلۡكَ مَN<ن> تَشَW<اۤ>ءُ وَتَN<ن>زِعُ ٱلۡمُلۡكَ مِN<مَّ>N<ن> تَشَW<اۤ>ءُ وَتُعِزُّ مَN<ن> تَشَW<اۤ>ءُ وَتُذِلُّ مَN<ن> تَشَW<اۤ>ءُۖ بِیَدِكَ ٱلۡخَیۡR<رُ>ۖ إِN<نَّ>كَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٢٦>",
"تُولِجُ ٱلَّیۡلَ فِی ٱX<ل>N<نَّ>هَارِ وَتُولِجُ ٱX<ل>N<نَّ>هَاR<رَ> فِی ٱلَّیۡلِۖ وَتُخۡرِجُ ٱلۡحَیَّ مِنَ ٱلۡمَیِّتِ وَتُخۡرِجُ ٱلۡمَیِّتَ مِنَ ٱلۡحَیِّۖ وَتَR<رۡ>زُقُ مَN<ن> تَشَW<اۤ>ءُ بِغَیۡرِ حِسَJ<ا>Q<ب>ࣲ A<۝>D<٢٧>",
"لَّا یَتَّخِذِ ٱلۡمُؤۡمِنُونَ ٱلۡكَT<ـٰ>فِرِینَ أَوۡلِیَW<اۤ>ءَ مِN<ن> دُونِ ٱلۡمُؤۡمِنِینَۖ وَمَX<ن> N<یَ>فۡعَلۡ ذَT< ٰ>لِكَ فَلَیۡسَ مِنَ ٱللَّهِ فِی شَیۡءٍ إِلَّW<اۤ> أَN<ن> تَتَّقُوX<ا۟> مِنۡهُمۡ تُقَX<ى>T<ٰ>ةX<ࣰ>ۗ N<وَ>یُحَذِّR<رُ>كُمُ ٱللَّهُ نَفۡسَهُT<ۥ>ۗ وَإِلَى ٱللَّهِ ٱلۡمَصِJ<ی>رُ A<۝>D<٢٨>",
"قُلۡ إِN<ن> تُخۡفُوX<ا۟> مَا فِی صُدُورِكُمۡ أَوۡ تُQ<بۡ>دُوهُ یَعۡلَمۡهُ ٱللَّهُۗ وَیَعۡلَمُ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۗ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٢٩>",
"یَوۡمَ تَجِدُ كُلُّ نَفۡسX<ࣲ> N<مَّ>ا عَمِلَتۡ مِنۡ خَیۡرX<ࣲ> N<مُّ>حۡضَR<ر>X<ࣰ>ا N<وَ>مَا عَمِلَتۡ مِN<ن> سُW<وۤ>ءN<ࣲ> تَوَدُّ لَوۡ أَN<نَّ> بَیۡنَهَا وَبَیۡنَهُW<ۥۤ> أَمَدَN<ۢ>ا بَعِیدX<ࣰ>اۗ N<وَ>یُحَذِّR<رُ>كُمُ ٱللَّهُ نَفۡسَهُT<ۥ>ۗ وَٱللَّهُ R<رَ>ءُوفُN<ۢ> بِX<ٱ>لۡعِبَJ<ا>Q<د>ِ A<۝>D<٣٠>",
"قُلۡ إِN<ن> كُN<ن>تُمۡ تُحِبُّونَ ٱللَّهَ فَX<ٱ>تَّبِعُونِی یُحۡبِQ<بۡ>كُمُ ٱللَّهُ وَیَغۡفِرۡ لَكُمۡ ذُنُوبَكُمۡۚ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٣١>",
"قُلۡ أَطِیعُوX<ا۟> ٱللَّهَ وَX<ٱ>X<ل>R<رَّ>سُولَۖ فَإِN<ن> تَوَلَّوۡX<ا۟> فَإِN<نَّ> ٱللَّهَ لَا یُحِبُّ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٣٢>",
"۞ إِN<نَّ> ٱللَّهَ ٱصۡطَفَىW<ٰۤ> ءَادَمَ وَنُوحX<ࣰ>ا N<وَ>ءَالَ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَءَالَ عِمۡR<رَ>T< ٰ>نَ عَلَى ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٣٣>",
"ذُرِّیَّةَN<ۢ> بَعۡضُهَا مِX<ن>N<ۢ> بَعۡضX<ࣲ>ۗ N<وَ>ٱللَّهُ سَمِیعٌ عَلِJ<ی>مٌ A<۝>D<٣٤>",
"إِذۡ قَالَتِ ٱمۡR<رَ>أَتُ عِمۡR<رَ>T< ٰ>نَ R<رَ>بِّ إِN<نِّ>ی نَذَR<رۡ>تُ لَكَ مَا فِی بَQ<طۡ>نِی مُحَR<رَّ>R<ر>N<ࣰ>ا فَتَقَبَّلۡ مِN<نِّ>W<یۤ>ۖ إِN<نَّ>كَ أَN<ن>تَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٣٥>",
"فَلَN<مَّ>ا وَضَعَتۡهَا قَالَتۡ R<رَ>بِّ إِN<نِّ>ی وَضَعۡتُهَW<اۤ> أُN<ن>ثَىٰ وَٱللَّهُ أَعۡلَمُ بِمَا وَضَعَتۡ وَلَیۡسَ ٱX<ل>ذَّكَR<رُ> كَX<ٱ>لۡأُN<ن>ثَىٰۖ وَإِN<نِّ>ی سَN<مَّ>یۡتُهَا مَR<رۡ>یَمَ وَإِN<نِّ>W<یۤ> أُعِیذُهَا بِكَ وَذُرِّیَّتَهَا مِنَ ٱX<ل>شَّیۡطَT<ـٰ>نِ ٱX<ل>R<رَّ>جِJ<ی>مِ A<۝>D<٣٦>",
"فَتَقَبَّلَهَا R<رَ>بُّهَا بِقَبُولٍ حَسَنX<ࣲ> N<وَ>أَX<ن>N<ۢ>بَتَهَا نَبَاتًا حَسَنX<ࣰ>ا N<وَ>كَفَّلَهَا زَكَرِیَّاۖ كُلَّمَا دَخَلَ عَلَیۡهَا زَكَرِیَّا ٱلۡمِحۡR<رَ>ابَ وَجَدَ عِN<ن>دَهَا رِزۡقN<ࣰ>اۖ قَالَ یَT<ـٰ>مَR<رۡ>یَمُ أَN<نَّ>ىٰ لَكِ هَT<ـٰ>ذَاۖ قَالَتۡ هُوَ مِنۡ عِN<ن>دِ ٱللَّهِۖ إِN<نَّ> ٱللَّهَ یَR<رۡ>زُقُ مَX<ن> N<یَ>شَW<اۤ>ءُ بِغَیۡرِ حِسَJ<ا>Q<ب>ٍ A<۝>D<٣٧>",
"هُنَالِكَ دَعَا زَكَرِیَّا R<رَ>بَّهُT<ۥ>ۖ قَالَ R<رَ>بِّ هَQ<بۡ> لِی مِX<ن> لَّدُN<ن>كَ ذُرِّیَّةN<ࣰ> طَیِّبَةًۖ إِN<نَّ>كَ سَمِیعُ ٱX<ل>دُّعَW<اۤ>ءِ A<۝>D<٣٨>",
"فَنَادَتۡهُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ وَهُوَ قَW<اۤ>ئِمX<ࣱ> N<یُ>صَلِّی فِی ٱلۡمِحۡR<رَ>ابِ أَN<نَّ> ٱللَّهَ یُبَشِّR<رُ>كَ بِیَحۡیَىٰ مُصَدِّقَN<ۢ>ا بِكَلِمَةX<ࣲ> N<مِّ>نَ ٱللَّهِ وَسَیِّدX<ࣰ>ا N<وَ>حَصُوR<ر>X<ࣰ>ا N<وَ>نَبِیّX<ࣰ>ا N<مِّ>نَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٣٩>",
"قَالَ R<رَ>بِّ أَN<نَّ>ىٰ یَكُونُ لِی غُلَT<ـٰ>مX<ࣱ> N<وَ>قَQ<دۡ> بَلَغَنِیَ ٱلۡكِبَR<رُ> وَX<ٱ>مۡR<رَ>أَتِی عَاقِR<ر>N<ࣱ>ۖ قَالَ كَذَT< ٰ>لِكَ ٱللَّهُ یَفۡعَلُ مَا یَشَW<اۤ>ءُ A<۝>D<٤٠>",
"قَالَ R<رَ>بِّ ٱQ<جۡ>عَل لِّW<یۤ> ءَایَةN<ࣰ>ۖ قَالَ ءَایَتُكَ أَلَّا تُكَلِّمَ ٱX<ل>N<نَّ>اسَ ثَلَT<ـٰ>ثَةَ أَیَّامٍ إِلَّا R<رَ>مۡزX<ࣰ>اۗ N<وَ>X<ٱ>ذۡكُر R<رَّ>بَّكَ كَثِیR<ر>X<ࣰ>ا N<وَ>سَبِّحۡ بِX<ٱ>لۡعَشِیِّ وَX<ٱ>لۡإِQ<بۡ>كَT<ـٰ>R<ر>ِ A<۝>D<٤١>",
"وَإِذۡ قَالَتِ ٱلۡمَلَW<ـٰۤ>ئِكَةُ یَT<ـٰ>مَR<رۡ>یَمُ إِN<نَّ> ٱللَّهَ ٱصۡطَفَX<ى>T<ٰ>كِ وَطَهَّR<رَ>كِ وَX<ٱ>صۡطَفَX<ى>T<ٰ>كِ عَلَىٰ نِسَW<اۤ>ءِ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٤٢>",
"یَT<ـٰ>مَR<رۡ>یَمُ ٱQ<قۡ>نُتِی لِR<رَ>بِّكِ وَX<ٱ>سۡجُدِی وَX<ٱ>R<رۡ>كَعِی مَعَ ٱX<ل>R<رَّ>T< ٰ>كِعِJ<ی>نَ A<۝>D<٤٣>",
"ذَT< ٰ>لِكَ مِنۡ أَX<ن>N<ۢ>بَW<اۤ>ءِ ٱلۡغَیۡبِ نُوحِیهِ إِلَیۡكَۚ وَمَا كُN<ن>تَ لَدَیۡهِمۡ إِذۡ یُلۡقُونَ أَQ<قۡ>لَT<ـٰ>مَهُمۡ أَیُّهُمۡ یَكۡفُلُ مَR<رۡ>یَمَ وَمَا كُN<ن>تَ لَدَیۡهِمۡ إِذۡ یَخۡتَصِمُJ<و>نَ A<۝>D<٤٤>",
"إِذۡ قَالَتِ ٱلۡمَلَW<ـٰۤ>ئِكَةُ یَT<ـٰ>مَR<رۡ>یَمُ إِN<نَّ> ٱللَّهَ یُبَشِّR<رُ>كِ بِكَلِمَةX<ࣲ> N<مِّ>نۡهُ ٱسۡمُهُ ٱلۡمَسِیحُ عِیسَى ٱQ<بۡ>نُ مَR<رۡ>یَمَ وَجِیهN<ࣰ>ا فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِ وَمِنَ ٱلۡمُقَR<رَّ>بِJ<ی>نَ A<۝>D<٤٥>",
"وَیُكَلِّمُ ٱX<ل>N<نَّ>اسَ فِی ٱلۡمَهۡدِ وَكَهۡلX<ࣰ>ا N<وَ>مِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٤٦>",
"قَالَتۡ R<رَ>بِّ أَN<نَّ>ىٰ یَكُونُ لِی وَلَدX<ࣱ> N<وَ>لَمۡ یَمۡسَسۡنِی بَشَR<ر>N<ࣱ>ۖ قَالَ كَذَT< ٰ>لِكِ ٱللَّهُ یَخۡلُقُ مَا یَشَW<اۤ>ءُۚ إِذَا قَضَىW<ٰۤ> أَمۡR<ر>N<ࣰ>ا فَإِN<نَّ>مَا یَقُولُ لَهُT<ۥ> كُN<ن> فَیَكُJ<و>نُ A<۝>D<٤٧>",
"وَیُعَلِّمُهُ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحِكۡمَةَ وَX<ٱ>X<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةَ وَX<ٱ>لۡإِN<ن>جِJ<ی>لَ A<۝>D<٤٨>",
"وَR<رَ>سُولًا إِلَىٰ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ أَN<نِّ>ی قَQ<دۡ> جِئۡتُكُN<م> بِءَایَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ أَN<نِّ>W<یۤ> أَخۡلُقُ لَكُم N<مِّ>نَ ٱX<ل>طِّینِ كَهَیۡءَةِ ٱX<ل>طَّیۡرِ فَأَN<ن>فُخُ فِیهِ فَیَكُونُ طَیۡR<رَ>N<ۢ>ا بِإِذۡنِ ٱللَّهِۖ وَأُQ<بۡ>رِئُ ٱلۡأَكۡمَهَ وَX<ٱ>لۡأَQ<بۡ>R<رَ>صَ وَأُحۡیِ ٱلۡمَوۡتَىٰ بِإِذۡنِ ٱللَّهِۖ وَأُنَبِّئُكُN<م> بِمَا تَأۡكُلُونَ وَمَا تَدَّخِR<رُ>ونَ فِی بُیُوتِكُمۡۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لَّكُمۡ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٤٩>",
"وَمُصَدِّقX<ࣰ>ا لِّمَا بَیۡنَ یَدَیَّ مِنَ ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةِ وَلِأُحِلَّ لَكُN<م> بَعۡضَ ٱلَّذِی حُرِّمَ عَلَیۡكُمۡۚ وَجِئۡتُكُN<م> بِءَایَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<٥٠>",
"إِN<نَّ> ٱللَّهَ R<رَ>بِّی وَR<رَ>بُّكُمۡ فَX<ٱ>عۡبُدُوهُۚ هَT<ـٰ>ذَا صِR<رَ>T< ٰ>طX<ࣱ> N<مُّ>سۡتَقِJ<ی>مࣱ A<۝>D<٥١>",
"۞ فَلَN<مَّ>W<اۤ> أَحَسَّ عِیسَىٰ مِنۡهُمُ ٱلۡكُفۡR<رَ> قَالَ مَنۡ أَN<ن>صَارِW<یۤ> إِلَى ٱللَّهِۖ قَالَ ٱلۡحَوَارِیُّونَ نَحۡنُ أَN<ن>صَاR<رُ> ٱللَّهِ ءَامَN<نَّ>ا بِٱللَّهِ وَX<ٱ>شۡهَQ<دۡ> بِأَN<نَّ>ا مُسۡلِمُJ<و>نَ A<۝>D<٥٢>",
"R<رَ>بَّنَW<اۤ> ءَامَN<نَّ>ا بِمَW<اۤ> أَN<ن>زَلۡتَ وَX<ٱ>تَّبَعۡنَا ٱX<ل>R<رَّ>سُولَ فَX<ٱ>كۡتُQ<بۡ>نَا مَعَ ٱX<ل>شَّT<ـٰ>هِدِJ<ی>نَ A<۝>D<٥٣>",
"وَمَكَR<رُ>وX<ا۟> وَمَكَR<رَ> ٱللَّهُۖ وَٱللَّهُ خَیۡR<رُ> ٱلۡمَT<ـٰ>كِرِJ<ی>نَ A<۝>D<٥٤>",
"إِذۡ قَالَ ٱللَّهُ یَT<ـٰ>عِیسَىW<ٰۤ> إِN<نِّ>ی مُتَوَفِّیكَ وَR<رَ>افِعُكَ إِلَیَّ وَمُطَهِّR<رُ>كَ مِنَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَجَاعِلُ ٱلَّذِینَ ٱتَّبَعُوكَ فَوۡقَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِۖ ثُN<مَّ> إِلَیَّ مَR<رۡ>جِعُكُمۡ فَأَحۡكُمُ بَیۡنَكُمۡ فِیمَا كُN<ن>تُمۡ فِیهِ تَخۡتَلِفُJ<و>نَ A<۝>D<٥٥>",
"فَأَN<مَّ>ا ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فَأُعَذِّبُهُمۡ عَذَابN<ࣰ>ا شَدِیدN<ࣰ>ا فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِ وَمَا لَهُم N<مِّ>ن N<نَّ>T<ـٰ>صِرِJ<ی>نَ A<۝>D<٥٦>",
"وَأَN<مَّ>ا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ فَیُوَفِّیهِمۡ أُجُوR<رَ>هُمۡۗ وَٱللَّهُ لَا یُحِبُّ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥٧>",
"ذَT< ٰ>لِكَ نَتۡلُوهُ عَلَیۡكَ مِنَ ٱلۡءَایَT<ـٰ>تِ وَX<ٱ>X<ل>ذِّكۡرِ ٱلۡحَكِJ<ی>مِ A<۝>D<٥٨>",
"إِN<نَّ> مَثَلَ عِیسَىٰ عِN<ن>دَ ٱللَّهِ كَمَثَلِ ءَادَمَۖ خَلَقَهُT<ۥ> مِN<ن> تُR<رَ>ابN<ࣲ> ثُN<مَّ> قَالَ لَهُT<ۥ> كُN<ن> فَیَكُJ<و>نُ A<۝>D<٥٩>",
"ٱلۡحَقُّ مِX<ن> R<رَّ>بِّكَ فَلَا تَكُX<ن> N<مِّ>نَ ٱلۡمُمۡتَرِJ<ی>نَ A<۝>D<٦٠>",
"فَمَنۡ حَL<اۤ>جَّكَ فِیهِ مِX<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَكَ مِنَ ٱلۡعِلۡمِ فَقُلۡ تَعَالَوۡX<ا۟> نَQ<دۡ>عُ أَQ<بۡ>نَW<اۤ>ءَنَا وَأَQ<بۡ>نَW<اۤ>ءَكُمۡ وَنِسَW<اۤ>ءَنَا وَنِسَW<اۤ>ءَكُمۡ وَأَN<ن>فُسَنَا وَأَN<ن>فُسَكُمۡ ثُN<مَّ> نَQ<بۡ>تَهِلۡ فَنَQ<جۡ>عَل لَّعۡنَتَ ٱللَّهِ عَلَى ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٦١>",
"إِN<نَّ> هَT<ـٰ>ذَا لَهُوَ ٱلۡقَصَصُ ٱلۡحَقُّۚ وَمَا مِنۡ إِلَT<ـٰ>هٍ إِلَّا ٱللَّهُۚ وَإِN<نَّ> ٱللَّهَ لَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٦٢>",
"فَإِN<ن> تَوَلَّوۡX<ا۟> فَإِN<نَّ> ٱللَّهَ عَلِیمُN<ۢ> بِX<ٱ>لۡمُفۡسِدِJ<ی>نَ A<۝>D<٦٣>",
"قُلۡ یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ تَعَالَوۡX<ا۟> إِلَىٰ كَلِمَةN<ࣲ> سَوَW<اۤ>ءِN<ۭ> بَیۡنَنَا وَبَیۡنَكُمۡ أَلَّا نَعۡبُدَ إِلَّا ٱللَّهَ وَلَا نُشۡرِكَ بِهِT<ۦ> شَیۡءX<ࣰ>ا N<وَ>لَا یَتَّخِذَ بَعۡضُنَا بَعۡضًا أَR<رۡ>بَابX<ࣰ>ا N<مِّ>N<ن> دُونِ ٱللَّهِۚ فَإِN<ن> تَوَلَّوۡX<ا۟> فَقُولُوX<ا۟> ٱشۡهَدُوX<ا۟> بِأَN<نَّ>ا مُسۡلِمُJ<و>نَ A<۝>D<٦٤>",
"یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ لِمَ تُحَL<اۤ>جُّونَ فِW<یۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَمَW<اۤ> أُN<ن>زِلَتِ ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةُ وَX<ٱ>لۡإِN<ن>جِیلُ إِلَّا مِX<ن>N<ۢ> بَعۡدِهِW<ۦۤ>ۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<٦٥>",
"هَW<ـٰۤ>أَN<ن>تُمۡ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ حَT<ـٰ>جَQ<جۡ>تُمۡ فِیمَا لَكُN<م> بِهِT<ۦ> عِلۡمN<ࣱ> فَلِمَ تُحَL<اۤ>جُّونَ فِیمَا لَیۡسَ لَكُN<م> بِهِT<ۦ> عِلۡمX<ࣱ>ۚ N<وَ>ٱللَّهُ یَعۡلَمُ وَأَN<ن>تُمۡ لَا تَعۡلَمُJ<و>نَ A<۝>D<٦٦>",
"مَا كَانَ إِQ<بۡ>R<رَ>T< ٰ>هِیمُ یَهُودِیّX<ࣰ>ا N<وَ>لَا نَصۡR<رَ>انِیّX<ࣰ>ا N<وَ>لَT<ـٰ>كِN<ن> كَانَ حَنِیفX<ࣰ>ا N<مُّ>سۡلِمX<ࣰ>ا N<وَ>مَا كَانَ مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<٦٧>",
"إِN<نَّ> أَوۡلَى ٱX<ل>N<نَّ>اسِ بِإِQ<بۡ>R<رَ>T< ٰ>هِیمَ لَلَّذِینَ ٱتَّبَعُوهُ وَهَT<ـٰ>ذَا ٱX<ل>N<نَّ>بِیُّ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟>ۗ وَٱللَّهُ وَلِیُّ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٦٨>",
"وَدَّت طَّW<اۤ>ئِفَةX<ࣱ> N<مِّ>نۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ لَوۡ یُضِلُّونَكُمۡ وَمَا یُضِلُّونَ إِلَّW<اۤ> أَN<ن>فُسَهُمۡ وَمَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٦٩>",
"یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ لِمَ تَكۡفُR<رُ>ونَ بِءَایَT<ـٰ>تِ ٱللَّهِ وَأَN<ن>تُمۡ تَشۡهَدُJ<و>نَ A<۝>D<٧٠>",
"یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ لِمَ تَلۡبِسُونَ ٱلۡحَقَّ بِX<ٱ>لۡبَT<ـٰ>طِلِ وَتَكۡتُمُونَ ٱلۡحَقَّ وَأَN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٧١>",
"وَقَالَت طَّW<اۤ>ئِفَةX<ࣱ> N<مِّ>نۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ ءَامِنُوX<ا۟> بِX<ٱ>لَّذِW<یۤ> أُN<ن>زِلَ عَلَى ٱلَّذِینَ ءَامَنُوX<ا۟> وَQ<جۡ>هَ ٱX<ل>N<نَّ>هَارِ وَX<ٱ>كۡفُR<رُ>W<وۤ>X<ا۟> ءَاخِR<رَ>هُT<ۥ> لَعَلَّهُمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<٧٢>",
"وَلَا تُؤۡمِنُW<وۤ>X<ا۟> إِلَّا لِمَN<ن> تَبِعَ دِینَكُمۡ قُلۡ إِN<نَّ> ٱلۡهُدَىٰ هُدَى ٱللَّهِ أَX<ن> N<یُ>ؤۡتَىW<ٰۤ> أَحَدX<ࣱ> N<مِّ>ثۡلَ مَW<اۤ> أُوتِیتُمۡ أَوۡ یُحَL<اۤ>جُّوكُمۡ عِN<ن>دَ R<رَ>بِّكُمۡۗ قُلۡ إِN<نَّ> ٱلۡفَضۡلَ بِیَدِ ٱللَّهِ یُؤۡتِیهِ مَX<ن> N<یَ>شَW<اۤ>ءُۗ وَٱللَّهُ وَT< ٰ>سِعٌ عَلِJ<ی>مࣱ A<۝>D<٧٣>",
"یَخۡتَصُّ بِR<رَ>حۡمَتِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُۗ وَٱللَّهُ ذُو ٱلۡفَضۡلِ ٱلۡعَظِJ<ی>مِ A<۝>D<٧٤>",
"۞ وَمِنۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ مَنۡ إِN<ن> تَأۡمَنۡهُ بِقِN<ن>طَارX<ࣲ> N<یُ>ؤَدِّهِW<ۦۤ> إِلَیۡكَ وَمِنۡهُم N<مَّ>نۡ إِN<ن> تَأۡمَنۡهُ بِدِینَارX<ࣲ> لَّا یُؤَدِّهِW<ۦۤ> إِلَیۡكَ إِلَّا مَا دُمۡتَ عَلَیۡهِ قَW<اۤ>ئِمN<ࣰ>اۗ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ قَالُوX<ا۟> لَیۡسَ عَلَیۡنَا فِی ٱلۡأُN<مِّ>یِّT<ـۧ>نَ سَبِیلX<ࣱ> N<وَ>یَقُولُونَ عَلَى ٱللَّهِ ٱلۡكَذِبَ وَهُمۡ یَعۡلَمُJ<و>نَ A<۝>D<٧٥>",
"بَلَىٰۚ مَنۡ أَوۡفَىٰ بِعَهۡدِهِT<ۦ> وَX<ٱ>تَّقَىٰ فَإِN<نَّ> ٱللَّهَ یُحِبُّ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٧٦>",
"إِN<نَّ> ٱلَّذِینَ یَشۡتَR<رُ>ونَ بِعَهۡدِ ٱللَّهِ وَأَیۡمَT<ـٰ>نِهِمۡ ثَمَنN<ࣰ>ا قَلِیلًا أُX<و۟>لَW<ـٰۤ>ئِكَ لَا خَلَT<ـٰ>قَ لَهُمۡ فِی ٱلۡءَاخِR<رَ>ةِ وَلَا یُكَلِّمُهُمُ ٱللَّهُ وَلَا یَN<ن>ظُR<رُ> إِلَیۡهِمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ وَلَا یُزَكِّیهِمۡ وَلَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٧٧>",
"وَإِN<نَّ> مِنۡهُمۡ لَفَرِیقX<ࣰ>ا N<یَ>لۡوُT<ۥ>نَ أَلۡسِنَتَهُN<م> بِX<ٱ>لۡكِتَT<ـٰ>بِ لِتَحۡسَبُوهُ مِنَ ٱلۡكِتَT<ـٰ>بِ وَمَا هُوَ مِنَ ٱلۡكِتَT<ـٰ>بِ وَیَقُولُونَ هُوَ مِنۡ عِN<ن>دِ ٱللَّهِ وَمَا هُوَ مِنۡ عِN<ن>دِ ٱللَّهِ وَیَقُولُونَ عَلَى ٱللَّهِ ٱلۡكَذِبَ وَهُمۡ یَعۡلَمُJ<و>نَ A<۝>D<٧٨>",
"مَا كَانَ لِبَشَرٍ أَX<ن> N<یُ>ؤۡتِیَهُ ٱللَّهُ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحُكۡمَ وَX<ٱ>X<ل>N<نُّ>بُوَّةَ ثُN<مَّ> یَقُولَ لِX<ل>N<نَّ>اسِ كُونُوX<ا۟> عِبَادX<ࣰ>ا لِّی مِN<ن> دُونِ ٱللَّهِ وَلَT<ـٰ>كِN<ن> كُونُوX<ا۟> R<رَ>بَّT<ـٰ>نِیِّT<ـۧ>نَ بِمَا كُN<ن>تُمۡ تُعَلِّمُونَ ٱلۡكِتَT<ـٰ>بَ وَبِمَا كُN<ن>تُمۡ تَQ<دۡ>R<رُ>سُJ<و>نَ A<۝>D<٧٩>",
"وَلَا یَأۡمُR<رَ>كُمۡ أَN<ن> تَتَّخِذُوX<ا۟> ٱلۡمَلَW<ـٰۤ>ئِكَةَ وَX<ٱ>X<ل>N<نَّ>بِیِّT<ـۧ>نَ أَR<رۡ>بَابًاۗ أَیَأۡمُR<رُ>كُN<م> بِX<ٱ>لۡكُفۡرِ بَعۡدَ إِذۡ أَN<ن>تُم N<مُّ>سۡلِمُJ<و>نَ A<۝>D<٨٠>",
"وَإِذۡ أَخَذَ ٱللَّهُ مِیثَT<ـٰ>قَ ٱX<ل>N<نَّ>بِیِّT<ـۧ>نَ لَمَW<اۤ> ءَاتَیۡتُكُم N<مِّ>N<ن> كِتَT<ـٰ>بX<ࣲ> N<وَ>حِكۡمَةN<ࣲ> ثُN<مَّ> جَW<اۤ>ءَكُمۡ R<رَ>سُولX<ࣱ> N<مُّ>صَدِّقX<ࣱ> لِّمَا مَعَكُمۡ لَتُؤۡمِنُN<نَّ> بِهِT<ۦ> وَلَتَN<ن>صُR<رُ>N<نَّ>هُT<ۥ>ۚ قَالَ ءَأَQ<قۡ>R<رَ>R<رۡ>تُمۡ وَأَخَذۡتُمۡ عَلَىٰ ذَT< ٰ>لِكُمۡ إِصۡرِیۖ قَالُW<وۤ>X<ا۟> أَQ<قۡ>R<رَ>R<رۡ>نَاۚ قَالَ فَX<ٱ>شۡهَدُوX<ا۟> وَأَنَا۠ مَعَكُم N<مِّ>نَ ٱX<ل>شَّT<ـٰ>هِدِJ<ی>نَ A<۝>D<٨١>",
"فَمَN<ن> تَوَلَّىٰ بَعۡدَ ذَT< ٰ>لِكَ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡفَT<ـٰ>سِقُJ<و>نَ A<۝>D<٨٢>",
"أَفَغَیۡR<رَ> دِینِ ٱللَّهِ یَQ<بۡ>غُونَ وَلَهُW<ۥۤ> أَسۡلَمَ مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ طَوۡعX<ࣰ>ا N<وَ>كَR<رۡ>هX<ࣰ>ا N<وَ>إِلَیۡهِ یُR<رۡ>جَعُJ<و>نَ A<۝>D<٨٣>",
"قُلۡ ءَامَN<نَّ>ا بِٱللَّهِ وَمَW<اۤ> أُN<ن>زِلَ عَلَیۡنَا وَمَW<اۤ> أُN<ن>زِلَ عَلَىW<ٰۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَإِسۡمَT<ـٰ>عِیلَ وَإِسۡحَT<ـٰ>قَ وَیَعۡقُوبَ وَX<ٱ>لۡأَسۡبَاطِ وَمَW<اۤ> أُوتِیَ مُوسَىٰ وَعِیسَىٰ وَX<ٱ>X<ل>N<نَّ>بِیُّونَ مِX<ن> R<رَّ>بِّهِمۡ لَا نُفَرِّقُ بَیۡنَ أَحَدX<ࣲ> N<مِّ>نۡهُمۡ وَنَحۡنُ لَهُT<ۥ> مُسۡلِمُJ<و>نَ A<۝>D<٨٤>",
"وَمَX<ن> N<یَ>Q<بۡ>تَغِ غَیۡR<رَ> ٱلۡإِسۡلَT<ـٰ>مِ دِینN<ࣰ>ا فَلَX<ن> N<یُ>Q<قۡ>بَلَ مِنۡهُ وَهُوَ فِی ٱلۡءَاخِR<رَ>ةِ مِنَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٨٥>",
"كَیۡفَ یَهۡدِی ٱللَّهُ قَوۡمN<ࣰ>ا كَفَR<رُ>وX<ا۟> بَعۡدَ إِیمَT<ـٰ>نِهِمۡ وَشَهِدُW<وۤ>X<ا۟> أَN<نَّ> ٱX<ل>R<رَّ>سُولَ حَقّX<ࣱ> N<وَ>جَW<اۤ>ءَهُمُ ٱلۡبَیِّنَT<ـٰ>تُۚ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٨٦>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ جَزَW<اۤ>ؤُهُمۡ أَN<نَّ> عَلَیۡهِمۡ لَعۡنَةَ ٱللَّهِ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةِ وَX<ٱ>X<ل>N<نَّ>اسِ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٨٧>",
"خَT<ـٰ>لِدِینَ فِیهَا لَا یُخَفَّفُ عَنۡهُمُ ٱلۡعَذَابُ وَلَا هُمۡ یُN<ن>ظَR<رُ>J<و>نَ A<۝>D<٨٨>",
"إِلَّا ٱلَّذِینَ تَابُوX<ا۟> مِX<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَ وَأَصۡلَحُوX<ا۟> فَإِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<٨٩>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بَعۡدَ إِیمَT<ـٰ>نِهِمۡ ثُN<مَّ> ٱزۡدَادُوX<ا۟> كُفۡR<ر>X<ࣰ>ا لَّN<ن> تُQ<قۡ>بَلَ تَوۡبَتُهُمۡ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>ضَّL<اۤ>لُّJ<و>نَ A<۝>D<٩٠>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَمَاتُوX<ا۟> وَهُمۡ كُفَّاR<ر>N<ࣱ> فَلَX<ن> N<یُ>Q<قۡ>بَلَ مِنۡ أَحَدِهِم N<مِّ>لۡءُ ٱلۡأَR<رۡ>ضِ ذَهَبX<ࣰ>ا N<وَ>لَوِ ٱفۡتَدَىٰ بِهِW<ۦۤ>ۗ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ عَذَابٌ أَلِیمX<ࣱ> N<وَ>مَا لَهُم N<مِّ>ن N<نَّ>T<ـٰ>صِرِJ<ی>نَ A<۝>D<٩١>",
"لَN<ن> تَنَالُوX<ا۟> ٱلۡبِR<رَّ> حَتَّىٰ تُN<ن>فِقُوX<ا۟> مِN<مَّ>ا تُحِبُّونَۚ وَمَا تُN<ن>فِقُوX<ا۟> مِN<ن> شَیۡءN<ࣲ> فَإِN<نَّ> ٱللَّهَ بِهِT<ۦ> عَلِJ<ی>مࣱ A<۝>D<٩٢>",
"۞ كُلُّ ٱX<ل>طَّعَامِ كَانَ حِلّX<ࣰ>ا لِّبَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ إِلَّا مَا حَR<رَّ>مَ إِسۡR<رَ>W< ٰۤ>ءِیلُ عَلَىٰ نَفۡسِهِT<ۦ> مِN<ن> قَQ<بۡ>لِ أَN<ن> تُنَزَّلَ ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةُۚ قُلۡ فَأۡتُوX<ا۟> بِX<ٱ>X<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةِ فَX<ٱ>تۡلُوهَW<اۤ> إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٩٣>",
"فَمَنِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ ٱلۡكَذِبَ مِX<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٩٤>",
"قُلۡ صَدَقَ ٱللَّهُۗ فَX<ٱ>تَّبِعُوX<ا۟> مِلَّةَ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ حَنِیفX<ࣰ>اۖ N<وَ>مَا كَانَ مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<٩٥>",
"إِN<نَّ> أَوَّلَ بَیۡتX<ࣲ> N<وُ>ضِعَ لِX<ل>N<نَّ>اسِ لَلَّذِی بِبَكَّةَ مُبَاR<رَ>كX<ࣰ>ا N<وَ>هُدX<ࣰ>ى لِّلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٩٦>",
"فِیهِ ءَایَT<ـٰ>تُN<ۢ> بَیِّنَT<ـٰ>تX<ࣱ> N<مَّ>قَامُ إِQ<بۡ>R<رَ>T< ٰ>هِیمَۖ وَمَN<ن> دَخَلَهُT<ۥ> كَانَ ءَامِنX<ࣰ>اۗ N<وَ>لِلَّهِ عَلَى ٱX<ل>N<نَّ>اسِ حِجُّ ٱلۡبَیۡتِ مَنِ ٱسۡتَطَاعَ إِلَیۡهِ سَبِیلX<ࣰ>اۚ N<وَ>مَN<ن> كَفَR<رَ> فَإِN<نَّ> ٱللَّهَ غَنِیٌّ عَنِ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٩٧>",
"قُلۡ یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ لِمَ تَكۡفُR<رُ>ونَ بِءَایَT<ـٰ>تِ ٱللَّهِ وَٱللَّهُ شَهِیدٌ عَلَىٰ مَا تَعۡمَلُJ<و>نَ A<۝>D<٩٨>",
"قُلۡ یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ لِمَ تَصُدُّونَ عَN<ن> سَبِیلِ ٱللَّهِ مَنۡ ءَامَنَ تَQ<بۡ>غُونَهَا عِوَجX<ࣰ>ا N<وَ>أَN<ن>تُمۡ شُهَدَW<اۤ>ءُۗ وَمَا ٱللَّهُ بِغَT<ـٰ>فِلٍ عَN<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<٩٩>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<ن> تُطِیعُوX<ا۟> فَرِیقX<ࣰ>ا N<مِّ>نَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ یَR<رُ>دُّوكُN<م> بَعۡدَ إِیمَT<ـٰ>نِكُمۡ كَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٠٠>",
"وَكَیۡفَ تَكۡفُR<رُ>ونَ وَأَN<ن>تُمۡ تُتۡلَىٰ عَلَیۡكُمۡ ءَایَT<ـٰ>تُ ٱللَّهِ وَفِیكُمۡ R<رَ>سُولُهُT<ۥ>ۗ وَمَX<ن> N<یَ>عۡتَصِN<م> بِٱللَّهِ فَقَQ<دۡ> هُدِیَ إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<١٠١>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱتَّقُوX<ا۟> ٱللَّهَ حَقَّ تُقَاتِهِT<ۦ> وَلَا تَمُوتُN<نَّ> إِلَّا وَأَN<ن>تُم N<مُّ>سۡلِمُJ<و>نَ A<۝>D<١٠٢>",
"وَX<ٱ>عۡتَصِمُوX<ا۟> بِحَQ<بۡ>لِ ٱللَّهِ جَمِیعX<ࣰ>ا N<وَ>لَا تَفَR<رَّ>قُوX<ا۟>ۚ وَX<ٱ>ذۡكُR<رُ>وX<ا۟> نِعۡمَتَ ٱللَّهِ عَلَیۡكُمۡ إِذۡ كُN<ن>تُمۡ أَعۡدَW<اۤ>ءN<ࣰ> فَأَلَّفَ بَیۡنَ قُلُوبِكُمۡ فَأَصۡبَحۡتُN<م> بِنِعۡمَتِهِW<ۦۤ> إِخۡوَT< ٰ>نX<ࣰ>ا N<وَ>كُN<ن>تُمۡ عَلَىٰ شَفَا حُفۡR<رَ>ةX<ࣲ> N<مِّ>نَ ٱX<ل>N<نَّ>ارِ فَأَN<ن>قَذَكُم N<مِّ>نۡهَاۗ كَذَT< ٰ>لِكَ یُبَیِّنُ ٱللَّهُ لَكُمۡ ءَایَT<ـٰ>تِهِT<ۦ> لَعَلَّكُمۡ تَهۡتَدُJ<و>نَ A<۝>D<١٠٣>",
"وَلۡتَكُX<ن> N<مِّ>N<ن>كُمۡ أُN<مَّ>ةX<ࣱ> N<یَ>Q<دۡ>عُونَ إِلَى ٱلۡخَیۡرِ وَیَأۡمُR<رُ>ونَ بِX<ٱ>لۡمَعۡR<رُ>وفِ وَیَنۡهَوۡنَ عَنِ ٱلۡمُN<ن>كَرِۚ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<١٠٤>",
"وَلَا تَكُونُوX<ا۟> كَX<ٱ>لَّذِینَ تَفَR<رَّ>قُوX<ا۟> وَX<ٱ>خۡتَلَفُوX<ا۟> مِX<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَهُمُ ٱلۡبَیِّنَT<ـٰ>تُۚ وَأُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ عَذَابٌ عَظِJ<ی>مࣱ A<۝>D<١٠٥>",
"یَوۡمَ تَQ<بۡ>یَضُّ وُجُوهX<ࣱ> N<وَ>تَسۡوَدُّ وُجُوهN<ࣱ>ۚ فَأَN<مَّ>ا ٱلَّذِینَ ٱسۡوَدَّتۡ وُجُوهُهُمۡ أَكَفَR<رۡ>تُN<م> بَعۡدَ إِیمَT<ـٰ>نِكُمۡ فَذُوقُوX<ا۟> ٱلۡعَذَابَ بِمَا كُN<ن>تُمۡ تَكۡفُR<رُ>J<و>نَ A<۝>D<١٠٦>",
"وَأَN<مَّ>ا ٱلَّذِینَ ٱQ<بۡ>یَضَّتۡ وُجُوهُهُمۡ فَفِی R<رَ>حۡمَةِ ٱللَّهِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<١٠٧>",
"تِلۡكَ ءَایَT<ـٰ>تُ ٱللَّهِ نَتۡلُوهَا عَلَیۡكَ بِX<ٱ>لۡحَقِّۗ وَمَا ٱللَّهُ یُرِیدُ ظُلۡمX<ࣰ>ا لِّلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٠٨>",
"وَلِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۚ وَإِلَى ٱللَّهِ تُR<رۡ>جَعُ ٱلۡأُمُJ<و>R<ر>ُ A<۝>D<١٠٩>",
"كُN<ن>تُمۡ خَیۡR<رَ> أُN<مَّ>ةٍ أُخۡرِجَتۡ لِX<ل>N<نَّ>اسِ تَأۡمُR<رُ>ونَ بِX<ٱ>لۡمَعۡR<رُ>وفِ وَتَنۡهَوۡنَ عَنِ ٱلۡمُN<ن>كَرِ وَتُؤۡمِنُونَ بِٱللَّهِۗ وَلَوۡ ءَامَنَ أَهۡلُ ٱلۡكِتَT<ـٰ>بِ لَكَانَ خَیۡR<ر>X<ࣰ>ا لَّهُمۚ N<مِّ>نۡهُمُ ٱلۡمُؤۡمِنُونَ وَأَكۡثَR<رُ>هُمُ ٱلۡفَT<ـٰ>سِقُJ<و>نَ A<۝>D<١١٠>",
"لَX<ن> N<یَ>ضُR<رُّ>وكُمۡ إِلَّW<اۤ> أَذX<ࣰ>ىۖ N<وَ>إِX<ن> N<یُ>قَT<ـٰ>تِلُوكُمۡ یُوَلُّوكُمُ ٱلۡأَQ<دۡ>بَاR<رَ> ثُN<مَّ> لَا یُN<ن>صَR<رُ>J<و>نَ A<۝>D<١١١>",
"ضُرِبَتۡ عَلَیۡهِمُ ٱX<ل>ذِّلَّةُ أَیۡنَ مَا ثُقِفُW<وۤ>X<ا۟> إِلَّا بِحَQ<بۡ>لX<ࣲ> N<مِّ>نَ ٱللَّهِ وَحَQ<بۡ>لX<ࣲ> N<مِّ>نَ ٱX<ل>N<نَّ>اسِ وَبَW<اۤ>ءُو بِغَضَبX<ࣲ> N<مِّ>نَ ٱللَّهِ وَضُرِبَتۡ عَلَیۡهِمُ ٱلۡمَسۡكَنَةُۚ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ كَانُوX<ا۟> یَكۡفُR<رُ>ونَ بِءَایَT<ـٰ>تِ ٱللَّهِ وَیَQ<قۡ>تُلُونَ ٱلۡأَX<ن>N<ۢ>بِیَW<اۤ>ءَ بِغَیۡرِ حَقّN<ࣲ>ۚ ذَT< ٰ>لِكَ بِمَا عَصَوX<ا۟> وَّكَانُوX<ا۟> یَعۡتَدُJ<و>نَ A<۝>D<١١٢>",
"۞ لَیۡسُوX<ا۟> سَوَW<اۤ>ءX<ࣰ>ۗ N<مِّ>نۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ أُN<مَّ>ةN<ࣱ> قَW<اۤ>ئِمَةX<ࣱ> N<یَ>تۡلُونَ ءَایَT<ـٰ>تِ ٱللَّهِ ءَانَW<اۤ>ءَ ٱلَّیۡلِ وَهُمۡ یَسۡجُدُJ<و>نَ A<۝>D<١١٣>",
"یُؤۡمِنُونَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَیَأۡمُR<رُ>ونَ بِX<ٱ>لۡمَعۡR<رُ>وفِ وَیَنۡهَوۡنَ عَنِ ٱلۡمُN<ن>كَرِ وَیُسَT<ـٰ>رِعُونَ فِی ٱلۡخَیۡR<رَ>T< ٰ>تِۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ مِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<١١٤>",
"وَمَا یَفۡعَلُوX<ا۟> مِنۡ خَیۡرN<ࣲ> فَلَX<ن> N<یُ>كۡفَR<رُ>وهُۗ وَٱللَّهُ عَلِیمُN<ۢ> بِX<ٱ>لۡمُتَّقِJ<ی>نَ A<۝>D<١١٥>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَN<ن> تُغۡنِیَ عَنۡهُمۡ أَمۡوَT< ٰ>لُهُمۡ وَلَW<اۤ> أَوۡلَT<ـٰ>دُهُم N<مِّ>نَ ٱللَّهِ شَیۡءX<ࣰ>اۖ N<وَ>أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<١١٦>",
"مَثَلُ مَا یُN<ن>فِقُونَ فِی هَT<ـٰ>ذِهِ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا كَمَثَلِ رِیحN<ࣲ> فِیهَا صِR<رّ>ٌ أَصَابَتۡ حَR<رۡ>ثَ قَوۡمN<ࣲ> ظَلَمُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ فَأَهۡلَكَتۡهُۚ وَمَا ظَلَمَهُمُ ٱللَّهُ وَلَT<ـٰ>كِنۡ أَN<ن>فُسَهُمۡ یَظۡلِمُJ<و>نَ A<۝>D<١١٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَتَّخِذُوX<ا۟> بِطَانَةX<ࣰ> N<مِّ>N<ن> دُونِكُمۡ لَا یَأۡلُونَكُمۡ خَبَالX<ࣰ>ا N<وَ>دُّوX<ا۟> مَا عَنِتُّمۡ قَQ<دۡ> بَدَتِ ٱلۡبَغۡضَW<اۤ>ءُ مِنۡ أَفۡوَT< ٰ>هِهِمۡ وَمَا تُخۡفِی صُدُوR<رُ>هُمۡ أَكۡبَR<رُ>ۚ قَQ<دۡ> بَیَّN<نَّ>ا لَكُمُ ٱلۡءَایَT<ـٰ>تِۖ إِN<ن> كُN<ن>تُمۡ تَعۡقِلُJ<و>نَ A<۝>D<١١٨>",
"هَW<ـٰۤ>أَN<ن>تُمۡ أُX<و۟>لَW<اۤ>ءِ تُحِبُّونَهُمۡ وَلَا یُحِبُّونَكُمۡ وَتُؤۡمِنُونَ بِX<ٱ>لۡكِتَT<ـٰ>بِ كُلِّهِT<ۦ> وَإِذَا لَقُوكُمۡ قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا وَإِذَا خَلَوۡX<ا۟> عَضُّوX<ا۟> عَلَیۡكُمُ ٱلۡأَنَامِلَ مِنَ ٱلۡغَیۡظِۚ قُلۡ مُوتُوX<ا۟> بِغَیۡظِكُمۡۗ إِN<نَّ> ٱللَّهَ عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<١١٩>",
"إِN<ن> تَمۡسَسۡكُمۡ حَسَنَةN<ࣱ> تَسُؤۡهُمۡ وَإِN<ن> تُصِQ<بۡ>كُمۡ سَیِّئَةX<ࣱ> N<یَ>فۡR<رَ>حُوX<ا۟> بِهَاۖ وَإِN<ن> تَصۡبِR<رُ>وX<ا۟> وَتَتَّقُوX<ا۟> لَا یَضُR<رُّ>كُمۡ كَیۡدُهُمۡ شَیۡءًاۗ إِN<نَّ> ٱللَّهَ بِمَا یَعۡمَلُونَ مُحِJ<ی>Q<ط>ࣱ A<۝>D<١٢٠>",
"وَإِذۡ غَدَوۡتَ مِنۡ أَهۡلِكَ تُبَوِّئُ ٱلۡمُؤۡمِنِینَ مَقَT<ـٰ>عِدَ لِلۡقِتَالِۗ وَٱللَّهُ سَمِیعٌ عَلِJ<ی>مٌ A<۝>D<١٢١>",
"إِذۡ هَN<مَّ>ت طَّW<اۤ>ئِفَتَانِ مِN<ن>كُمۡ أَN<ن> تَفۡشَلَا وَٱللَّهُ وَلِیُّهُمَاۗ وَعَلَى ٱللَّهِ فَلۡیَتَوَكَّلِ ٱلۡمُؤۡمِنُJ<و>نَ A<۝>D<١٢٢>",
"وَلَقَQ<دۡ> نَصَR<رَ>كُمُ ٱللَّهُ بِبَQ<دۡ>رX<ࣲ> N<وَ>أَN<ن>تُمۡ أَذِلَّةN<ࣱ>ۖ فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ لَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<١٢٣>",
"إِذۡ تَقُولُ لِلۡمُؤۡمِنِینَ أَلَX<ن> N<یَ>كۡفِیَكُمۡ أَX<ن> N<یُ>مِدَّكُمۡ R<رَ>بُّكُN<م> بِثَلَT<ـٰ>ثَةِ ءَالَT<ـٰ>فX<ࣲ> N<مِّ>نَ ٱلۡمَلَW<ـٰۤ>ئِكَةِ مُN<ن>زَلِJ<ی>نَ A<۝>D<١٢٤>",
"بَلَىW<ٰۤ>ۚ إِN<ن> تَصۡبِR<رُ>وX<ا۟> وَتَتَّقُوX<ا۟> وَیَأۡتُوكُم N<مِّ>N<ن> فَوۡرِهِمۡ هَT<ـٰ>ذَا یُمۡدِQ<دۡ>كُمۡ R<رَ>بُّكُN<م> بِخَمۡسَةِ ءَالَT<ـٰ>فX<ࣲ> N<مِّ>نَ ٱلۡمَلَW<ـٰۤ>ئِكَةِ مُسَوِّمِJ<ی>نَ A<۝>D<١٢٥>",
"وَمَا جَعَلَهُ ٱللَّهُ إِلَّا بُشۡR<رَ>ىٰ لَكُمۡ وَلِتَQ<طۡ>مَئِN<نَّ> قُلُوبُكُN<م> بِهِT<ۦ>ۗ وَمَا ٱX<ل>N<نَّ>صۡR<رُ> إِلَّا مِنۡ عِN<ن>دِ ٱللَّهِ ٱلۡعَزِیزِ ٱلۡحَكِJ<ی>مِ A<۝>D<١٢٦>",
"لِیَQ<قۡ>طَعَ طَR<رَ>فX<ࣰ>ا N<مِّ>نَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَوۡ یَكۡبِتَهُمۡ فَیَN<ن>قَلِبُوX<ا۟> خَW<اۤ>ئِبِJ<ی>نَ A<۝>D<١٢٧>",
"لَیۡسَ لَكَ مِنَ ٱلۡأَمۡرِ شَیۡءٌ أَوۡ یَتُوبَ عَلَیۡهِمۡ أَوۡ یُعَذِّبَهُمۡ فَإِN<نَّ>هُمۡ ظَT<ـٰ>لِمُJ<و>نَ A<۝>D<١٢٨>",
"وَلِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۚ یَغۡفِR<رُ> لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیُعَذِّبُ مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٢٩>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَأۡكُلُوX<ا۟> ٱX<ل>رِّبَX<و>W<ٰۤ>X<ا۟> أَضۡعَT<ـٰ>فX<ࣰ>ا N<مُّ>ضَT<ـٰ>عَفَةX<ࣰ>ۖ N<وَ>X<ٱ>تَّقُوX<ا۟> ٱللَّهَ لَعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<١٣٠>",
"وَX<ٱ>تَّقُوX<ا۟> ٱX<ل>N<نَّ>اR<رَ> ٱلَّتِW<یۤ> أُعِدَّتۡ لِلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٣١>",
"وَأَطِیعُوX<ا۟> ٱللَّهَ وَX<ٱ>X<ل>R<رَّ>سُولَ لَعَلَّكُمۡ تُR<رۡ>حَمُJ<و>نَ A<۝>D<١٣٢>",
"۞ وَسَارِعُW<وۤ>X<ا۟> إِلَىٰ مَغۡفِR<رَ>ةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ وَجَN<نَّ>ةٍ عَR<رۡ>ضُهَا ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تُ وَX<ٱ>لۡأَR<رۡ>ضُ أُعِدَّتۡ لِلۡمُتَّقِJ<ی>نَ A<۝>D<١٣٣>",
"ٱلَّذِینَ یُN<ن>فِقُونَ فِی ٱX<ل>سَّR<رَّ>W<اۤ>ءِ وَX<ٱ>X<ل>ضَّR<رَّ>W<اۤ>ءِ وَX<ٱ>لۡكَT<ـٰ>ظِمِینَ ٱلۡغَیۡظَ وَX<ٱ>لۡعَافِینَ عَنِ ٱX<ل>N<نَّ>اسِۗ وَٱللَّهُ یُحِبُّ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٣٤>",
"وَX<ٱ>لَّذِینَ إِذَا فَعَلُوX<ا۟> فَT<ـٰ>حِشَةً أَوۡ ظَلَمُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ ذَكَR<رُ>وX<ا۟> ٱللَّهَ فَX<ٱ>سۡتَغۡفَR<رُ>وX<ا۟> لِذُنُوبِهِمۡ وَمَX<ن> N<یَ>غۡفِR<رُ> ٱX<ل>ذُّنُوبَ إِلَّا ٱللَّهُ وَلَمۡ یُصِR<رُّ>وX<ا۟> عَلَىٰ مَا فَعَلُوX<ا۟> وَهُمۡ یَعۡلَمُJ<و>نَ A<۝>D<١٣٥>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ جَزَW<اۤ>ؤُهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِمۡ وَجَN<نَّ>T<ـٰ>تN<ࣱ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَاۚ وَنِعۡمَ أَQ<جۡ>R<رُ> ٱلۡعَT<ـٰ>مِلِJ<ی>نَ A<۝>D<١٣٦>",
"قَQ<دۡ> خَلَتۡ مِN<ن> قَQ<بۡ>لِكُمۡ سُنَنN<ࣱ> فَسِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَX<ٱ>N<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُكَذِّبِJ<ی>نَ A<۝>D<١٣٧>",
"هَT<ـٰ>ذَا بَیَانX<ࣱ> لِّX<ل>N<نَّ>اسِ وَهُدX<ࣰ>ى N<وَ>مَوۡعِظَةX<ࣱ> لِّلۡمُتَّقِJ<ی>نَ A<۝>D<١٣٨>",
"وَلَا تَهِنُوX<ا۟> وَلَا تَحۡزَنُوX<ا۟> وَأَN<ن>تُمُ ٱلۡأَعۡلَوۡنَ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٣٩>",
"إِX<ن> N<یَ>مۡسَسۡكُمۡ قَR<رۡ>حN<ࣱ> فَقَQ<دۡ> مَسَّ ٱلۡقَوۡمَ قَR<رۡ>حX<ࣱ> N<مِّ>ثۡلُهُT<ۥ>ۚ وَتِلۡكَ ٱلۡأَیَّامُ نُدَاوِلُهَا بَیۡنَ ٱX<ل>N<نَّ>اسِ وَلِیَعۡلَمَ ٱللَّهُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَیَتَّخِذَ مِN<ن>كُمۡ شُهَدَW<اۤ>ءَۗ وَٱللَّهُ لَا یُحِبُّ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٤٠>",
"وَلِیُمَحِّصَ ٱللَّهُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَیَمۡحَقَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٤١>",
"أَمۡ حَسِQ<بۡ>تُمۡ أَN<ن> تَQ<دۡ>خُلُوX<ا۟> ٱلۡجَN<نَّ>ةَ وَلَN<مَّ>ا یَعۡلَمِ ٱللَّهُ ٱلَّذِینَ جَT<ـٰ>هَدُوX<ا۟> مِN<ن>كُمۡ وَیَعۡلَمَ ٱX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<١٤٢>",
"وَلَقَQ<دۡ> كُN<ن>تُمۡ تَمَN<نَّ>وۡنَ ٱلۡمَوۡتَ مِN<ن> قَQ<بۡ>لِ أَN<ن> تَلۡقَوۡهُ فَقَQ<دۡ> R<رَ>أَیۡتُمُوهُ وَأَN<ن>تُمۡ تَN<ن>ظُR<رُ>J<و>نَ A<۝>D<١٤٣>",
"وَمَا مُحَN<مَّ>دٌ إِلَّا R<رَ>سُولN<ࣱ> قَQ<دۡ> خَلَتۡ مِN<ن> قَQ<بۡ>لِهِ ٱX<ل>R<رُّ>سُلُۚ أَفَإِX<ی۟>X<ن> N<مَّ>اتَ أَوۡ قُتِلَ ٱN<ن>قَلَQ<بۡ>تُمۡ عَلَىW<ٰۤ> أَعۡقَT<ـٰ>بِكُمۡۚ وَمَX<ن> N<یَ>N<ن>قَلِQ<بۡ> عَلَىٰ عَقِبَیۡهِ فَلَX<ن> N<یَ>ضُR<رَّ> ٱللَّهَ شَیۡءX<ࣰ>اۗ N<وَ>سَیَQ<جۡ>زِی ٱللَّهُ ٱX<ل>شَّT<ـٰ>كِرِJ<ی>نَ A<۝>D<١٤٤>",
"وَمَا كَانَ لِنَفۡسٍ أَN<ن> تَمُوتَ إِلَّا بِإِذۡنِ ٱللَّهِ كِتَT<ـٰ>بX<ࣰ>ا N<مُّ>ؤَجَّلX<ࣰ>اۗ N<وَ>مَX<ن> N<یُ>رِQ<دۡ> ثَوَابَ ٱX<ل>دُّنۡیَا نُؤۡتِهِT<ۦ> مِنۡهَا وَمَX<ن> N<یُ>رِQ<دۡ> ثَوَابَ ٱلۡءَاخِR<رَ>ةِ نُؤۡتِهِT<ۦ> مِنۡهَاۚ وَسَنَQ<جۡ>زِی ٱX<ل>شَّT<ـٰ>كِرِJ<ی>نَ A<۝>D<١٤٥>",
"وَكَأَیِّX<ن> N<مِّ>ن N<نَّ>بِیّN<ࣲ> قَT<ـٰ>تَلَ مَعَهُT<ۥ> رِبِّیُّونَ كَثِیR<ر>N<ࣱ> فَمَا وَهَنُوX<ا۟> لِمَW<اۤ> أَصَابَهُمۡ فِی سَبِیلِ ٱللَّهِ وَمَا ضَعُفُوX<ا۟> وَمَا ٱسۡتَكَانُوX<ا۟>ۗ وَٱللَّهُ یُحِبُّ ٱX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<١٤٦>",
"وَمَا كَانَ قَوۡلَهُمۡ إِلَّW<اۤ> أَN<ن> قَالُوX<ا۟> R<رَ>بَّنَا ٱغۡفِرۡ لَنَا ذُنُوبَنَا وَإِسۡR<رَ>افَنَا فِW<یۤ> أَمۡرِنَا وَثَبِّتۡ أَQ<قۡ>دَامَنَا وَX<ٱ>N<ن>صُR<رۡ>نَا عَلَى ٱلۡقَوۡمِ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٤٧>",
"فَءَاتَX<ى>T<ٰ>هُمُ ٱللَّهُ ثَوَابَ ٱX<ل>دُّنۡیَا وَحُسۡنَ ثَوَابِ ٱلۡءَاخِR<رَ>ةِۗ وَٱللَّهُ یُحِبُّ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٤٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<ن> تُطِیعُوX<ا۟> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> یَR<رُ>دُّوكُمۡ عَلَىW<ٰۤ> أَعۡقَT<ـٰ>بِكُمۡ فَتَN<ن>قَلِبُوX<ا۟> خَT<ـٰ>سِرِJ<ی>نَ A<۝>D<١٤٩>",
"بَلِ ٱللَّهُ مَوۡلَX<ى>T<ٰ>كُمۡۖ وَهُوَ خَیۡR<رُ> ٱX<ل>N<نَّ>T<ـٰ>صِرِJ<ی>نَ A<۝>D<١٥٠>",
"سَنُلۡقِی فِی قُلُوبِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> ٱX<ل>R<رُّ>عۡبَ بِمَW<اۤ> أَشۡR<رَ>كُوX<ا۟> بِٱللَّهِ مَا لَمۡ یُنَزِّلۡ بِهِT<ۦ> سُلۡطَT<ـٰ>نX<ࣰ>اۖ N<وَ>مَأۡوَX<ى>T<ٰ>هُمُ ٱX<ل>N<نَّ>اR<رُ>ۖ وَبِئۡسَ مَثۡوَى ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٥١>",
"وَلَقَQ<دۡ> صَدَقَكُمُ ٱللَّهُ وَعۡدَهُW<ۥۤ> إِذۡ تَحُسُّونَهُN<م> بِإِذۡنِهِT<ۦ>ۖ حَتَّىW<ٰۤ> إِذَا فَشِلۡتُمۡ وَتَنَT<ـٰ>زَعۡتُمۡ فِی ٱلۡأَمۡرِ وَعَصَیۡتُم N<مِّ>X<ن>N<ۢ> بَعۡدِ مَW<اۤ> أَR<رَ>X<ى>T<ٰ>كُم N<مَّ>ا تُحِبُّونَۚ مِN<ن>كُم N<مَّ>X<ن> N<یُ>رِیدُ ٱX<ل>دُّنۡیَا وَمِN<ن>كُم N<مَّ>X<ن> N<یُ>رِیدُ ٱلۡءَاخِR<رَ>ةَۚ ثُN<مَّ> صَR<رَ>فَكُمۡ عَنۡهُمۡ لِیَQ<بۡ>تَلِیَكُمۡۖ وَلَقَQ<دۡ> عَفَا عَN<ن>كُمۡۗ وَٱللَّهُ ذُو فَضۡلٍ عَلَى ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٥٢>",
"۞ إِذۡ تُصۡعِدُونَ وَلَا تَلۡوُT<ۥ>نَ عَلَىW<ٰۤ> أَحَدX<ࣲ> N<وَ>X<ٱ>X<ل>R<رَّ>سُولُ یَQ<دۡ>عُوكُمۡ فِW<یۤ> أُخۡR<رَ>X<ى>T<ٰ>كُمۡ فَأَثَT<ـٰ>بَكُمۡ غَN<مَّ>N<ۢ>ا بِغَN<مّ>X<ࣲ> لِّكَیۡلَا تَحۡزَنُوX<ا۟> عَلَىٰ مَا فَاتَكُمۡ وَلَا مَW<اۤ> أَصَT<ـٰ>بَكُمۡۗ وَٱللَّهُ خَبِیR<رُ>N<ۢ> بِمَا تَعۡمَلُJ<و>نَ A<۝>D<١٥٣>",
"ثُN<مَّ> أَN<ن>زَلَ عَلَیۡكُم N<مِّ>X<ن>N<ۢ> بَعۡدِ ٱلۡغَN<مِّ> أَمَنَةX<ࣰ> N<نُّ>عَاسX<ࣰ>ا N<یَ>غۡشَىٰ طَW<اۤ>ئِفَةX<ࣰ> N<مِّ>N<ن>كُمۡۖ وَطَW<اۤ>ئِفَةN<ࣱ> قَQ<دۡ> أَهَN<مَّ>تۡهُمۡ أَN<ن>فُسُهُمۡ یَظُN<نُّ>ونَ بِٱللَّهِ غَیۡR<رَ> ٱلۡحَقِّ ظَN<نَّ> ٱلۡجَT<ـٰ>هِلِیَّةِۖ یَقُولُونَ هَل لَّنَا مِنَ ٱلۡأَمۡرِ مِN<ن> شَیۡءN<ࣲ>ۗ قُلۡ إِN<نَّ> ٱلۡأَمۡR<رَ> كُلَّهُT<ۥ> لِلَّهِۗ یُخۡفُونَ فِW<یۤ> أَN<ن>فُسِهِم N<مَّ>ا لَا یُQ<بۡ>دُونَ لَكَۖ یَقُولُونَ لَوۡ كَانَ لَنَا مِنَ ٱلۡأَمۡرِ شَیۡءX<ࣱ> N<مَّ>ا قُتِلۡنَا هَT<ـٰ>هُنَاۗ قُل لَّوۡ كُN<ن>تُمۡ فِی بُیُوتِكُمۡ لَبَR<رَ>زَ ٱلَّذِینَ كُتِبَ عَلَیۡهِمُ ٱلۡقَتۡلُ إِلَىٰ مَضَاجِعِهِمۡۖ وَلِیَQ<بۡ>تَلِیَ ٱللَّهُ مَا فِی صُدُورِكُمۡ وَلِیُمَحِّصَ مَا فِی قُلُوبِكُمۡۚ وَٱللَّهُ عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<١٥٤>",
"إِN<نَّ> ٱلَّذِینَ تَوَلَّوۡX<ا۟> مِN<ن>كُمۡ یَوۡمَ ٱلۡتَقَى ٱلۡجَمۡعَانِ إِN<نَّ>مَا ٱسۡتَزَلَّهُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ بِبَعۡضِ مَا كَسَبُوX<ا۟>ۖ وَلَقَQ<دۡ> عَفَا ٱللَّهُ عَنۡهُمۡۗ إِN<نَّ> ٱللَّهَ غَفُوR<ر>ٌ حَلِJ<ی>مࣱ A<۝>D<١٥٥>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَكُونُوX<ا۟> كَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> وَقَالُوX<ا۟> لِإِخۡوَT< ٰ>نِهِمۡ إِذَا ضَR<رَ>بُوX<ا۟> فِی ٱلۡأَR<رۡ>ضِ أَوۡ كَانُوX<ا۟> غُزّX<ࣰ>ى لَّوۡ كَانُوX<ا۟> عِN<ن>دَنَا مَا مَاتُوX<ا۟> وَمَا قُتِلُوX<ا۟> لِیَQ<جۡ>عَلَ ٱللَّهُ ذَT< ٰ>لِكَ حَسۡR<رَ>ةN<ࣰ> فِی قُلُوبِهِمۡۗ وَٱللَّهُ یُحۡیِT<ۦ> وَیُمِیتُۗ وَٱللَّهُ بِمَا تَعۡمَلُونَ بَصِJ<ی>رࣱ A<۝>D<١٥٦>",
"وَلَئِN<ن> قُتِلۡتُمۡ فِی سَبِیلِ ٱللَّهِ أَوۡ مُتُّمۡ لَمَغۡفِR<رَ>ةX<ࣱ> N<مِّ>نَ ٱللَّهِ وَR<رَ>حۡمَةٌ خَیۡR<ر>X<ࣱ> N<مِّ>N<مَّ>ا یَQ<جۡ>مَعُJ<و>نَ A<۝>D<١٥٧>",
"وَلَئِX<ن> N<مُّ>تُّمۡ أَوۡ قُتِلۡتُمۡ لَإِلَى ٱللَّهِ تُحۡشَR<رُ>J<و>نَ A<۝>D<١٥٨>",
"فَبِمَا R<رَ>حۡمَةX<ࣲ> N<مِّ>نَ ٱللَّهِ لِN<ن>تَ لَهُمۡۖ وَلَوۡ كُN<ن>تَ فَظًّا غَلِیظَ ٱلۡقَلۡبِ لَX<ٱ>N<ن>فَضُّوX<ا۟> مِنۡ حَوۡلِكَۖ فَX<ٱ>عۡفُ عَنۡهُمۡ وَX<ٱ>سۡتَغۡفِرۡ لَهُمۡ وَشَاوِرۡهُمۡ فِی ٱلۡأَمۡرِۖ فَإِذَا عَزَمۡتَ فَتَوَكَّلۡ عَلَى ٱللَّهِۚ إِN<نَّ> ٱللَّهَ یُحِبُّ ٱلۡمُتَوَكِّلِJ<ی>نَ A<۝>D<١٥٩>",
"إِX<ن> N<یَ>N<ن>صُR<رۡ>كُمُ ٱللَّهُ فَلَا غَالِبَ لَكُمۡۖ وَإِX<ن> N<یَ>خۡذُلۡكُمۡ فَمَN<ن> ذَا ٱلَّذِی یَN<ن>صُR<رُ>كُم N<مِّ>X<ن>N<ۢ> بَعۡدِهِT<ۦ>ۗ وَعَلَى ٱللَّهِ فَلۡیَتَوَكَّلِ ٱلۡمُؤۡمِنُJ<و>نَ A<۝>D<١٦٠>",
"وَمَا كَانَ لِنَبِیٍّ أَX<ن> N<یَ>غُلَّۚ وَمَX<ن> N<یَ>غۡلُلۡ یَأۡتِ بِمَا غَلَّ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۚ ثُN<مَّ> تُوَفَّىٰ كُلُّ نَفۡسX<ࣲ> N<مَّ>ا كَسَبَتۡ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<١٦١>",
"أَفَمَنِ ٱتَّبَعَ رِضۡوَT< ٰ>نَ ٱللَّهِ كَمَX<ن>N<ۢ> بَW<اۤ>ءَ بِسَخَطX<ࣲ> N<مِّ>نَ ٱللَّهِ وَمَأۡوَX<ى>T<ٰ>هُ جَهَN<نَّ>مُۖ وَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<١٦٢>",
"هُمۡ دَR<رَ>جَT<ـٰ>تٌ عِN<ن>دَ ٱللَّهِۗ وَٱللَّهُ بَصِیR<رُ>N<ۢ> بِمَا یَعۡمَلُJ<و>نَ A<۝>D<١٦٣>",
"لَقَQ<دۡ> مَN<نَّ> ٱللَّهُ عَلَى ٱلۡمُؤۡمِنِینَ إِذۡ بَعَثَ فِیهِمۡ R<رَ>سُولX<ࣰ>ا N<مِّ>نۡ أَN<ن>فُسِهِمۡ یَتۡلُوX<ا۟> عَلَیۡهِمۡ ءَایَT<ـٰ>تِهِT<ۦ> وَیُزَكِّیهِمۡ وَیُعَلِّمُهُمُ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحِكۡمَةَ وَإِN<ن> كَانُوX<ا۟> مِN<ن> قَQ<بۡ>لُ لَفِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<١٦٤>",
"أَوَ لَN<مَّ>W<اۤ> أَصَT<ـٰ>بَتۡكُم N<مُّ>صِیبَةN<ࣱ> قَQ<دۡ> أَصَQ<بۡ>تُم N<مِّ>ثۡلَیۡهَا قُلۡتُمۡ أَN<نَّ>ىٰ هَT<ـٰ>ذَاۖ قُلۡ هُوَ مِنۡ عِN<ن>دِ أَN<ن>فُسِكُمۡۗ إِN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<١٦٥>",
"وَمَW<اۤ> أَصَT<ـٰ>بَكُمۡ یَوۡمَ ٱلۡتَقَى ٱلۡجَمۡعَانِ فَبِإِذۡنِ ٱللَّهِ وَلِیَعۡلَمَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٦٦>",
"وَلِیَعۡلَمَ ٱلَّذِینَ نَافَقُوX<ا۟>ۚ وَقِیلَ لَهُمۡ تَعَالَوۡX<ا۟> قَT<ـٰ>تِلُوX<ا۟> فِی سَبِیلِ ٱللَّهِ أَوِ ٱQ<دۡ>فَعُوX<ا۟>ۖ قَالُوX<ا۟> لَوۡ نَعۡلَمُ قِتَالX<ࣰ>ا لَّX<ٱ>تَّبَعۡنَT<ـٰ>كُمۡۗ هُمۡ لِلۡكُفۡرِ یَوۡمَئِذٍ أَQ<قۡ>R<رَ>بُ مِنۡهُمۡ لِلۡإِیمَT<ـٰ>نِۚ یَقُولُونَ بِأَفۡوَT< ٰ>هِهِم N<مَّ>ا لَیۡسَ فِی قُلُوبِهِمۡۚ وَٱللَّهُ أَعۡلَمُ بِمَا یَكۡتُمُJ<و>نَ A<۝>D<١٦٧>",
"ٱلَّذِینَ قَالُوX<ا۟> لِإِخۡوَT< ٰ>نِهِمۡ وَقَعَدُوX<ا۟> لَوۡ أَطَاعُونَا مَا قُتِلُوX<ا۟>ۗ قُلۡ فَX<ٱ>Q<دۡ>R<رَ>ءُوX<ا۟> عَنۡ أَN<ن>فُسِكُمُ ٱلۡمَوۡتَ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٦٨>",
"وَلَا تَحۡسَبَN<نَّ> ٱلَّذِینَ قُتِلُوX<ا۟> فِی سَبِیلِ ٱللَّهِ أَمۡوَT< ٰ>تَN<ۢ>اۚ بَلۡ أَحۡیَW<اۤ>ءٌ عِN<ن>دَ R<رَ>بِّهِمۡ یُR<رۡ>زَقُJ<و>نَ A<۝>D<١٦٩>",
"فَرِحِینَ بِمَW<اۤ> ءَاتَX<ى>T<ٰ>هُمُ ٱللَّهُ مِN<ن> فَضۡلِهِT<ۦ> وَیَسۡتَQ<بۡ>شِR<رُ>ونَ بِX<ٱ>لَّذِینَ لَمۡ یَلۡحَقُوX<ا۟> بِهِم N<مِّ>نۡ خَلۡفِهِمۡ أَلَّا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<١٧٠>",
"۞ یَسۡتَQ<بۡ>شِR<رُ>ونَ بِنِعۡمَةX<ࣲ> N<مِّ>نَ ٱللَّهِ وَفَضۡلX<ࣲ> N<وَ>أَN<نَّ> ٱللَّهَ لَا یُضِیعُ أَQ<جۡ>R<رَ> ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٧١>",
"ٱلَّذِینَ ٱسۡتَجَابُوX<ا۟> لِلَّهِ وَX<ٱ>X<ل>R<رَّ>سُولِ مِX<ن>N<ۢ> بَعۡدِ مَW<اۤ> أَصَابَهُمُ ٱلۡقَR<رۡ>حُۚ لِلَّذِینَ أَحۡسَنُوX<ا۟> مِنۡهُمۡ وَX<ٱ>تَّقَوۡX<ا۟> أَQ<جۡ>R<ر>ٌ عَظِJ<ی>مٌ A<۝>D<١٧٢>",
"ٱلَّذِینَ قَالَ لَهُمُ ٱX<ل>N<نَّ>اسُ إِN<نَّ> ٱX<ل>N<نَّ>اسَ قَQ<دۡ> جَمَعُوX<ا۟> لَكُمۡ فَX<ٱ>خۡشَوۡهُمۡ فَزَادَهُمۡ إِیمَT<ـٰ>نX<ࣰ>ا N<وَ>قَالُوX<ا۟> حَسۡبُنَا ٱللَّهُ وَنِعۡمَ ٱلۡوَكِJ<ی>لُ A<۝>D<١٧٣>",
"فَX<ٱ>N<ن>قَلَبُوX<ا۟> بِنِعۡمَةX<ࣲ> N<مِّ>نَ ٱللَّهِ وَفَضۡلX<ࣲ> لَّمۡ یَمۡسَسۡهُمۡ سُW<وۤ>ءX<ࣱ> N<وَ>X<ٱ>تَّبَعُوX<ا۟> رِضۡوَT< ٰ>نَ ٱللَّهِۗ وَٱللَّهُ ذُو فَضۡلٍ عَظِJ<ی>مٍ A<۝>D<١٧٤>",
"إِN<نَّ>مَا ذَT< ٰ>لِكُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ یُخَوِّفُ أَوۡلِیَW<اۤ>ءَهُT<ۥ> فَلَا تَخَافُوهُمۡ وَخَافُونِ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٧٥>",
"وَلَا یَحۡزُN<ن>كَ ٱلَّذِینَ یُسَT<ـٰ>رِعُونَ فِی ٱلۡكُفۡرِۚ إِN<نَّ>هُمۡ لَX<ن> N<یَ>ضُR<رُّ>وX<ا۟> ٱللَّهَ شَیۡءX<ࣰ>اۗ N<یُ>رِیدُ ٱللَّهُ أَلَّا یَQ<جۡ>عَلَ لَهُمۡ حَظّN<ࣰ>ا فِی ٱلۡءَاخِR<رَ>ةِۖ وَلَهُمۡ عَذَابٌ عَظِJ<ی>مٌ A<۝>D<١٧٦>",
"إِN<نَّ> ٱلَّذِینَ ٱشۡتَR<رَ>وُX<ا۟> ٱلۡكُفۡR<رَ> بِX<ٱ>لۡإِیمَT<ـٰ>نِ لَX<ن> N<یَ>ضُR<رُّ>وX<ا۟> ٱللَّهَ شَیۡءX<ࣰ>اۖ N<وَ>لَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<١٧٧>",
"وَلَا یَحۡسَبَN<نَّ> ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَN<نَّ>مَا نُمۡلِی لَهُمۡ خَیۡR<ر>X<ࣱ> لِّأَN<ن>فُسِهِمۡۚ إِN<نَّ>مَا نُمۡلِی لَهُمۡ لِیَزۡدَادُW<وۤ>X<ا۟> إِثۡمX<ࣰ>اۖ N<وَ>لَهُمۡ عَذَابX<ࣱ> N<مُّ>هِJ<ی>نࣱ A<۝>D<١٧٨>",
"مَّا كَانَ ٱللَّهُ لِیَذَR<رَ> ٱلۡمُؤۡمِنِینَ عَلَىٰ مَW<اۤ> أَN<ن>تُمۡ عَلَیۡهِ حَتَّىٰ یَمِیزَ ٱلۡخَبِیثَ مِنَ ٱX<ل>طَّیِّبِۗ وَمَا كَانَ ٱللَّهُ لِیُQ<طۡ>لِعَكُمۡ عَلَى ٱلۡغَیۡبِ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ یَQ<جۡ>تَبِی مِX<ن> R<رُّ>سُلِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُۖ فَءَامِنُوX<ا۟> بِٱللَّهِ وَR<رُ>سُلِهِT<ۦ>ۚ وَإِN<ن> تُؤۡمِنُوX<ا۟> وَتَتَّقُوX<ا۟> فَلَكُمۡ أَQ<جۡ>R<ر>ٌ عَظِJ<ی>مࣱ A<۝>D<١٧٩>",
"وَلَا یَحۡسَبَN<نَّ> ٱلَّذِینَ یَQ<بۡ>خَلُونَ بِمَW<اۤ> ءَاتَX<ى>T<ٰ>هُمُ ٱللَّهُ مِN<ن> فَضۡلِهِT<ۦ> هُوَ خَیۡR<ر>X<ࣰ>ا لَّهُN<م>ۖ بَلۡ هُوَ شَR<رّ>X<ࣱ> لَّهُمۡۖ سَیُطَوَّقُونَ مَا بَخِلُوX<ا۟> بِهِT<ۦ> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۗ وَلِلَّهِ مِیR<رَ>T< ٰ>ثُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۗ وَٱللَّهُ بِمَا تَعۡمَلُونَ خَبِJ<ی>رࣱ A<۝>D<١٨٠>",
"لَّقَQ<دۡ> سَمِعَ ٱللَّهُ قَوۡلَ ٱلَّذِینَ قَالُW<وۤ>X<ا۟> إِN<نَّ> ٱللَّهَ فَقِیR<ر>X<ࣱ> N<وَ>نَحۡنُ أَغۡنِیَW<اۤ>ءُۘ سَنَكۡتُبُ مَا قَالُوX<ا۟> وَقَتۡلَهُمُ ٱلۡأَX<ن>N<ۢ>بِیَW<اۤ>ءَ بِغَیۡرِ حَقّX<ࣲ> N<وَ>نَقُولُ ذُوقُوX<ا۟> عَذَابَ ٱلۡحَرِJ<ی>Q<ق>ِ A<۝>D<١٨١>",
"ذَT< ٰ>لِكَ بِمَا قَدَّمَتۡ أَیۡدِیكُمۡ وَأَN<نَّ> ٱللَّهَ لَیۡسَ بِظَلَّامX<ࣲ> لِّلۡعَبِJ<ی>Q<د>ِ A<۝>D<١٨٢>",
"ٱلَّذِینَ قَالُW<وۤ>X<ا۟> إِN<نَّ> ٱللَّهَ عَهِدَ إِلَیۡنَW<اۤ> أَلَّا نُؤۡمِنَ لِR<رَ>سُولٍ حَتَّىٰ یَأۡتِیَنَا بِقُR<رۡ>بَانN<ࣲ> تَأۡكُلُهُ ٱX<ل>N<نَّ>اR<رُ>ۗ قُلۡ قَQ<دۡ> جَW<اۤ>ءَكُمۡ R<رُ>سُلX<ࣱ> N<مِّ>N<ن> قَQ<بۡ>لِی بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ وَبِX<ٱ>لَّذِی قُلۡتُمۡ فَلِمَ قَتَلۡتُمُوهُمۡ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٨٣>",
"فَإِN<ن> كَذَّبُوكَ فَقَQ<دۡ> كُذِّبَ R<رُ>سُلX<ࣱ> N<مِّ>N<ن> قَQ<بۡ>لِكَ جَW<اۤ>ءُو بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ وَX<ٱ>X<ل>زُّبُرِ وَX<ٱ>لۡكِتَT<ـٰ>بِ ٱلۡمُنِJ<ی>رِ A<۝>D<١٨٤>",
"كُلُّ نَفۡسN<ࣲ> ذَW<اۤ>ئِقَةُ ٱلۡمَوۡتِۗ وَإِN<نَّ>مَا تُوَفَّوۡنَ أُجُوR<رَ>كُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۖ فَمَN<ن> زُحۡزِحَ عَنِ ٱX<ل>N<نَّ>ارِ وَأُQ<دۡ>خِلَ ٱلۡجَN<نَّ>ةَ فَقَQ<دۡ> فَازَۗ وَمَا ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَW<اۤ> إِلَّا مَتَT<ـٰ>عُ ٱلۡغُR<رُ>J<و>R<ر>ِ A<۝>D<١٨٥>",
"۞ لَتُQ<بۡ>لَوُN<نَّ> فِW<یۤ> أَمۡوَT< ٰ>لِكُمۡ وَأَN<ن>فُسِكُمۡ وَلَتَسۡمَعُN<نَّ> مِنَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ مِN<ن> قَQ<بۡ>لِكُمۡ وَمِنَ ٱلَّذِینَ أَشۡR<رَ>كُW<وۤ>X<ا۟> أَذN<ࣰ>ى كَثِیR<ر>X<ࣰ>اۚ N<وَ>إِN<ن> تَصۡبِR<رُ>وX<ا۟> وَتَتَّقُوX<ا۟> فَإِN<نَّ> ذَT< ٰ>لِكَ مِنۡ عَزۡمِ ٱلۡأُمُJ<و>R<ر>ِ A<۝>D<١٨٦>",
"وَإِذۡ أَخَذَ ٱللَّهُ مِیثَT<ـٰ>قَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ لَتُبَیِّنُN<نَّ>هُT<ۥ> لِX<ل>N<نَّ>اسِ وَلَا تَكۡتُمُونَهُT<ۥ> فَنَبَذُوهُ وَR<رَ>W<اۤ>ءَ ظُهُورِهِمۡ وَX<ٱ>شۡتَR<رَ>وۡX<ا۟> بِهِT<ۦ> ثَمَنN<ࣰ>ا قَلِیلN<ࣰ>اۖ فَبِئۡسَ مَا یَشۡتَR<رُ>J<و>نَ A<۝>D<١٨٧>",
"لَا تَحۡسَبَN<نَّ> ٱلَّذِینَ یَفۡR<رَ>حُونَ بِمَW<اۤ> أَتَوX<ا۟> وَّیُحِبُّونَ أَX<ن> N<یُ>حۡمَدُوX<ا۟> بِمَا لَمۡ یَفۡعَلُوX<ا۟> فَلَا تَحۡسَبَN<نَّ>هُN<م> بِمَفَازَةX<ࣲ> N<مِّ>نَ ٱلۡعَذَابِۖ وَلَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<١٨٨>",
"وَلِلَّهِ مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۗ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<١٨٩>",
"إِN<نَّ> فِی خَلۡقِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَX<ٱ>خۡتِلَT<ـٰ>فِ ٱلَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِ لَءَایَT<ـٰ>تX<ࣲ> لِّأُX<و۟>لِی ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<١٩٠>",
"ٱلَّذِینَ یَذۡكُR<رُ>ونَ ٱللَّهَ قِیَT<ـٰ>مX<ࣰ>ا N<وَ>قُعُودX<ࣰ>ا N<وَ>عَلَىٰ جُنُوبِهِمۡ وَیَتَفَكَّR<رُ>ونَ فِی خَلۡقِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ R<رَ>بَّنَا مَا خَلَQ<قۡ>تَ هَT<ـٰ>ذَا بَT<ـٰ>طِلN<ࣰ>ا سُQ<بۡ>حَT<ـٰ>نَكَ فَقِنَا عَذَابَ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<١٩١>",
"R<رَ>بَّنَW<اۤ> إِN<نَّ>كَ مَN<ن> تُQ<دۡ>خِلِ ٱX<ل>N<نَّ>اR<رَ> فَقَQ<دۡ> أَخۡزَیۡتَهُT<ۥ>ۖ وَمَا لِX<ل>ظَّT<ـٰ>لِمِینَ مِنۡ أَN<ن>صَJ<ا>R<ر>ࣲ A<۝>D<١٩٢>",
"R<رَّ>بَّنَW<اۤ> إِN<نَّ>نَا سَمِعۡنَا مُنَادِیX<ࣰ>ا N<یُ>نَادِی لِلۡإِیمَT<ـٰ>نِ أَنۡ ءَامِنُوX<ا۟> بِR<رَ>بِّكُمۡ فَءَامَN<نَّ>اۚ R<رَ>بَّنَا فَX<ٱ>غۡفِرۡ لَنَا ذُنُوبَنَا وَكَفِّرۡ عَN<نَّ>ا سَیِّءَاتِنَا وَتَوَفَّنَا مَعَ ٱلۡأَQ<بۡ>R<رَ>J<ا>R<ر>ِ A<۝>D<١٩٣>",
"R<رَ>بَّنَا وَءَاتِنَا مَا وَعَX<د>تَّنَا عَلَىٰ R<رُ>سُلِكَ وَلَا تُخۡزِنَا یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۖ إِN<نَّ>كَ لَا تُخۡلِفُ ٱلۡمِیعَJ<ا>Q<د>َ A<۝>D<١٩٤>",
"فَX<ٱ>سۡتَجَابَ لَهُمۡ R<رَ>بُّهُمۡ أَN<نِّ>ی لَW<اۤ> أُضِیعُ عَمَلَ عَT<ـٰ>مِلX<ࣲ> N<مِّ>N<ن>كُم N<مِّ>N<ن> ذَكَرٍ أَوۡ أُN<ن>ثَىٰۖ بَعۡضُكُم N<مِّ>X<ن>N<ۢ> بَعۡضN<ࣲ>ۖ فَX<ٱ>لَّذِینَ هَاجَR<رُ>وX<ا۟> وَأُخۡرِجُوX<ا۟> مِN<ن> دِیَT<ـٰ>رِهِمۡ وَأُوذُوX<ا۟> فِی سَبِیلِی وَقَT<ـٰ>تَلُوX<ا۟> وَقُتِلُوX<ا۟> لَأُكَفِّR<رَ>N<نَّ> عَنۡهُمۡ سَیِّءَاتِهِمۡ وَلَأُQ<دۡ>خِلَN<نَّ>هُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> ثَوَابX<ࣰ>ا N<مِّ>نۡ عِN<ن>دِ ٱللَّهِۚ وَٱللَّهُ عِN<ن>دَهُT<ۥ> حُسۡنُ ٱX<ل>ثَّوَJ<ا>Q<ب>ِ A<۝>D<١٩٥>",
"لَا یَغُR<رَّ>N<نَّ>كَ تَقَلُّبُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فِی ٱلۡبِلَT<ـٰ>Q<د>ِ A<۝>D<١٩٦>",
"مَتَT<ـٰ>عN<ࣱ> قَلِیلN<ࣱ> ثُN<مَّ> مَأۡوَX<ى>T<ٰ>هُمۡ جَهَN<نَّ>مُۖ وَبِئۡسَ ٱلۡمِهَJ<ا>Q<د>ُ A<۝>D<١٩٧>",
"لَT<ـٰ>كِنِ ٱلَّذِینَ ٱتَّقَوۡX<ا۟> R<رَ>بَّهُمۡ لَهُمۡ جَN<نَّ>T<ـٰ>تN<ࣱ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَا نُزُلX<ࣰ>ا N<مِّ>نۡ عِN<ن>دِ ٱللَّهِۗ وَمَا عِN<ن>دَ ٱللَّهِ خَیۡR<ر>X<ࣱ> لِّلۡأَQ<بۡ>R<رَ>J<ا>R<ر>ِ A<۝>D<١٩٨>",
"وَإِN<نَّ> مِنۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ لَمَX<ن> N<یُ>ؤۡمِنُ بِٱللَّهِ وَمَW<اۤ> أُN<ن>زِلَ إِلَیۡكُمۡ وَمَW<اۤ> أُN<ن>زِلَ إِلَیۡهِمۡ خَT<ـٰ>شِعِینَ لِلَّهِ لَا یَشۡتَR<رُ>ونَ بِءَایَT<ـٰ>تِ ٱللَّهِ ثَمَنN<ࣰ>ا قَلِیلًاۚ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ أَQ<جۡ>R<رُ>هُمۡ عِN<ن>دَ R<رَ>بِّهِمۡۗ إِN<نَّ> ٱللَّهَ سَرِیعُ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<١٩٩>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱصۡبِR<رُ>وX<ا۟> وَصَابِR<رُ>وX<ا۟> وَR<رَ>ابِطُوX<ا۟> وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ لَعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<٢٠٠>",
"#۞ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ ٱتَّقُوX<ا۟> R<رَ>بَّكُمُ ٱلَّذِی خَلَقَكُم N<مِّ>ن N<نَّ>فۡسX<ࣲ> N<وَ>T< ٰ>حِدَةX<ࣲ> N<وَ>خَلَقَ مِنۡهَا زَوۡجَهَا وَبَثَّ مِنۡهُمَا رِجَالN<ࣰ>ا كَثِیR<ر>X<ࣰ>ا N<وَ>نِسَW<اۤ>ءX<ࣰ>ۚ N<وَ>X<ٱ>تَّقُوX<ا۟> ٱللَّهَ ٱلَّذِی تَسَW<اۤ>ءَلُونَ بِهِT<ۦ> وَX<ٱ>لۡأَR<رۡ>حَامَۚ إِN<نَّ> ٱللَّهَ كَانَ عَلَیۡكُمۡ R<رَ>قِیبࣰا A<۝>D<١>",
"وَءَاتُوX<ا۟> ٱلۡیَتَT<ـٰ>مَىW<ٰۤ> أَمۡوَT< ٰ>لَهُمۡۖ وَلَا تَتَبَدَّلُوX<ا۟> ٱلۡخَبِیثَ بِX<ٱ>X<ل>طَّیِّبِۖ وَلَا تَأۡكُلُW<وۤ>X<ا۟> أَمۡوَT< ٰ>لَهُمۡ إِلَىW<ٰۤ> أَمۡوَT< ٰ>لِكُمۡۚ إِN<نَّ>هُT<ۥ> كَانَ حُوبN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٢>",
"وَإِنۡ خِفۡتُمۡ أَلَّا تُQ<قۡ>سِطُوX<ا۟> فِی ٱلۡیَتَT<ـٰ>مَىٰ فَX<ٱ>N<ن>كِحُوX<ا۟> مَا طَابَ لَكُم N<مِّ>نَ ٱX<ل>N<نِّ>سَW<اۤ>ءِ مَثۡنَىٰ وَثُلَT<ـٰ>ثَ وَR<رُ>بَT<ـٰ>عَۖ فَإِنۡ خِفۡتُمۡ أَلَّا تَعۡدِلُوX<ا۟> فَوَT< ٰ>حِدَةً أَوۡ مَا مَلَكَتۡ أَیۡمَT<ـٰ>نُكُمۡۚ ذَT< ٰ>لِكَ أَQ<دۡ>نَىW<ٰۤ> أَلَّا تَعُولُوX<ا۟> A<۝>D<٣>",
"وَءَاتُوX<ا۟> ٱX<ل>N<نِّ>سَW<اۤ>ءَ صَدُقَT<ـٰ>تِهِN<نَّ> نِحۡلَةN<ࣰ>ۚ فَإِN<ن> طِQ<بۡ>نَ لَكُمۡ عَN<ن> شَیۡءX<ࣲ> N<مِّ>نۡهُ نَفۡسN<ࣰ>ا فَكُلُوهُ هَنِW<یۤ>ءX<ࣰ>ا N<مَّ>رِW<یۤ>ءࣰا A<۝>D<٤>",
"وَلَا تُؤۡتُوX<ا۟> ٱX<ل>سُّفَهَW<اۤ>ءَ أَمۡوَT< ٰ>لَكُمُ ٱلَّتِی جَعَلَ ٱللَّهُ لَكُمۡ قِیَT<ـٰ>مX<ࣰ>ا N<وَ>X<ٱ>R<رۡ>زُقُوهُمۡ فِیهَا وَX<ٱ>كۡسُوهُمۡ وَقُولُوX<ا۟> لَهُمۡ قَوۡلX<ࣰ>ا N<مَّ>عۡR<رُ>وفࣰا A<۝>D<٥>",
"وَX<ٱ>Q<بۡ>تَلُوX<ا۟> ٱلۡیَتَT<ـٰ>مَىٰ حَتَّىW<ٰۤ> إِذَا بَلَغُوX<ا۟> ٱX<ل>N<نِّ>كَاحَ فَإِنۡ ءَانَسۡتُم N<مِّ>نۡهُمۡ R<رُ>شۡدN<ࣰ>ا فَX<ٱ>Q<دۡ>فَعُW<وۤ>X<ا۟> إِلَیۡهِمۡ أَمۡوَT< ٰ>لَهُمۡۖ وَلَا تَأۡكُلُوهَW<اۤ> إِسۡR<رَ>افX<ࣰ>ا N<وَ>بِدَاR<ر>ًا أَX<ن> N<یَ>كۡبَR<رُ>وX<ا۟>ۚ وَمَN<ن> كَانَ غَنِیّN<ࣰ>ا فَلۡیَسۡتَعۡفِفۡۖ وَمَN<ن> كَانَ فَقِیR<ر>N<ࣰ>ا فَلۡیَأۡكُلۡ بِX<ٱ>لۡمَعۡR<رُ>وفِۚ فَإِذَا دَفَعۡتُمۡ إِلَیۡهِمۡ أَمۡوَT< ٰ>لَهُمۡ فَأَشۡهِدُوX<ا۟> عَلَیۡهِمۡۚ وَكَفَىٰ بِٱللَّهِ حَسِیبࣰا A<۝>D<٦>",
"لِّX<ل>رِّجَالِ نَصِیبX<ࣱ> N<مِّ>N<مَّ>ا تَR<رَ>كَ ٱلۡوَT< ٰ>لِدَانِ وَX<ٱ>لۡأَQ<قۡ>R<رَ>بُونَ وَلِX<ل>N<نِّ>سَW<اۤ>ءِ نَصِیبX<ࣱ> N<مِّ>N<مَّ>ا تَR<رَ>كَ ٱلۡوَT< ٰ>لِدَانِ وَX<ٱ>لۡأَQ<قۡ>R<رَ>بُونَ مِN<مَّ>ا قَلَّ مِنۡهُ أَوۡ كَثُR<رَ>ۚ نَصِیبX<ࣰ>ا N<مَّ>فۡR<رُ>وضࣰا A<۝>D<٧>",
"وَإِذَا حَضَR<رَ> ٱلۡقِسۡمَةَ أُX<و۟>لُوX<ا۟> ٱلۡقُR<رۡ>بَىٰ وَX<ٱ>لۡیَتَT<ـٰ>مَىٰ وَX<ٱ>لۡمَسَT<ـٰ>كِینُ فَX<ٱ>R<رۡ>زُقُوهُم N<مِّ>نۡهُ وَقُولُوX<ا۟> لَهُمۡ قَوۡلX<ࣰ>ا N<مَّ>عۡR<رُ>وفࣰا A<۝>D<٨>",
"وَلۡیَخۡشَ ٱلَّذِینَ لَوۡ تَR<رَ>كُوX<ا۟> مِنۡ خَلۡفِهِمۡ ذُرِّیَّةN<ࣰ> ضِعَT<ـٰ>فًا خَافُوX<ا۟> عَلَیۡهِمۡ فَلۡیَتَّقُوX<ا۟> ٱللَّهَ وَلۡیَقُولُوX<ا۟> قَوۡلN<ࣰ>ا سَدِیدًا A<۝>D<٩>",
"إِN<نَّ> ٱلَّذِینَ یَأۡكُلُونَ أَمۡوَT< ٰ>لَ ٱلۡیَتَT<ـٰ>مَىٰ ظُلۡمًا إِN<نَّ>مَا یَأۡكُلُونَ فِی بُطُونِهِمۡ نَاR<ر>X<ࣰ>اۖ N<وَ>سَیَصۡلَوۡنَ سَعِیR<ر>ࣰا A<۝>D<١٠>",
"یُوصِیكُمُ ٱللَّهُ فِW<یۤ> أَوۡلَT<ـٰ>دِكُمۡۖ لِX<ل>ذَّكَرِ مِثۡلُ حَظِّ ٱلۡأُN<ن>ثَیَیۡنِۚ فَإِN<ن> كُN<نَّ> نِسَW<اۤ>ءN<ࣰ> فَوۡقَ ٱثۡنَتَیۡنِ فَلَهُN<نَّ> ثُلُثَا مَا تَR<رَ>كَۖ وَإِN<ن> كَانَتۡ وَT< ٰ>حِدَةN<ࣰ> فَلَهَا ٱX<ل>N<نِّ>صۡفُۚ وَلِأَبَوَیۡهِ لِكُلِّ وَT< ٰ>حِدX<ࣲ> N<مِّ>نۡهُمَا ٱX<ل>سُّدُسُ مِN<مَّ>ا تَR<رَ>كَ إِN<ن> كَانَ لَهُT<ۥ> وَلَدN<ࣱ>ۚ فَإِX<ن> لَّمۡ یَكُX<ن> لَّهُT<ۥ> وَلَدX<ࣱ> N<وَ>وَرِثَهُW<ۥۤ> أَبَوَاهُ فَلِأُN<مِّ>هِ ٱX<ل>ثُّلُثُۚ فَإِN<ن> كَانَ لَهُW<ۥۤ> إِخۡوَةN<ࣱ> فَلِأُN<مِّ>هِ ٱX<ل>سُّدُسُۚ مِX<ن>N<ۢ> بَعۡدِ وَصِیَّةX<ࣲ> N<یُ>وصِی بِهَW<اۤ> أَوۡ دَیۡنٍۗ ءَابَW<اۤ>ؤُكُمۡ وَأَQ<بۡ>نَW<اۤ>ؤُكُمۡ لَا تَQ<دۡ>R<رُ>ونَ أَیُّهُمۡ أَQ<قۡ>R<رَ>بُ لَكُمۡ نَفۡعN<ࣰ>اۚ فَرِیضَةX<ࣰ> N<مِّ>نَ ٱللَّهِۗ إِN<نَّ> ٱللَّهَ كَانَ عَلِیمًا حَكِیمࣰا A<۝>D<١١>",
"۞ وَلَكُمۡ نِصۡفُ مَا تَR<رَ>كَ أَزۡوَT< ٰ>جُكُمۡ إِX<ن> لَّمۡ یَكُX<ن> لَّهُN<نَّ> وَلَدN<ࣱ>ۚ فَإِN<ن> كَانَ لَهُN<نَّ> وَلَدN<ࣱ> فَلَكُمُ ٱX<ل>R<رُّ>بُعُ مِN<مَّ>ا تَR<رَ>كۡنَۚ مِX<ن>N<ۢ> بَعۡدِ وَصِیَّةX<ࣲ> N<یُ>وصِینَ بِهَW<اۤ> أَوۡ دَیۡنX<ࣲ>ۚ N<وَ>لَهُN<نَّ> ٱX<ل>R<رُّ>بُعُ مِN<مَّ>ا تَR<رَ>كۡتُمۡ إِX<ن> لَّمۡ یَكُX<ن> لَّكُمۡ وَلَدN<ࣱ>ۚ فَإِN<ن> كَانَ لَكُمۡ وَلَدN<ࣱ> فَلَهُN<نَّ> ٱX<ل>ثُّمُنُ مِN<مَّ>ا تَR<رَ>كۡتُمۚ N<مِّ>X<ن>N<ۢ> بَعۡدِ وَصِیَّةN<ࣲ> تُوصُونَ بِهَW<اۤ> أَوۡ دَیۡنX<ࣲ>ۗ N<وَ>إِN<ن> كَانَ R<رَ>جُلX<ࣱ> N<یُ>وR<رَ>ثُ كَلَT<ـٰ>لَةً أَوِ ٱمۡR<رَ>أَةX<ࣱ> N<وَ>لَهُW<ۥۤ> أَخٌ أَوۡ أُخۡتN<ࣱ> فَلِكُلِّ وَT< ٰ>حِدX<ࣲ> N<مِّ>نۡهُمَا ٱX<ل>سُّدُسُۚ فَإِN<ن> كَانُW<وۤ>X<ا۟> أَكۡثَR<رَ> مِN<ن> ذَT< ٰ>لِكَ فَهُمۡ شُR<رَ>كَW<اۤ>ءُ فِی ٱX<ل>ثُّلُثِۚ مِX<ن>N<ۢ> بَعۡدِ وَصِیَّةX<ࣲ> N<یُ>وصَىٰ بِهَW<اۤ> أَوۡ دَیۡنٍ غَیۡR<رَ> مُضَL<اۤ>رّX<ࣲ>ۚ N<وَ>صِیَّةX<ࣰ> N<مِّ>نَ ٱللَّهِۗ وَٱللَّهُ عَلِیمٌ حَلِJ<ی>مࣱ A<۝>D<١٢>",
"تِلۡكَ حُدُودُ ٱللَّهِۚ وَمَX<ن> N<یُ>طِعِ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> یُQ<دۡ>خِلۡهُ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَاۚ وَذَT< ٰ>لِكَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<١٣>",
"وَمَX<ن> N<یَ>عۡصِ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> وَیَتَعَدَّ حُدُودَهُT<ۥ> یُQ<دۡ>خِلۡهُ نَاR<ر>ًا خَT<ـٰ>لِدN<ࣰ>ا فِیهَا وَلَهُT<ۥ> عَذَابX<ࣱ> N<مُّ>هِJ<ی>نࣱ A<۝>D<١٤>",
"وَX<ٱ>لَّT<ـٰ>تِی یَأۡتِینَ ٱلۡفَT<ـٰ>حِشَةَ مِن N<نِّ>سَW<اۤ>ئِكُمۡ فَX<ٱ>سۡتَشۡهِدُوX<ا۟> عَلَیۡهِN<نَّ> أَR<رۡ>بَعَةX<ࣰ> N<مِّ>N<ن>كُمۡۖ فَإِN<ن> شَهِدُوX<ا۟> فَأَمۡسِكُوهُN<نَّ> فِی ٱلۡبُیُوتِ حَتَّىٰ یَتَوَفَّX<ى>T<ٰ>هُN<نَّ> ٱلۡمَوۡتُ أَوۡ یَQ<جۡ>عَلَ ٱللَّهُ لَهُN<نَّ> سَبِیلࣰا A<۝>D<١٥>",
"وَX<ٱ>لَّذَانِ یَأۡتِیَT<ـٰ>نِهَا مِN<ن>كُمۡ فَءَاذُوهُمَاۖ فَإِN<ن> تَابَا وَأَصۡلَحَا فَأَعۡرِضُوX<ا۟> عَنۡهُمَW<اۤ>ۗ إِN<نَّ> ٱللَّهَ كَانَ تَوَّابX<ࣰ>ا R<رَّ>حِیمًا A<۝>D<١٦>",
"إِN<نَّ>مَا ٱX<ل>تَّوۡبَةُ عَلَى ٱللَّهِ لِلَّذِینَ یَعۡمَلُونَ ٱX<ل>سُّW<وۤ>ءَ بِجَهَT<ـٰ>لَةN<ࣲ> ثُN<مَّ> یَتُوبُونَ مِN<ن> قَرِیبN<ࣲ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ یَتُوبُ ٱللَّهُ عَلَیۡهِمۡۗ وَكَانَ ٱللَّهُ عَلِیمًا حَكِیمࣰا A<۝>D<١٧>",
"وَلَیۡسَتِ ٱX<ل>تَّوۡبَةُ لِلَّذِینَ یَعۡمَلُونَ ٱX<ل>سَّیِّءَاتِ حَتَّىW<ٰۤ> إِذَا حَضَR<رَ> أَحَدَهُمُ ٱلۡمَوۡتُ قَالَ إِN<نِّ>ی تُQ<بۡ>تُ ٱلۡءَT<ـٰ>نَ وَلَا ٱلَّذِینَ یَمُوتُونَ وَهُمۡ كُفَّاR<ر>ٌۚ أُX<و۟>لَW<ـٰۤ>ئِكَ أَعۡتَQ<دۡ>نَا لَهُمۡ عَذَابًا أَلِیمࣰا A<۝>D<١٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا یَحِلُّ لَكُمۡ أَN<ن> تَرِثُوX<ا۟> ٱX<ل>N<نِّ>سَW<اۤ>ءَ كَR<رۡ>هX<ࣰ>اۖ N<وَ>لَا تَعۡضُلُوهُN<نَّ> لِتَذۡهَبُوX<ا۟> بِبَعۡضِ مَW<اۤ> ءَاتَیۡتُمُوهُN<نَّ> إِلَّW<اۤ> أَX<ن> N<یَ>أۡتِینَ بِفَT<ـٰ>حِشَةX<ࣲ> N<مُّ>بَیِّنَةX<ࣲ>ۚ N<وَ>عَاشِR<رُ>وهُN<نَّ> بِX<ٱ>لۡمَعۡR<رُ>وفِۚ فَإِN<ن> كَرِهۡتُمُوهُN<نَّ> فَعَسَىW<ٰۤ> أَN<ن> تَكۡR<رَ>هُوX<ا۟> شَیۡءX<ࣰ>ا N<وَ>یَQ<جۡ>عَلَ ٱللَّهُ فِیهِ خَیۡR<ر>N<ࣰ>ا كَثِیR<ر>ࣰا A<۝>D<١٩>",
"وَإِنۡ أَR<رَ>X<د>تُّمُ ٱسۡتِQ<بۡ>دَالَ زَوۡجX<ࣲ> N<مَّ>كَانَ زَوۡجX<ࣲ> N<وَ>ءَاتَیۡتُمۡ إِحۡدَX<ى>T<ٰ>هُN<نَّ> قِN<ن>طَاR<ر>N<ࣰ>ا فَلَا تَأۡخُذُوX<ا۟> مِنۡهُ شَیۡءًاۚ أَتَأۡخُذُونَهُT<ۥ> بُهۡتَT<ـٰ>نX<ࣰ>ا N<وَ>إِثۡمX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<٢٠>",
"وَكَیۡفَ تَأۡخُذُونَهُT<ۥ> وَقَQ<دۡ> أَفۡضَىٰ بَعۡضُكُمۡ إِلَىٰ بَعۡضX<ࣲ> N<وَ>أَخَذۡنَ مِN<ن>كُم N<مِّ>یثَT<ـٰ>قًا غَلِیظࣰا A<۝>D<٢١>",
"وَلَا تَN<ن>كِحُوX<ا۟> مَا نَكَحَ ءَابَW<اۤ>ؤُكُم N<مِّ>نَ ٱX<ل>N<نِّ>سَW<اۤ>ءِ إِلَّا مَا قَQ<دۡ> سَلَفَۚ إِN<نَّ>هُT<ۥ> كَانَ فَT<ـٰ>حِشَةX<ࣰ> N<وَ>مَQ<قۡ>تX<ࣰ>ا N<وَ>سَW<اۤ>ءَ سَبِیلًا A<۝>D<٢٢>",
"حُرِّمَتۡ عَلَیۡكُمۡ أُN<مَّ>هَT<ـٰ>تُكُمۡ وَبَنَاتُكُمۡ وَأَخَوَT< ٰ>تُكُمۡ وَعَN<مَّ>T<ـٰ>تُكُمۡ وَخَT<ـٰ>لَT<ـٰ>تُكُمۡ وَبَنَاتُ ٱلۡأَخِ وَبَنَاتُ ٱلۡأُخۡتِ وَأُN<مَّ>هَT<ـٰ>تُكُمُ ٱلَّT<ـٰ>تِW<یۤ> أَR<رۡ>ضَعۡنَكُمۡ وَأَخَوَT< ٰ>تُكُم N<مِّ>نَ ٱX<ل>R<رَّ>ضَT<ـٰ>عَةِ وَأُN<مَّ>هَT<ـٰ>تُ نِسَW<اۤ>ئِكُمۡ وَR<رَ>بَW<ـٰۤ>ئِبُكُمُ ٱلَّT<ـٰ>تِی فِی حُجُورِكُم N<مِّ>ن N<نِّ>سَW<اۤ>ئِكُمُ ٱلَّT<ـٰ>تِی دَخَلۡتُN<م> بِهِN<نَّ> فَإِX<ن> لَّمۡ تَكُونُوX<ا۟> دَخَلۡتُN<م> بِهِN<نَّ> فَلَا جُنَاحَ عَلَیۡكُمۡ وَحَلَW<ـٰۤ>ئِلُ أَQ<بۡ>نَW<اۤ>ئِكُمُ ٱلَّذِینَ مِنۡ أَصۡلَT<ـٰ>بِكُمۡ وَأَN<ن> تَQ<جۡ>مَعُوX<ا۟> بَیۡنَ ٱلۡأُخۡتَیۡنِ إِلَّا مَا قَQ<دۡ> سَلَفَۗ إِN<نَّ> ٱللَّهَ كَانَ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<٢٣>",
"۞ وَX<ٱ>لۡمُحۡصَنَT<ـٰ>تُ مِنَ ٱX<ل>N<نِّ>سَW<اۤ>ءِ إِلَّا مَا مَلَكَتۡ أَیۡمَT<ـٰ>نُكُمۡۖ كِتَT<ـٰ>بَ ٱللَّهِ عَلَیۡكُمۡۚ وَأُحِلَّ لَكُم N<مَّ>ا وَR<رَ>W<اۤ>ءَ ذَT< ٰ>لِكُمۡ أَN<ن> تَQ<بۡ>تَغُوX<ا۟> بِأَمۡوَT< ٰ>لِكُم N<مُّ>حۡصِنِینَ غَیۡR<رَ> مُسَT<ـٰ>فِحِینَۚ فَمَا ٱسۡتَمۡتَعۡتُN<م> بِهِT<ۦ> مِنۡهُN<نَّ> فَءَاتُوهُN<نَّ> أُجُوR<رَ>هُN<نَّ> فَرِیضَةX<ࣰ>ۚ N<وَ>لَا جُنَاحَ عَلَیۡكُمۡ فِیمَا تَR<رَ>T< ٰ>ضَیۡتُN<م> بِهِT<ۦ> مِX<ن>N<ۢ> بَعۡدِ ٱلۡفَرِیضَةِۚ إِN<نَّ> ٱللَّهَ كَانَ عَلِیمًا حَكِیمࣰا A<۝>D<٢٤>",
"وَمَX<ن> لَّمۡ یَسۡتَطِعۡ مِN<ن>كُمۡ طَوۡلًا أَX<ن> N<یَ>N<ن>كِحَ ٱلۡمُحۡصَنَT<ـٰ>تِ ٱلۡمُؤۡمِنَT<ـٰ>تِ فَمِX<ن> N<مَّ>ا مَلَكَتۡ أَیۡمَT<ـٰ>نُكُم N<مِّ>N<ن> فَتَیَT<ـٰ>تِكُمُ ٱلۡمُؤۡمِنَT<ـٰ>تِۚ وَٱللَّهُ أَعۡلَمُ بِإِیمَT<ـٰ>نِكُN<م>ۚ بَعۡضُكُم N<مِّ>X<ن>N<ۢ> بَعۡضN<ࣲ>ۚ فَX<ٱ>N<ن>كِحُوهُN<نَّ> بِإِذۡنِ أَهۡلِهِN<نَّ> وَءَاتُوهُN<نَّ> أُجُوR<رَ>هُN<نَّ> بِX<ٱ>لۡمَعۡR<رُ>وفِ مُحۡصَنَT<ـٰ>تٍ غَیۡR<رَ> مُسَT<ـٰ>فِحَT<ـٰ>تX<ࣲ> N<وَ>لَا مُتَّخِذَT< ٰ>تِ أَخۡدَانN<ࣲ>ۚ فَإِذَW<اۤ> أُحۡصِN<نَّ> فَإِنۡ أَتَیۡنَ بِفَT<ـٰ>حِشَةN<ࣲ> فَعَلَیۡهِN<نَّ> نِصۡفُ مَا عَلَى ٱلۡمُحۡصَنَT<ـٰ>تِ مِنَ ٱلۡعَذَابِۚ ذَT< ٰ>لِكَ لِمَنۡ خَشِیَ ٱلۡعَنَتَ مِN<ن>كُمۡۚ وَأَN<ن> تَصۡبِR<رُ>وX<ا۟> خَیۡR<ر>X<ࣱ> لَّكُمۡۗ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٢٥>",
"یُرِیدُ ٱللَّهُ لِیُبَیِّنَ لَكُمۡ وَیَهۡدِیَكُمۡ سُنَنَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِكُمۡ وَیَتُوبَ عَلَیۡكُمۡۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<٢٦>",
"وَٱللَّهُ یُرِیدُ أَX<ن> N<یَ>تُوبَ عَلَیۡكُمۡ وَیُرِیدُ ٱلَّذِینَ یَتَّبِعُونَ ٱX<ل>شَّهَوَT< ٰ>تِ أَN<ن> تَمِیلُوX<ا۟> مَیۡلًا عَظِیمࣰا A<۝>D<٢٧>",
"یُرِیدُ ٱللَّهُ أَX<ن> N<یُ>خَفِّفَ عَN<ن>كُمۡۚ وَخُلِقَ ٱلۡإِN<ن>سَT<ـٰ>نُ ضَعِیفࣰا A<۝>D<٢٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَأۡكُلُW<وۤ>X<ا۟> أَمۡوَT< ٰ>لَكُN<م> بَیۡنَكُN<م> بِX<ٱ>لۡبَT<ـٰ>طِلِ إِلَّW<اۤ> أَN<ن> تَكُونَ تِجَT<ـٰ>R<رَ>ةً عَN<ن> تَR<رَ>اضX<ࣲ> N<مِّ>N<ن>كُمۡۚ وَلَا تَQ<قۡ>تُلُW<وۤ>X<ا۟> أَN<ن>فُسَكُمۡۚ إِN<نَّ> ٱللَّهَ كَانَ بِكُمۡ R<رَ>حِیمࣰا A<۝>D<٢٩>",
"وَمَX<ن> N<یَ>فۡعَلۡ ذَT< ٰ>لِكَ عُQ<دۡ>وَT< ٰ>نX<ࣰ>ا N<وَ>ظُلۡمN<ࣰ>ا فَسَوۡفَ نُصۡلِیهِ نَاR<ر>X<ࣰ>اۚ N<وَ>كَانَ ذَT< ٰ>لِكَ عَلَى ٱللَّهِ یَسِیR<ر>ًا A<۝>D<٣٠>",
"إِN<ن> تَQ<جۡ>تَنِبُوX<ا۟> كَبَW<اۤ>ئِR<رَ> مَا تُنۡهَوۡنَ عَنۡهُ نُكَفِّرۡ عَN<ن>كُمۡ سَیِّءَاتِكُمۡ وَنُQ<دۡ>خِلۡكُم N<مُّ>Q<دۡ>خَلN<ࣰ>ا كَرِیمࣰا A<۝>D<٣١>",
"وَلَا تَتَمَN<نَّ>وۡX<ا۟> مَا فَضَّلَ ٱللَّهُ بِهِT<ۦ> بَعۡضَكُمۡ عَلَىٰ بَعۡضX<ࣲ>ۚ لِّX<ل>رِّجَالِ نَصِیبX<ࣱ> N<مِّ>N<مَّ>ا ٱكۡتَسَبُوX<ا۟>ۖ وَلِX<ل>N<نِّ>سَW<اۤ>ءِ نَصِیبX<ࣱ> N<مِّ>N<مَّ>ا ٱكۡتَسَQ<بۡ>نَۚ وَسۡءَلُوX<ا۟> ٱللَّهَ مِN<ن> فَضۡلِهِW<ۦۤ>ۚ إِN<نَّ> ٱللَّهَ كَانَ بِكُلِّ شَیۡءٍ عَلِیمࣰا A<۝>D<٣٢>",
"وَلِكُلّN<ࣲ> جَعَلۡنَا مَوَT< ٰ>لِیَ مِN<مَّ>ا تَR<رَ>كَ ٱلۡوَT< ٰ>لِدَانِ وَX<ٱ>لۡأَQ<قۡ>R<رَ>بُونَۚ وَX<ٱ>لَّذِینَ عَقَدَتۡ أَیۡمَT<ـٰ>نُكُمۡ فَءَاتُوهُمۡ نَصِیبَهُمۡۚ إِN<نَّ> ٱللَّهَ كَانَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> شَهِیدًا A<۝>D<٣٣>",
"ٱX<ل>رِّجَالُ قَوَّT< ٰ>مُونَ عَلَى ٱX<ل>N<نِّ>سَW<اۤ>ءِ بِمَا فَضَّلَ ٱللَّهُ بَعۡضَهُمۡ عَلَىٰ بَعۡضX<ࣲ> N<وَ>بِمَW<اۤ> أَN<ن>فَقُوX<ا۟> مِنۡ أَمۡوَT< ٰ>لِهِمۡۚ فَX<ٱ>X<ل>صَّT<ـٰ>لِحَT<ـٰ>تُ قَT<ـٰ>نِتَT<ـٰ>تٌ حَT<ـٰ>فِظَT<ـٰ>تX<ࣱ> لِّلۡغَیۡبِ بِمَا حَفِظَ ٱللَّهُۚ وَX<ٱ>لَّT<ـٰ>تِی تَخَافُونَ نُشُوزَهُN<نَّ> فَعِظُوهُN<نَّ> وَX<ٱ>هۡجُR<رُ>وهُN<نَّ> فِی ٱلۡمَضَاجِعِ وَX<ٱ>ضۡرِبُوهُN<نَّ>ۖ فَإِنۡ أَطَعۡنَكُمۡ فَلَا تَQ<بۡ>غُوX<ا۟> عَلَیۡهِN<نَّ> سَبِیلًاۗ إِN<نَّ> ٱللَّهَ كَانَ عَلِیّN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٣٤>",
"وَإِنۡ خِفۡتُمۡ شِقَاقَ بَیۡنِهِمَا فَX<ٱ>Q<بۡ>عَثُوX<ا۟> حَكَمX<ࣰ>ا N<مِّ>نۡ أَهۡلِهِT<ۦ> وَحَكَمX<ࣰ>ا N<مِّ>نۡ أَهۡلِهَW<اۤ> إِX<ن> N<یُ>رِیدَW<اۤ> إِصۡلَT<ـٰ>حX<ࣰ>ا N<یُ>وَفِّقِ ٱللَّهُ بَیۡنَهُمَW<اۤ>ۗ إِN<نَّ> ٱللَّهَ كَانَ عَلِیمًا خَبِیR<ر>ࣰا A<۝>D<٣٥>",
"۞ وَX<ٱ>عۡبُدُوX<ا۟> ٱللَّهَ وَلَا تُشۡرِكُوX<ا۟> بِهِT<ۦ> شَیۡءX<ࣰ>اۖ N<وَ>بِX<ٱ>لۡوَT< ٰ>لِدَیۡنِ إِحۡسَT<ـٰ>نX<ࣰ>ا N<وَ>بِذِی ٱلۡقُR<رۡ>بَىٰ وَX<ٱ>لۡیَتَT<ـٰ>مَىٰ وَX<ٱ>لۡمَسَT<ـٰ>كِینِ وَX<ٱ>لۡجَارِ ذِی ٱلۡقُR<رۡ>بَىٰ وَX<ٱ>لۡجَارِ ٱلۡجُنُبِ وَX<ٱ>X<ل>صَّاحِبِ بِX<ٱ>لۡجَX<ن>N<ۢ>بِ وَX<ٱ>Q<بۡ>نِ ٱX<ل>سَّبِیلِ وَمَا مَلَكَتۡ أَیۡمَT<ـٰ>نُكُمۡۗ إِN<نَّ> ٱللَّهَ لَا یُحِبُّ مَN<ن> كَانَ مُخۡتَالN<ࣰ>ا فَخُوR<ر>ًا A<۝>D<٣٦>",
"ٱلَّذِینَ یَQ<بۡ>خَلُونَ وَیَأۡمُR<رُ>ونَ ٱX<ل>N<نَّ>اسَ بِX<ٱ>لۡبُخۡلِ وَیَكۡتُمُونَ مَW<اۤ> ءَاتَX<ى>T<ٰ>هُمُ ٱللَّهُ مِN<ن> فَضۡلِهِT<ۦ>ۗ وَأَعۡتَQ<دۡ>نَا لِلۡكَT<ـٰ>فِرِینَ عَذَابX<ࣰ>ا N<مُّ>هِینࣰا A<۝>D<٣٧>",
"وَX<ٱ>لَّذِینَ یُN<ن>فِقُونَ أَمۡوَT< ٰ>لَهُمۡ رِئَW<اۤ>ءَ ٱX<ل>N<نَّ>اسِ وَلَا یُؤۡمِنُونَ بِٱللَّهِ وَلَا بِX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِۗ وَمَX<ن> N<یَ>كُنِ ٱX<ل>شَّیۡطَT<ـٰ>نُ لَهُT<ۥ> قَرِینN<ࣰ>ا فَسَW<اۤ>ءَ قَرِینࣰا A<۝>D<٣٨>",
"وَمَاذَا عَلَیۡهِمۡ لَوۡ ءَامَنُوX<ا۟> بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَأَN<ن>فَقُوX<ا۟> مِN<مَّ>ا R<رَ>زَقَهُمُ ٱللَّهُۚ وَكَانَ ٱللَّهُ بِهِمۡ عَلِیمًا A<۝>D<٣٩>",
"إِN<نَّ> ٱللَّهَ لَا یَظۡلِمُ مِثۡقَالَ ذَR<رَّ>ةX<ࣲ>ۖ N<وَ>إِN<ن> تَكُ حَسَنَةX<ࣰ> N<یُ>ضَT<ـٰ>عِفۡهَا وَیُؤۡتِ مِX<ن> لَّدُنۡهُ أَQ<جۡ>R<ر>ًا عَظِیمࣰا A<۝>D<٤٠>",
"فَكَیۡفَ إِذَا جِئۡنَا مِN<ن> كُلِّ أُN<مَّ>ةِN<ۭ> بِشَهِیدX<ࣲ> N<وَ>جِئۡنَا بِكَ عَلَىٰ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ شَهِیدࣰا A<۝>D<٤١>",
"یَوۡمَئِذX<ࣲ> N<یَ>وَدُّ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَعَصَوُX<ا۟> ٱX<ل>R<رَّ>سُولَ لَوۡ تُسَوَّىٰ بِهِمُ ٱلۡأَR<رۡ>ضُ وَلَا یَكۡتُمُونَ ٱللَّهَ حَدِیثࣰا A<۝>D<٤٢>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَQ<قۡ>R<رَ>بُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَأَN<ن>تُمۡ سُكَT<ـٰ>R<رَ>ىٰ حَتَّىٰ تَعۡلَمُوX<ا۟> مَا تَقُولُونَ وَلَا جُنُبًا إِلَّا عَابِرِی سَبِیلٍ حَتَّىٰ تَغۡتَسِلُوX<ا۟>ۚ وَإِN<ن> كُN<ن>تُم N<مَّ>R<رۡ>ضَىW<ٰۤ> أَوۡ عَلَىٰ سَفَرٍ أَوۡ جَW<اۤ>ءَ أَحَدX<ࣱ> N<مِّ>N<ن>كُم N<مِّ>نَ ٱلۡغَW<اۤ>ئِطِ أَوۡ لَT<ـٰ>مَسۡتُمُ ٱX<ل>N<نِّ>سَW<اۤ>ءَ فَلَمۡ تَجِدُوX<ا۟> مَW<اۤ>ءN<ࣰ> فَتَیَN<مَّ>مُوX<ا۟> صَعِیدN<ࣰ>ا طَیِّبN<ࣰ>ا فَX<ٱ>مۡسَحُوX<ا۟> بِوُجُوهِكُمۡ وَأَیۡدِیكُمۡۗ إِN<نَّ> ٱللَّهَ كَانَ عَفُوًّا غَفُوR<ر>ًا A<۝>D<٤٣>",
"أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ أُوتُوX<ا۟> نَصِیبX<ࣰ>ا N<مِّ>نَ ٱلۡكِتَT<ـٰ>بِ یَشۡتَR<رُ>ونَ ٱX<ل>ضَّلَT<ـٰ>لَةَ وَیُرِیدُونَ أَN<ن> تَضِلُّوX<ا۟> ٱX<ل>سَّبِJ<ی>لَ A<۝>D<٤٤>",
"وَٱللَّهُ أَعۡلَمُ بِأَعۡدَW<اۤ>ئِكُمۡۚ وَكَفَىٰ بِٱللَّهِ وَلِیّX<ࣰ>ا N<وَ>كَفَىٰ بِٱللَّهِ نَصِیR<ر>ࣰا A<۝>D<٤٥>",
"مِّنَ ٱلَّذِینَ هَادُوX<ا۟> یُحَرِّفُونَ ٱلۡكَلِمَ عَX<ن> N<مَّ>وَاضِعِهِT<ۦ> وَیَقُولُونَ سَمِعۡنَا وَعَصَیۡنَا وَX<ٱ>سۡمَعۡ غَیۡR<رَ> مُسۡمَعX<ࣲ> N<وَ>R<رَ>T< ٰ>عِنَا لَیَّN<ۢ>ا بِأَلۡسِنَتِهِمۡ وَطَعۡنN<ࣰ>ا فِی ٱX<ل>دِّینِۚ وَلَوۡ أَN<نَّ>هُمۡ قَالُوX<ا۟> سَمِعۡنَا وَأَطَعۡنَا وَX<ٱ>سۡمَعۡ وَX<ٱ>N<ن>ظُR<رۡ>نَا لَكَانَ خَیۡR<ر>X<ࣰ>ا لَّهُمۡ وَأَQ<قۡ>وَمَ وَلَT<ـٰ>كِX<ن> لَّعَنَهُمُ ٱللَّهُ بِكُفۡرِهِمۡ فَلَا یُؤۡمِنُونَ إِلَّا قَلِیلࣰا A<۝>D<٤٦>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ ءَامِنُوX<ا۟> بِمَا نَزَّلۡنَا مُصَدِّقX<ࣰ>ا لِّمَا مَعَكُم N<مِّ>N<ن> قَQ<بۡ>لِ أَن N<نَّ>Q<طۡ>مِسَ وُجُوهN<ࣰ>ا فَنَR<رُ>دَّهَا عَلَىW<ٰۤ> أَQ<دۡ>بَارِهَW<اۤ> أَوۡ نَلۡعَنَهُمۡ كَمَا لَعَN<نَّ>W<اۤ> أَصۡحَT<ـٰ>بَ ٱX<ل>سَّQ<بۡ>تِۚ وَكَانَ أَمۡR<رُ> ٱللَّهِ مَفۡعُولًا A<۝>D<٤٧>",
"إِN<نَّ> ٱللَّهَ لَا یَغۡفِR<رُ> أَX<ن> N<یُ>شۡR<رَ>كَ بِهِT<ۦ> وَیَغۡفِR<رُ> مَا دُونَ ذَT< ٰ>لِكَ لِمَX<ن> N<یَ>شَW<اۤ>ءُۚ وَمَX<ن> N<یُ>شۡرِكۡ بِٱللَّهِ فَقَدِ ٱفۡتَR<رَ>ىW<ٰۤ> إِثۡمًا عَظِیمًا A<۝>D<٤٨>",
"أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ یُزَكُّونَ أَN<ن>فُسَهُN<م>ۚ بَلِ ٱللَّهُ یُزَكِّی مَX<ن> N<یَ>شَW<اۤ>ءُ وَلَا یُظۡلَمُونَ فَتِیلًا A<۝>D<٤٩>",
"ٱN<ن>ظُR<رۡ> كَیۡفَ یَفۡتَR<رُ>ونَ عَلَى ٱللَّهِ ٱلۡكَذِبَۖ وَكَفَىٰ بِهِW<ۦۤ> إِثۡمX<ࣰ>ا N<مُّ>بِینًا A<۝>D<٥٠>",
"أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ أُوتُوX<ا۟> نَصِیبX<ࣰ>ا N<مِّ>نَ ٱلۡكِتَT<ـٰ>بِ یُؤۡمِنُونَ بِX<ٱ>لۡجِQ<بۡ>تِ وَX<ٱ>X<ل>طَّT<ـٰ>غُوتِ وَیَقُولُونَ لِلَّذِینَ كَفَR<رُ>وX<ا۟> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ أَهۡدَىٰ مِنَ ٱلَّذِینَ ءَامَنُوX<ا۟> سَبِیلًا A<۝>D<٥١>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ لَعَنَهُمُ ٱللَّهُۖ وَمَX<ن> N<یَ>لۡعَنِ ٱللَّهُ فَلَN<ن> تَجِدَ لَهُT<ۥ> نَصِیR<ر>ًا A<۝>D<٥٢>",
"أَمۡ لَهُمۡ نَصِیبX<ࣱ> N<مِّ>نَ ٱلۡمُلۡكِ فَإِذX<ࣰ>ا لَّا یُؤۡتُونَ ٱX<ل>N<نَّ>اسَ نَقِیR<ر>ًا A<۝>D<٥٣>",
"أَمۡ یَحۡسُدُونَ ٱX<ل>N<نَّ>اسَ عَلَىٰ مَW<اۤ> ءَاتَX<ى>T<ٰ>هُمُ ٱللَّهُ مِN<ن> فَضۡلِهِT<ۦ>ۖ فَقَQ<دۡ> ءَاتَیۡنَW<اۤ> ءَالَ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحِكۡمَةَ وَءَاتَیۡنَT<ـٰ>هُم N<مُّ>لۡكًا عَظِیمࣰا A<۝>D<٥٤>",
"فَمِنۡهُم N<مَّ>نۡ ءَامَنَ بِهِT<ۦ> وَمِنۡهُم N<مَّ>N<ن> صَدَّ عَنۡهُۚ وَكَفَىٰ بِجَهَN<نَّ>مَ سَعِیR<ر>ًا A<۝>D<٥٥>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِنَا سَوۡفَ نُصۡلِیهِمۡ نَاR<ر>N<ࣰ>ا كُلَّمَا نَضِجَتۡ جُلُودُهُN<م> بَدَّلۡنَT<ـٰ>هُمۡ جُلُودًا غَیۡR<رَ>هَا لِیَذُوقُوX<ا۟> ٱلۡعَذَابَۗ إِN<نَّ> ٱللَّهَ كَانَ عَزِیزًا حَكِیمࣰا A<۝>D<٥٦>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ سَنُQ<دۡ>خِلُهُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدX<ࣰ>اۖ لَّهُمۡ فِیهَW<اۤ> أَزۡوَT< ٰ>جX<ࣱ> N<مُّ>طَهَّR<رَ>ةX<ࣱ>ۖ N<وَ>نُQ<دۡ>خِلُهُمۡ ظِلّN<ࣰ>ا ظَلِیلًا A<۝>D<٥٧>",
"۞ إِN<نَّ> ٱللَّهَ یَأۡمُR<رُ>كُمۡ أَN<ن> تُؤَدُّوX<ا۟> ٱلۡأَمَT<ـٰ>نَT<ـٰ>تِ إِلَىW<ٰۤ> أَهۡلِهَا وَإِذَا حَكَمۡتُN<م> بَیۡنَ ٱX<ل>N<نَّ>اسِ أَN<ن> تَحۡكُمُوX<ا۟> بِX<ٱ>لۡعَQ<دۡ>لِۚ إِN<نَّ> ٱللَّهَ نِعِN<مَّ>ا یَعِظُكُN<م> بِهِW<ۦۤ>ۗ إِN<نَّ> ٱللَّهَ كَانَ سَمِیعَN<ۢ>ا بَصِیR<ر>ࣰا A<۝>D<٥٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَطِیعُوX<ا۟> ٱللَّهَ وَأَطِیعُوX<ا۟> ٱX<ل>R<رَّ>سُولَ وَأُX<و۟>لِی ٱلۡأَمۡرِ مِN<ن>كُمۡۖ فَإِN<ن> تَنَT<ـٰ>زَعۡتُمۡ فِی شَیۡءN<ࣲ> فَR<رُ>دُّوهُ إِلَى ٱللَّهِ وَX<ٱ>X<ل>R<رَّ>سُولِ إِN<ن> كُN<ن>تُمۡ تُؤۡمِنُونَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِۚ ذَT< ٰ>لِكَ خَیۡR<ر>X<ࣱ> N<وَ>أَحۡسَنُ تَأۡوِیلًا A<۝>D<٥٩>",
"أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ یَزۡعُمُونَ أَN<نَّ>هُمۡ ءَامَنُوX<ا۟> بِمَW<اۤ> أُN<ن>زِلَ إِلَیۡكَ وَمَW<اۤ> أُN<ن>زِلَ مِN<ن> قَQ<بۡ>لِكَ یُرِیدُونَ أَX<ن> N<یَ>تَحَاكَمُW<وۤ>X<ا۟> إِلَى ٱX<ل>طَّT<ـٰ>غُوتِ وَقَQ<دۡ> أُمِR<رُ>W<وۤ>X<ا۟> أَX<ن> N<یَ>كۡفُR<رُ>وX<ا۟> بِهِT<ۦ>ۖ وَیُرِیدُ ٱX<ل>شَّیۡطَT<ـٰ>نُ أَX<ن> N<یُ>ضِلَّهُمۡ ضَلَT<ـٰ>لَN<ۢ>ا بَعِیدࣰا A<۝>D<٦٠>",
"وَإِذَا قِیلَ لَهُمۡ تَعَالَوۡX<ا۟> إِلَىٰ مَW<اۤ> أَN<ن>زَلَ ٱللَّهُ وَإِلَى ٱX<ل>R<رَّ>سُولِ R<رَ>أَیۡتَ ٱلۡمُنَT<ـٰ>فِقِینَ یَصُدُّونَ عَN<ن>كَ صُدُودࣰا A<۝>D<٦١>",
"فَكَیۡفَ إِذَW<اۤ> أَصَT<ـٰ>بَتۡهُم N<مُّ>صِیبَةُN<ۢ> بِمَا قَدَّمَتۡ أَیۡدِیهِمۡ ثُN<مَّ> جَW<اۤ>ءُوكَ یَحۡلِفُونَ بِٱللَّهِ إِنۡ أَR<رَ>Q<دۡ>نَW<اۤ> إِلَّW<اۤ> إِحۡسَT<ـٰ>نX<ࣰ>ا N<وَ>تَوۡفِیقًا A<۝>D<٦٢>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ یَعۡلَمُ ٱللَّهُ مَا فِی قُلُوبِهِمۡ فَأَعۡرِضۡ عَنۡهُمۡ وَعِظۡهُمۡ وَقُل لَّهُمۡ فِW<یۤ> أَN<ن>فُسِهِمۡ قَوۡلَN<ۢ>ا بَلِیغࣰا A<۝>D<٦٣>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا مِX<ن> R<رَّ>سُولٍ إِلَّا لِیُطَاعَ بِإِذۡنِ ٱللَّهِۚ وَلَوۡ أَN<نَّ>هُمۡ إِذ ظَّلَمُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ جَW<اۤ>ءُوكَ فَX<ٱ>سۡتَغۡفَR<رُ>وX<ا۟> ٱللَّهَ وَX<ٱ>سۡتَغۡفَR<رَ> لَهُمُ ٱX<ل>R<رَّ>سُولُ لَوَجَدُوX<ا۟> ٱللَّهَ تَوَّابX<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<٦٤>",
"فَلَا وَR<رَ>بِّكَ لَا یُؤۡمِنُونَ حَتَّىٰ یُحَكِّمُوكَ فِیمَا شَجَR<رَ> بَیۡنَهُمۡ ثُN<مَّ> لَا یَجِدُوX<ا۟> فِW<یۤ> أَN<ن>فُسِهِمۡ حَR<رَ>جX<ࣰ>ا N<مِّ>N<مَّ>ا قَضَیۡتَ وَیُسَلِّمُوX<ا۟> تَسۡلِیمࣰا A<۝>D<٦٥>",
"وَلَوۡ أَN<نَّ>ا كَتَQ<بۡ>نَا عَلَیۡهِمۡ أَنِ ٱQ<قۡ>تُلُW<وۤ>X<ا۟> أَN<ن>فُسَكُمۡ أَوِ ٱخۡR<رُ>جُوX<ا۟> مِN<ن> دِیَT<ـٰ>رِكُم N<مَّ>ا فَعَلُوهُ إِلَّا قَلِیلX<ࣱ> N<مِّ>نۡهُمۡۖ وَلَوۡ أَN<نَّ>هُمۡ فَعَلُوX<ا۟> مَا یُوعَظُونَ بِهِT<ۦ> لَكَانَ خَیۡR<ر>X<ࣰ>ا لَّهُمۡ وَأَشَدَّ تَثۡبِیتࣰا A<۝>D<٦٦>",
"وَإِذX<ࣰ>ا لَّءَاتَیۡنَT<ـٰ>هُم N<مِّ>X<ن> لَّدُN<نَّ>W<اۤ> أَQ<جۡ>R<ر>ًا عَظِیمࣰا A<۝>D<٦٧>",
"وَلَهَدَیۡنَT<ـٰ>هُمۡ صِR<رَ>T< ٰ>طX<ࣰ>ا N<مُّ>سۡتَقِیمࣰا A<۝>D<٦٨>",
"وَمَX<ن> N<یُ>طِعِ ٱللَّهَ وَX<ٱ>X<ل>R<رَّ>سُولَ فَأُX<و۟>لَW<ـٰۤ>ئِكَ مَعَ ٱلَّذِینَ أَنۡعَمَ ٱللَّهُ عَلَیۡهِم N<مِّ>نَ ٱX<ل>N<نَّ>بِیِّT<ـۧ>نَ وَX<ٱ>X<ل>صِّدِّیقِینَ وَX<ٱ>X<ل>شُّهَدَW<اۤ>ءِ وَX<ٱ>X<ل>صَّT<ـٰ>لِحِینَۚ وَحَسُنَ أُX<و۟>لَW<ـٰۤ>ئِكَ R<رَ>فِیقࣰا A<۝>D<٦٩>",
"ذَT< ٰ>لِكَ ٱلۡفَضۡلُ مِنَ ٱللَّهِۚ وَكَفَىٰ بِٱللَّهِ عَلِیمࣰا A<۝>D<٧٠>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> خُذُوX<ا۟> حِذۡR<رَ>كُمۡ فَX<ٱ>N<ن>فِR<رُ>وX<ا۟> ثُبَاتٍ أَوِ ٱN<ن>فِR<رُ>وX<ا۟> جَمِیعࣰا A<۝>D<٧١>",
"وَإِN<نَّ> مِN<ن>كُمۡ لَمَX<ن> لَّیُبَطِّئَN<نَّ> فَإِنۡ أَصَT<ـٰ>بَتۡكُم N<مُّ>صِیبَةN<ࣱ> قَالَ قَQ<دۡ> أَنۡعَمَ ٱللَّهُ عَلَیَّ إِذۡ لَمۡ أَكُX<ن> N<مَّ>عَهُمۡ شَهِیدࣰا A<۝>D<٧٢>",
"وَلَئِنۡ أَصَT<ـٰ>بَكُمۡ فَضۡلX<ࣱ> N<مِّ>نَ ٱللَّهِ لَیَقُولَN<نَّ> كَأَX<ن> لَّمۡ تَكُX<ن>N<ۢ> بَیۡنَكُمۡ وَبَیۡنَهُT<ۥ> مَوَدَّةX<ࣱ> N<یَ>T<ـٰ>لَیۡتَنِی كُN<ن>تُ مَعَهُمۡ فَأَفُوزَ فَوۡزًا عَظِیمࣰا A<۝>D<٧٣>",
"۞ فَلۡیُقَT<ـٰ>تِلۡ فِی سَبِیلِ ٱللَّهِ ٱلَّذِینَ یَشۡR<رُ>ونَ ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا بِX<ٱ>لۡءَاخِR<رَ>ةِۚ وَمَX<ن> N<یُ>قَT<ـٰ>تِلۡ فِی سَبِیلِ ٱللَّهِ فَیُQ<قۡ>تَلۡ أَوۡ یَغۡلِQ<بۡ> فَسَوۡفَ نُؤۡتِیهِ أَQ<جۡ>R<ر>ًا عَظِیمࣰا A<۝>D<٧٤>",
"وَمَا لَكُمۡ لَا تُقَT<ـٰ>تِلُونَ فِی سَبِیلِ ٱللَّهِ وَX<ٱ>لۡمُسۡتَضۡعَفِینَ مِنَ ٱX<ل>رِّجَالِ وَX<ٱ>X<ل>N<نِّ>سَW<اۤ>ءِ وَX<ٱ>لۡوِلۡدَT< ٰ>نِ ٱلَّذِینَ یَقُولُونَ R<رَ>بَّنَW<اۤ> أَخۡرِQ<جۡ>نَا مِنۡ هَT<ـٰ>ذِهِ ٱلۡقَR<رۡ>یَةِ ٱX<ل>ظَّالِمِ أَهۡلُهَا وَX<ٱ>Q<جۡ>عَل لَّنَا مِX<ن> لَّدُN<ن>كَ وَلِیّX<ࣰ>ا N<وَ>X<ٱ>Q<جۡ>عَل لَّنَا مِX<ن> لَّدُN<ن>كَ نَصِیR<ر>ًا A<۝>D<٧٥>",
"ٱلَّذِینَ ءَامَنُوX<ا۟> یُقَT<ـٰ>تِلُونَ فِی سَبِیلِ ٱللَّهِۖ وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> یُقَT<ـٰ>تِلُونَ فِی سَبِیلِ ٱX<ل>طَّT<ـٰ>غُوتِ فَقَT<ـٰ>تِلُW<وۤ>X<ا۟> أَوۡلِیَW<اۤ>ءَ ٱX<ل>شَّیۡطَT<ـٰ>نِۖ إِN<نَّ> كَیۡدَ ٱX<ل>شَّیۡطَT<ـٰ>نِ كَانَ ضَعِیفًا A<۝>D<٧٦>",
"أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ قِیلَ لَهُمۡ كُفُّW<وۤ>X<ا۟> أَیۡدِیَكُمۡ وَأَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتُوX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ فَلَN<مَّ>ا كُتِبَ عَلَیۡهِمُ ٱلۡقِتَالُ إِذَا فَرِیقX<ࣱ> N<مِّ>نۡهُمۡ یَخۡشَوۡنَ ٱX<ل>N<نَّ>اسَ كَخَشۡیَةِ ٱللَّهِ أَوۡ أَشَدَّ خَشۡیَةX<ࣰ>ۚ N<وَ>قَالُوX<ا۟> R<رَ>بَّنَا لِمَ كَتَQ<بۡ>تَ عَلَیۡنَا ٱلۡقِتَالَ لَوۡلَW<اۤ> أَخَّR<رۡ>تَنَW<اۤ> إِلَىW<ٰۤ> أَجَلN<ࣲ> قَرِیبN<ࣲ>ۗ قُلۡ مَتَT<ـٰ>عُ ٱX<ل>دُّنۡیَا قَلِیلX<ࣱ> N<وَ>X<ٱ>لۡءَاخِR<رَ>ةُ خَیۡR<ر>X<ࣱ> لِّمَنِ ٱتَّقَىٰ وَلَا تُظۡلَمُونَ فَتِیلًا A<۝>D<٧٧>",
"أَیۡنَمَا تَكُونُوX<ا۟> یُQ<دۡ>رِX<ك>كُّمُ ٱلۡمَوۡتُ وَلَوۡ كُN<ن>تُمۡ فِی بُR<رُ>وجX<ࣲ> N<مُّ>شَیَّدَةX<ࣲ>ۗ N<وَ>إِN<ن> تُصِQ<بۡ>هُمۡ حَسَنَةX<ࣱ> N<یَ>قُولُوX<ا۟> هَT<ـٰ>ذِهِT<ۦ> مِنۡ عِN<ن>دِ ٱللَّهِۖ وَإِN<ن> تُصِQ<بۡ>هُمۡ سَیِّئَةX<ࣱ> N<یَ>قُولُوX<ا۟> هَT<ـٰ>ذِهِT<ۦ> مِنۡ عِN<ن>دِكَۚ قُلۡ كُلّX<ࣱ> N<مِّ>نۡ عِN<ن>دِ ٱللَّهِۖ فَمَالِ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ ٱلۡقَوۡمِ لَا یَكَادُونَ یَفۡقَهُونَ حَدِیثࣰا A<۝>D<٧٨>",
"مَّW<اۤ> أَصَابَكَ مِنۡ حَسَنَةN<ࣲ> فَمِنَ ٱللَّهِۖ وَمَW<اۤ> أَصَابَكَ مِN<ن> سَیِّئَةN<ࣲ> فَمِن N<نَّ>فۡسِكَۚ وَأَR<رۡ>سَلۡنَT<ـٰ>كَ لِX<ل>N<نَّ>اسِ R<رَ>سُولX<ࣰ>اۚ N<وَ>كَفَىٰ بِٱللَّهِ شَهِیدࣰا A<۝>D<٧٩>",
"مَّX<ن> N<یُ>طِعِ ٱX<ل>R<رَّ>سُولَ فَقَQ<دۡ> أَطَاعَ ٱللَّهَۖ وَمَN<ن> تَوَلَّىٰ فَمَW<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ عَلَیۡهِمۡ حَفِیظࣰا A<۝>D<٨٠>",
"وَیَقُولُونَ طَاعَةN<ࣱ> فَإِذَا بَR<رَ>زُوX<ا۟> مِنۡ عِN<ن>دِكَ بَیَّتَ طَW<اۤ>ئِفَةX<ࣱ> N<مِّ>نۡهُمۡ غَیۡR<رَ> ٱلَّذِی تَقُولُۖ وَٱللَّهُ یَكۡتُبُ مَا یُبَیِّتُونَۖ فَأَعۡرِضۡ عَنۡهُمۡ وَتَوَكَّلۡ عَلَى ٱللَّهِۚ وَكَفَىٰ بِٱللَّهِ وَكِیلًا A<۝>D<٨١>",
"أَفَلَا یَتَدَبَّR<رُ>ونَ ٱلۡقُR<رۡ>ءَانَۚ وَلَوۡ كَانَ مِنۡ عِN<ن>دِ غَیۡرِ ٱللَّهِ لَوَجَدُوX<ا۟> فِیهِ ٱخۡتِلَT<ـٰ>فN<ࣰ>ا كَثِیR<ر>ࣰا A<۝>D<٨٢>",
"وَإِذَا جَW<اۤ>ءَهُمۡ أَمۡR<ر>X<ࣱ> N<مِّ>نَ ٱلۡأَمۡنِ أَوِ ٱلۡخَوۡفِ أَذَاعُوX<ا۟> بِهِT<ۦ>ۖ وَلَوۡ R<رَ>دُّوهُ إِلَى ٱX<ل>R<رَّ>سُولِ وَإِلَىW<ٰۤ> أُX<و۟>لِی ٱلۡأَمۡرِ مِنۡهُمۡ لَعَلِمَهُ ٱلَّذِینَ یَسۡتَX<ن>N<ۢ>بِطُونَهُT<ۥ> مِنۡهُمۡۗ وَلَوۡلَا فَضۡلُ ٱللَّهِ عَلَیۡكُمۡ وَR<رَ>حۡمَتُهُT<ۥ> لَX<ٱ>تَّبَعۡتُمُ ٱX<ل>شَّیۡطَT<ـٰ>نَ إِلَّا قَلِیلࣰا A<۝>D<٨٣>",
"فَقَT<ـٰ>تِلۡ فِی سَبِیلِ ٱللَّهِ لَا تُكَلَّفُ إِلَّا نَفۡسَكَۚ وَحَرِّضِ ٱلۡمُؤۡمِنِینَۖ عَسَى ٱللَّهُ أَX<ن> N<یَ>كُفَّ بَأۡسَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟>ۚ وَٱللَّهُ أَشَدُّ بَأۡسX<ࣰ>ا N<وَ>أَشَدُّ تَN<ن>كِیلࣰا A<۝>D<٨٤>",
"مَّX<ن> N<یَ>شۡفَعۡ شَفَT<ـٰ>عَةً حَسَنَةX<ࣰ> N<یَ>كُX<ن> لَّهُT<ۥ> نَصِیبX<ࣱ> N<مِّ>نۡهَاۖ وَمَX<ن> N<یَ>شۡفَعۡ شَفَT<ـٰ>عَةN<ࣰ> سَیِّئَةX<ࣰ> N<یَ>كُX<ن> لَّهُT<ۥ> كِفۡلX<ࣱ> N<مِّ>نۡهَاۗ وَكَانَ ٱللَّهُ عَلَىٰ كُلِّ شَیۡءX<ࣲ> N<مُّ>قِیتࣰا A<۝>D<٨٥>",
"وَإِذَا حُیِّیتُN<م> بِتَحِیَّةN<ࣲ> فَحَیُّوX<ا۟> بِأَحۡسَنَ مِنۡهَW<اۤ> أَوۡ R<رُ>دُّوهَW<اۤ>ۗ إِN<نَّ> ٱللَّهَ كَانَ عَلَىٰ كُلِّ شَیۡءٍ حَسِیبًا A<۝>D<٨٦>",
"ٱللَّهُ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۚ لَیَQ<جۡ>مَعَN<نَّ>كُمۡ إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ لَا R<رَ>یۡبَ فِیهِۗ وَمَنۡ أَصۡدَقُ مِنَ ٱللَّهِ حَدِیثࣰا A<۝>D<٨٧>",
"۞ فَمَا لَكُمۡ فِی ٱلۡمُنَT<ـٰ>فِقِینَ فِئَتَیۡنِ وَٱللَّهُ أَR<رۡ>كَسَهُN<م> بِمَا كَسَبُW<وۤ>X<ا۟>ۚ أَتُرِیدُونَ أَN<ن> تَهۡدُوX<ا۟> مَنۡ أَضَلَّ ٱللَّهُۖ وَمَX<ن> N<یُ>ضۡلِلِ ٱللَّهُ فَلَN<ن> تَجِدَ لَهُT<ۥ> سَبِیلࣰا A<۝>D<٨٨>",
"وَدُّوX<ا۟> لَوۡ تَكۡفُR<رُ>ونَ كَمَا كَفَR<رُ>وX<ا۟> فَتَكُونُونَ سَوَW<اۤ>ءN<ࣰ>ۖ فَلَا تَتَّخِذُوX<ا۟> مِنۡهُمۡ أَوۡلِیَW<اۤ>ءَ حَتَّىٰ یُهَاجِR<رُ>وX<ا۟> فِی سَبِیلِ ٱللَّهِۚ فَإِN<ن> تَوَلَّوۡX<ا۟> فَخُذُوهُمۡ وَX<ٱ>Q<قۡ>تُلُوهُمۡ حَیۡثُ وَجَX<د>تُّمُوهُمۡۖ وَلَا تَتَّخِذُوX<ا۟> مِنۡهُمۡ وَلِیّX<ࣰ>ا N<وَ>لَا نَصِیR<ر>ًا A<۝>D<٨٩>",
"إِلَّا ٱلَّذِینَ یَصِلُونَ إِلَىٰ قَوۡمِN<ۭ> بَیۡنَكُمۡ وَبَیۡنَهُم N<مِّ>یثَT<ـٰ>قٌ أَوۡ جَW<اۤ>ءُوكُمۡ حَصِR<رَ>تۡ صُدُوR<رُ>هُمۡ أَX<ن> N<یُ>قَT<ـٰ>تِلُوكُمۡ أَوۡ یُقَT<ـٰ>تِلُوX<ا۟> قَوۡمَهُمۡۚ وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ لَسَلَّطَهُمۡ عَلَیۡكُمۡ فَلَقَT<ـٰ>تَلُوكُمۡۚ فَإِنِ ٱعۡتَزَلُوكُمۡ فَلَمۡ یُقَT<ـٰ>تِلُوكُمۡ وَأَلۡقَوۡX<ا۟> إِلَیۡكُمُ ٱX<ل>سَّلَمَ فَمَا جَعَلَ ٱللَّهُ لَكُمۡ عَلَیۡهِمۡ سَبِیلࣰا A<۝>D<٩٠>",
"سَتَجِدُونَ ءَاخَرِینَ یُرِیدُونَ أَX<ن> N<یَ>أۡمَنُوكُمۡ وَیَأۡمَنُوX<ا۟> قَوۡمَهُمۡ كُلَّ مَا R<رُ>دُّW<وۤ>X<ا۟> إِلَى ٱلۡفِتۡنَةِ أُR<رۡ>كِسُوX<ا۟> فِیهَاۚ فَإِX<ن> لَّمۡ یَعۡتَزِلُوكُمۡ وَیُلۡقُW<وۤ>X<ا۟> إِلَیۡكُمُ ٱX<ل>سَّلَمَ وَیَكُفُّW<وۤ>X<ا۟> أَیۡدِیَهُمۡ فَخُذُوهُمۡ وَX<ٱ>Q<قۡ>تُلُوهُمۡ حَیۡثُ ثَقِفۡتُمُوهُمۡۚ وَأُX<و۟>لَW<ـٰۤ>ئِكُمۡ جَعَلۡنَا لَكُمۡ عَلَیۡهِمۡ سُلۡطَT<ـٰ>نX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<٩١>",
"وَمَا كَانَ لِمُؤۡمِنٍ أَX<ن> N<یَ>Q<قۡ>تُلَ مُؤۡمِنًا إِلَّا خَطَءX<ࣰ>اۚ N<وَ>مَN<ن> قَتَلَ مُؤۡمِنًا خَطَءN<ࣰ>ا فَتَحۡرِیR<رُ> R<رَ>قَبَةX<ࣲ> N<مُّ>ؤۡمِنَةX<ࣲ> N<وَ>دِیَةX<ࣱ> N<مُّ>سَلَّمَةٌ إِلَىW<ٰۤ> أَهۡلِهِW<ۦۤ> إِلَّW<اۤ> أَX<ن> N<یَ>صَّدَّقُوX<ا۟>ۚ فَإِN<ن> كَانَ مِN<ن> قَوۡمٍ عَدُوّX<ࣲ> لَّكُمۡ وَهُوَ مُؤۡمِنN<ࣱ> فَتَحۡرِیR<رُ> R<رَ>قَبَةX<ࣲ> N<مُّ>ؤۡمِنَةX<ࣲ>ۖ N<وَ>إِN<ن> كَانَ مِN<ن> قَوۡمِN<ۭ> بَیۡنَكُمۡ وَبَیۡنَهُم N<مِّ>یثَT<ـٰ>قN<ࣱ> فَدِیَةX<ࣱ> N<مُّ>سَلَّمَةٌ إِلَىW<ٰۤ> أَهۡلِهِT<ۦ> وَتَحۡرِیR<رُ> R<رَ>قَبَةX<ࣲ> N<مُّ>ؤۡمِنَةN<ࣲ>ۖ فَمَX<ن> لَّمۡ یَجِQ<دۡ> فَصِیَامُ شَهۡR<رَ>یۡنِ مُتَتَابِعَیۡنِ تَوۡبَةX<ࣰ> N<مِّ>نَ ٱللَّهِۗ وَكَانَ ٱللَّهُ عَلِیمًا حَكِیمࣰا A<۝>D<٩٢>",
"وَمَX<ن> N<یَ>Q<قۡ>تُلۡ مُؤۡمِنX<ࣰ>ا N<مُّ>تَعَN<مِّ>دN<ࣰ>ا فَجَزَW<اۤ>ؤُهُT<ۥ> جَهَN<نَّ>مُ خَT<ـٰ>لِدN<ࣰ>ا فِیهَا وَغَضِبَ ٱللَّهُ عَلَیۡهِ وَلَعَنَهُT<ۥ> وَأَعَدَّ لَهُT<ۥ> عَذَابًا عَظِیمࣰا A<۝>D<٩٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا ضَR<رَ>Q<بۡ>تُمۡ فِی سَبِیلِ ٱللَّهِ فَتَبَیَّنُوX<ا۟> وَلَا تَقُولُوX<ا۟> لِمَنۡ أَلۡقَىW<ٰۤ> إِلَیۡكُمُ ٱX<ل>سَّلَT<ـٰ>مَ لَسۡتَ مُؤۡمِنN<ࣰ>ا تَQ<بۡ>تَغُونَ عَR<رَ>ضَ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا فَعِN<ن>دَ ٱللَّهِ مَغَانِمُ كَثِیR<رَ>ةN<ࣱ>ۚ كَذَT< ٰ>لِكَ كُN<ن>تُم N<مِّ>N<ن> قَQ<بۡ>لُ فَمَN<نَّ> ٱللَّهُ عَلَیۡكُمۡ فَتَبَیَّنُW<وۤ>X<ا۟>ۚ إِN<نَّ> ٱللَّهَ كَانَ بِمَا تَعۡمَلُونَ خَبِیR<ر>ࣰا A<۝>D<٩٤>",
"لَّا یَسۡتَوِی ٱلۡقَT<ـٰ>عِدُونَ مِنَ ٱلۡمُؤۡمِنِینَ غَیۡR<رُ> أُX<و۟>لِی ٱX<ل>ضَّR<رَ>رِ وَX<ٱ>لۡمُجَT<ـٰ>هِدُونَ فِی سَبِیلِ ٱللَّهِ بِأَمۡوَT< ٰ>لِهِمۡ وَأَN<ن>فُسِهِمۡۚ فَضَّلَ ٱللَّهُ ٱلۡمُجَT<ـٰ>هِدِینَ بِأَمۡوَT< ٰ>لِهِمۡ وَأَN<ن>فُسِهِمۡ عَلَى ٱلۡقَT<ـٰ>عِدِینَ دَR<رَ>جَةX<ࣰ>ۚ N<وَ>كُلّX<ࣰ>ا N<وَ>عَدَ ٱللَّهُ ٱلۡحُسۡنَىٰۚ وَفَضَّلَ ٱللَّهُ ٱلۡمُجَT<ـٰ>هِدِینَ عَلَى ٱلۡقَT<ـٰ>عِدِینَ أَQ<جۡ>R<ر>ًا عَظِیمࣰا A<۝>D<٩٥>",
"دَR<رَ>جَT<ـٰ>تX<ࣲ> N<مِّ>نۡهُ وَمَغۡفِR<رَ>ةX<ࣰ> N<وَ>R<رَ>حۡمَةX<ࣰ>ۚ N<وَ>كَانَ ٱللَّهُ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمًا A<۝>D<٩٦>",
"إِN<نَّ> ٱلَّذِینَ تَوَفَّX<ى>T<ٰ>هُمُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ ظَالِمِW<یۤ> أَN<ن>فُسِهِمۡ قَالُوX<ا۟> فِیمَ كُN<ن>تُمۡۖ قَالُوX<ا۟> كُN<نَّ>ا مُسۡتَضۡعَفِینَ فِی ٱلۡأَR<رۡ>ضِۚ قَالُW<وۤ>X<ا۟> أَلَمۡ تَكُنۡ أَR<رۡ>ضُ ٱللَّهِ وَT< ٰ>سِعَةN<ࣰ> فَتُهَاجِR<رُ>وX<ا۟> فِیهَاۚ فَأُX<و۟>لَW<ـٰۤ>ئِكَ مَأۡوَX<ى>T<ٰ>هُمۡ جَهَN<نَّ>مُۖ وَسَW<اۤ>ءَتۡ مَصِیR<ر>ًا A<۝>D<٩٧>",
"إِلَّا ٱلۡمُسۡتَضۡعَفِینَ مِنَ ٱX<ل>رِّجَالِ وَX<ٱ>X<ل>N<نِّ>سَW<اۤ>ءِ وَX<ٱ>لۡوِلۡدَT< ٰ>نِ لَا یَسۡتَطِیعُونَ حِیلَةX<ࣰ> N<وَ>لَا یَهۡتَدُونَ سَبِیلࣰا A<۝>D<٩٨>",
"فَأُX<و۟>لَW<ـٰۤ>ئِكَ عَسَى ٱللَّهُ أَX<ن> N<یَ>عۡفُوَ عَنۡهُمۡۚ وَكَانَ ٱللَّهُ عَفُوًّا غَفُوR<ر>ࣰا A<۝>D<٩٩>",
"۞ وَمَX<ن> N<یُ>هَاجِرۡ فِی سَبِیلِ ٱللَّهِ یَجِQ<دۡ> فِی ٱلۡأَR<رۡ>ضِ مُR<رَ>T< ٰ>غَمN<ࣰ>ا كَثِیR<ر>X<ࣰ>ا N<وَ>سَعَةX<ࣰ>ۚ N<وَ>مَX<ن> N<یَ>خۡR<رُ>Q<جۡ> مِX<ن>N<ۢ> بَیۡتِهِT<ۦ> مُهَاجِR<ر>ًا إِلَى ٱللَّهِ وَR<رَ>سُولِهِT<ۦ> ثُN<مَّ> یُQ<دۡ>رِكۡهُ ٱلۡمَوۡتُ فَقَQ<دۡ> وَقَعَ أَQ<جۡ>R<رُ>هُT<ۥ> عَلَى ٱللَّهِۗ وَكَانَ ٱللَّهُ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<١٠٠>",
"وَإِذَا ضَR<رَ>Q<بۡ>تُمۡ فِی ٱلۡأَR<رۡ>ضِ فَلَیۡسَ عَلَیۡكُمۡ جُنَاحٌ أَN<ن> تَQ<قۡ>صُR<رُ>وX<ا۟> مِنَ ٱX<ل>صَّلَX<و>T<ٰ>ةِ إِنۡ خِفۡتُمۡ أَX<ن> N<یَ>فۡتِنَكُمُ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟>ۚ إِN<نَّ> ٱلۡكَT<ـٰ>فِرِینَ كَانُوX<ا۟> لَكُمۡ عَدُوّX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<١٠١>",
"وَإِذَا كُN<ن>تَ فِیهِمۡ فَأَقَمۡتَ لَهُمُ ٱX<ل>صَّلَX<و>T<ٰ>ةَ فَلۡتَقُمۡ طَW<اۤ>ئِفَةX<ࣱ> N<مِّ>نۡهُم N<مَّ>عَكَ وَلۡیَأۡخُذُW<وۤ>X<ا۟> أَسۡلِحَتَهُمۡۖ فَإِذَا سَجَدُوX<ا۟> فَلۡیَكُونُوX<ا۟> مِX<ن> N<وَ>R<رَ>W<اۤ>ئِكُمۡ وَلۡتَأۡتِ طَW<اۤ>ئِفَةٌ أُخۡR<رَ>ىٰ لَمۡ یُصَلُّوX<ا۟> فَلۡیُصَلُّوX<ا۟> مَعَكَ وَلۡیَأۡخُذُوX<ا۟> حِذۡR<رَ>هُمۡ وَأَسۡلِحَتَهُمۡۗ وَدَّ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَوۡ تَغۡفُلُونَ عَنۡ أَسۡلِحَتِكُمۡ وَأَمۡتِعَتِكُمۡ فَیَمِیلُونَ عَلَیۡكُم N<مَّ>یۡلَةX<ࣰ> N<وَ>T< ٰ>حِدَةX<ࣰ>ۚ N<وَ>لَا جُنَاحَ عَلَیۡكُمۡ إِN<ن> كَانَ بِكُمۡ أَذX<ࣰ>ى N<مِّ>X<ن> N<مَّ>طَرٍ أَوۡ كُN<ن>تُم N<مَّ>R<رۡ>ضَىW<ٰۤ> أَN<ن> تَضَعُW<وۤ>X<ا۟> أَسۡلِحَتَكُمۡۖ وَخُذُوX<ا۟> حِذۡR<رَ>كُمۡۗ إِN<نَّ> ٱللَّهَ أَعَدَّ لِلۡكَT<ـٰ>فِرِینَ عَذَابX<ࣰ>ا N<مُّ>هِینࣰا A<۝>D<١٠٢>",
"فَإِذَا قَضَیۡتُمُ ٱX<ل>صَّلَX<و>T<ٰ>ةَ فَX<ٱ>ذۡكُR<رُ>وX<ا۟> ٱللَّهَ قِیَT<ـٰ>مX<ࣰ>ا N<وَ>قُعُودX<ࣰ>ا N<وَ>عَلَىٰ جُنُوبِكُمۡۚ فَإِذَا ٱQ<طۡ>مَأۡنَN<ن>تُمۡ فَأَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَۚ إِN<نَّ> ٱX<ل>صَّلَX<و>T<ٰ>ةَ كَانَتۡ عَلَى ٱلۡمُؤۡمِنِینَ كِتَT<ـٰ>بX<ࣰ>ا N<مَّ>وۡقُوتࣰا A<۝>D<١٠٣>",
"وَلَا تَهِنُوX<ا۟> فِی ٱQ<بۡ>تِغَW<اۤ>ءِ ٱلۡقَوۡمِۖ إِN<ن> تَكُونُوX<ا۟> تَأۡلَمُونَ فَإِN<نَّ>هُمۡ یَأۡلَمُونَ كَمَا تَأۡلَمُونَۖ وَتَR<رۡ>جُونَ مِنَ ٱللَّهِ مَا لَا یَR<رۡ>جُونَۗ وَكَانَ ٱللَّهُ عَلِیمًا حَكِیمًا A<۝>D<١٠٤>",
"إِN<نَّ>W<اۤ> أَN<ن>زَلۡنَW<اۤ> إِلَیۡكَ ٱلۡكِتَT<ـٰ>بَ بِX<ٱ>لۡحَقِّ لِتَحۡكُمَ بَیۡنَ ٱX<ل>N<نَّ>اسِ بِمَW<اۤ> أَR<رَ>X<ى>T<ٰ>كَ ٱللَّهُۚ وَلَا تَكُX<ن> لِّلۡخَW<اۤ>ئِنِینَ خَصِیمࣰا A<۝>D<١٠٥>",
"وَX<ٱ>سۡتَغۡفِرِ ٱللَّهَۖ إِN<نَّ> ٱللَّهَ كَانَ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<١٠٦>",
"وَلَا تُجَT<ـٰ>دِلۡ عَنِ ٱلَّذِینَ یَخۡتَانُونَ أَN<ن>فُسَهُمۡۚ إِN<نَّ> ٱللَّهَ لَا یُحِبُّ مَN<ن> كَانَ خَوَّانًا أَثِیمࣰا A<۝>D<١٠٧>",
"یَسۡتَخۡفُونَ مِنَ ٱX<ل>N<نَّ>اسِ وَلَا یَسۡتَخۡفُونَ مِنَ ٱللَّهِ وَهُوَ مَعَهُمۡ إِذۡ یُبَیِّتُونَ مَا لَا یَR<رۡ>ضَىٰ مِنَ ٱلۡقَوۡلِۚ وَكَانَ ٱللَّهُ بِمَا یَعۡمَلُونَ مُحِیطًا A<۝>D<١٠٨>",
"هَW<ـٰۤ>أَN<ن>تُمۡ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ جَT<ـٰ>دَلۡتُمۡ عَنۡهُمۡ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا فَمَX<ن> N<یُ>جَT<ـٰ>دِلُ ٱللَّهَ عَنۡهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ أَم N<مَّ>X<ن> N<یَ>كُونُ عَلَیۡهِمۡ وَكِیلࣰا A<۝>D<١٠٩>",
"وَمَX<ن> N<یَ>عۡمَلۡ سُW<وۤ>ءًا أَوۡ یَظۡلِمۡ نَفۡسَهُT<ۥ> ثُN<مَّ> یَسۡتَغۡفِرِ ٱللَّهَ یَجِدِ ٱللَّهَ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<١١٠>",
"وَمَX<ن> N<یَ>كۡسِQ<بۡ> إِثۡمN<ࣰ>ا فَإِN<نَّ>مَا یَكۡسِبُهُT<ۥ> عَلَىٰ نَفۡسِهِT<ۦ>ۚ وَكَانَ ٱللَّهُ عَلِیمًا حَكِیمࣰا A<۝>D<١١١>",
"وَمَX<ن> N<یَ>كۡسِQ<بۡ> خَطِW<یۤ>ءَةً أَوۡ إِثۡمN<ࣰ>ا ثُN<مَّ> یَR<رۡ>مِ بِهِT<ۦ> بَرِW<یۤ>ءN<ࣰ>ا فَقَدِ ٱحۡتَمَلَ بُهۡتَT<ـٰ>نX<ࣰ>ا N<وَ>إِثۡمX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<١١٢>",
"وَلَوۡلَا فَضۡلُ ٱللَّهِ عَلَیۡكَ وَR<رَ>حۡمَتُهُT<ۥ> لَهَN<مَّ>ت طَّW<اۤ>ئِفَةX<ࣱ> N<مِّ>نۡهُمۡ أَX<ن> N<یُ>ضِلُّوكَ وَمَا یُضِلُّونَ إِلَّW<اۤ> أَN<ن>فُسَهُمۡۖ وَمَا یَضُR<رُّ>ونَكَ مِN<ن> شَیۡءX<ࣲ>ۚ N<وَ>أَN<ن>زَلَ ٱللَّهُ عَلَیۡكَ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحِكۡمَةَ وَعَلَّمَكَ مَا لَمۡ تَكُN<ن> تَعۡلَمُۚ وَكَانَ فَضۡلُ ٱللَّهِ عَلَیۡكَ عَظِیمࣰا A<۝>D<١١٣>",
"۞ لَّا خَیۡR<رَ> فِی كَثِیرX<ࣲ> N<مِّ>ن N<نَّ>Q<جۡ>وَX<ى>T<ٰ>هُمۡ إِلَّا مَنۡ أَمَR<رَ> بِصَدَقَةٍ أَوۡ مَعۡR<رُ>وفٍ أَوۡ إِصۡلَT<ـٰ>حِN<ۭ> بَیۡنَ ٱX<ل>N<نَّ>اسِۚ وَمَX<ن> N<یَ>فۡعَلۡ ذَT< ٰ>لِكَ ٱQ<بۡ>تِغَW<اۤ>ءَ مَR<رۡ>ضَاتِ ٱللَّهِ فَسَوۡفَ نُؤۡتِیهِ أَQ<جۡ>R<ر>ًا عَظِیمࣰا A<۝>D<١١٤>",
"وَمَX<ن> N<یُ>شَاقِقِ ٱX<ل>R<رَّ>سُولَ مِX<ن>N<ۢ> بَعۡدِ مَا تَبَیَّنَ لَهُ ٱلۡهُدَىٰ وَیَتَّبِعۡ غَیۡR<رَ> سَبِیلِ ٱلۡمُؤۡمِنِینَ نُوَلِّهِT<ۦ> مَا تَوَلَّىٰ وَنُصۡلِهِT<ۦ> جَهَN<نَّ>مَۖ وَسَW<اۤ>ءَتۡ مَصِیR<ر>ًا A<۝>D<١١٥>",
"إِN<نَّ> ٱللَّهَ لَا یَغۡفِR<رُ> أَX<ن> N<یُ>شۡR<رَ>كَ بِهِT<ۦ> وَیَغۡفِR<رُ> مَا دُونَ ذَT< ٰ>لِكَ لِمَX<ن> N<یَ>شَW<اۤ>ءُۚ وَمَX<ن> N<یُ>شۡرِكۡ بِٱللَّهِ فَقَQ<دۡ> ضَلَّ ضَلَT<ـٰ>لَN<ۢ>ا بَعِیدًا A<۝>D<١١٦>",
"إِX<ن> N<یَ>Q<دۡ>عُونَ مِN<ن> دُونِهِW<ۦۤ> إِلَّW<اۤ> إِنَT<ـٰ>ثX<ࣰ>ا N<وَ>إِX<ن> N<یَ>Q<دۡ>عُونَ إِلَّا شَیۡطَT<ـٰ>نX<ࣰ>ا N<مَّ>رِیدࣰا A<۝>D<١١٧>",
"لَّعَنَهُ ٱللَّهُۘ وَقَالَ لَأَتَّخِذَN<نَّ> مِنۡ عِبَادِكَ نَصِیبX<ࣰ>ا N<مَّ>فۡR<رُ>وضࣰا A<۝>D<١١٨>",
"وَلَأُضِلَّN<نَّ>هُمۡ وَلَأُمَN<نِّ>یَN<نَّ>هُمۡ وَلَءَامُR<رَ>N<نَّ>هُمۡ فَلَیُبَتِّكُN<نَّ> ءَاذَانَ ٱلۡأَنۡعَT<ـٰ>مِ وَلَءَامُR<رَ>N<نَّ>هُمۡ فَلَیُغَیِّR<رُ>N<نَّ> خَلۡقَ ٱللَّهِۚ وَمَX<ن> N<یَ>تَّخِذِ ٱX<ل>شَّیۡطَT<ـٰ>نَ وَلِیّX<ࣰ>ا N<مِّ>N<ن> دُونِ ٱللَّهِ فَقَQ<دۡ> خَسِR<رَ> خُسۡR<رَ>انX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<١١٩>",
"یَعِدُهُمۡ وَیُمَN<نِّ>یهِمۡۖ وَمَا یَعِدُهُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ إِلَّا غُR<رُ>وR<ر>ًا A<۝>D<١٢٠>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ مَأۡوَX<ى>T<ٰ>هُمۡ جَهَN<نَّ>مُ وَلَا یَجِدُونَ عَنۡهَا مَحِیصࣰا A<۝>D<١٢١>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ سَنُQ<دۡ>خِلُهُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدX<ࣰ>اۖ N<وَ>عۡدَ ٱللَّهِ حَقّX<ࣰ>اۚ N<وَ>مَنۡ أَصۡدَقُ مِنَ ٱللَّهِ قِیلࣰا A<۝>D<١٢٢>",
"لَّیۡسَ بِأَمَانِیِّكُمۡ وَلَW<اۤ> أَمَانِیِّ أَهۡلِ ٱلۡكِتَT<ـٰ>بِۗ مَX<ن> N<یَ>عۡمَلۡ سُW<وۤ>ءX<ࣰ>ا N<یُ>Q<جۡ>زَ بِهِT<ۦ> وَلَا یَجِQ<دۡ> لَهُT<ۥ> مِN<ن> دُونِ ٱللَّهِ وَلِیّX<ࣰ>ا N<وَ>لَا نَصِیR<ر>ࣰا A<۝>D<١٢٣>",
"وَمَX<ن> N<یَ>عۡمَلۡ مِنَ ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ مِN<ن> ذَكَرٍ أَوۡ أُN<ن>ثَىٰ وَهُوَ مُؤۡمِنN<ࣱ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ یَQ<دۡ>خُلُونَ ٱلۡجَN<نَّ>ةَ وَلَا یُظۡلَمُونَ نَقِیR<ر>ࣰا A<۝>D<١٢٤>",
"وَمَنۡ أَحۡسَنُ دِینX<ࣰ>ا N<مِّ>N<مَّ>نۡ أَسۡلَمَ وَQ<جۡ>هَهُT<ۥ> لِلَّهِ وَهُوَ مُحۡسِنX<ࣱ> N<وَ>X<ٱ>تَّبَعَ مِلَّةَ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ حَنِیفX<ࣰ>اۗ N<وَ>X<ٱ>تَّخَذَ ٱللَّهُ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ خَلِیلࣰا A<۝>D<١٢٥>",
"وَلِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۚ وَكَانَ ٱللَّهُ بِكُلِّ شَیۡءX<ࣲ> N<مُّ>حِیطࣰا A<۝>D<١٢٦>",
"وَیَسۡتَفۡتُونَكَ فِی ٱX<ل>N<نِّ>سَW<اۤ>ءِۖ قُلِ ٱللَّهُ یُفۡتِیكُمۡ فِیهِN<نَّ> وَمَا یُتۡلَىٰ عَلَیۡكُمۡ فِی ٱلۡكِتَT<ـٰ>بِ فِی یَتَT<ـٰ>مَى ٱX<ل>N<نِّ>سَW<اۤ>ءِ ٱلَّT<ـٰ>تِی لَا تُؤۡتُونَهُN<نَّ> مَا كُتِبَ لَهُN<نَّ> وَتَR<رۡ>غَبُونَ أَN<ن> تَN<ن>كِحُوهُN<نَّ> وَX<ٱ>لۡمُسۡتَضۡعَفِینَ مِنَ ٱلۡوِلۡدَT< ٰ>نِ وَأَN<ن> تَقُومُوX<ا۟> لِلۡیَتَT<ـٰ>مَىٰ بِX<ٱ>لۡقِسۡطِۚ وَمَا تَفۡعَلُوX<ا۟> مِنۡ خَیۡرN<ࣲ> فَإِN<نَّ> ٱللَّهَ كَانَ بِهِT<ۦ> عَلِیمࣰا A<۝>D<١٢٧>",
"وَإِنِ ٱمۡR<رَ>أَةٌ خَافَتۡ مِX<ن>N<ۢ> بَعۡلِهَا نُشُوزًا أَوۡ إِعۡR<رَ>اضN<ࣰ>ا فَلَا جُنَاحَ عَلَیۡهِمَW<اۤ> أَX<ن> N<یُ>صۡلِحَا بَیۡنَهُمَا صُلۡحX<ࣰ>اۚ N<وَ>X<ٱ>X<ل>صُّلۡحُ خَیۡR<ر>X<ࣱ>ۗ N<وَ>أُحۡضِR<رَ>تِ ٱلۡأَN<ن>فُسُ ٱX<ل>شُّحَّۚ وَإِN<ن> تُحۡسِنُوX<ا۟> وَتَتَّقُوX<ا۟> فَإِN<نَّ> ٱللَّهَ كَانَ بِمَا تَعۡمَلُونَ خَبِیR<ر>ࣰا A<۝>D<١٢٨>",
"وَلَN<ن> تَسۡتَطِیعُW<وۤ>X<ا۟> أَN<ن> تَعۡدِلُوX<ا۟> بَیۡنَ ٱX<ل>N<نِّ>سَW<اۤ>ءِ وَلَوۡ حَR<رَ>صۡتُمۡۖ فَلَا تَمِیلُوX<ا۟> كُلَّ ٱلۡمَیۡلِ فَتَذَR<رُ>وهَا كَX<ٱ>لۡمُعَلَّقَةِۚ وَإِN<ن> تُصۡلِحُوX<ا۟> وَتَتَّقُوX<ا۟> فَإِN<نَّ> ٱللَّهَ كَانَ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<١٢٩>",
"وَإِX<ن> N<یَ>تَفَR<رَّ>قَا یُغۡنِ ٱللَّهُ كُلّX<ࣰ>ا N<مِّ>N<ن> سَعَتِهِT<ۦ>ۚ وَكَانَ ٱللَّهُ وَT< ٰ>سِعًا حَكِیمࣰا A<۝>D<١٣٠>",
"وَلِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۗ وَلَقَQ<دۡ> وَصَّیۡنَا ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ مِN<ن> قَQ<بۡ>لِكُمۡ وَإِیَّاكُمۡ أَنِ ٱتَّقُوX<ا۟> ٱللَّهَۚ وَإِN<ن> تَكۡفُR<رُ>وX<ا۟> فَإِN<نَّ> لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۚ وَكَانَ ٱللَّهُ غَنِیًّا حَمِیدࣰا A<۝>D<١٣١>",
"وَلِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۚ وَكَفَىٰ بِٱللَّهِ وَكِیلًا A<۝>D<١٣٢>",
"إِX<ن> N<یَ>شَأۡ یُذۡهِQ<بۡ>كُمۡ أَیُّهَا ٱX<ل>N<نَّ>اسُ وَیَأۡتِ بِءَاخَرِینَۚ وَكَانَ ٱللَّهُ عَلَىٰ ذَT< ٰ>لِكَ قَدِیR<ر>ࣰا A<۝>D<١٣٣>",
"مَّN<ن> كَانَ یُرِیدُ ثَوَابَ ٱX<ل>دُّنۡیَا فَعِN<ن>دَ ٱللَّهِ ثَوَابُ ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِۚ وَكَانَ ٱللَّهُ سَمِیعَN<ۢ>ا بَصِیR<ر>ࣰا A<۝>D<١٣٤>",
"۞ یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> كُونُوX<ا۟> قَوَّT< ٰ>مِینَ بِX<ٱ>لۡقِسۡطِ شُهَدَW<اۤ>ءَ لِلَّهِ وَلَوۡ عَلَىW<ٰۤ> أَN<ن>فُسِكُمۡ أَوِ ٱلۡوَT< ٰ>لِدَیۡنِ وَX<ٱ>لۡأَQ<قۡ>R<رَ>بِینَۚ إِX<ن> N<یَ>كُنۡ غَنِیًّا أَوۡ فَقِیR<ر>N<ࣰ>ا فَٱللَّهُ أَوۡلَىٰ بِهِمَاۖ فَلَا تَتَّبِعُوX<ا۟> ٱلۡهَوَىW<ٰۤ> أَN<ن> تَعۡدِلُوX<ا۟>ۚ وَإِN<ن> تَلۡوُW<ۥۤ>X<ا۟> أَوۡ تُعۡرِضُوX<ا۟> فَإِN<نَّ> ٱللَّهَ كَانَ بِمَا تَعۡمَلُونَ خَبِیR<ر>ࣰا A<۝>D<١٣٥>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> ءَامِنُوX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ> وَX<ٱ>لۡكِتَT<ـٰ>بِ ٱلَّذِی نَزَّلَ عَلَىٰ R<رَ>سُولِهِT<ۦ> وَX<ٱ>لۡكِتَT<ـٰ>بِ ٱلَّذِW<یۤ> أَN<ن>زَلَ مِN<ن> قَQ<بۡ>لُۚ وَمَX<ن> N<یَ>كۡفُR<رۡ> بِٱللَّهِ وَمَلَW<ـٰۤ>ئِكَتِهِT<ۦ> وَكُتُبِهِT<ۦ> وَR<رُ>سُلِهِT<ۦ> وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ فَقَQ<دۡ> ضَلَّ ضَلَT<ـٰ>لَN<ۢ>ا بَعِیدًا A<۝>D<١٣٦>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> ثُN<مَّ> كَفَR<رُ>وX<ا۟> ثُN<مَّ> ءَامَنُوX<ا۟> ثُN<مَّ> كَفَR<رُ>وX<ا۟> ثُN<مَّ> ٱزۡدَادُوX<ا۟> كُفۡR<ر>X<ࣰ>ا لَّمۡ یَكُنِ ٱللَّهُ لِیَغۡفِR<رَ> لَهُمۡ وَلَا لِیَهۡدِیَهُمۡ سَبِیلَۢا A<۝>D<١٣٧>",
"بَشِّرِ ٱلۡمُنَT<ـٰ>فِقِینَ بِأَN<نَّ> لَهُمۡ عَذَابًا أَلِیمًا A<۝>D<١٣٨>",
"ٱلَّذِینَ یَتَّخِذُونَ ٱلۡكَT<ـٰ>فِرِینَ أَوۡلِیَW<اۤ>ءَ مِN<ن> دُونِ ٱلۡمُؤۡمِنِینَۚ أَیَQ<بۡ>تَغُونَ عِN<ن>دَهُمُ ٱلۡعِزَّةَ فَإِN<نَّ> ٱلۡعِزَّةَ لِلَّهِ جَمِیعࣰا A<۝>D<١٣٩>",
"وَقَQ<دۡ> نَزَّلَ عَلَیۡكُمۡ فِی ٱلۡكِتَT<ـٰ>بِ أَنۡ إِذَا سَمِعۡتُمۡ ءَایَT<ـٰ>تِ ٱللَّهِ یُكۡفَR<رُ> بِهَا وَیُسۡتَهۡزَأُ بِهَا فَلَا تَQ<قۡ>عُدُوX<ا۟> مَعَهُمۡ حَتَّىٰ یَخُوضُوX<ا۟> فِی حَدِیثٍ غَیۡرِهِW<ۦۤ> إِN<نَّ>كُمۡ إِذX<ࣰ>ا N<مِّ>ثۡلُهُمۡۗ إِN<نَّ> ٱللَّهَ جَامِعُ ٱلۡمُنَT<ـٰ>فِقِینَ وَX<ٱ>لۡكَT<ـٰ>فِرِینَ فِی جَهَN<نَّ>مَ جَمِیعًا A<۝>D<١٤٠>",
"ٱلَّذِینَ یَتَR<رَ>بَّصُونَ بِكُمۡ فَإِN<ن> كَانَ لَكُمۡ فَتۡحX<ࣱ> N<مِّ>نَ ٱللَّهِ قَالُW<وۤ>X<ا۟> أَلَمۡ نَكُX<ن> N<مَّ>عَكُمۡ وَإِN<ن> كَانَ لِلۡكَT<ـٰ>فِرِینَ نَصِیبN<ࣱ> قَالُW<وۤ>X<ا۟> أَلَمۡ نَسۡتَحۡوِذۡ عَلَیۡكُمۡ وَنَمۡنَعۡكُم N<مِّ>نَ ٱلۡمُؤۡمِنِینَۚ فَٱللَّهُ یَحۡكُمُ بَیۡنَكُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۗ وَلَX<ن> N<یَ>Q<جۡ>عَلَ ٱللَّهُ لِلۡكَT<ـٰ>فِرِینَ عَلَى ٱلۡمُؤۡمِنِینَ سَبِیلًا A<۝>D<١٤١>",
"إِN<نَّ> ٱلۡمُنَT<ـٰ>فِقِینَ یُخَT<ـٰ>دِعُونَ ٱللَّهَ وَهُوَ خَT<ـٰ>دِعُهُمۡ وَإِذَا قَامُW<وۤ>X<ا۟> إِلَى ٱX<ل>صَّلَX<و>T<ٰ>ةِ قَامُوX<ا۟> كُسَالَىٰ یُR<رَ>W<اۤ>ءُونَ ٱX<ل>N<نَّ>اسَ وَلَا یَذۡكُR<رُ>ونَ ٱللَّهَ إِلَّا قَلِیلࣰا A<۝>D<١٤٢>",
"مُّذَQ<بۡ>ذَبِینَ بَیۡنَ ذَT< ٰ>لِكَ لَW<اۤ> إِلَىٰ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ وَلَW<اۤ> إِلَىٰ هَW<ـٰۤ>ؤُلَW<اۤ>ءِۚ وَمَX<ن> N<یُ>ضۡلِلِ ٱللَّهُ فَلَN<ن> تَجِدَ لَهُT<ۥ> سَبِیلࣰا A<۝>D<١٤٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَتَّخِذُوX<ا۟> ٱلۡكَT<ـٰ>فِرِینَ أَوۡلِیَW<اۤ>ءَ مِN<ن> دُونِ ٱلۡمُؤۡمِنِینَۚ أَتُرِیدُونَ أَN<ن> تَQ<جۡ>عَلُوX<ا۟> لِلَّهِ عَلَیۡكُمۡ سُلۡطَT<ـٰ>نX<ࣰ>ا N<مُّ>بِینًا A<۝>D<١٤٤>",
"إِN<نَّ> ٱلۡمُنَT<ـٰ>فِقِینَ فِی ٱX<ل>دَّR<رۡ>كِ ٱلۡأَسۡفَلِ مِنَ ٱX<ل>N<نَّ>ارِ وَلَN<ن> تَجِدَ لَهُمۡ نَصِیR<ر>ًا A<۝>D<١٤٥>",
"إِلَّا ٱلَّذِینَ تَابُوX<ا۟> وَأَصۡلَحُوX<ا۟> وَX<ٱ>عۡتَصَمُوX<ا۟> بِٱللَّهِ وَأَخۡلَصُوX<ا۟> دِینَهُمۡ لِلَّهِ فَأُX<و۟>لَW<ـٰۤ>ئِكَ مَعَ ٱلۡمُؤۡمِنِینَۖ وَسَوۡفَ یُؤۡتِ ٱللَّهُ ٱلۡمُؤۡمِنِینَ أَQ<جۡ>R<ر>ًا عَظِیمࣰا A<۝>D<١٤٦>",
"مَّا یَفۡعَلُ ٱللَّهُ بِعَذَابِكُمۡ إِN<ن> شَكَR<رۡ>تُمۡ وَءَامَN<ن>تُمۡۚ وَكَانَ ٱللَّهُ شَاكِR<ر>ًا عَلِیمࣰا A<۝>D<١٤٧>",
"۞ لَّا یُحِبُّ ٱللَّهُ ٱلۡجَهۡR<رَ> بِX<ٱ>X<ل>سُّW<وۤ>ءِ مِنَ ٱلۡقَوۡلِ إِلَّا مَN<ن> ظُلِمَۚ وَكَانَ ٱللَّهُ سَمِیعًا عَلِیمًا A<۝>D<١٤٨>",
"إِN<ن> تُQ<بۡ>دُوX<ا۟> خَیۡR<ر>ًا أَوۡ تُخۡفُوهُ أَوۡ تَعۡفُوX<ا۟> عَN<ن> سُW<وۤ>ءN<ࣲ> فَإِN<نَّ> ٱللَّهَ كَانَ عَفُوّN<ࣰ>ا قَدِیR<ر>ًا A<۝>D<١٤٩>",
"إِN<نَّ> ٱلَّذِینَ یَكۡفُR<رُ>ونَ بِٱللَّهِ وَR<رُ>سُلِهِT<ۦ> وَیُرِیدُونَ أَX<ن> N<یُ>فَرِّقُوX<ا۟> بَیۡنَ ٱللَّهِ وَR<رُ>سُلِهِT<ۦ> وَیَقُولُونَ نُؤۡمِنُ بِبَعۡضX<ࣲ> N<وَ>نَكۡفُR<رُ> بِبَعۡضX<ࣲ> N<وَ>یُرِیدُونَ أَX<ن> N<یَ>تَّخِذُوX<ا۟> بَیۡنَ ذَT< ٰ>لِكَ سَبِیلًا A<۝>D<١٥٠>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡكَT<ـٰ>فِR<رُ>ونَ حَقّX<ࣰ>اۚ N<وَ>أَعۡتَQ<دۡ>نَا لِلۡكَT<ـٰ>فِرِینَ عَذَابX<ࣰ>ا N<مُّ>هِینࣰا A<۝>D<١٥١>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> بِٱللَّهِ وَR<رُ>سُلِهِT<ۦ> وَلَمۡ یُفَرِّقُوX<ا۟> بَیۡنَ أَحَدX<ࣲ> N<مِّ>نۡهُمۡ أُX<و۟>لَW<ـٰۤ>ئِكَ سَوۡفَ یُؤۡتِیهِمۡ أُجُوR<رَ>هُمۡۚ وَكَانَ ٱللَّهُ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<١٥٢>",
"یَسۡءَلُكَ أَهۡلُ ٱلۡكِتَT<ـٰ>بِ أَN<ن> تُنَزِّلَ عَلَیۡهِمۡ كِتَT<ـٰ>بX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِۚ فَقَQ<دۡ> سَأَلُوX<ا۟> مُوسَىW<ٰۤ> أَكۡبَR<رَ> مِN<ن> ذَT< ٰ>لِكَ فَقَالُW<وۤ>X<ا۟> أَرِنَا ٱللَّهَ جَهۡR<رَ>ةN<ࣰ> فَأَخَذَتۡهُمُ ٱX<ل>صَّT<ـٰ>عِقَةُ بِظُلۡمِهِمۡۚ ثُN<مَّ> ٱتَّخَذُوX<ا۟> ٱلۡعِQ<جۡ>لَ مِX<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَتۡهُمُ ٱلۡبَیِّنَT<ـٰ>تُ فَعَفَوۡنَا عَN<ن> ذَT< ٰ>لِكَۚ وَءَاتَیۡنَا مُوسَىٰ سُلۡطَT<ـٰ>نX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<١٥٣>",
"وَR<رَ>فَعۡنَا فَوۡقَهُمُ ٱX<ل>طُّوR<رَ> بِمِیثَT<ـٰ>قِهِمۡ وَقُلۡنَا لَهُمُ ٱQ<دۡ>خُلُوX<ا۟> ٱلۡبَابَ سُجَّدX<ࣰ>ا N<وَ>قُلۡنَا لَهُمۡ لَا تَعۡدُوX<ا۟> فِی ٱX<ل>سَّQ<بۡ>تِ وَأَخَذۡنَا مِنۡهُم N<مِّ>یثَT<ـٰ>قًا غَلِیظࣰا A<۝>D<١٥٤>",
"فَبِمَا نَQ<قۡ>ضِهِم N<مِّ>یثَT<ـٰ>قَهُمۡ وَكُفۡرِهِN<م> بِءَایَT<ـٰ>تِ ٱللَّهِ وَقَتۡلِهِمُ ٱلۡأَX<ن>N<ۢ>بِیَW<اۤ>ءَ بِغَیۡرِ حَقّX<ࣲ> N<وَ>قَوۡلِهِمۡ قُلُوبُنَا غُلۡفُN<ۢ>ۚ بَلۡ طَبَعَ ٱللَّهُ عَلَیۡهَا بِكُفۡرِهِمۡ فَلَا یُؤۡمِنُونَ إِلَّا قَلِیلࣰا A<۝>D<١٥٥>",
"وَبِكُفۡرِهِمۡ وَقَوۡلِهِمۡ عَلَىٰ مَR<رۡ>یَمَ بُهۡتَT<ـٰ>نًا عَظِیمࣰا A<۝>D<١٥٦>",
"وَقَوۡلِهِمۡ إِN<نَّ>ا قَتَلۡنَا ٱلۡمَسِیحَ عِیسَى ٱQ<بۡ>نَ مَR<رۡ>یَمَ R<رَ>سُولَ ٱللَّهِ وَمَا قَتَلُوهُ وَمَا صَلَبُوهُ وَلَT<ـٰ>كِN<ن> شُبِّهَ لَهُمۡۚ وَإِN<نَّ> ٱلَّذِینَ ٱخۡتَلَفُوX<ا۟> فِیهِ لَفِی شَكّX<ࣲ> N<مِّ>نۡهُۚ مَا لَهُN<م> بِهِT<ۦ> مِنۡ عِلۡمٍ إِلَّا ٱتِّبَاعَ ٱX<ل>ظَّN<نِّ>ۚ وَمَا قَتَلُوهُ یَقِینَۢا A<۝>D<١٥٧>",
"بَل R<رَّ>فَعَهُ ٱللَّهُ إِلَیۡهِۚ وَكَانَ ٱللَّهُ عَزِیزًا حَكِیمࣰا A<۝>D<١٥٨>",
"وَإِX<ن> N<مِّ>نۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ إِلَّا لَیُؤۡمِنَN<نَّ> بِهِT<ۦ> قَQ<بۡ>لَ مَوۡتِهِT<ۦ>ۖ وَیَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ یَكُونُ عَلَیۡهِمۡ شَهِیدࣰا A<۝>D<١٥٩>",
"فَبِظُلۡمX<ࣲ> N<مِّ>نَ ٱلَّذِینَ هَادُوX<ا۟> حَR<رَّ>مۡنَا عَلَیۡهِمۡ طَیِّبَT<ـٰ>تٍ أُحِلَّتۡ لَهُمۡ وَبِصَدِّهِمۡ عَN<ن> سَبِیلِ ٱللَّهِ كَثِیR<ر>ࣰا A<۝>D<١٦٠>",
"وَأَخۡذِهِمُ ٱX<ل>رِّبَX<و>T<ٰ>X<ا۟> وَقَQ<دۡ> نُهُوX<ا۟> عَنۡهُ وَأَكۡلِهِمۡ أَمۡوَT< ٰ>لَ ٱX<ل>N<نَّ>اسِ بِX<ٱ>لۡبَT<ـٰ>طِلِۚ وَأَعۡتَQ<دۡ>نَا لِلۡكَT<ـٰ>فِرِینَ مِنۡهُمۡ عَذَابًا أَلِیمࣰا A<۝>D<١٦١>",
"لَّT<ـٰ>كِنِ ٱX<ل>R<رَّ>T< ٰ>سِخُونَ فِی ٱلۡعِلۡمِ مِنۡهُمۡ وَX<ٱ>لۡمُؤۡمِنُونَ یُؤۡمِنُونَ بِمَW<اۤ> أُN<ن>زِلَ إِلَیۡكَ وَمَW<اۤ> أُN<ن>زِلَ مِN<ن> قَQ<بۡ>لِكَۚ وَX<ٱ>لۡمُقِیمِینَ ٱX<ل>صَّلَX<و>T<ٰ>ةَۚ وَX<ٱ>لۡمُؤۡتُونَ ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَX<ٱ>لۡمُؤۡمِنُونَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ أُX<و۟>لَW<ـٰۤ>ئِكَ سَنُؤۡتِیهِمۡ أَQ<جۡ>R<ر>ًا عَظِیمًا A<۝>D<١٦٢>",
"۞ إِN<نَّ>W<اۤ> أَوۡحَیۡنَW<اۤ> إِلَیۡكَ كَمَW<اۤ> أَوۡحَیۡنَW<اۤ> إِلَىٰ نُوحX<ࣲ> N<وَ>X<ٱ>X<ل>N<نَّ>بِیِّT<ـۧ>نَ مِX<ن>N<ۢ> بَعۡدِهِT<ۦ>ۚ وَأَوۡحَیۡنَW<اۤ> إِلَىW<ٰۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَإِسۡمَT<ـٰ>عِیلَ وَإِسۡحَT<ـٰ>قَ وَیَعۡقُوبَ وَX<ٱ>لۡأَسۡبَاطِ وَعِیسَىٰ وَأَیُّوبَ وَیُونُسَ وَهَT<ـٰ>R<رُ>ونَ وَسُلَیۡمَT<ـٰ>نَۚ وَءَاتَیۡنَا دَاوُT<ۥ>دَ زَبُوR<ر>ࣰا A<۝>D<١٦٣>",
"وَR<رُ>سُلN<ࣰ>ا قَQ<دۡ> قَصَصۡنَT<ـٰ>هُمۡ عَلَیۡكَ مِN<ن> قَQ<بۡ>لُ وَR<رُ>سُلX<ࣰ>ا لَّمۡ نَQ<قۡ>صُصۡهُمۡ عَلَیۡكَۚ وَكَلَّمَ ٱللَّهُ مُوسَىٰ تَكۡلِیمࣰا A<۝>D<١٦٤>",
"R<رُّ>سُلX<ࣰ>ا N<مُّ>بَشِّرِینَ وَمُN<ن>ذِرِینَ لِئَلَّا یَكُونَ لِX<ل>N<نَّ>اسِ عَلَى ٱللَّهِ حُجَّةُN<ۢ> بَعۡدَ ٱX<ل>R<رُّ>سُلِۚ وَكَانَ ٱللَّهُ عَزِیزًا حَكِیمࣰا A<۝>D<١٦٥>",
"لَّT<ـٰ>كِنِ ٱللَّهُ یَشۡهَدُ بِمَW<اۤ> أَN<ن>زَلَ إِلَیۡكَۖ أَN<ن>زَلَهُT<ۥ> بِعِلۡمِهِT<ۦ>ۖ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ یَشۡهَدُونَۚ وَكَفَىٰ بِٱللَّهِ شَهِیدًا A<۝>D<١٦٦>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَصَدُّوX<ا۟> عَN<ن> سَبِیلِ ٱللَّهِ قَQ<دۡ> ضَلُّوX<ا۟> ضَلَT<ـٰ>لَN<ۢ>ا بَعِیدًا A<۝>D<١٦٧>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَظَلَمُوX<ا۟> لَمۡ یَكُنِ ٱللَّهُ لِیَغۡفِR<رَ> لَهُمۡ وَلَا لِیَهۡدِیَهُمۡ طَرِیقًا A<۝>D<١٦٨>",
"إِلَّا طَرِیقَ جَهَN<نَّ>مَ خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدX<ࣰ>اۚ N<وَ>كَانَ ذَT< ٰ>لِكَ عَلَى ٱللَّهِ یَسِیR<ر>ࣰا A<۝>D<١٦٩>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ قَQ<دۡ> جَW<اۤ>ءَكُمُ ٱX<ل>R<رَّ>سُولُ بِX<ٱ>لۡحَقِّ مِX<ن> R<رَّ>بِّكُمۡ فَءَامِنُوX<ا۟> خَیۡR<ر>X<ࣰ>ا لَّكُمۡۚ وَإِN<ن> تَكۡفُR<رُ>وX<ا۟> فَإِN<نَّ> لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَكَانَ ٱللَّهُ عَلِیمًا حَكِیمࣰا A<۝>D<١٧٠>",
"یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ لَا تَغۡلُوX<ا۟> فِی دِینِكُمۡ وَلَا تَقُولُوX<ا۟> عَلَى ٱللَّهِ إِلَّا ٱلۡحَقَّۚ إِN<نَّ>مَا ٱلۡمَسِیحُ عِیسَى ٱQ<بۡ>نُ مَR<رۡ>یَمَ R<رَ>سُولُ ٱللَّهِ وَكَلِمَتُهُW<ۥۤ> أَلۡقَX<ى>T<ٰ>هَW<اۤ> إِلَىٰ مَR<رۡ>یَمَ وَR<رُ>وحX<ࣱ> N<مِّ>نۡهُۖ فَءَامِنُوX<ا۟> بِٱللَّهِ وَR<رُ>سُلِهِT<ۦ>ۖ وَلَا تَقُولُوX<ا۟> ثَلَT<ـٰ>ثَةٌۚ ٱN<ن>تَهُوX<ا۟> خَیۡR<ر>X<ࣰ>ا لَّكُمۡۚ إِN<نَّ>مَا ٱللَّهُ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدN<ࣱ>ۖ سُQ<بۡ>حَT<ـٰ>نَهُW<ۥۤ> أَX<ن> N<یَ>كُونَ لَهُT<ۥ> وَلَدX<ࣱ>ۘ لَّهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۗ وَكَفَىٰ بِٱللَّهِ وَكِیلࣰا A<۝>D<١٧١>",
"لَّX<ن> N<یَ>سۡتَN<ن>كِفَ ٱلۡمَسِیحُ أَX<ن> N<یَ>كُونَ عَQ<بۡ>دX<ࣰ>ا لِّلَّهِ وَلَا ٱلۡمَلَW<ـٰۤ>ئِكَةُ ٱلۡمُقَR<رَّ>بُونَۚ وَمَX<ن> N<یَ>سۡتَN<ن>كِفۡ عَنۡ عِبَادَتِهِT<ۦ> وَیَسۡتَكۡبِرۡ فَسَیَحۡشُR<رُ>هُمۡ إِلَیۡهِ جَمِیعࣰا A<۝>D<١٧٢>",
"فَأَN<مَّ>ا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ فَیُوَفِّیهِمۡ أُجُوR<رَ>هُمۡ وَیَزِیدُهُم N<مِّ>N<ن> فَضۡلِهِT<ۦ>ۖ وَأَN<مَّ>ا ٱلَّذِینَ ٱسۡتَN<ن>كَفُوX<ا۟> وَX<ٱ>سۡتَكۡبَR<رُ>وX<ا۟> فَیُعَذِّبُهُمۡ عَذَابًا أَلِیمX<ࣰ>ا N<وَ>لَا یَجِدُونَ لَهُم N<مِّ>N<ن> دُونِ ٱللَّهِ وَلِیّX<ࣰ>ا N<وَ>لَا نَصِیR<ر>ࣰا A<۝>D<١٧٣>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ قَQ<دۡ> جَW<اۤ>ءَكُN<م> بُR<رۡ>هَT<ـٰ>نX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ وَأَN<ن>زَلۡنَW<اۤ> إِلَیۡكُمۡ نُوR<ر>X<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<١٧٤>",
"فَأَN<مَّ>ا ٱلَّذِینَ ءَامَنُوX<ا۟> بِٱللَّهِ وَX<ٱ>عۡتَصَمُوX<ا۟> بِهِT<ۦ> فَسَیُQ<دۡ>خِلُهُمۡ فِی R<رَ>حۡمَةX<ࣲ> N<مِّ>نۡهُ وَفَضۡلX<ࣲ> N<وَ>یَهۡدِیهِمۡ إِلَیۡهِ صِR<رَ>T< ٰ>طX<ࣰ>ا N<مُّ>سۡتَقِیمࣰا A<۝>D<١٧٥>",
"یَسۡتَفۡتُونَكَ قُلِ ٱللَّهُ یُفۡتِیكُمۡ فِی ٱلۡكَلَT<ـٰ>لَةِۚ إِنِ ٱمۡR<رُ>ؤٌX<ا۟> هَلَكَ لَیۡسَ لَهُT<ۥ> وَلَدX<ࣱ> N<وَ>لَهُW<ۥۤ> أُخۡتN<ࣱ> فَلَهَا نِصۡفُ مَا تَR<رَ>كَۚ وَهُوَ یَرِثُهَW<اۤ> إِX<ن> لَّمۡ یَكُX<ن> لَّهَا وَلَدN<ࣱ>ۚ فَإِN<ن> كَانَتَا ٱثۡنَتَیۡنِ فَلَهُمَا ٱX<ل>ثُّلُثَانِ مِN<مَّ>ا تَR<رَ>كَۚ وَإِN<ن> كَانُW<وۤ>X<ا۟> إِخۡوَةX<ࣰ> رِّجَالX<ࣰ>ا N<وَ>نِسَW<اۤ>ءN<ࣰ> فَلِX<ل>ذَّكَرِ مِثۡلُ حَظِّ ٱلۡأُN<ن>ثَیَیۡنِۗ یُبَیِّنُ ٱللَّهُ لَكُمۡ أَN<ن> تَضِلُّوX<ا۟>ۗ وَٱللَّهُ بِكُلِّ شَیۡءٍ عَلِJ<ی>مُۢ A<۝>D<١٧٦>",
"#۞ یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَوۡفُوX<ا۟> بِX<ٱ>لۡعُقُودِۚ أُحِلَّتۡ لَكُN<م> بَهِیمَةُ ٱلۡأَنۡعَT<ـٰ>مِ إِلَّا مَا یُتۡلَىٰ عَلَیۡكُمۡ غَیۡR<رَ> مُحِلِّی ٱX<ل>صَّیۡدِ وَأَN<ن>تُمۡ حُR<رُ>مٌۗ إِN<نَّ> ٱللَّهَ یَحۡكُمُ مَا یُرِJ<ی>Q<د>ُ A<۝>D<١>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تُحِلُّوX<ا۟> شَعَW<ـٰۤ>ئِR<رَ> ٱللَّهِ وَلَا ٱX<ل>شَّهۡR<رَ> ٱلۡحَR<رَ>امَ وَلَا ٱلۡهَQ<دۡ>یَ وَلَا ٱلۡقَلَW<ـٰۤ>ئِدَ وَلَW<اۤ> ءَL<اۤ>N<مِّ>ینَ ٱلۡبَیۡتَ ٱلۡحَR<رَ>امَ یَQ<بۡ>تَغُونَ فَضۡلX<ࣰ>ا N<مِّ>X<ن> R<رَّ>بِّهِمۡ وَرِضۡوَT< ٰ>نX<ࣰ>اۚ N<وَ>إِذَا حَلَلۡتُمۡ فَX<ٱ>صۡطَادُوX<ا۟>ۚ وَلَا یَQ<جۡ>رِمَN<نَّ>كُمۡ شَنَءَانُ قَوۡمٍ أَN<ن> صَدُّوكُمۡ عَنِ ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِ أَN<ن> تَعۡتَدُوX<ا۟>ۘ وَتَعَاوَنُوX<ا۟> عَلَى ٱلۡبِرِّ وَX<ٱ>X<ل>تَّQ<قۡ>وَىٰۖ وَلَا تَعَاوَنُوX<ا۟> عَلَى ٱلۡإِثۡمِ وَX<ٱ>لۡعُQ<دۡ>وَT< ٰ>نِۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَۖ إِN<نَّ> ٱللَّهَ شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<٢>",
"حُرِّمَتۡ عَلَیۡكُمُ ٱلۡمَیۡتَةُ وَX<ٱ>X<ل>دَّمُ وَلَحۡمُ ٱلۡخِN<ن>زِیرِ وَمَW<اۤ> أُهِلَّ لِغَیۡرِ ٱللَّهِ بِهِT<ۦ> وَX<ٱ>لۡمُنۡخَنِقَةُ وَX<ٱ>لۡمَوۡقُوذَةُ وَX<ٱ>لۡمُتَR<رَ>دِّیَةُ وَX<ٱ>X<ل>N<نَّ>طِیحَةُ وَمَW<اۤ> أَكَلَ ٱX<ل>سَّبُعُ إِلَّا مَا ذَكَّیۡتُمۡ وَمَا ذُبِحَ عَلَى ٱX<ل>N<نُّ>صُبِ وَأَN<ن> تَسۡتَQ<قۡ>سِمُوX<ا۟> بِX<ٱ>لۡأَزۡلَT<ـٰ>مِۚ ذَT< ٰ>لِكُمۡ فِسۡقٌۗ ٱلۡیَوۡمَ یَئِسَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِN<ن> دِینِكُمۡ فَلَا تَخۡشَوۡهُمۡ وَX<ٱ>خۡشَوۡنِۚ ٱلۡیَوۡمَ أَكۡمَلۡتُ لَكُمۡ دِینَكُمۡ وَأَتۡمَمۡتُ عَلَیۡكُمۡ نِعۡمَتِی وَR<رَ>ضِیتُ لَكُمُ ٱلۡإِسۡلَT<ـٰ>مَ دِینN<ࣰ>اۚ فَمَنِ ٱضۡطُR<رَّ> فِی مَخۡمَصَةٍ غَیۡR<رَ> مُتَجَانِفX<ࣲ> لِّإِثۡمN<ࣲ> فَإِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٣>",
"یَسۡءَلُونَكَ مَاذَW<اۤ> أُحِلَّ لَهُمۡۖ قُلۡ أُحِلَّ لَكُمُ ٱX<ل>طَّیِّبَT<ـٰ>تُ وَمَا عَلَّمۡتُم N<مِّ>نَ ٱلۡجَوَارِحِ مُكَلِّبِینَ تُعَلِّمُونَهُN<نَّ> مِN<مَّ>ا عَلَّمَكُمُ ٱللَّهُۖ فَكُلُوX<ا۟> مِN<مَّ>W<اۤ> أَمۡسَكۡنَ عَلَیۡكُمۡ وَX<ٱ>ذۡكُR<رُ>وX<ا۟> ٱسۡمَ ٱللَّهِ عَلَیۡهِۖ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَۚ إِN<نَّ> ٱللَّهَ سَرِیعُ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<٤>",
"ٱلۡیَوۡمَ أُحِلَّ لَكُمُ ٱX<ل>طَّیِّبَT<ـٰ>تُۖ وَطَعَامُ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ حِلّX<ࣱ> لَّكُمۡ وَطَعَامُكُمۡ حِلّX<ࣱ> لَّهُمۡۖ وَX<ٱ>لۡمُحۡصَنَT<ـٰ>تُ مِنَ ٱلۡمُؤۡمِنَT<ـٰ>تِ وَX<ٱ>لۡمُحۡصَنَT<ـٰ>تُ مِنَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ مِN<ن> قَQ<بۡ>لِكُمۡ إِذَW<اۤ> ءَاتَیۡتُمُوهُN<نَّ> أُجُوR<رَ>هُN<نَّ> مُحۡصِنِینَ غَیۡR<رَ> مُسَT<ـٰ>فِحِینَ وَلَا مُتَّخِذِW<یۤ> أَخۡدَانX<ࣲ>ۗ N<وَ>مَX<ن> N<یَ>كۡفُR<رۡ> بِX<ٱ>لۡإِیمَT<ـٰ>نِ فَقَQ<دۡ> حَبِطَ عَمَلُهُT<ۥ> وَهُوَ فِی ٱلۡءَاخِR<رَ>ةِ مِنَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٥>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا قُمۡتُمۡ إِلَى ٱX<ل>صَّلَX<و>T<ٰ>ةِ فَX<ٱ>غۡسِلُوX<ا۟> وُجُوهَكُمۡ وَأَیۡدِیَكُمۡ إِلَى ٱلۡمَR<رَ>افِقِ وَX<ٱ>مۡسَحُوX<ا۟> بِR<رُ>ءُوسِكُمۡ وَأَR<رۡ>جُلَكُمۡ إِلَى ٱلۡكَعۡبَیۡنِۚ وَإِN<ن> كُN<ن>تُمۡ جُنُبN<ࣰ>ا فَX<ٱ>طَّهَّR<رُ>وX<ا۟>ۚ وَإِN<ن> كُN<ن>تُم N<مَّ>R<رۡ>ضَىW<ٰۤ> أَوۡ عَلَىٰ سَفَرٍ أَوۡ جَW<اۤ>ءَ أَحَدX<ࣱ> N<مِّ>N<ن>كُم N<مِّ>نَ ٱلۡغَW<اۤ>ئِطِ أَوۡ لَT<ـٰ>مَسۡتُمُ ٱX<ل>N<نِّ>سَW<اۤ>ءَ فَلَمۡ تَجِدُوX<ا۟> مَW<اۤ>ءN<ࣰ> فَتَیَN<مَّ>مُوX<ا۟> صَعِیدN<ࣰ>ا طَیِّبN<ࣰ>ا فَX<ٱ>مۡسَحُوX<ا۟> بِوُجُوهِكُمۡ وَأَیۡدِیكُم N<مِّ>نۡهُۚ مَا یُرِیدُ ٱللَّهُ لِیَQ<جۡ>عَلَ عَلَیۡكُم N<مِّ>نۡ حَR<رَ>جX<ࣲ> N<وَ>لَT<ـٰ>كِX<ن> N<یُ>رِیدُ لِیُطَهِّR<رَ>كُمۡ وَلِیُتِN<مَّ> نِعۡمَتَهُT<ۥ> عَلَیۡكُمۡ لَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<٦>",
"وَX<ٱ>ذۡكُR<رُ>وX<ا۟> نِعۡمَةَ ٱللَّهِ عَلَیۡكُمۡ وَمِیثَT<ـٰ>قَهُ ٱلَّذِی وَاثَقَكُN<م> بِهِW<ۦۤ> إِذۡ قُلۡتُمۡ سَمِعۡنَا وَأَطَعۡنَاۖ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَۚ إِN<نَّ> ٱللَّهَ عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> كُونُوX<ا۟> قَوَّT< ٰ>مِینَ لِلَّهِ شُهَدَW<اۤ>ءَ بِX<ٱ>لۡقِسۡطِۖ وَلَا یَQ<جۡ>رِمَN<نَّ>كُمۡ شَنَءَانُ قَوۡمٍ عَلَىW<ٰۤ> أَلَّا تَعۡدِلُوX<ا۟>ۚ ٱعۡدِلُوX<ا۟> هُوَ أَQ<قۡ>R<رَ>بُ لِX<ل>تَّQ<قۡ>وَىٰۖ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَۚ إِN<نَّ> ٱللَّهَ خَبِیR<رُ>N<ۢ> بِمَا تَعۡمَلُJ<و>نَ A<۝>D<٨>",
"وَعَدَ ٱللَّهُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<وَ>أَQ<جۡ>R<ر>ٌ عَظِJ<ی>مࣱ A<۝>D<٩>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> وَكَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَW<اۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡجَحِJ<ی>مِ A<۝>D<١٠>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱذۡكُR<رُ>وX<ا۟> نِعۡمَتَ ٱللَّهِ عَلَیۡكُمۡ إِذۡ هَN<مَّ> قَوۡمٌ أَX<ن> N<یَ>Q<بۡ>سُطُW<وۤ>X<ا۟> إِلَیۡكُمۡ أَیۡدِیَهُمۡ فَكَفَّ أَیۡدِیَهُمۡ عَN<ن>كُمۡۖ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَۚ وَعَلَى ٱللَّهِ فَلۡیَتَوَكَّلِ ٱلۡمُؤۡمِنُJ<و>نَ A<۝>D<١١>",
"۞ وَلَقَQ<دۡ> أَخَذَ ٱللَّهُ مِیثَT<ـٰ>قَ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ وَبَعَثۡنَا مِنۡهُمُ ٱثۡنَیۡ عَشَR<رَ> نَقِیبX<ࣰ>اۖ N<وَ>قَالَ ٱللَّهُ إِN<نِّ>ی مَعَكُمۡۖ لَئِنۡ أَقَمۡتُمُ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتَیۡتُمُ ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَءَامَN<ن>تُN<م> بِR<رُ>سُلِی وَعَزَّR<رۡ>تُمُوهُمۡ وَأَQ<قۡ>R<رَ>ضۡتُمُ ٱللَّهَ قَR<رۡ>ضًا حَسَنX<ࣰ>ا لَّأُكَفِّR<رَ>N<نَّ> عَN<ن>كُمۡ سَیِّءَاتِكُمۡ وَلَأُQ<دۡ>خِلَN<نَّ>كُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ>ۚ فَمَN<ن> كَفَR<رَ> بَعۡدَ ذَT< ٰ>لِكَ مِN<ن>كُمۡ فَقَQ<دۡ> ضَلَّ سَوَW<اۤ>ءَ ٱX<ل>سَّبِJ<ی>لِ A<۝>D<١٢>",
"فَبِمَا نَQ<قۡ>ضِهِم N<مِّ>یثَT<ـٰ>قَهُمۡ لَعَN<نَّ>T<ـٰ>هُمۡ وَجَعَلۡنَا قُلُوبَهُمۡ قَT<ـٰ>سِیَةX<ࣰ>ۖ N<یُ>حَرِّفُونَ ٱلۡكَلِمَ عَX<ن> N<مَّ>وَاضِعِهِT<ۦ> وَنَسُوX<ا۟> حَظّX<ࣰ>ا N<مِّ>N<مَّ>ا ذُكِّR<رُ>وX<ا۟> بِهِT<ۦ>ۚ وَلَا تَزَالُ تَطَّلِعُ عَلَىٰ خَW<اۤ>ئِنَةX<ࣲ> N<مِّ>نۡهُمۡ إِلَّا قَلِیلX<ࣰ>ا N<مِّ>نۡهُمۡۖ فَX<ٱ>عۡفُ عَنۡهُمۡ وَX<ٱ>صۡفَحۡۚ إِN<نَّ> ٱللَّهَ یُحِبُّ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٣>",
"وَمِنَ ٱلَّذِینَ قَالُW<وۤ>X<ا۟> إِN<نَّ>ا نَصَT<ـٰ>R<رَ>ىW<ٰۤ> أَخَذۡنَا مِیثَT<ـٰ>قَهُمۡ فَنَسُوX<ا۟> حَظّX<ࣰ>ا N<مِّ>N<مَّ>ا ذُكِّR<رُ>وX<ا۟> بِهِT<ۦ> فَأَغۡR<رَ>یۡنَا بَیۡنَهُمُ ٱلۡعَدَاوَةَ وَX<ٱ>لۡبَغۡضَW<اۤ>ءَ إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِۚ وَسَوۡفَ یُنَبِّئُهُمُ ٱللَّهُ بِمَا كَانُوX<ا۟> یَصۡنَعُJ<و>نَ A<۝>D<١٤>",
"یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ قَQ<دۡ> جَW<اۤ>ءَكُمۡ R<رَ>سُولُنَا یُبَیِّنُ لَكُمۡ كَثِیR<ر>X<ࣰ>ا N<مِّ>N<مَّ>ا كُN<ن>تُمۡ تُخۡفُونَ مِنَ ٱلۡكِتَT<ـٰ>بِ وَیَعۡفُوX<ا۟> عَN<ن> كَثِیرN<ࣲ>ۚ قَQ<دۡ> جَW<اۤ>ءَكُم N<مِّ>نَ ٱللَّهِ نُوR<ر>X<ࣱ> N<وَ>كِتَT<ـٰ>بX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٥>",
"یَهۡدِی بِهِ ٱللَّهُ مَنِ ٱتَّبَعَ رِضۡوَT< ٰ>نَهُT<ۥ> سُبُلَ ٱX<ل>سَّلَT<ـٰ>مِ وَیُخۡرِجُهُم N<مِّ>نَ ٱX<ل>ظُّلُمَT<ـٰ>تِ إِلَى ٱX<ل>N<نُّ>ورِ بِإِذۡنِهِT<ۦ> وَیَهۡدِیهِمۡ إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<١٦>",
"لَّقَQ<دۡ> كَفَR<رَ> ٱلَّذِینَ قَالُW<وۤ>X<ا۟> إِN<نَّ> ٱللَّهَ هُوَ ٱلۡمَسِیحُ ٱQ<بۡ>نُ مَR<رۡ>یَمَۚ قُلۡ فَمَX<ن> N<یَ>مۡلِكُ مِنَ ٱللَّهِ شَیۡءًا إِنۡ أَR<رَ>ادَ أَX<ن> N<یُ>هۡلِكَ ٱلۡمَسِیحَ ٱQ<بۡ>نَ مَR<رۡ>یَمَ وَأُN<مَّ>هُT<ۥ> وَمَN<ن> فِی ٱلۡأَR<رۡ>ضِ جَمِیعX<ࣰ>اۗ N<وَ>لِلَّهِ مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَاۚ یَخۡلُقُ مَا یَشَW<اۤ>ءُۚ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<١٧>",
"وَقَالَتِ ٱلۡیَهُودُ وَX<ٱ>X<ل>N<نَّ>صَT<ـٰ>R<رَ>ىٰ نَحۡنُ أَQ<بۡ>نَW<ـٰۤ>ؤُX<ا۟> ٱللَّهِ وَأَحِبَّW<ـٰۤ>ؤُهُT<ۥ>ۚ قُلۡ فَلِمَ یُعَذِّبُكُN<م> بِذُنُوبِكُN<م>ۖ بَلۡ أَN<ن>تُN<م> بَشَR<ر>X<ࣱ> N<مِّ>N<مَّ>نۡ خَلَقَۚ یَغۡفِR<رُ> لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیُعَذِّبُ مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَلِلَّهِ مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَاۖ وَإِلَیۡهِ ٱلۡمَصِJ<ی>رُ A<۝>D<١٨>",
"یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ قَQ<دۡ> جَW<اۤ>ءَكُمۡ R<رَ>سُولُنَا یُبَیِّنُ لَكُمۡ عَلَىٰ فَتۡR<رَ>ةX<ࣲ> N<مِّ>نَ ٱX<ل>R<رُّ>سُلِ أَN<ن> تَقُولُوX<ا۟> مَا جَW<اۤ>ءَنَا مِX<ن>N<ۢ> بَشِیرX<ࣲ> N<وَ>لَا نَذِیرN<ࣲ>ۖ فَقَQ<دۡ> جَW<اۤ>ءَكُN<م> بَشِیR<ر>X<ࣱ> N<وَ>نَذِیR<ر>X<ࣱ>ۗ N<وَ>ٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<١٩>",
"وَإِذۡ قَالَ مُوسَىٰ لِقَوۡمِهِT<ۦ> یَT<ـٰ>قَوۡمِ ٱذۡكُR<رُ>وX<ا۟> نِعۡمَةَ ٱللَّهِ عَلَیۡكُمۡ إِذۡ جَعَلَ فِیكُمۡ أَX<ن>N<ۢ>بِیَW<اۤ>ءَ وَجَعَلَكُم N<مُّ>لُوكX<ࣰ>ا N<وَ>ءَاتَX<ى>T<ٰ>كُم N<مَّ>ا لَمۡ یُؤۡتِ أَحَدX<ࣰ>ا N<مِّ>نَ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٢٠>",
"یَT<ـٰ>قَوۡمِ ٱQ<دۡ>خُلُوX<ا۟> ٱلۡأَR<رۡ>ضَ ٱلۡمُقَدَّسَةَ ٱلَّتِی كَتَبَ ٱللَّهُ لَكُمۡ وَلَا تَR<رۡ>تَدُّوX<ا۟> عَلَىW<ٰۤ> أَQ<دۡ>بَارِكُمۡ فَتَN<ن>قَلِبُوX<ا۟> خَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٢١>",
"قَالُوX<ا۟> یَT<ـٰ>مُوسَىW<ٰۤ> إِN<نَّ> فِیهَا قَوۡمN<ࣰ>ا جَبَّارِینَ وَإِN<نَّ>ا لَن N<نَّ>Q<دۡ>خُلَهَا حَتَّىٰ یَخۡR<رُ>جُوX<ا۟> مِنۡهَا فَإِX<ن> N<یَ>خۡR<رُ>جُوX<ا۟> مِنۡهَا فَإِN<نَّ>ا دَT< ٰ>خِلُJ<و>نَ A<۝>D<٢٢>",
"قَالَ R<رَ>جُلَانِ مِنَ ٱلَّذِینَ یَخَافُونَ أَنۡعَمَ ٱللَّهُ عَلَیۡهِمَا ٱQ<دۡ>خُلُوX<ا۟> عَلَیۡهِمُ ٱلۡبَابَ فَإِذَا دَخَلۡتُمُوهُ فَإِN<نَّ>كُمۡ غَT<ـٰ>لِبُونَۚ وَعَلَى ٱللَّهِ فَتَوَكَّلُW<وۤ>X<ا۟> إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٢٣>",
"قَالُوX<ا۟> یَT<ـٰ>مُوسَىW<ٰۤ> إِN<نَّ>ا لَن N<نَّ>Q<دۡ>خُلَهَW<اۤ> أَبَدX<ࣰ>ا N<مَّ>ا دَامُوX<ا۟> فِیهَا فَX<ٱ>ذۡهَQ<بۡ> أَN<ن>تَ وَR<رَ>بُّكَ فَقَT<ـٰ>تِلَW<اۤ> إِN<نَّ>ا هَT<ـٰ>هُنَا قَT<ـٰ>عِدُJ<و>نَ A<۝>D<٢٤>",
"قَالَ R<رَ>بِّ إِN<نِّ>ی لَW<اۤ> أَمۡلِكُ إِلَّا نَفۡسِی وَأَخِیۖ فَX<ٱ>فۡR<رُ>Q<قۡ> بَیۡنَنَا وَبَیۡنَ ٱلۡقَوۡمِ ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٢٥>",
"قَالَ فَإِN<نَّ>هَا مُحَR<رَّ>مَةٌ عَلَیۡهِمۡۛ أَR<رۡ>بَعِینَ سَنَةX<ࣰ>ۛ N<یَ>تِیهُونَ فِی ٱلۡأَR<رۡ>ضِۚ فَلَا تَأۡسَ عَلَى ٱلۡقَوۡمِ ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٢٦>",
"۞ وَX<ٱ>تۡلُ عَلَیۡهِمۡ نَبَأَ ٱQ<بۡ>نَیۡ ءَادَمَ بِX<ٱ>لۡحَقِّ إِذۡ قَR<رَّ>بَا قُR<رۡ>بَانN<ࣰ>ا فَتُقُبِّلَ مِنۡ أَحَدِهِمَا وَلَمۡ یُتَقَبَّلۡ مِنَ ٱلۡءَاخَرِ قَالَ لَأَQ<قۡ>تُلَN<نَّ>كَۖ قَالَ إِN<نَّ>مَا یَتَقَبَّلُ ٱللَّهُ مِنَ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٢٧>",
"لَئِX<ن>N<ۢ> بَسَطتَ إِلَیَّ یَدَكَ لِتَQ<قۡ>تُلَنِی مَW<اۤ> أَنَا۠ بِبَاسِطX<ࣲ> N<یَ>دِیَ إِلَیۡكَ لِأَQ<قۡ>تُلَكَۖ إِN<نِّ>W<یۤ> أَخَافُ ٱللَّهَ R<رَ>بَّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٢٨>",
"إِN<نِّ>W<یۤ> أُرِیدُ أَN<ن> تَبُW<وۤ>أَ بِإِثۡمِی وَإِثۡمِكَ فَتَكُونَ مِنۡ أَصۡحَT<ـٰ>بِ ٱX<ل>N<نَّ>ارِۚ وَذَT< ٰ>لِكَ جَزَW< ٰۤ>ؤُX<ا۟> ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢٩>",
"فَطَوَّعَتۡ لَهُT<ۥ> نَفۡسُهُT<ۥ> قَتۡلَ أَخِیهِ فَقَتَلَهُT<ۥ> فَأَصۡبَحَ مِنَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٣٠>",
"فَبَعَثَ ٱللَّهُ غُR<رَ>ابX<ࣰ>ا N<یَ>Q<بۡ>حَثُ فِی ٱلۡأَR<رۡ>ضِ لِیُرِیَهُT<ۥ> كَیۡفَ یُوَT< ٰ>رِی سَوۡءَةَ أَخِیهِۚ قَالَ یَT<ـٰ>وَیۡلَتَىW<ٰۤ> أَعَجَزۡتُ أَنۡ أَكُونَ مِثۡلَ هَT<ـٰ>ذَا ٱلۡغُR<رَ>ابِ فَأُوَT< ٰ>رِیَ سَوۡءَةَ أَخِیۖ فَأَصۡبَحَ مِنَ ٱX<ل>N<نَّ>T<ـٰ>دِمِJ<ی>نَ A<۝>D<٣١>",
"مِنۡ أَQ<جۡ>لِ ذَT< ٰ>لِكَ كَتَQ<بۡ>نَا عَلَىٰ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ أَN<نَّ>هُT<ۥ> مَN<ن> قَتَلَ نَفۡسَN<ۢ>ا بِغَیۡرِ نَفۡسٍ أَوۡ فَسَادN<ࣲ> فِی ٱلۡأَR<رۡ>ضِ فَكَأَN<نَّ>مَا قَتَلَ ٱX<ل>N<نَّ>اسَ جَمِیعX<ࣰ>ا N<وَ>مَنۡ أَحۡیَاهَا فَكَأَN<نَّ>مَW<اۤ> أَحۡیَا ٱX<ل>N<نَّ>اسَ جَمِیعX<ࣰ>اۚ N<وَ>لَقَQ<دۡ> جَW<اۤ>ءَتۡهُمۡ R<رُ>سُلُنَا بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ ثُN<مَّ> إِN<نَّ> كَثِیR<ر>X<ࣰ>ا N<مِّ>نۡهُN<م> بَعۡدَ ذَT< ٰ>لِكَ فِی ٱلۡأَR<رۡ>ضِ لَمُسۡرِفُJ<و>نَ A<۝>D<٣٢>",
"إِN<نَّ>مَا جَزَW< ٰۤ>ؤُX<ا۟> ٱلَّذِینَ یُحَارِبُونَ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> وَیَسۡعَوۡنَ فِی ٱلۡأَR<رۡ>ضِ فَسَادًا أَX<ن> N<یُ>قَتَّلُW<وۤ>X<ا۟> أَوۡ یُصَلَّبُW<وۤ>X<ا۟> أَوۡ تُقَطَّعَ أَیۡدِیهِمۡ وَأَR<رۡ>جُلُهُم N<مِّ>نۡ خِلَT<ـٰ>فٍ أَوۡ یُN<ن>فَوۡX<ا۟> مِنَ ٱلۡأَR<رۡ>ضِۚ ذَT< ٰ>لِكَ لَهُمۡ خِزۡیN<ࣱ> فِی ٱX<ل>دُّنۡیَاۖ وَلَهُمۡ فِی ٱلۡءَاخِR<رَ>ةِ عَذَابٌ عَظِJ<ی>مٌ A<۝>D<٣٣>",
"إِلَّا ٱلَّذِینَ تَابُوX<ا۟> مِN<ن> قَQ<بۡ>لِ أَN<ن> تَQ<قۡ>دِR<رُ>وX<ا۟> عَلَیۡهِمۡۖ فَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٣٤>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱتَّقُوX<ا۟> ٱللَّهَ وَX<ٱ>Q<بۡ>تَغُW<وۤ>X<ا۟> إِلَیۡهِ ٱلۡوَسِیلَةَ وَجَT<ـٰ>هِدُوX<ا۟> فِی سَبِیلِهِT<ۦ> لَعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<٣٥>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَوۡ أَN<نَّ> لَهُم N<مَّ>ا فِی ٱلۡأَR<رۡ>ضِ جَمِیعX<ࣰ>ا N<وَ>مِثۡلَهُT<ۥ> مَعَهُT<ۥ> لِیَفۡتَدُوX<ا۟> بِهِT<ۦ> مِنۡ عَذَابِ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ مَا تُقُبِّلَ مِنۡهُمۡۖ وَلَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٣٦>",
"یُرِیدُونَ أَX<ن> N<یَ>خۡR<رُ>جُوX<ا۟> مِنَ ٱX<ل>N<نَّ>ارِ وَمَا هُN<م> بِخَT<ـٰ>رِجِینَ مِنۡهَاۖ وَلَهُمۡ عَذَابX<ࣱ> N<مُّ>قِJ<ی>مࣱ A<۝>D<٣٧>",
"وَX<ٱ>X<ل>سَّارِقُ وَX<ٱ>X<ل>سَّارِقَةُ فَX<ٱ>Q<قۡ>طَعُW<وۤ>X<ا۟> أَیۡدِیَهُمَا جَزَW<اۤ>ءَN<ۢ> بِمَا كَسَبَا نَكَT<ـٰ>لX<ࣰ>ا N<مِّ>نَ ٱللَّهِۗ وَٱللَّهُ عَزِیزٌ حَكِJ<ی>مࣱ A<۝>D<٣٨>",
"فَمَN<ن> تَابَ مِX<ن>N<ۢ> بَعۡدِ ظُلۡمِهِT<ۦ> وَأَصۡلَحَ فَإِN<نَّ> ٱللَّهَ یَتُوبُ عَلَیۡهِۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<٣٩>",
"أَلَمۡ تَعۡلَمۡ أَN<نَّ> ٱللَّهَ لَهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ یُعَذِّبُ مَX<ن> N<یَ>شَW<اۤ>ءُ وَیَغۡفِR<رُ> لِمَX<ن> N<یَ>شَW<اۤ>ءُۗ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٤٠>",
"۞ یَW<ـٰۤ>أَیُّهَا ٱX<ل>R<رَّ>سُولُ لَا یَحۡزُN<ن>كَ ٱلَّذِینَ یُسَT<ـٰ>رِعُونَ فِی ٱلۡكُفۡرِ مِنَ ٱلَّذِینَ قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا بِأَفۡوَT< ٰ>هِهِمۡ وَلَمۡ تُؤۡمِN<ن> قُلُوبُهُمۡۛ وَمِنَ ٱلَّذِینَ هَادُوX<ا۟>ۛ سَN<مَّ>T<ـٰ>عُونَ لِلۡكَذِبِ سَN<مَّ>T<ـٰ>عُونَ لِقَوۡمٍ ءَاخَرِینَ لَمۡ یَأۡتُوكَۖ یُحَرِّفُونَ ٱلۡكَلِمَ مِX<ن>N<ۢ> بَعۡدِ مَوَاضِعِهِT<ۦ>ۖ یَقُولُونَ إِنۡ أُوتِیتُمۡ هَT<ـٰ>ذَا فَخُذُوهُ وَإِX<ن> لَّمۡ تُؤۡتَوۡهُ فَX<ٱ>حۡذَR<رُ>وX<ا۟>ۚ وَمَX<ن> N<یُ>رِدِ ٱللَّهُ فِتۡنَتَهُT<ۥ> فَلَN<ن> تَمۡلِكَ لَهُT<ۥ> مِنَ ٱللَّهِ شَیۡءًاۚ أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ لَمۡ یُرِدِ ٱللَّهُ أَX<ن> N<یُ>طَهِّR<رَ> قُلُوبَهُمۡۚ لَهُمۡ فِی ٱX<ل>دُّنۡیَا خِزۡیX<ࣱ>ۖ N<وَ>لَهُمۡ فِی ٱلۡءَاخِR<رَ>ةِ عَذَابٌ عَظِJ<ی>مࣱ A<۝>D<٤١>",
"سَN<مَّ>T<ـٰ>عُونَ لِلۡكَذِبِ أَكَّT<ـٰ>لُونَ لِX<ل>سُّحۡتِۚ فَإِN<ن> جَW<اۤ>ءُوكَ فَX<ٱ>حۡكُN<م> بَیۡنَهُمۡ أَوۡ أَعۡرِضۡ عَنۡهُمۡۖ وَإِN<ن> تُعۡرِضۡ عَنۡهُمۡ فَلَX<ن> N<یَ>ضُR<رُّ>وكَ شَیۡءX<ࣰ>اۖ N<وَ>إِنۡ حَكَمۡتَ فَX<ٱ>حۡكُN<م> بَیۡنَهُN<م> بِX<ٱ>لۡقِسۡطِۚ إِN<نَّ> ٱللَّهَ یُحِبُّ ٱلۡمُQ<قۡ>سِطِJ<ی>نَ A<۝>D<٤٢>",
"وَكَیۡفَ یُحَكِّمُونَكَ وَعِN<ن>دَهُمُ ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةُ فِیهَا حُكۡمُ ٱللَّهِ ثُN<مَّ> یَتَوَلَّوۡنَ مِX<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَۚ وَمَW<اۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ بِX<ٱ>لۡمُؤۡمِنِJ<ی>نَ A<۝>D<٤٣>",
"إِN<نَّ>W<اۤ> أَN<ن>زَلۡنَا ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةَ فِیهَا هُدX<ࣰ>ى N<وَ>نُوR<ر>X<ࣱ>ۚ N<یَ>حۡكُمُ بِهَا ٱX<ل>N<نَّ>بِیُّونَ ٱلَّذِینَ أَسۡلَمُوX<ا۟> لِلَّذِینَ هَادُوX<ا۟> وَX<ٱ>X<ل>R<رَّ>بَّT<ـٰ>نِیُّونَ وَX<ٱ>لۡأَحۡبَاR<رُ> بِمَا ٱسۡتُحۡفِظُوX<ا۟> مِN<ن> كِتَT<ـٰ>بِ ٱللَّهِ وَكَانُوX<ا۟> عَلَیۡهِ شُهَدَW<اۤ>ءَۚ فَلَا تَخۡشَوُX<ا۟> ٱX<ل>N<نَّ>اسَ وَX<ٱ>خۡشَوۡنِ وَلَا تَشۡتَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِی ثَمَنN<ࣰ>ا قَلِیلX<ࣰ>اۚ N<وَ>مَX<ن> لَّمۡ یَحۡكُN<م> بِمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٤٤>",
"وَكَتَQ<بۡ>نَا عَلَیۡهِمۡ فِیهَW<اۤ> أَN<نَّ> ٱX<ل>N<نَّ>فۡسَ بِX<ٱ>X<ل>N<نَّ>فۡسِ وَX<ٱ>لۡعَیۡنَ بِX<ٱ>لۡعَیۡنِ وَX<ٱ>لۡأَN<ن>فَ بِX<ٱ>لۡأَN<ن>فِ وَX<ٱ>لۡأُذُنَ بِX<ٱ>لۡأُذُنِ وَX<ٱ>X<ل>سِّN<نَّ> بِX<ٱ>X<ل>سِّN<نِّ> وَX<ٱ>لۡجُR<رُ>وحَ قِصَاصN<ࣱ>ۚ فَمَN<ن> تَصَدَّقَ بِهِT<ۦ> فَهُوَ كَفَّاR<رَ>ةX<ࣱ> لَّهُT<ۥ>ۚ وَمَX<ن> لَّمۡ یَحۡكُN<م> بِمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٤٥>",
"وَقَفَّیۡنَا عَلَىW<ٰۤ> ءَاثَT<ـٰ>رِهِN<م> بِعِیسَى ٱQ<بۡ>نِ مَR<رۡ>یَمَ مُصَدِّقX<ࣰ>ا لِّمَا بَیۡنَ یَدَیۡهِ مِنَ ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةِۖ وَءَاتَیۡنَT<ـٰ>هُ ٱلۡإِN<ن>جِیلَ فِیهِ هُدX<ࣰ>ى N<وَ>نُوR<ر>X<ࣱ> N<وَ>مُصَدِّقX<ࣰ>ا لِّمَا بَیۡنَ یَدَیۡهِ مِنَ ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةِ وَهُدX<ࣰ>ى N<وَ>مَوۡعِظَةX<ࣰ> لِّلۡمُتَّقِJ<ی>نَ A<۝>D<٤٦>",
"وَلۡیَحۡكُمۡ أَهۡلُ ٱلۡإِN<ن>جِیلِ بِمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ فِیهِۚ وَمَX<ن> لَّمۡ یَحۡكُN<م> بِمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡفَT<ـٰ>سِقُJ<و>نَ A<۝>D<٤٧>",
"وَأَN<ن>زَلۡنَW<اۤ> إِلَیۡكَ ٱلۡكِتَT<ـٰ>بَ بِX<ٱ>لۡحَقِّ مُصَدِّقX<ࣰ>ا لِّمَا بَیۡنَ یَدَیۡهِ مِنَ ٱلۡكِتَT<ـٰ>بِ وَمُهَیۡمِنًا عَلَیۡهِۖ فَX<ٱ>حۡكُN<م> بَیۡنَهُN<م> بِمَW<اۤ> أَN<ن>زَلَ ٱللَّهُۖ وَلَا تَتَّبِعۡ أَهۡوَW<اۤ>ءَهُمۡ عَN<مَّ>ا جَW<اۤ>ءَكَ مِنَ ٱلۡحَقِّۚ لِكُلّN<ࣲ> جَعَلۡنَا مِN<ن>كُمۡ شِرۡعَةX<ࣰ> N<وَ>مِنۡهَاجX<ࣰ>اۚ N<وَ>لَوۡ شَW<اۤ>ءَ ٱللَّهُ لَجَعَلَكُمۡ أُN<مَّ>ةX<ࣰ> N<وَ>T< ٰ>حِدَةX<ࣰ> N<وَ>لَT<ـٰ>كِX<ن> لِّیَQ<بۡ>لُوَكُمۡ فِی مَW<اۤ> ءَاتَX<ى>T<ٰ>كُمۡۖ فَX<ٱ>سۡتَبِقُوX<ا۟> ٱلۡخَیۡR<رَ>T< ٰ>تِۚ إِلَى ٱللَّهِ مَR<رۡ>جِعُكُمۡ جَمِیعN<ࣰ>ا فَیُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ فِیهِ تَخۡتَلِفُJ<و>نَ A<۝>D<٤٨>",
"وَأَنِ ٱحۡكُN<م> بَیۡنَهُN<م> بِمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ وَلَا تَتَّبِعۡ أَهۡوَW<اۤ>ءَهُمۡ وَX<ٱ>حۡذَR<رۡ>هُمۡ أَX<ن> N<یَ>فۡتِنُوكَ عَX<ن>N<ۢ> بَعۡضِ مَW<اۤ> أَN<ن>زَلَ ٱللَّهُ إِلَیۡكَۖ فَإِN<ن> تَوَلَّوۡX<ا۟> فَX<ٱ>عۡلَمۡ أَN<نَّ>مَا یُرِیدُ ٱللَّهُ أَX<ن> N<یُ>صِیبَهُN<م> بِبَعۡضِ ذُنُوبِهِمۡۗ وَإِN<نَّ> كَثِیR<ر>X<ࣰ>ا N<مِّ>نَ ٱX<ل>N<نَّ>اسِ لَفَT<ـٰ>سِقُJ<و>نَ A<۝>D<٤٩>",
"أَفَحُكۡمَ ٱلۡجَT<ـٰ>هِلِیَّةِ یَQ<بۡ>غُونَۚ وَمَنۡ أَحۡسَنُ مِنَ ٱللَّهِ حُكۡمX<ࣰ>ا لِّقَوۡمX<ࣲ> N<یُ>وقِنُJ<و>نَ A<۝>D<٥٠>",
"۞ یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَتَّخِذُوX<ا۟> ٱلۡیَهُودَ وَX<ٱ>X<ل>N<نَّ>صَT<ـٰ>R<رَ>ىW<ٰۤ> أَوۡلِیَW<اۤ>ءَۘ بَعۡضُهُمۡ أَوۡلِیَW<اۤ>ءُ بَعۡضX<ࣲ>ۚ N<وَ>مَX<ن> N<یَ>تَوَلَّهُم N<مِّ>N<ن>كُمۡ فَإِN<نَّ>هُT<ۥ> مِنۡهُمۡۗ إِN<نَّ> ٱللَّهَ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥١>",
"فَتَR<رَ>ى ٱلَّذِینَ فِی قُلُوبِهِم N<مَّ>R<رَ>ضX<ࣱ> N<یُ>سَT<ـٰ>رِعُونَ فِیهِمۡ یَقُولُونَ نَخۡشَىW<ٰۤ> أَN<ن> تُصِیبَنَا دَW<اۤ>ئِR<رَ>ةN<ࣱ>ۚ فَعَسَى ٱللَّهُ أَX<ن> N<یَ>أۡتِیَ بِX<ٱ>لۡفَتۡحِ أَوۡ أَمۡرX<ࣲ> N<مِّ>نۡ عِN<ن>دِهِT<ۦ> فَیُصۡبِحُوX<ا۟> عَلَىٰ مَW<اۤ> أَسَR<رُّ>وX<ا۟> فِW<یۤ> أَN<ن>فُسِهِمۡ نَT<ـٰ>دِمِJ<ی>نَ A<۝>D<٥٢>",
"وَیَقُولُ ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَهَW<ـٰۤ>ؤُلَW<اۤ>ءِ ٱلَّذِینَ أَQ<قۡ>سَمُوX<ا۟> بِٱللَّهِ جَهۡدَ أَیۡمَT<ـٰ>نِهِمۡ إِN<نَّ>هُمۡ لَمَعَكُمۡۚ حَبِطَتۡ أَعۡمَT<ـٰ>لُهُمۡ فَأَصۡبَحُوX<ا۟> خَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٥٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> مَX<ن> N<یَ>R<رۡ>تَدَّ مِN<ن>كُمۡ عَN<ن> دِینِهِT<ۦ> فَسَوۡفَ یَأۡتِی ٱللَّهُ بِقَوۡمX<ࣲ> N<یُ>حِبُّهُمۡ وَیُحِبُّونَهُW<ۥۤ> أَذِلَّةٍ عَلَى ٱلۡمُؤۡمِنِینَ أَعِزَّةٍ عَلَى ٱلۡكَT<ـٰ>فِرِینَ یُجَT<ـٰ>هِدُونَ فِی سَبِیلِ ٱللَّهِ وَلَا یَخَافُونَ لَوۡمَةَ لَW<اۤ>ئِمN<ࣲ>ۚ ذَT< ٰ>لِكَ فَضۡلُ ٱللَّهِ یُؤۡتِیهِ مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَٱللَّهُ وَT< ٰ>سِعٌ عَلِJ<ی>مٌ A<۝>D<٥٤>",
"إِN<نَّ>مَا وَلِیُّكُمُ ٱللَّهُ وَR<رَ>سُولُهُT<ۥ> وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> ٱلَّذِینَ یُقِیمُونَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَیُؤۡتُونَ ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَهُمۡ R<رَ>T< ٰ>كِعُJ<و>نَ A<۝>D<٥٥>",
"وَمَX<ن> N<یَ>تَوَلَّ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> فَإِN<نَّ> حِزۡبَ ٱللَّهِ هُمُ ٱلۡغَT<ـٰ>لِبُJ<و>نَ A<۝>D<٥٦>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَتَّخِذُوX<ا۟> ٱلَّذِینَ ٱتَّخَذُوX<ا۟> دِینَكُمۡ هُزُوX<ࣰ>ا N<وَ>لَعِبX<ࣰ>ا N<مِّ>نَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ مِN<ن> قَQ<بۡ>لِكُمۡ وَX<ٱ>لۡكُفَّاR<رَ> أَوۡلِیَW<اۤ>ءَۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٥٧>",
"وَإِذَا نَادَیۡتُمۡ إِلَى ٱX<ل>صَّلَX<و>T<ٰ>ةِ ٱتَّخَذُوهَا هُزُوX<ࣰ>ا N<وَ>لَعِبN<ࣰ>اۚ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ قَوۡمX<ࣱ> لَّا یَعۡقِلُJ<و>نَ A<۝>D<٥٨>",
"قُلۡ یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ هَلۡ تَN<ن>قِمُونَ مِN<نَّ>W<اۤ> إِلَّW<اۤ> أَنۡ ءَامَN<نَّ>ا بِٱللَّهِ وَمَW<اۤ> أُN<ن>زِلَ إِلَیۡنَا وَمَW<اۤ> أُN<ن>زِلَ مِN<ن> قَQ<بۡ>لُ وَأَN<نَّ> أَكۡثَR<رَ>كُمۡ فَT<ـٰ>سِقُJ<و>نَ A<۝>D<٥٩>",
"قُلۡ هَلۡ أُنَبِّئُكُN<م> بِشَرّX<ࣲ> N<مِّ>N<ن> ذَT< ٰ>لِكَ مَثُوبَةً عِN<ن>دَ ٱللَّهِۚ مَX<ن> لَّعَنَهُ ٱللَّهُ وَغَضِبَ عَلَیۡهِ وَجَعَلَ مِنۡهُمُ ٱلۡقِR<رَ>دَةَ وَX<ٱ>لۡخَنَازِیR<رَ> وَعَبَدَ ٱX<ل>طَّT<ـٰ>غُوتَۚ أُX<و۟>لَW<ـٰۤ>ئِكَ شَR<رّ>X<ࣱ> N<مَّ>كَانX<ࣰ>ا N<وَ>أَضَلُّ عَN<ن> سَوَW<اۤ>ءِ ٱX<ل>سَّبِJ<ی>لِ A<۝>D<٦٠>",
"وَإِذَا جَW<اۤ>ءُوكُمۡ قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا وَقَد دَّخَلُوX<ا۟> بِX<ٱ>لۡكُفۡرِ وَهُمۡ قَQ<دۡ> خَR<رَ>جُوX<ا۟> بِهِT<ۦ>ۚ وَٱللَّهُ أَعۡلَمُ بِمَا كَانُوX<ا۟> یَكۡتُمُJ<و>نَ A<۝>D<٦١>",
"وَتَR<رَ>ىٰ كَثِیR<ر>X<ࣰ>ا N<مِّ>نۡهُمۡ یُسَT<ـٰ>رِعُونَ فِی ٱلۡإِثۡمِ وَX<ٱ>لۡعُQ<دۡ>وَT< ٰ>نِ وَأَكۡلِهِمُ ٱX<ل>سُّحۡتَۚ لَبِئۡسَ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٦٢>",
"لَوۡلَا یَنۡهَX<ى>T<ٰ>هُمُ ٱX<ل>R<رَّ>بَّT<ـٰ>نِیُّونَ وَX<ٱ>لۡأَحۡبَاR<رُ> عَN<ن> قَوۡلِهِمُ ٱلۡإِثۡمَ وَأَكۡلِهِمُ ٱX<ل>سُّحۡتَۚ لَبِئۡسَ مَا كَانُوX<ا۟> یَصۡنَعُJ<و>نَ A<۝>D<٦٣>",
"وَقَالَتِ ٱلۡیَهُودُ یَدُ ٱللَّهِ مَغۡلُولَةٌۚ غُلَّتۡ أَیۡدِیهِمۡ وَلُعِنُوX<ا۟> بِمَا قَالُوX<ا۟>ۘ بَلۡ یَدَاهُ مَQ<بۡ>سُوطَتَانِ یُN<ن>فِقُ كَیۡفَ یَشَW<اۤ>ءُۚ وَلَیَزِیدَN<نَّ> كَثِیR<ر>X<ࣰ>ا N<مِّ>نۡهُم N<مَّ>W<اۤ> أُN<ن>زِلَ إِلَیۡكَ مِX<ن> R<رَّ>بِّكَ طُغۡیَT<ـٰ>نX<ࣰ>ا N<وَ>كُفۡR<ر>X<ࣰ>اۚ N<وَ>أَلۡقَیۡنَا بَیۡنَهُمُ ٱلۡعَدَT< ٰ>وَةَ وَX<ٱ>لۡبَغۡضَW<اۤ>ءَ إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِۚ كُلَّمَW<اۤ> أَوۡقَدُوX<ا۟> نَاR<ر>X<ࣰ>ا لِّلۡحَR<رۡ>بِ أَQ<طۡ>فَأَهَا ٱللَّهُۚ وَیَسۡعَوۡنَ فِی ٱلۡأَR<رۡ>ضِ فَسَادX<ࣰ>اۚ N<وَ>ٱللَّهُ لَا یُحِبُّ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<٦٤>",
"وَلَوۡ أَN<نَّ> أَهۡلَ ٱلۡكِتَT<ـٰ>بِ ءَامَنُوX<ا۟> وَX<ٱ>تَّقَوۡX<ا۟> لَكَفَّR<رۡ>نَا عَنۡهُمۡ سَیِّءَاتِهِمۡ وَلَأَQ<دۡ>خَلۡنَT<ـٰ>هُمۡ جَN<نَّ>T<ـٰ>تِ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<٦٥>",
"وَلَوۡ أَN<نَّ>هُمۡ أَقَامُوX<ا۟> ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةَ وَX<ٱ>لۡإِN<ن>جِیلَ وَمَW<اۤ> أُN<ن>زِلَ إِلَیۡهِم N<مِّ>X<ن> R<رَّ>بِّهِمۡ لَأَكَلُوX<ا۟> مِN<ن> فَوۡقِهِمۡ وَمِN<ن> تَحۡتِ أَR<رۡ>جُلِهِمۚ N<مِّ>نۡهُمۡ أُN<مَّ>ةX<ࣱ> N<مُّ>Q<قۡ>تَصِدَةX<ࣱ>ۖ N<وَ>كَثِیR<ر>X<ࣱ> N<مِّ>نۡهُمۡ سَW<اۤ>ءَ مَا یَعۡمَلُJ<و>نَ A<۝>D<٦٦>",
"۞ یَW<ـٰۤ>أَیُّهَا ٱX<ل>R<رَّ>سُولُ بَلِّغۡ مَW<اۤ> أُN<ن>زِلَ إِلَیۡكَ مِX<ن> R<رَّ>بِّكَۖ وَإِX<ن> لَّمۡ تَفۡعَلۡ فَمَا بَلَّغۡتَ رِسَالَتَهُT<ۥ>ۚ وَٱللَّهُ یَعۡصِمُكَ مِنَ ٱX<ل>N<نَّ>اسِۗ إِN<نَّ> ٱللَّهَ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٦٧>",
"قُلۡ یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ لَسۡتُمۡ عَلَىٰ شَیۡءٍ حَتَّىٰ تُقِیمُوX<ا۟> ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةَ وَX<ٱ>لۡإِN<ن>جِیلَ وَمَW<اۤ> أُN<ن>زِلَ إِلَیۡكُم N<مِّ>X<ن> R<رَّ>بِّكُمۡۗ وَلَیَزِیدَN<نَّ> كَثِیR<ر>X<ࣰ>ا N<مِّ>نۡهُم N<مَّ>W<اۤ> أُN<ن>زِلَ إِلَیۡكَ مِX<ن> R<رَّ>بِّكَ طُغۡیَT<ـٰ>نX<ࣰ>ا N<وَ>كُفۡR<ر>N<ࣰ>اۖ فَلَا تَأۡسَ عَلَى ٱلۡقَوۡمِ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٦٨>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَX<ٱ>لَّذِینَ هَادُوX<ا۟> وَX<ٱ>X<ل>صَّT<ـٰ>بِءُونَ وَX<ٱ>X<ل>N<نَّ>صَT<ـٰ>R<رَ>ىٰ مَنۡ ءَامَنَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَعَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٦٩>",
"لَقَQ<دۡ> أَخَذۡنَا مِیثَT<ـٰ>قَ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ وَأَR<رۡ>سَلۡنَW<اۤ> إِلَیۡهِمۡ R<رُ>سُلN<ࣰ>اۖ كُلَّمَا جَW<اۤ>ءَهُمۡ R<رَ>سُولُN<ۢ> بِمَا لَا تَهۡوَىW<ٰۤ> أَN<ن>فُسُهُمۡ فَرِیقN<ࣰ>ا كَذَّبُوX<ا۟> وَفَرِیقX<ࣰ>ا N<یَ>Q<قۡ>تُلُJ<و>نَ A<۝>D<٧٠>",
"وَحَسِبُW<وۤ>X<ا۟> أَلَّا تَكُونَ فِتۡنَةN<ࣱ> فَعَمُوX<ا۟> وَصَN<مُّ>وX<ا۟> ثُN<مَّ> تَابَ ٱللَّهُ عَلَیۡهِمۡ ثُN<مَّ> عَمُوX<ا۟> وَصَN<مُّ>وX<ا۟> كَثِیR<ر>X<ࣱ> N<مِّ>نۡهُمۡۚ وَٱللَّهُ بَصِیR<رُ>N<ۢ> بِمَا یَعۡمَلُJ<و>نَ A<۝>D<٧١>",
"لَقَQ<دۡ> كَفَR<رَ> ٱلَّذِینَ قَالُW<وۤ>X<ا۟> إِN<نَّ> ٱللَّهَ هُوَ ٱلۡمَسِیحُ ٱQ<بۡ>نُ مَR<رۡ>یَمَۖ وَقَالَ ٱلۡمَسِیحُ یَT<ـٰ>بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ ٱعۡبُدُوX<ا۟> ٱللَّهَ R<رَ>بِّی وَR<رَ>بَّكُمۡۖ إِN<نَّ>هُT<ۥ> مَX<ن> N<یُ>شۡرِكۡ بِٱللَّهِ فَقَQ<دۡ> حَR<رَّ>مَ ٱللَّهُ عَلَیۡهِ ٱلۡجَN<نَّ>ةَ وَمَأۡوَX<ى>T<ٰ>هُ ٱX<ل>N<نَّ>اR<رُ>ۖ وَمَا لِX<ل>ظَّT<ـٰ>لِمِینَ مِنۡ أَN<ن>صَJ<ا>R<ر>ࣲ A<۝>D<٧٢>",
"لَّقَQ<دۡ> كَفَR<رَ> ٱلَّذِینَ قَالُW<وۤ>X<ا۟> إِN<نَّ> ٱللَّهَ ثَالِثُ ثَلَT<ـٰ>ثَةX<ࣲ>ۘ N<وَ>مَا مِنۡ إِلَT<ـٰ>هٍ إِلَّW<اۤ> إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدX<ࣱ>ۚ N<وَ>إِX<ن> لَّمۡ یَN<ن>تَهُوX<ا۟> عَN<مَّ>ا یَقُولُونَ لَیَمَسَّN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِنۡهُمۡ عَذَابٌ أَلِJ<ی>مٌ A<۝>D<٧٣>",
"أَفَلَا یَتُوبُونَ إِلَى ٱللَّهِ وَیَسۡتَغۡفِR<رُ>ونَهُT<ۥ>ۚ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٧٤>",
"مَّا ٱلۡمَسِیحُ ٱQ<بۡ>نُ مَR<رۡ>یَمَ إِلَّا R<رَ>سُولN<ࣱ> قَQ<دۡ> خَلَتۡ مِN<ن> قَQ<بۡ>لِهِ ٱX<ل>R<رُّ>سُلُ وَأُN<مُّ>هُT<ۥ> صِدِّیقَةN<ࣱ>ۖ كَانَا یَأۡكُلَانِ ٱX<ل>طَّعَامَۗ ٱN<ن>ظُR<رۡ> كَیۡفَ نُبَیِّنُ لَهُمُ ٱلۡءَایَT<ـٰ>تِ ثُN<مَّ> ٱN<ن>ظُR<رۡ> أَN<نَّ>ىٰ یُؤۡفَكُJ<و>نَ A<۝>D<٧٥>",
"قُلۡ أَتَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ مَا لَا یَمۡلِكُ لَكُمۡ ضَR<رّ>X<ࣰ>ا N<وَ>لَا نَفۡعX<ࣰ>اۚ N<وَ>ٱللَّهُ هُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٧٦>",
"قُلۡ یَW<ـٰۤ>أَهۡلَ ٱلۡكِتَT<ـٰ>بِ لَا تَغۡلُوX<ا۟> فِی دِینِكُمۡ غَیۡR<رَ> ٱلۡحَقِّ وَلَا تَتَّبِعُW<وۤ>X<ا۟> أَهۡوَW<اۤ>ءَ قَوۡمN<ࣲ> قَQ<دۡ> ضَلُّوX<ا۟> مِN<ن> قَQ<بۡ>لُ وَأَضَلُّوX<ا۟> كَثِیR<ر>X<ࣰ>ا N<وَ>ضَلُّوX<ا۟> عَN<ن> سَوَW<اۤ>ءِ ٱX<ل>سَّبِJ<ی>لِ A<۝>D<٧٧>",
"لُعِنَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِX<ن>N<ۢ> بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ عَلَىٰ لِسَانِ دَاوُT<ۥ>دَ وَعِیسَى ٱQ<بۡ>نِ مَR<رۡ>یَمَۚ ذَT< ٰ>لِكَ بِمَا عَصَوX<ا۟> وَّكَانُوX<ا۟> یَعۡتَدُJ<و>نَ A<۝>D<٧٨>",
"كَانُوX<ا۟> لَا یَتَنَاهَوۡنَ عَX<ن> N<مُّ>N<ن>كَرN<ࣲ> فَعَلُوهُۚ لَبِئۡسَ مَا كَانُوX<ا۟> یَفۡعَلُJ<و>نَ A<۝>D<٧٩>",
"تَR<رَ>ىٰ كَثِیR<ر>X<ࣰ>ا N<مِّ>نۡهُمۡ یَتَوَلَّوۡنَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟>ۚ لَبِئۡسَ مَا قَدَّمَتۡ لَهُمۡ أَN<ن>فُسُهُمۡ أَN<ن> سَخِطَ ٱللَّهُ عَلَیۡهِمۡ وَفِی ٱلۡعَذَابِ هُمۡ خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٨٠>",
"وَلَوۡ كَانُوX<ا۟> یُؤۡمِنُونَ بِٱللَّهِ وَX<ٱ>X<ل>N<نَّ>بِیِّ وَمَW<اۤ> أُN<ن>زِلَ إِلَیۡهِ مَا ٱتَّخَذُوهُمۡ أَوۡلِیَW<اۤ>ءَ وَلَT<ـٰ>كِN<نَّ> كَثِیR<ر>X<ࣰ>ا N<مِّ>نۡهُمۡ فَT<ـٰ>سِقُJ<و>نَ A<۝>D<٨١>",
"۞ لَتَجِدَN<نَّ> أَشَدَّ ٱX<ل>N<نَّ>اسِ عَدَT< ٰ>وَةX<ࣰ> لِّلَّذِینَ ءَامَنُوX<ا۟> ٱلۡیَهُودَ وَX<ٱ>لَّذِینَ أَشۡR<رَ>كُوX<ا۟>ۖ وَلَتَجِدَN<نَّ> أَQ<قۡ>R<رَ>بَهُم N<مَّ>وَدَّةX<ࣰ> لِّلَّذِینَ ءَامَنُوX<ا۟> ٱلَّذِینَ قَالُW<وۤ>X<ا۟> إِN<نَّ>ا نَصَT<ـٰ>R<رَ>ىٰۚ ذَT< ٰ>لِكَ بِأَN<نَّ> مِنۡهُمۡ قِسِّیسِینَ وَR<رُ>هۡبَانX<ࣰ>ا N<وَ>أَN<نَّ>هُمۡ لَا یَسۡتَكۡبِR<رُ>J<و>نَ A<۝>D<٨٢>",
"وَإِذَا سَمِعُوX<ا۟> مَW<اۤ> أُN<ن>زِلَ إِلَى ٱX<ل>R<رَّ>سُولِ تَR<رَ>ىW<ٰۤ> أَعۡیُنَهُمۡ تَفِیضُ مِنَ ٱX<ل>دَّمۡعِ مِN<مَّ>ا عَR<رَ>فُوX<ا۟> مِنَ ٱلۡحَقِّۖ یَقُولُونَ R<رَ>بَّنَW<اۤ> ءَامَN<نَّ>ا فَX<ٱ>كۡتُQ<بۡ>نَا مَعَ ٱX<ل>شَّT<ـٰ>هِدِJ<ی>نَ A<۝>D<٨٣>",
"وَمَا لَنَا لَا نُؤۡمِنُ بِٱللَّهِ وَمَا جَW<اۤ>ءَنَا مِنَ ٱلۡحَقِّ وَنَQ<طۡ>مَعُ أَX<ن> N<یُ>Q<دۡ>خِلَنَا R<رَ>بُّنَا مَعَ ٱلۡقَوۡمِ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٨٤>",
"فَأَثَT<ـٰ>بَهُمُ ٱللَّهُ بِمَا قَالُوX<ا۟> جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَاۚ وَذَT< ٰ>لِكَ جَزَW<اۤ>ءُ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٨٥>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> وَكَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَW<اۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡجَحِJ<ی>مِ A<۝>D<٨٦>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تُحَرِّمُوX<ا۟> طَیِّبَT<ـٰ>تِ مَW<اۤ> أَحَلَّ ٱللَّهُ لَكُمۡ وَلَا تَعۡتَدُW<وۤ>X<ا۟>ۚ إِN<نَّ> ٱللَّهَ لَا یُحِبُّ ٱلۡمُعۡتَدِJ<ی>نَ A<۝>D<٨٧>",
"وَكُلُوX<ا۟> مِN<مَّ>ا R<رَ>زَقَكُمُ ٱللَّهُ حَلَT<ـٰ>لN<ࣰ>ا طَیِّبX<ࣰ>اۚ N<وَ>X<ٱ>تَّقُوX<ا۟> ٱللَّهَ ٱلَّذِW<یۤ> أَN<ن>تُN<م> بِهِT<ۦ> مُؤۡمِنُJ<و>نَ A<۝>D<٨٨>",
"لَا یُؤَاخِذُكُمُ ٱللَّهُ بِX<ٱ>X<ل>لَّغۡوِ فِW<یۤ> أَیۡمَT<ـٰ>نِكُمۡ وَلَT<ـٰ>كِX<ن> N<یُ>ؤَاخِذُكُN<م> بِمَا عَقَّX<د>تُّمُ ٱلۡأَیۡمَT<ـٰ>نَۖ فَكَفَّT<ـٰ>R<رَ>تُهُW<ۥۤ> إِQ<طۡ>عَامُ عَشَR<رَ>ةِ مَسَT<ـٰ>كِینَ مِنۡ أَوۡسَطِ مَا تُQ<طۡ>عِمُونَ أَهۡلِیكُمۡ أَوۡ كِسۡوَتُهُمۡ أَوۡ تَحۡرِیR<رُ> R<رَ>قَبَةN<ࣲ>ۖ فَمَX<ن> لَّمۡ یَجِQ<دۡ> فَصِیَامُ ثَلَT<ـٰ>ثَةِ أَیَّامN<ࣲ>ۚ ذَT< ٰ>لِكَ كَفَّT<ـٰ>R<رَ>ةُ أَیۡمَT<ـٰ>نِكُمۡ إِذَا حَلَفۡتُمۡۚ وَX<ٱ>حۡفَظُW<وۤ>X<ا۟> أَیۡمَT<ـٰ>نَكُمۡۚ كَذَT< ٰ>لِكَ یُبَیِّنُ ٱللَّهُ لَكُمۡ ءَایَT<ـٰ>تِهِT<ۦ> لَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<٨٩>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<نَّ>مَا ٱلۡخَمۡR<رُ> وَX<ٱ>لۡمَیۡسِR<رُ> وَX<ٱ>لۡأَN<ن>صَابُ وَX<ٱ>لۡأَزۡلَT<ـٰ>مُ رِQ<جۡ>سX<ࣱ> N<مِّ>نۡ عَمَلِ ٱX<ل>شَّیۡطَT<ـٰ>نِ فَX<ٱ>Q<جۡ>تَنِبُوهُ لَعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<٩٠>",
"إِN<نَّ>مَا یُرِیدُ ٱX<ل>شَّیۡطَT<ـٰ>نُ أَX<ن> N<یُ>وقِعَ بَیۡنَكُمُ ٱلۡعَدَT< ٰ>وَةَ وَX<ٱ>لۡبَغۡضَW<اۤ>ءَ فِی ٱلۡخَمۡرِ وَX<ٱ>لۡمَیۡسِرِ وَیَصُدَّكُمۡ عَN<ن> ذِكۡرِ ٱللَّهِ وَعَنِ ٱX<ل>صَّلَX<و>T<ٰ>ةِۖ فَهَلۡ أَN<ن>تُم N<مُّ>N<ن>تَهُJ<و>نَ A<۝>D<٩١>",
"وَأَطِیعُوX<ا۟> ٱللَّهَ وَأَطِیعُوX<ا۟> ٱX<ل>R<رَّ>سُولَ وَX<ٱ>حۡذَR<رُ>وX<ا۟>ۚ فَإِN<ن> تَوَلَّیۡتُمۡ فَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ>مَا عَلَىٰ R<رَ>سُولِنَا ٱلۡبَلَT<ـٰ>غُ ٱلۡمُبِJ<ی>نُ A<۝>D<٩٢>",
"لَیۡسَ عَلَى ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ جُنَاحN<ࣱ> فِیمَا طَعِمُW<وۤ>X<ا۟> إِذَا مَا ٱتَّقَوX<ا۟> وَّءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ ثُN<مَّ> ٱتَّقَوX<ا۟> وَّءَامَنُوX<ا۟> ثُN<مَّ> ٱتَّقَوX<ا۟> وَّأَحۡسَنُوX<ا۟>ۚ وَٱللَّهُ یُحِبُّ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٩٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَیَQ<بۡ>لُوَN<نَّ>كُمُ ٱللَّهُ بِشَیۡءX<ࣲ> N<مِّ>نَ ٱX<ل>صَّیۡدِ تَنَالُهُW<ۥۤ> أَیۡدِیكُمۡ وَرِمَاحُكُمۡ لِیَعۡلَمَ ٱللَّهُ مَX<ن> N<یَ>خَافُهُT<ۥ> بِX<ٱ>لۡغَیۡبِۚ فَمَنِ ٱعۡتَدَىٰ بَعۡدَ ذَT< ٰ>لِكَ فَلَهُT<ۥ> عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٩٤>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَQ<قۡ>تُلُوX<ا۟> ٱX<ل>صَّیۡدَ وَأَN<ن>تُمۡ حُR<رُ>مX<ࣱ>ۚ N<وَ>مَN<ن> قَتَلَهُT<ۥ> مِN<ن>كُم N<مُّ>تَعَN<مِّ>دN<ࣰ>ا فَجَزَW<اۤ>ءX<ࣱ> N<مِّ>ثۡلُ مَا قَتَلَ مِنَ ٱX<ل>N<نَّ>عَمِ یَحۡكُمُ بِهِT<ۦ> ذَوَا عَQ<دۡ>لX<ࣲ> N<مِّ>N<ن>كُمۡ هَQ<دۡ>یَN<ۢ>ا بَT<ـٰ>لِغَ ٱلۡكَعۡبَةِ أَوۡ كَفَّT<ـٰ>R<رَ>ةN<ࣱ> طَعَامُ مَسَT<ـٰ>كِینَ أَوۡ عَQ<دۡ>لُ ذَT< ٰ>لِكَ صِیَامX<ࣰ>ا لِّیَذُوقَ وَبَالَ أَمۡرِهِT<ۦ>ۗ عَفَا ٱللَّهُ عَN<مَّ>ا سَلَفَۚ وَمَنۡ عَادَ فَیَN<ن>تَقِمُ ٱللَّهُ مِنۡهُۚ وَٱللَّهُ عَزِیزN<ࣱ> ذُو ٱN<ن>تِقَJ<ا>مٍ A<۝>D<٩٥>",
"أُحِلَّ لَكُمۡ صَیۡدُ ٱلۡبَحۡرِ وَطَعَامُهُT<ۥ> مَتَT<ـٰ>عX<ࣰ>ا لَّكُمۡ وَلِX<ل>سَّیَّاR<رَ>ةِۖ وَحُرِّمَ عَلَیۡكُمۡ صَیۡدُ ٱلۡبَرِّ مَا دُمۡتُمۡ حُR<رُ>مX<ࣰ>اۗ N<وَ>X<ٱ>تَّقُوX<ا۟> ٱللَّهَ ٱلَّذِW<یۤ> إِلَیۡهِ تُحۡشَR<رُ>J<و>نَ A<۝>D<٩٦>",
"۞ جَعَلَ ٱللَّهُ ٱلۡكَعۡبَةَ ٱلۡبَیۡتَ ٱلۡحَR<رَ>امَ قِیَT<ـٰ>مX<ࣰ>ا لِّX<ل>N<نَّ>اسِ وَX<ٱ>X<ل>شَّهۡR<رَ> ٱلۡحَR<رَ>امَ وَX<ٱ>لۡهَQ<دۡ>یَ وَX<ٱ>لۡقَلَW<ـٰۤ>ئِدَۚ ذَT< ٰ>لِكَ لِتَعۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ یَعۡلَمُ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِ وَأَN<نَّ> ٱللَّهَ بِكُلِّ شَیۡءٍ عَلِJ<ی>مٌ A<۝>D<٩٧>",
"ٱعۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ شَدِیدُ ٱلۡعِقَابِ وَأَN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٩٨>",
"مَّا عَلَى ٱX<ل>R<رَّ>سُولِ إِلَّا ٱلۡبَلَT<ـٰ>غُۗ وَٱللَّهُ یَعۡلَمُ مَا تُQ<بۡ>دُونَ وَمَا تَكۡتُمُJ<و>نَ A<۝>D<٩٩>",
"قُل لَّا یَسۡتَوِی ٱلۡخَبِیثُ وَX<ٱ>X<ل>طَّیِّبُ وَلَوۡ أَعۡجَبَكَ كَثۡR<رَ>ةُ ٱلۡخَبِیثِۚ فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ یَW<ـٰۤ>أُX<و۟>لِی ٱلۡأَلۡبَT<ـٰ>بِ لَعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<١٠٠>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَسۡءَلُوX<ا۟> عَنۡ أَشۡیَW<اۤ>ءَ إِN<ن> تُQ<بۡ>دَ لَكُمۡ تَسُؤۡكُمۡ وَإِN<ن> تَسۡءَلُوX<ا۟> عَنۡهَا حِینَ یُنَزَّلُ ٱلۡقُR<رۡ>ءَانُ تُQ<بۡ>دَ لَكُمۡ عَفَا ٱللَّهُ عَنۡهَاۗ وَٱللَّهُ غَفُوR<ر>ٌ حَلِJ<ی>مࣱ A<۝>D<١٠١>",
"قَQ<دۡ> سَأَلَهَا قَوۡمX<ࣱ> N<مِّ>N<ن> قَQ<بۡ>لِكُمۡ ثُN<مَّ> أَصۡبَحُوX<ا۟> بِهَا كَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٠٢>",
"مَا جَعَلَ ٱللَّهُ مِX<ن>N<ۢ> بَحِیR<رَ>ةX<ࣲ> N<وَ>لَا سَW<اۤ>ئِبَةX<ࣲ> N<وَ>لَا وَصِیلَةX<ࣲ> N<وَ>لَا حَامX<ࣲ> N<وَ>لَT<ـٰ>كِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> یَفۡتَR<رُ>ونَ عَلَى ٱللَّهِ ٱلۡكَذِبَۖ وَأَكۡثَR<رُ>هُمۡ لَا یَعۡقِلُJ<و>نَ A<۝>D<١٠٣>",
"وَإِذَا قِیلَ لَهُمۡ تَعَالَوۡX<ا۟> إِلَىٰ مَW<اۤ> أَN<ن>زَلَ ٱللَّهُ وَإِلَى ٱX<ل>R<رَّ>سُولِ قَالُوX<ا۟> حَسۡبُنَا مَا وَجَQ<دۡ>نَا عَلَیۡهِ ءَابَW<اۤ>ءَنَW<اۤ>ۚ أَوَ لَوۡ كَانَ ءَابَW<اۤ>ؤُهُمۡ لَا یَعۡلَمُونَ شَیۡءX<ࣰ>ا N<وَ>لَا یَهۡتَدُJ<و>نَ A<۝>D<١٠٤>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> عَلَیۡكُمۡ أَN<ن>فُسَكُمۡۖ لَا یَضُR<رُّ>كُم N<مَّ>N<ن> ضَلَّ إِذَا ٱهۡتَدَیۡتُمۡۚ إِلَى ٱللَّهِ مَR<رۡ>جِعُكُمۡ جَمِیعN<ࣰ>ا فَیُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<١٠٥>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> شَهَT<ـٰ>دَةُ بَیۡنِكُمۡ إِذَا حَضَR<رَ> أَحَدَكُمُ ٱلۡمَوۡتُ حِینَ ٱلۡوَصِیَّةِ ٱثۡنَانِ ذَوَا عَQ<دۡ>لX<ࣲ> N<مِّ>N<ن>كُمۡ أَوۡ ءَاخَR<رَ>انِ مِنۡ غَیۡرِكُمۡ إِنۡ أَN<ن>تُمۡ ضَR<رَ>Q<بۡ>تُمۡ فِی ٱلۡأَR<رۡ>ضِ فَأَصَT<ـٰ>بَتۡكُم N<مُّ>صِیبَةُ ٱلۡمَوۡتِۚ تَحۡبِسُونَهُمَا مِX<ن>N<ۢ> بَعۡدِ ٱX<ل>صَّلَX<و>T<ٰ>ةِ فَیُQ<قۡ>سِمَانِ بِٱللَّهِ إِنِ ٱR<رۡ>تَQ<بۡ>تُمۡ لَا نَشۡتَرِی بِهِT<ۦ> ثَمَنX<ࣰ>ا N<وَ>لَوۡ كَانَ ذَا قُR<رۡ>بَىٰ وَلَا نَكۡتُمُ شَهَT<ـٰ>دَةَ ٱللَّهِ إِN<نَّ>W<اۤ> إِذX<ࣰ>ا لَّمِنَ ٱلۡءَاثِمِJ<ی>نَ A<۝>D<١٠٦>",
"فَإِنۡ عُثِR<رَ> عَلَىW<ٰۤ> أَN<نَّ>هُمَا ٱسۡتَحَقَّW<اۤ> إِثۡمN<ࣰ>ا فَءَاخَR<رَ>انِ یَقُومَانِ مَقَامَهُمَا مِنَ ٱلَّذِینَ ٱسۡتَحَقَّ عَلَیۡهِمُ ٱلۡأَوۡلَیَT<ـٰ>نِ فَیُQ<قۡ>سِمَانِ بِٱللَّهِ لَشَهَT<ـٰ>دَتُنَW<اۤ> أَحَقُّ مِN<ن> شَهَT<ـٰ>دَتِهِمَا وَمَا ٱعۡتَدَیۡنَW<اۤ> إِN<نَّ>W<اۤ> إِذX<ࣰ>ا لَّمِنَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٠٧>",
"ذَT< ٰ>لِكَ أَQ<دۡ>نَىW<ٰۤ> أَX<ن> N<یَ>أۡتُوX<ا۟> بِX<ٱ>X<ل>شَّهَT<ـٰ>دَةِ عَلَىٰ وَQ<جۡ>هِهَW<اۤ> أَوۡ یَخَافُW<وۤ>X<ا۟> أَN<ن> تُR<رَ>دَّ أَیۡمَT<ـٰ>نُN<ۢ> بَعۡدَ أَیۡمَT<ـٰ>نِهِمۡۗ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَX<ٱ>سۡمَعُوX<ا۟>ۗ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<١٠٨>",
"۞ یَوۡمَ یَQ<جۡ>مَعُ ٱللَّهُ ٱX<ل>R<رُّ>سُلَ فَیَقُولُ مَاذَW<اۤ> أُجِQ<بۡ>تُمۡۖ قَالُوX<ا۟> لَا عِلۡمَ لَنَW<اۤ>ۖ إِN<نَّ>كَ أَN<ن>تَ عَلَّT<ـٰ>مُ ٱلۡغُیُJ<و>Q<ب>ِ A<۝>D<١٠٩>",
"إِذۡ قَالَ ٱللَّهُ یَT<ـٰ>عِیسَى ٱQ<بۡ>نَ مَR<رۡ>یَمَ ٱذۡكُR<رۡ> نِعۡمَتِی عَلَیۡكَ وَعَلَىٰ وَT< ٰ>لِدَتِكَ إِذۡ أَیَّX<د>تُّكَ بِR<رُ>وحِ ٱلۡقُدُسِ تُكَلِّمُ ٱX<ل>N<نَّ>اسَ فِی ٱلۡمَهۡدِ وَكَهۡلX<ࣰ>اۖ N<وَ>إِذۡ عَلَّمۡتُكَ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحِكۡمَةَ وَX<ٱ>X<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةَ وَX<ٱ>لۡإِN<ن>جِیلَۖ وَإِذۡ تَخۡلُقُ مِنَ ٱX<ل>طِّینِ كَهَیۡءَةِ ٱX<ل>طَّیۡرِ بِإِذۡنِی فَتَN<ن>فُخُ فِیهَا فَتَكُونُ طَیۡR<رَ>N<ۢ>ا بِإِذۡنِیۖ وَتُQ<بۡ>رِئُ ٱلۡأَكۡمَهَ وَX<ٱ>لۡأَQ<بۡ>R<رَ>صَ بِإِذۡنِیۖ وَإِذۡ تُخۡرِجُ ٱلۡمَوۡتَىٰ بِإِذۡنِیۖ وَإِذۡ كَفَفۡتُ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ عَN<ن>كَ إِذۡ جِئۡتَهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِنۡهُمۡ إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا سِحۡR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١١٠>",
"وَإِذۡ أَوۡحَیۡتُ إِلَى ٱلۡحَوَارِیِّT<ـۧ>نَ أَنۡ ءَامِنُوX<ا۟> بِی وَبِR<رَ>سُولِی قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا وَX<ٱ>شۡهَQ<دۡ> بِأَN<نَّ>نَا مُسۡلِمُJ<و>نَ A<۝>D<١١١>",
"إِذۡ قَالَ ٱلۡحَوَارِیُّونَ یَT<ـٰ>عِیسَى ٱQ<بۡ>نَ مَR<رۡ>یَمَ هَلۡ یَسۡتَطِیعُ R<رَ>بُّكَ أَX<ن> N<یُ>نَزِّلَ عَلَیۡنَا مَW<اۤ>ئِدَةX<ࣰ> N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِۖ قَالَ ٱتَّقُوX<ا۟> ٱللَّهَ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١١٢>",
"قَالُوX<ا۟> نُرِیدُ أَن N<نَّ>أۡكُلَ مِنۡهَا وَتَQ<طۡ>مَئِN<نَّ> قُلُوبُنَا وَنَعۡلَمَ أَN<ن> قَQ<دۡ> صَدَQ<قۡ>تَنَا وَنَكُونَ عَلَیۡهَا مِنَ ٱX<ل>شَّT<ـٰ>هِدِJ<ی>نَ A<۝>D<١١٣>",
"قَالَ عِیسَى ٱQ<بۡ>نُ مَR<رۡ>یَمَ ٱللَّهُN<مَّ> R<رَ>بَّنَW<اۤ> أَN<ن>زِلۡ عَلَیۡنَا مَW<اۤ>ئِدَةX<ࣰ> N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ تَكُونُ لَنَا عِیدX<ࣰ>ا لِّأَوَّلِنَا وَءَاخِرِنَا وَءَایَةX<ࣰ> N<مِّ>N<ن>كَۖ وَX<ٱ>R<رۡ>زُQ<قۡ>نَا وَأَN<ن>تَ خَیۡR<رُ> ٱX<ل>R<رَّ>T< ٰ>زِقِJ<ی>نَ A<۝>D<١١٤>",
"قَالَ ٱللَّهُ إِN<نِّ>ی مُنَزِّلُهَا عَلَیۡكُمۡۖ فَمَX<ن> N<یَ>كۡفُR<رۡ> بَعۡدُ مِN<ن>كُمۡ فَإِN<نِّ>W<یۤ> أُعَذِّبُهُT<ۥ> عَذَابX<ࣰ>ا لَّW<اۤ> أُعَذِّبُهُW<ۥۤ> أَحَدX<ࣰ>ا N<مِّ>نَ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١١٥>",
"وَإِذۡ قَالَ ٱللَّهُ یَT<ـٰ>عِیسَى ٱQ<بۡ>نَ مَR<رۡ>یَمَ ءَأَN<ن>تَ قُلۡتَ لِX<ل>N<نَّ>اسِ ٱتَّخِذُونِی وَأُN<مِّ>یَ إِلَT<ـٰ>هَیۡنِ مِN<ن> دُونِ ٱللَّهِۖ قَالَ سُQ<بۡ>حَT<ـٰ>نَكَ مَا یَكُونُ لِW<یۤ> أَنۡ أَقُولَ مَا لَیۡسَ لِی بِحَقٍّۚ إِN<ن> كُN<ن>تُ قُلۡتُهُT<ۥ> فَقَQ<دۡ> عَلِمۡتَهُT<ۥ>ۚ تَعۡلَمُ مَا فِی نَفۡسِی وَلَW<اۤ> أَعۡلَمُ مَا فِی نَفۡسِكَۚ إِN<نَّ>كَ أَN<ن>تَ عَلَّT<ـٰ>مُ ٱلۡغُیُJ<و>Q<ب>ِ A<۝>D<١١٦>",
"مَا قُلۡتُ لَهُمۡ إِلَّا مَW<اۤ> أَمَR<رۡ>تَنِی بِهِW<ۦۤ> أَنِ ٱعۡبُدُوX<ا۟> ٱللَّهَ R<رَ>بِّی وَR<رَ>بَّكُمۡۚ وَكُN<ن>تُ عَلَیۡهِمۡ شَهِیدX<ࣰ>ا N<مَّ>ا دُمۡتُ فِیهِمۡۖ فَلَN<مَّ>ا تَوَفَّیۡتَنِی كُN<ن>تَ أَN<ن>تَ ٱX<ل>R<رَّ>قِیبَ عَلَیۡهِمۡۚ وَأَN<ن>تَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> شَهِJ<ی>Q<د>ٌ A<۝>D<١١٧>",
"إِN<ن> تُعَذِّQ<بۡ>هُمۡ فَإِN<نَّ>هُمۡ عِبَادُكَۖ وَإِN<ن> تَغۡفِرۡ لَهُمۡ فَإِN<نَّ>كَ أَN<ن>تَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<١١٨>",
"قَالَ ٱللَّهُ هَT<ـٰ>ذَا یَوۡمُ یَN<ن>فَعُ ٱX<ل>صَّT<ـٰ>دِقِینَ صِQ<دۡ>قُهُمۡۚ لَهُمۡ جَN<نَّ>T<ـٰ>تN<ࣱ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدX<ࣰ>اۖ R<رَّ>ضِیَ ٱللَّهُ عَنۡهُمۡ وَR<رَ>ضُوX<ا۟> عَنۡهُۚ ذَT< ٰ>لِكَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<١١٩>",
"لِلَّهِ مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا فِیهِN<نَّ>ۚ وَهُوَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رُۢ A<۝>D<١٢٠>",
"#ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَجَعَلَ ٱX<ل>ظُّلُمَT<ـٰ>تِ وَX<ٱ>X<ل>N<نُّ>وR<رَ>ۖ ثُN<مَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِR<رَ>بِّهِمۡ یَعۡدِلُJ<و>نَ A<۝>D<١>",
"هُوَ ٱلَّذِی خَلَقَكُم N<مِّ>N<ن> طِینN<ࣲ> ثُN<مَّ> قَضَىW<ٰۤ> أَجَلX<ࣰ>اۖ N<وَ>أَجَلX<ࣱ> N<مُّ>سَN<مّ>ًى عِN<ن>دَهُT<ۥ>ۖ ثُN<مَّ> أَN<ن>تُمۡ تَمۡتَR<رُ>J<و>نَ A<۝>D<٢>",
"وَهُوَ ٱللَّهُ فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَفِی ٱلۡأَR<رۡ>ضِ یَعۡلَمُ سِR<رَّ>كُمۡ وَجَهۡR<رَ>كُمۡ وَیَعۡلَمُ مَا تَكۡسِبُJ<و>نَ A<۝>D<٣>",
"وَمَا تَأۡتِیهِم N<مِّ>نۡ ءَایَةX<ࣲ> N<مِّ>نۡ ءَایَT<ـٰ>تِ R<رَ>بِّهِمۡ إِلَّا كَانُوX<ا۟> عَنۡهَا مُعۡرِضِJ<ی>نَ A<۝>D<٤>",
"فَقَQ<دۡ> كَذَّبُوX<ا۟> بِX<ٱ>لۡحَقِّ لَN<مَّ>ا جَW<اۤ>ءَهُمۡ فَسَوۡفَ یَأۡتِیهِمۡ أَX<ن>N<ۢ>بَW<ـٰۤ>ؤُX<ا۟> مَا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٥>",
"أَلَمۡ یَR<رَ>وۡX<ا۟> كَمۡ أَهۡلَكۡنَا مِN<ن> قَQ<بۡ>لِهِم N<مِّ>N<ن> قَR<رۡ>نX<ࣲ> N<مَّ>كَّN<نَّ>T<ـٰ>هُمۡ فِی ٱلۡأَR<رۡ>ضِ مَا لَمۡ نُمَكِّX<ن> لَّكُمۡ وَأَR<رۡ>سَلۡنَا ٱX<ل>سَّمَW<اۤ>ءَ عَلَیۡهِم N<مِّ>Q<دۡ>R<رَ>اR<ر>X<ࣰ>ا N<وَ>جَعَلۡنَا ٱلۡأَنۡهَT<ـٰ>R<رَ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهِمۡ فَأَهۡلَكۡنَT<ـٰ>هُN<م> بِذُنُوبِهِمۡ وَأَN<ن>شَأۡنَا مِX<ن>N<ۢ> بَعۡدِهِمۡ قَR<رۡ>نًا ءَاخَرِJ<ی>نَ A<۝>D<٦>",
"وَلَوۡ نَزَّلۡنَا عَلَیۡكَ كِتَT<ـٰ>بN<ࣰ>ا فِی قِرۡطَاسN<ࣲ> فَلَمَسُوهُ بِأَیۡدِیهِمۡ لَقَالَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا سِحۡR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٧>",
"وَقَالُوX<ا۟> لَوۡلَW<اۤ> أُN<ن>زِلَ عَلَیۡهِ مَلَكX<ࣱ>ۖ N<وَ>لَوۡ أَN<ن>زَلۡنَا مَلَكX<ࣰ>ا لَّقُضِیَ ٱلۡأَمۡR<رُ> ثُN<مَّ> لَا یُN<ن>ظَR<رُ>J<و>نَ A<۝>D<٨>",
"وَلَوۡ جَعَلۡنَT<ـٰ>هُ مَلَكX<ࣰ>ا لَّجَعَلۡنَT<ـٰ>هُ R<رَ>جُلX<ࣰ>ا N<وَ>لَلَبَسۡنَا عَلَیۡهِم N<مَّ>ا یَلۡبِسُJ<و>نَ A<۝>D<٩>",
"وَلَقَدِ ٱسۡتُهۡزِئَ بِR<رُ>سُلX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِكَ فَحَاقَ بِX<ٱ>لَّذِینَ سَخِR<رُ>وX<ا۟> مِنۡهُم N<مَّ>ا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<١٠>",
"قُلۡ سِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ ثُN<مَّ> ٱN<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُكَذِّبِJ<ی>نَ A<۝>D<١١>",
"قُل لِّمَX<ن> N<مَّ>ا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ قُل لِّلَّهِۚ كَتَبَ عَلَىٰ نَفۡسِهِ ٱX<ل>R<رَّ>حۡمَةَۚ لَیَQ<جۡ>مَعَN<نَّ>كُمۡ إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ لَا R<رَ>یۡبَ فِیهِۚ ٱلَّذِینَ خَسِR<رُ>W<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ فَهُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<١٢>",
"۞ وَلَهُT<ۥ> مَا سَكَنَ فِی ٱلَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِۚ وَهُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<١٣>",
"قُلۡ أَغَیۡR<رَ> ٱللَّهِ أَتَّخِذُ وَلِیّN<ࣰ>ا فَاطِرِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَهُوَ یُQ<طۡ>عِمُ وَلَا یُQ<طۡ>عَمُۗ قُلۡ إِN<نِّ>W<یۤ> أُمِرۡتُ أَنۡ أَكُونَ أَوَّلَ مَنۡ أَسۡلَمَۖ وَلَا تَكُونَN<نَّ> مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<١٤>",
"قُلۡ إِN<نِّ>W<یۤ> أَخَافُ إِنۡ عَصَیۡتُ R<رَ>بِّی عَذَابَ یَوۡمٍ عَظِJ<ی>مࣲ A<۝>D<١٥>",
"مَّX<ن> N<یُ>صۡR<رَ>فۡ عَنۡهُ یَوۡمَئِذN<ࣲ> فَقَQ<دۡ> R<رَ>حِمَهُT<ۥ>ۚ وَذَT< ٰ>لِكَ ٱلۡفَوۡزُ ٱلۡمُبِJ<ی>نُ A<۝>D<١٦>",
"وَإِX<ن> N<یَ>مۡسَسۡكَ ٱللَّهُ بِضُرّN<ࣲ> فَلَا كَاشِفَ لَهُW<ۥۤ> إِلَّا هُوَۖ وَإِX<ن> N<یَ>مۡسَسۡكَ بِخَیۡرN<ࣲ> فَهُوَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<١٧>",
"وَهُوَ ٱلۡقَاهِR<رُ> فَوۡقَ عِبَادِهِT<ۦ>ۚ وَهُوَ ٱلۡحَكِیمُ ٱلۡخَبِJ<ی>رُ A<۝>D<١٨>",
"قُلۡ أَیُّ شَیۡءٍ أَكۡبَR<رُ> شَهَT<ـٰ>دَةN<ࣰ>ۖ قُلِ ٱللَّهُۖ شَهِیدُN<ۢ> بَیۡنِی وَبَیۡنَكُمۡۚ وَأُوحِیَ إِلَیَّ هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانُ لِأُN<ن>ذِR<رَ>كُN<م> بِهِT<ۦ> وَمَX<ن>N<ۢ> بَلَغَۚ أَئِN<نَّ>كُمۡ لَتَشۡهَدُونَ أَN<نَّ> مَعَ ٱللَّهِ ءَالِهَةً أُخۡR<رَ>ىٰۚ قُل لَّW<اۤ> أَشۡهَدُۚ قُلۡ إِN<نَّ>مَا هُوَ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدX<ࣱ> N<وَ>إِN<نَّ>نِی بَرِW<یۤ>ءX<ࣱ> N<مِّ>N<مَّ>ا تُشۡرِكُJ<و>نَ A<۝>D<١٩>",
"ٱلَّذِینَ ءَاتَیۡنَT<ـٰ>هُمُ ٱلۡكِتَT<ـٰ>بَ یَعۡرِفُونَهُT<ۥ> كَمَا یَعۡرِفُونَ أَQ<بۡ>نَW<اۤ>ءَهُمُۘ ٱلَّذِینَ خَسِR<رُ>W<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ فَهُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٢٠>",
"وَمَنۡ أَظۡلَمُ مِN<مَّ>نِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبًا أَوۡ كَذَّبَ بِءَایَT<ـٰ>تِهِW<ۦۤ>ۚ إِN<نَّ>هُT<ۥ> لَا یُفۡلِحُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٢١>",
"وَیَوۡمَ نَحۡشُR<رُ>هُمۡ جَمِیعN<ࣰ>ا ثُN<مَّ> نَقُولُ لِلَّذِینَ أَشۡR<رَ>كُW<وۤ>X<ا۟> أَیۡنَ شُR<رَ>كَW<اۤ>ؤُكُمُ ٱلَّذِینَ كُN<ن>تُمۡ تَزۡعُمُJ<و>نَ A<۝>D<٢٢>",
"ثُN<مَّ> لَمۡ تَكُN<ن> فِتۡنَتُهُمۡ إِلَّW<اۤ> أَN<ن> قَالُوX<ا۟> وَٱللَّهِ R<رَ>بِّنَا مَا كُN<نَّ>ا مُشۡرِكِJ<ی>نَ A<۝>D<٢٣>",
"ٱN<ن>ظُR<رۡ> كَیۡفَ كَذَبُوX<ا۟> عَلَىW<ٰۤ> أَN<ن>فُسِهِمۡۚ وَضَلَّ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<٢٤>",
"وَمِنۡهُم N<مَّ>X<ن> N<یَ>سۡتَمِعُ إِلَیۡكَۖ وَجَعَلۡنَا عَلَىٰ قُلُوبِهِمۡ أَكِN<نَّ>ةً أَX<ن> N<یَ>فۡقَهُوهُ وَفِW<یۤ> ءَاذَانِهِمۡ وَQ<قۡ>R<ر>X<ࣰ>اۚ N<وَ>إِX<ن> N<یَ>R<رَ>وۡX<ا۟> كُلَّ ءَایَةX<ࣲ> لَّا یُؤۡمِنُوX<ا۟> بِهَاۖ حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءُوكَ یُجَT<ـٰ>دِلُونَكَ یَقُولُ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّW<اۤ> أَسَT<ـٰ>طِیR<رُ> ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٢٥>",
"وَهُمۡ یَنۡهَوۡنَ عَنۡهُ وَیَنۡءَوۡنَ عَنۡهُۖ وَإِX<ن> N<یُ>هۡلِكُونَ إِلَّW<اۤ> أَN<ن>فُسَهُمۡ وَمَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٢٦>",
"وَلَوۡ تَR<رَ>ىW<ٰۤ> إِذۡ وُقِفُوX<ا۟> عَلَى ٱX<ل>N<نَّ>ارِ فَقَالُوX<ا۟> یَT<ـٰ>لَیۡتَنَا نُR<رَ>دُّ وَلَا نُكَذِّبَ بِءَایَT<ـٰ>تِ R<رَ>بِّنَا وَنَكُونَ مِنَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٢٧>",
"بَلۡ بَدَا لَهُم N<مَّ>ا كَانُوX<ا۟> یُخۡفُونَ مِN<ن> قَQ<بۡ>لُۖ وَلَوۡ R<رُ>دُّوX<ا۟> لَعَادُوX<ا۟> لِمَا نُهُوX<ا۟> عَنۡهُ وَإِN<نَّ>هُمۡ لَكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<٢٨>",
"وَقَالُW<وۤ>X<ا۟> إِنۡ هِیَ إِلَّا حَیَاتُنَا ٱX<ل>دُّنۡیَا وَمَا نَحۡنُ بِمَQ<بۡ>عُوثِJ<ی>نَ A<۝>D<٢٩>",
"وَلَوۡ تَR<رَ>ىW<ٰۤ> إِذۡ وُقِفُوX<ا۟> عَلَىٰ R<رَ>بِّهِمۡۚ قَالَ أَلَیۡسَ هَT<ـٰ>ذَا بِX<ٱ>لۡحَقِّۚ قَالُوX<ا۟> بَلَىٰ وَR<رَ>بِّنَاۚ قَالَ فَذُوقُوX<ا۟> ٱلۡعَذَابَ بِمَا كُN<ن>تُمۡ تَكۡفُR<رُ>J<و>نَ A<۝>D<٣٠>",
"قَQ<دۡ> خَسِR<رَ> ٱلَّذِینَ كَذَّبُوX<ا۟> بِلِقَW<اۤ>ءِ ٱللَّهِۖ حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءَتۡهُمُ ٱX<ل>سَّاعَةُ بَغۡتَةN<ࣰ> قَالُوX<ا۟> یَT<ـٰ>حَسۡR<رَ>تَنَا عَلَىٰ مَا فَR<رَّ>Q<طۡ>نَا فِیهَا وَهُمۡ یَحۡمِلُونَ أَوۡزَاR<رَ>هُمۡ عَلَىٰ ظُهُورِهِمۡۚ أَلَا سَW<اۤ>ءَ مَا یَزِR<رُ>J<و>نَ A<۝>D<٣١>",
"وَمَا ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَW<اۤ> إِلَّا لَعِبX<ࣱ> N<وَ>لَهۡوX<ࣱ>ۖ N<وَ>لَX<ل>دَّاR<رُ> ٱلۡءَاخِR<رَ>ةُ خَیۡR<ر>X<ࣱ> لِّلَّذِینَ یَتَّقُونَۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<٣٢>",
"قَQ<دۡ> نَعۡلَمُ إِN<نَّ>هُT<ۥ> لَیَحۡزُنُكَ ٱلَّذِی یَقُولُونَۖ فَإِN<نَّ>هُمۡ لَا یُكَذِّبُونَكَ وَلَT<ـٰ>كِN<نَّ> ٱX<ل>ظَّT<ـٰ>لِمِینَ بِءَایَT<ـٰ>تِ ٱللَّهِ یَQ<جۡ>حَدُJ<و>نَ A<۝>D<٣٣>",
"وَلَقَQ<دۡ> كُذِّبَتۡ R<رُ>سُلX<ࣱ> N<مِّ>N<ن> قَQ<بۡ>لِكَ فَصَبَR<رُ>وX<ا۟> عَلَىٰ مَا كُذِّبُوX<ا۟> وَأُوذُوX<ا۟> حَتَّىW<ٰۤ> أَتَX<ى>T<ٰ>هُمۡ نَصۡR<رُ>نَاۚ وَلَا مُبَدِّلَ لِكَلِمَT<ـٰ>تِ ٱللَّهِۚ وَلَقَQ<دۡ> جَW<اۤ>ءَكَ مِن N<نَّ>بَإِX<ی۟> ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٣٤>",
"وَإِN<ن> كَانَ كَبُR<رَ> عَلَیۡكَ إِعۡR<رَ>اضُهُمۡ فَإِنِ ٱسۡتَطَعۡتَ أَN<ن> تَQ<بۡ>تَغِیَ نَفَقN<ࣰ>ا فِی ٱلۡأَR<رۡ>ضِ أَوۡ سُلَّمN<ࣰ>ا فِی ٱX<ل>سَّمَW<اۤ>ءِ فَتَأۡتِیَهُN<م> بِءَایَةX<ࣲ>ۚ N<وَ>لَوۡ شَW<اۤ>ءَ ٱللَّهُ لَجَمَعَهُمۡ عَلَى ٱلۡهُدَىٰۚ فَلَا تَكُونَN<نَّ> مِنَ ٱلۡجَT<ـٰ>هِلِJ<ی>نَ A<۝>D<٣٥>",
"۞ إِN<نَّ>مَا یَسۡتَجِیبُ ٱلَّذِینَ یَسۡمَعُونَۘ وَX<ٱ>لۡمَوۡتَىٰ یَQ<بۡ>عَثُهُمُ ٱللَّهُ ثُN<مَّ> إِلَیۡهِ یُR<رۡ>جَعُJ<و>نَ A<۝>D<٣٦>",
"وَقَالُوX<ا۟> لَوۡلَا نُزِّلَ عَلَیۡهِ ءَایَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ>ۚ قُلۡ إِN<نَّ> ٱللَّهَ قَادِR<ر>ٌ عَلَىW<ٰۤ> أَX<ن> N<یُ>نَزِّلَ ءَایَةX<ࣰ> N<وَ>لَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٣٧>",
"وَمَا مِN<ن> دَL<اۤ>بَّةN<ࣲ> فِی ٱلۡأَR<رۡ>ضِ وَلَا طَW<ـٰۤ>ئِرX<ࣲ> N<یَ>طِیR<رُ> بِجَنَاحَیۡهِ إِلَّW<اۤ> أُمَمٌ أَمۡثَالُكُمۚ N<مَّ>ا فَR<رَّ>Q<طۡ>نَا فِی ٱلۡكِتَT<ـٰ>بِ مِN<ن> شَیۡءN<ࣲ>ۚ ثُN<مَّ> إِلَىٰ R<رَ>بِّهِمۡ یُحۡشَR<رُ>J<و>نَ A<۝>D<٣٨>",
"وَX<ٱ>لَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا صُN<مّ>X<ࣱ> N<وَ>بُكۡمN<ࣱ> فِی ٱX<ل>ظُّلُمَT<ـٰ>تِۗ مَX<ن> N<یَ>شَإِ ٱللَّهُ یُضۡلِلۡهُ وَمَX<ن> N<یَ>شَأۡ یَQ<جۡ>عَلۡهُ عَلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٣٩>",
"قُلۡ أَR<رَ>ءَیۡتَكُمۡ إِنۡ أَتَX<ى>T<ٰ>كُمۡ عَذَابُ ٱللَّهِ أَوۡ أَتَتۡكُمُ ٱX<ل>سَّاعَةُ أَغَیۡR<رَ> ٱللَّهِ تَQ<دۡ>عُونَ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٤٠>",
"بَلۡ إِیَّاهُ تَQ<دۡ>عُونَ فَیَكۡشِفُ مَا تَQ<دۡ>عُونَ إِلَیۡهِ إِN<ن> شَW<اۤ>ءَ وَتَN<ن>سَوۡنَ مَا تُشۡرِكُJ<و>نَ A<۝>D<٤١>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَW<اۤ> إِلَىW<ٰۤ> أُمَمX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِكَ فَأَخَذۡنَT<ـٰ>هُN<م> بِX<ٱ>لۡبَأۡسَW<اۤ>ءِ وَX<ٱ>X<ل>ضَّR<رَّ>W<اۤ>ءِ لَعَلَّهُمۡ یَتَضَR<رَّ>عُJ<و>نَ A<۝>D<٤٢>",
"فَلَوۡلَW<اۤ> إِذۡ جَW<اۤ>ءَهُN<م> بَأۡسُنَا تَضَR<رَّ>عُوX<ا۟> وَلَT<ـٰ>كِN<ن> قَسَتۡ قُلُوبُهُمۡ وَزَیَّنَ لَهُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٤٣>",
"فَلَN<مَّ>ا نَسُوX<ا۟> مَا ذُكِّR<رُ>وX<ا۟> بِهِT<ۦ> فَتَحۡنَا عَلَیۡهِمۡ أَQ<بۡ>وَT< ٰ>بَ كُلِّ شَیۡءٍ حَتَّىW<ٰۤ> إِذَا فَرِحُوX<ا۟> بِمَW<اۤ> أُوتُW<وۤ>X<ا۟> أَخَذۡنَT<ـٰ>هُN<م> بَغۡتَةN<ࣰ> فَإِذَا هُم N<مُّ>Q<بۡ>لِسُJ<و>نَ A<۝>D<٤٤>",
"فَقُطِعَ دَابِR<رُ> ٱلۡقَوۡمِ ٱلَّذِینَ ظَلَمُوX<ا۟>ۚ وَX<ٱ>لۡحَمۡدُ لِلَّهِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٤٥>",
"قُلۡ أَR<رَ>ءَیۡتُمۡ إِنۡ أَخَذَ ٱللَّهُ سَمۡعَكُمۡ وَأَQ<بۡ>صَT<ـٰ>R<رَ>كُمۡ وَخَتَمَ عَلَىٰ قُلُوبِكُم N<مَّ>نۡ إِلَT<ـٰ>هٌ غَیۡR<رُ> ٱللَّهِ یَأۡتِیكُN<م> بِهِۗ ٱN<ن>ظُR<رۡ> كَیۡفَ نُصَرِّفُ ٱلۡءَایَT<ـٰ>تِ ثُN<مَّ> هُمۡ یَصۡدِفُJ<و>نَ A<۝>D<٤٦>",
"قُلۡ أَR<رَ>ءَیۡتَكُمۡ إِنۡ أَتَX<ى>T<ٰ>كُمۡ عَذَابُ ٱللَّهِ بَغۡتَةً أَوۡ جَهۡR<رَ>ةً هَلۡ یُهۡلَكُ إِلَّا ٱلۡقَوۡمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٤٧>",
"وَمَا نُR<رۡ>سِلُ ٱلۡمُR<رۡ>سَلِینَ إِلَّا مُبَشِّرِینَ وَمُN<ن>ذِرِینَۖ فَمَنۡ ءَامَنَ وَأَصۡلَحَ فَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٤٨>",
"وَX<ٱ>لَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا یَمَسُّهُمُ ٱلۡعَذَابُ بِمَا كَانُوX<ا۟> یَفۡسُقُJ<و>نَ A<۝>D<٤٩>",
"قُل لَّW<اۤ> أَقُولُ لَكُمۡ عِN<ن>دِی خَزَW<اۤ>ئِنُ ٱللَّهِ وَلَW<اۤ> أَعۡلَمُ ٱلۡغَیۡبَ وَلَW<اۤ> أَقُولُ لَكُمۡ إِN<نِّ>ی مَلَكٌۖ إِنۡ أَتَّبِعُ إِلَّا مَا یُوحَىW<ٰۤ> إِلَیَّۚ قُلۡ هَلۡ یَسۡتَوِی ٱلۡأَعۡمَىٰ وَX<ٱ>لۡبَصِیR<رُ>ۚ أَفَلَا تَتَفَكَّR<رُ>J<و>نَ A<۝>D<٥٠>",
"وَأَN<ن>ذِرۡ بِهِ ٱلَّذِینَ یَخَافُونَ أَX<ن> N<یُ>حۡشَR<رُ>W<وۤ>X<ا۟> إِلَىٰ R<رَ>بِّهِمۡ لَیۡسَ لَهُم N<مِّ>N<ن> دُونِهِT<ۦ> وَلِیّX<ࣱ> N<وَ>لَا شَفِیعX<ࣱ> لَّعَلَّهُمۡ یَتَّقُJ<و>نَ A<۝>D<٥١>",
"وَلَا تَQ<طۡ>R<رُ>دِ ٱلَّذِینَ یَQ<دۡ>عُونَ R<رَ>بَّهُN<م> بِX<ٱ>لۡغَدَX<و>T<ٰ>ةِ وَX<ٱ>لۡعَشِیِّ یُرِیدُونَ وَQ<جۡ>هَهُT<ۥ>ۖ مَا عَلَیۡكَ مِنۡ حِسَابِهِم N<مِّ>N<ن> شَیۡءX<ࣲ> N<وَ>مَا مِنۡ حِسَابِكَ عَلَیۡهِم N<مِّ>N<ن> شَیۡءN<ࣲ> فَتَQ<طۡ>R<رُ>دَهُمۡ فَتَكُونَ مِنَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥٢>",
"وَكَذَT< ٰ>لِكَ فَتَN<نَّ>ا بَعۡضَهُN<م> بِبَعۡضX<ࣲ> لِّیَقُولُW<وۤ>X<ا۟> أَهَW<ـٰۤ>ؤُلَW<اۤ>ءِ مَN<نَّ> ٱللَّهُ عَلَیۡهِم N<مِّ>X<ن>N<ۢ> بَیۡنِنَW<اۤ>ۗ أَلَیۡسَ ٱللَّهُ بِأَعۡلَمَ بِX<ٱ>X<ل>شَّT<ـٰ>كِرِJ<ی>نَ A<۝>D<٥٣>",
"وَإِذَا جَW<اۤ>ءَكَ ٱلَّذِینَ یُؤۡمِنُونَ بِءَایَT<ـٰ>تِنَا فَقُلۡ سَلَT<ـٰ>مٌ عَلَیۡكُمۡۖ كَتَبَ R<رَ>بُّكُمۡ عَلَىٰ نَفۡسِهِ ٱX<ل>R<رَّ>حۡمَةَ أَN<نَّ>هُT<ۥ> مَنۡ عَمِلَ مِN<ن>كُمۡ سُW<وۤ>ءَN<ۢ>ا بِجَهَT<ـٰ>لَةN<ࣲ> ثُN<مَّ> تَابَ مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> وَأَصۡلَحَ فَأَN<نَّ>هُT<ۥ> غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٥٤>",
"وَكَذَT< ٰ>لِكَ نُفَصِّلُ ٱلۡءَایَT<ـٰ>تِ وَلِتَسۡتَبِینَ سَبِیلُ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٥٥>",
"قُلۡ إِN<نِّ>ی نُهِیتُ أَنۡ أَعۡبُدَ ٱلَّذِینَ تَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِۚ قُل لَّW<اۤ> أَتَّبِعُ أَهۡوَW<اۤ>ءَكُمۡ قَQ<دۡ> ضَلَلۡتُ إِذX<ࣰ>ا N<وَ>مَW<اۤ> أَنَا۠ مِنَ ٱلۡمُهۡتَدِJ<ی>نَ A<۝>D<٥٦>",
"قُلۡ إِN<نِّ>ی عَلَىٰ بَیِّنَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّی وَكَذَّQ<بۡ>تُN<م> بِهِT<ۦ>ۚ مَا عِN<ن>دِی مَا تَسۡتَعۡجِلُونَ بِهِW<ۦۤ>ۚ إِنِ ٱلۡحُكۡمُ إِلَّا لِلَّهِۖ یَقُصُّ ٱلۡحَقَّۖ وَهُوَ خَیۡR<رُ> ٱلۡفَT<ـٰ>صِلِJ<ی>نَ A<۝>D<٥٧>",
"قُل لَّوۡ أَN<نَّ> عِN<ن>دِی مَا تَسۡتَعۡجِلُونَ بِهِT<ۦ> لَقُضِیَ ٱلۡأَمۡR<رُ> بَیۡنِی وَبَیۡنَكُمۡۗ وَٱللَّهُ أَعۡلَمُ بِX<ٱ>X<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥٨>",
"۞ وَعِN<ن>دَهُT<ۥ> مَفَاتِحُ ٱلۡغَیۡبِ لَا یَعۡلَمُهَW<اۤ> إِلَّا هُوَۚ وَیَعۡلَمُ مَا فِی ٱلۡبَرِّ وَX<ٱ>لۡبَحۡرِۚ وَمَا تَسۡقُطُ مِX<ن> N<وَ>R<رَ>قَةٍ إِلَّا یَعۡلَمُهَا وَلَا حَبَّةN<ࣲ> فِی ظُلُمَT<ـٰ>تِ ٱلۡأَR<رۡ>ضِ وَلَا R<رَ>Q<طۡ>بX<ࣲ> N<وَ>لَا یَابِسٍ إِلَّا فِی كِتَT<ـٰ>بX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٥٩>",
"وَهُوَ ٱلَّذِی یَتَوَفَّX<ى>T<ٰ>كُN<م> بِX<ٱ>لَّیۡلِ وَیَعۡلَمُ مَا جَR<رَ>حۡتُN<م> بِX<ٱ>X<ل>N<نَّ>هَارِ ثُN<مَّ> یَQ<بۡ>عَثُكُمۡ فِیهِ لِیُQ<قۡ>ضَىW<ٰۤ> أَجَلX<ࣱ> N<مُّ>سَN<مّ>N<ࣰ>ىۖ ثُN<مَّ> إِلَیۡهِ مَR<رۡ>جِعُكُمۡ ثُN<مَّ> یُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٦٠>",
"وَهُوَ ٱلۡقَاهِR<رُ> فَوۡقَ عِبَادِهِT<ۦ>ۖ وَیُR<رۡ>سِلُ عَلَیۡكُمۡ حَفَظَةً حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءَ أَحَدَكُمُ ٱلۡمَوۡتُ تَوَفَّتۡهُ R<رُ>سُلُنَا وَهُمۡ لَا یُفَرِّطُJ<و>نَ A<۝>D<٦١>",
"ثُN<مَّ> R<رُ>دُّW<وۤ>X<ا۟> إِلَى ٱللَّهِ مَوۡلَX<ى>T<ٰ>هُمُ ٱلۡحَقِّۚ أَلَا لَهُ ٱلۡحُكۡمُ وَهُوَ أَسۡR<رَ>عُ ٱلۡحَT<ـٰ>سِبِJ<ی>نَ A<۝>D<٦٢>",
"قُلۡ مَX<ن> N<یُ>نَجِّیكُم N<مِّ>N<ن> ظُلُمَT<ـٰ>تِ ٱلۡبَرِّ وَX<ٱ>لۡبَحۡرِ تَQ<دۡ>عُونَهُT<ۥ> تَضَR<رُّ>عX<ࣰ>ا N<وَ>خُفۡیَةX<ࣰ> لَّئِنۡ أَN<ن>جَX<ى>T<ٰ>نَا مِنۡ هَT<ـٰ>ذِهِT<ۦ> لَنَكُونَN<نَّ> مِنَ ٱX<ل>شَّT<ـٰ>كِرِJ<ی>نَ A<۝>D<٦٣>",
"قُلِ ٱللَّهُ یُنَجِّیكُم N<مِّ>نۡهَا وَمِN<ن> كُلِّ كَR<رۡ>بN<ࣲ> ثُN<مَّ> أَN<ن>تُمۡ تُشۡرِكُJ<و>نَ A<۝>D<٦٤>",
"قُلۡ هُوَ ٱلۡقَادِR<رُ> عَلَىW<ٰۤ> أَX<ن> N<یَ>Q<بۡ>عَثَ عَلَیۡكُمۡ عَذَابX<ࣰ>ا N<مِّ>N<ن> فَوۡقِكُمۡ أَوۡ مِN<ن> تَحۡتِ أَR<رۡ>جُلِكُمۡ أَوۡ یَلۡبِسَكُمۡ شِیَعX<ࣰ>ا N<وَ>یُذِیقَ بَعۡضَكُN<م> بَأۡسَ بَعۡضٍۗ ٱN<ن>ظُR<رۡ> كَیۡفَ نُصَرِّفُ ٱلۡءَایَT<ـٰ>تِ لَعَلَّهُمۡ یَفۡقَهُJ<و>نَ A<۝>D<٦٥>",
"وَكَذَّبَ بِهِT<ۦ> قَوۡمُكَ وَهُوَ ٱلۡحَقُّۚ قُل لَّسۡتُ عَلَیۡكُN<م> بِوَكِJ<ی>لࣲ A<۝>D<٦٦>",
"لِّكُلِّ نَبَإX<ࣲ> N<مُّ>سۡتَقَR<رّ>X<ࣱ>ۚ N<وَ>سَوۡفَ تَعۡلَمُJ<و>نَ A<۝>D<٦٧>",
"وَإِذَا R<رَ>أَیۡتَ ٱلَّذِینَ یَخُوضُونَ فِW<یۤ> ءَایَT<ـٰ>تِنَا فَأَعۡرِضۡ عَنۡهُمۡ حَتَّىٰ یَخُوضُوX<ا۟> فِی حَدِیثٍ غَیۡرِهِT<ۦ>ۚ وَإِN<مَّ>ا یُN<ن>سِیَN<نَّ>كَ ٱX<ل>شَّیۡطَT<ـٰ>نُ فَلَا تَQ<قۡ>عُQ<دۡ> بَعۡدَ ٱX<ل>ذِّكۡR<رَ>ىٰ مَعَ ٱلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٦٨>",
"وَمَا عَلَى ٱلَّذِینَ یَتَّقُونَ مِنۡ حِسَابِهِم N<مِّ>N<ن> شَیۡءX<ࣲ> N<وَ>لَT<ـٰ>كِN<ن> ذِكۡR<رَ>ىٰ لَعَلَّهُمۡ یَتَّقُJ<و>نَ A<۝>D<٦٩>",
"وَذَرِ ٱلَّذِینَ ٱتَّخَذُوX<ا۟> دِینَهُمۡ لَعِبX<ࣰ>ا N<وَ>لَهۡوX<ࣰ>ا N<وَ>غَR<رَّ>تۡهُمُ ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَاۚ وَذَكِّرۡ بِهِW<ۦۤ> أَN<ن> تُQ<بۡ>سَلَ نَفۡسُN<ۢ> بِمَا كَسَبَتۡ لَیۡسَ لَهَا مِN<ن> دُونِ ٱللَّهِ وَلِیّX<ࣱ> N<وَ>لَا شَفِیعX<ࣱ> N<وَ>إِN<ن> تَعۡدِلۡ كُلَّ عَQ<دۡ>لX<ࣲ> لَّا یُؤۡخَذۡ مِنۡهَW<اۤ>ۗ أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ أُQ<بۡ>سِلُوX<ا۟> بِمَا كَسَبُوX<ا۟>ۖ لَهُمۡ شَR<رَ>ابX<ࣱ> N<مِّ>نۡ حَمِیمX<ࣲ> N<وَ>عَذَابٌ أَلِیمُN<ۢ> بِمَا كَانُوX<ا۟> یَكۡفُR<رُ>J<و>نَ A<۝>D<٧٠>",
"قُلۡ أَنَQ<دۡ>عُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ مَا لَا یَN<ن>فَعُنَا وَلَا یَضُR<رُّ>نَا وَنُR<رَ>دُّ عَلَىW<ٰۤ> أَعۡقَابِنَا بَعۡدَ إِذۡ هَدَX<ى>T<ٰ>نَا ٱللَّهُ كَX<ٱ>لَّذِی ٱسۡتَهۡوَتۡهُ ٱX<ل>شَّیَT<ـٰ>طِینُ فِی ٱلۡأَR<رۡ>ضِ حَیۡR<رَ>انَ لَهُW<ۥۤ> أَصۡحَT<ـٰ>بX<ࣱ> N<یَ>Q<دۡ>عُونَهُW<ۥۤ> إِلَى ٱلۡهُدَى ٱئۡتِنَاۗ قُلۡ إِN<نَّ> هُدَى ٱللَّهِ هُوَ ٱلۡهُدَىٰۖ وَأُمِرۡنَا لِنُسۡلِمَ لِR<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٧١>",
"وَأَنۡ أَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَX<ٱ>تَّقُوهُۚ وَهُوَ ٱلَّذِW<یۤ> إِلَیۡهِ تُحۡشَR<رُ>J<و>نَ A<۝>D<٧٢>",
"وَهُوَ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ بِX<ٱ>لۡحَقِّۖ وَیَوۡمَ یَقُولُ كُN<ن> فَیَكُونُۚ قَوۡلُهُ ٱلۡحَقُّۚ وَلَهُ ٱلۡمُلۡكُ یَوۡمَ یُN<ن>فَخُ فِی ٱX<ل>صُّورِۚ عَT<ـٰ>لِمُ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِۚ وَهُوَ ٱلۡحَكِیمُ ٱلۡخَبِJ<ی>رُ A<۝>D<٧٣>",
"۞ وَإِذۡ قَالَ إِQ<بۡ>R<رَ>T< ٰ>هِیمُ لِأَبِیهِ ءَازَR<رَ> أَتَتَّخِذُ أَصۡنَامًا ءَالِهَةً إِN<نِّ>W<یۤ> أَR<رَ>X<ى>T<ٰ>كَ وَقَوۡمَكَ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٧٤>",
"وَكَذَT< ٰ>لِكَ نُرِW<یۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ مَلَكُوتَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَلِیَكُونَ مِنَ ٱلۡمُوقِنِJ<ی>نَ A<۝>D<٧٥>",
"فَلَN<مَّ>ا جَN<نَّ> عَلَیۡهِ ٱلَّیۡلُ R<رَ>ءَا كَوۡكَبN<ࣰ>اۖ قَالَ هَT<ـٰ>ذَا R<رَ>بِّیۖ فَلَN<مَّ>W<اۤ> أَفَلَ قَالَ لَW<اۤ> أُحِبُّ ٱلۡءَافِلِJ<ی>نَ A<۝>D<٧٦>",
"فَلَN<مَّ>ا R<رَ>ءَا ٱلۡقَمَR<رَ> بَازِغN<ࣰ>ا قَالَ هَT<ـٰ>ذَا R<رَ>بِّیۖ فَلَN<مَّ>W<اۤ> أَفَلَ قَالَ لَئِX<ن> لَّمۡ یَهۡدِنِی R<رَ>بِّی لَأَكُونَN<نَّ> مِنَ ٱلۡقَوۡمِ ٱX<ل>ضَّL<اۤ>لِّJ<ی>نَ A<۝>D<٧٧>",
"فَلَN<مَّ>ا R<رَ>ءَا ٱX<ل>شَّمۡسَ بَازِغَةN<ࣰ> قَالَ هَT<ـٰ>ذَا R<رَ>بِّی هَT<ـٰ>ذَW<اۤ> أَكۡبَR<رُ>ۖ فَلَN<مَّ>W<اۤ> أَفَلَتۡ قَالَ یَT<ـٰ>قَوۡمِ إِN<نِّ>ی بَرِW<یۤ>ءX<ࣱ> N<مِّ>N<مَّ>ا تُشۡرِكُJ<و>نَ A<۝>D<٧٨>",
"إِN<نِّ>ی وَجَّهۡتُ وَQ<جۡ>هِیَ لِلَّذِی فَطَR<رَ> ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ حَنِیفX<ࣰ>اۖ N<وَ>مَW<اۤ> أَنَا۠ مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<٧٩>",
"وَحَL<اۤ>جَّهُT<ۥ> قَوۡمُهُT<ۥ>ۚ قَالَ أَتُحَL<ـٰۤ>جُّL<وۤ>N<نِّ>ی فِی ٱللَّهِ وَقَQ<دۡ> هَدَX<ى>T<ٰ>نِۚ وَلَW<اۤ> أَخَافُ مَا تُشۡرِكُونَ بِهِW<ۦۤ> إِلَّW<اۤ> أَX<ن> N<یَ>شَW<اۤ>ءَ R<رَ>بِّی شَیۡءX<ࣰ>اۚ N<وَ>سِعَ R<رَ>بِّی كُلَّ شَیۡءٍ عِلۡمًاۚ أَفَلَا تَتَذَكَّR<رُ>J<و>نَ A<۝>D<٨٠>",
"وَكَیۡفَ أَخَافُ مَW<اۤ> أَشۡR<رَ>كۡتُمۡ وَلَا تَخَافُونَ أَN<نَّ>كُمۡ أَشۡR<رَ>كۡتُN<م> بِٱللَّهِ مَا لَمۡ یُنَزِّلۡ بِهِT<ۦ> عَلَیۡكُمۡ سُلۡطَT<ـٰ>نN<ࣰ>اۚ فَأَیُّ ٱلۡفَرِیقَیۡنِ أَحَقُّ بِX<ٱ>لۡأَمۡنِۖ إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٨١>",
"ٱلَّذِینَ ءَامَنُوX<ا۟> وَلَمۡ یَلۡبِسُW<وۤ>X<ا۟> إِیمَT<ـٰ>نَهُN<م> بِظُلۡمٍ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمُ ٱلۡأَمۡنُ وَهُم N<مُّ>هۡتَدُJ<و>نَ A<۝>D<٨٢>",
"وَتِلۡكَ حُجَّتُنَW<اۤ> ءَاتَیۡنَT<ـٰ>هَW<اۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ عَلَىٰ قَوۡمِهِT<ۦ>ۚ نَR<رۡ>فَعُ دَR<رَ>جَT<ـٰ>تX<ࣲ> N<مَّ>ن N<نَّ>شَW<اۤ>ءُۗ إِN<نَّ> R<رَ>بَّكَ حَكِیمٌ عَلِJ<ی>مࣱ A<۝>D<٨٣>",
"وَوَهَQ<بۡ>نَا لَهُW<ۥۤ> إِسۡحَT<ـٰ>قَ وَیَعۡقُوبَۚ كُلًّا هَدَیۡنَاۚ وَنُوحًا هَدَیۡنَا مِN<ن> قَQ<بۡ>لُۖ وَمِN<ن> ذُرِّیَّتِهِT<ۦ> دَاوُT<ۥ>دَ وَسُلَیۡمَT<ـٰ>نَ وَأَیُّوبَ وَیُوسُفَ وَمُوسَىٰ وَهَT<ـٰ>R<رُ>ونَۚ وَكَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٨٤>",
"وَزَكَرِیَّا وَیَحۡیَىٰ وَعِیسَىٰ وَإِلۡیَاسَۖ كُلّX<ࣱ> N<مِّ>نَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٨٥>",
"وَإِسۡمَT<ـٰ>عِیلَ وَX<ٱ>لۡیَسَعَ وَیُونُسَ وَلُوطX<ࣰ>اۚ N<وَ>كُلّN<ࣰ>ا فَضَّلۡنَا عَلَى ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٨٦>",
"وَمِنۡ ءَابَW<اۤ>ئِهِمۡ وَذُرِّیَّT<ـٰ>تِهِمۡ وَإِخۡوَT< ٰ>نِهِمۡۖ وَX<ٱ>Q<جۡ>تَبَیۡنَT<ـٰ>هُمۡ وَهَدَیۡنَT<ـٰ>هُمۡ إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٨٧>",
"ذَT< ٰ>لِكَ هُدَى ٱللَّهِ یَهۡدِی بِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِT<ۦ>ۚ وَلَوۡ أَشۡR<رَ>كُوX<ا۟> لَحَبِطَ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٨٨>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ ءَاتَیۡنَT<ـٰ>هُمُ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحُكۡمَ وَX<ٱ>X<ل>N<نُّ>بُوَّةَۚ فَإِX<ن> N<یَ>كۡفُR<رۡ> بِهَا هَW<ـٰۤ>ؤُلَW<اۤ>ءِ فَقَQ<دۡ> وَكَّلۡنَا بِهَا قَوۡمX<ࣰ>ا لَّیۡسُوX<ا۟> بِهَا بِكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٨٩>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ هَدَى ٱللَّهُۖ فَبِهُدَX<ى>T<ٰ>هُمُ ٱQ<قۡ>تَدِهۡۗ قُل لَّW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ أَQ<جۡ>R<ر>ًاۖ إِنۡ هُوَ إِلَّا ذِكۡR<رَ>ىٰ لِلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٩٠>",
"وَمَا قَدَR<رُ>وX<ا۟> ٱللَّهَ حَقَّ قَQ<دۡ>رِهِW<ۦۤ> إِذۡ قَالُوX<ا۟> مَW<اۤ> أَN<ن>زَلَ ٱللَّهُ عَلَىٰ بَشَرX<ࣲ> N<مِّ>N<ن> شَیۡءN<ࣲ>ۗ قُلۡ مَنۡ أَN<ن>زَلَ ٱلۡكِتَT<ـٰ>بَ ٱلَّذِی جَW<اۤ>ءَ بِهِT<ۦ> مُوسَىٰ نُوR<ر>X<ࣰ>ا N<وَ>هُدX<ࣰ>ى لِّX<ل>N<نَّ>اسِۖ تَQ<جۡ>عَلُونَهُT<ۥ> قَR<رَ>اطِیسَ تُQ<بۡ>دُونَهَا وَتُخۡفُونَ كَثِیR<ر>X<ࣰ>اۖ N<وَ>عُلِّمۡتُم N<مَّ>ا لَمۡ تَعۡلَمُW<وۤ>X<ا۟> أَN<ن>تُمۡ وَلَW<اۤ> ءَابَW<اۤ>ؤُكُمۡۖ قُلِ ٱللَّهُۖ ثُN<مَّ> ذَR<رۡ>هُمۡ فِی خَوۡضِهِمۡ یَلۡعَبُJ<و>نَ A<۝>D<٩١>",
"وَهَT<ـٰ>ذَا كِتَT<ـٰ>بٌ أَN<ن>زَلۡنَT<ـٰ>هُ مُبَاR<رَ>كX<ࣱ> N<مُّ>صَدِّقُ ٱلَّذِی بَیۡنَ یَدَیۡهِ وَلِتُN<ن>ذِR<رَ> أُN<مَّ> ٱلۡقُR<رَ>ىٰ وَمَنۡ حَوۡلَهَاۚ وَX<ٱ>لَّذِینَ یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ یُؤۡمِنُونَ بِهِT<ۦ>ۖ وَهُمۡ عَلَىٰ صَلَاتِهِمۡ یُحَافِظُJ<و>نَ A<۝>D<٩٢>",
"وَمَنۡ أَظۡلَمُ مِN<مَّ>نِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبًا أَوۡ قَالَ أُوحِیَ إِلَیَّ وَلَمۡ یُوحَ إِلَیۡهِ شَیۡءX<ࣱ> N<وَ>مَN<ن> قَالَ سَأُN<ن>زِلُ مِثۡلَ مَW<اۤ> أَN<ن>زَلَ ٱللَّهُۗ وَلَوۡ تَR<رَ>ىW<ٰۤ> إِذِ ٱX<ل>ظَّT<ـٰ>لِمُونَ فِی غَمَR<رَ>T< ٰ>تِ ٱلۡمَوۡتِ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ بَاسِطُW<وۤ>X<ا۟> أَیۡدِیهِمۡ أَخۡرِجُW<وۤ>X<ا۟> أَN<ن>فُسَكُمُۖ ٱلۡیَوۡمَ تُQ<جۡ>زَوۡنَ عَذَابَ ٱلۡهُونِ بِمَا كُN<ن>تُمۡ تَقُولُونَ عَلَى ٱللَّهِ غَیۡR<رَ> ٱلۡحَقِّ وَكُN<ن>تُمۡ عَنۡ ءَایَT<ـٰ>تِهِT<ۦ> تَسۡتَكۡبِR<رُ>J<و>نَ A<۝>D<٩٣>",
"وَلَقَQ<دۡ> جِئۡتُمُونَا فُR<رَ>T< ٰ>دَىٰ كَمَا خَلَQ<قۡ>نَT<ـٰ>كُمۡ أَوَّلَ مَR<رَّ>ةX<ࣲ> N<وَ>تَR<رَ>كۡتُم N<مَّ>ا خَوَّلۡنَT<ـٰ>كُمۡ وَR<رَ>W<اۤ>ءَ ظُهُورِكُمۡۖ وَمَا نَR<رَ>ىٰ مَعَكُمۡ شُفَعَW<اۤ>ءَكُمُ ٱلَّذِینَ زَعَمۡتُمۡ أَN<نَّ>هُمۡ فِیكُمۡ شُR<رَ>كَW<ـٰۤ>ؤُX<ا۟>ۚ لَقَد تَّقَطَّعَ بَیۡنَكُمۡ وَضَلَّ عَN<ن>كُم N<مَّ>ا كُN<ن>تُمۡ تَزۡعُمُJ<و>نَ A<۝>D<٩٤>",
"۞ إِN<نَّ> ٱللَّهَ فَالِقُ ٱلۡحَبِّ وَX<ٱ>X<ل>N<نَّ>وَىٰۖ یُخۡرِجُ ٱلۡحَیَّ مِنَ ٱلۡمَیِّتِ وَمُخۡرِجُ ٱلۡمَیِّتِ مِنَ ٱلۡحَیِّۚ ذَT< ٰ>لِكُمُ ٱللَّهُۖ فَأَN<نَّ>ىٰ تُؤۡفَكُJ<و>نَ A<۝>D<٩٥>",
"فَالِقُ ٱلۡإِصۡبَاحِ وَجَعَلَ ٱلَّیۡلَ سَكَنX<ࣰ>ا N<وَ>X<ٱ>X<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ> حُسۡبَانN<ࣰ>اۚ ذَT< ٰ>لِكَ تَQ<قۡ>دِیR<رُ> ٱلۡعَزِیزِ ٱلۡعَلِJ<ی>مِ A<۝>D<٩٦>",
"وَهُوَ ٱلَّذِی جَعَلَ لَكُمُ ٱX<ل>N<نُّ>جُومَ لِتَهۡتَدُوX<ا۟> بِهَا فِی ظُلُمَT<ـٰ>تِ ٱلۡبَرِّ وَX<ٱ>لۡبَحۡرِۗ قَQ<دۡ> فَصَّلۡنَا ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یَ>عۡلَمُJ<و>نَ A<۝>D<٩٧>",
"وَهُوَ ٱلَّذِW<یۤ> أَN<ن>شَأَكُم N<مِّ>ن N<نَّ>فۡسX<ࣲ> N<وَ>T< ٰ>حِدَةN<ࣲ> فَمُسۡتَقَR<رّ>X<ࣱ> N<وَ>مُسۡتَوۡدَعN<ࣱ>ۗ قَQ<دۡ> فَصَّلۡنَا ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یَ>فۡقَهُJ<و>نَ A<۝>D<٩٨>",
"وَهُوَ ٱلَّذِW<یۤ> أَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَخۡR<رَ>Q<جۡ>نَا بِهِT<ۦ> نَبَاتَ كُلِّ شَیۡءN<ࣲ> فَأَخۡR<رَ>Q<جۡ>نَا مِنۡهُ خَضِR<ر>X<ࣰ>ا N<نُّ>خۡرِجُ مِنۡهُ حَبّX<ࣰ>ا N<مُّ>تَR<رَ>اكِبX<ࣰ>ا N<وَ>مِنَ ٱX<ل>N<نَّ>خۡلِ مِN<ن> طَلۡعِهَا قِنۡوَانN<ࣱ> دَانِیَةX<ࣱ> N<وَ>جَN<نَّ>T<ـٰ>تX<ࣲ> N<مِّ>نۡ أَعۡنَابX<ࣲ> N<وَ>X<ٱ>X<ل>زَّیۡتُونَ وَX<ٱ>X<ل>R<رُّ>N<مَّ>انَ مُشۡتَبِهX<ࣰ>ا N<وَ>غَیۡR<رَ> مُتَشَT<ـٰ>بِهٍۗ ٱN<ن>ظُR<رُ>W<وۤ>X<ا۟> إِلَىٰ ثَمَرِهِW<ۦۤ> إِذَW<اۤ> أَثۡمَR<رَ> وَیَنۡعِهِW<ۦۤ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكُمۡ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٩٩>",
"وَجَعَلُوX<ا۟> لِلَّهِ شُR<رَ>كَW<اۤ>ءَ ٱلۡجِN<نَّ> وَخَلَقَهُمۡۖ وَخَR<رَ>قُوX<ا۟> لَهُT<ۥ> بَنِینَ وَبَنَT<ـٰ>تِN<ۭ> بِغَیۡرِ عِلۡمN<ࣲ>ۚ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ> وَتَعَT<ـٰ>لَىٰ عَN<مَّ>ا یَصِفُJ<و>نَ A<۝>D<١٠٠>",
"بَدِیعُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ أَN<نَّ>ىٰ یَكُونُ لَهُT<ۥ> وَلَدX<ࣱ> N<وَ>لَمۡ تَكُX<ن> لَّهُT<ۥ> صَT<ـٰ>حِبَةX<ࣱ>ۖ N<وَ>خَلَقَ كُلَّ شَیۡءX<ࣲ>ۖ N<وَ>هُوَ بِكُلِّ شَیۡءٍ عَلِJ<ی>مࣱ A<۝>D<١٠١>",
"ذَT< ٰ>لِكُمُ ٱللَّهُ R<رَ>بُّكُمۡۖ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ خَT<ـٰ>لِقُ كُلِّ شَیۡءN<ࣲ> فَX<ٱ>عۡبُدُوهُۚ وَهُوَ عَلَىٰ كُلِّ شَیۡءX<ࣲ> N<وَ>كِJ<ی>لࣱ A<۝>D<١٠٢>",
"لَّا تُQ<دۡ>رِكُهُ ٱلۡأَQ<بۡ>صَT<ـٰ>R<رُ> وَهُوَ یُQ<دۡ>رِكُ ٱلۡأَQ<بۡ>صَT<ـٰ>R<رَ>ۖ وَهُوَ ٱX<ل>لَّطِیفُ ٱلۡخَبِJ<ی>رُ A<۝>D<١٠٣>",
"قَQ<دۡ> جَW<اۤ>ءَكُN<م> بَصَW<اۤ>ئِR<رُ> مِX<ن> R<رَّ>بِّكُمۡۖ فَمَنۡ أَQ<بۡ>صَR<رَ> فَلِنَفۡسِهِT<ۦ>ۖ وَمَنۡ عَمِیَ فَعَلَیۡهَاۚ وَمَW<اۤ> أَنَا۠ عَلَیۡكُN<م> بِحَفِJ<ی>ظࣲ A<۝>D<١٠٤>",
"وَكَذَT< ٰ>لِكَ نُصَرِّفُ ٱلۡءَایَT<ـٰ>تِ وَلِیَقُولُوX<ا۟> دَR<رَ>سۡتَ وَلِنُبَیِّنَهُT<ۥ> لِقَوۡمX<ࣲ> N<یَ>عۡلَمُJ<و>نَ A<۝>D<١٠٥>",
"ٱتَّبِعۡ مَW<اۤ> أُوحِیَ إِلَیۡكَ مِX<ن> R<رَّ>بِّكَۖ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ وَأَعۡرِضۡ عَنِ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<١٠٦>",
"وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ مَW<اۤ> أَشۡR<رَ>كُوX<ا۟>ۗ وَمَا جَعَلۡنَT<ـٰ>كَ عَلَیۡهِمۡ حَفِیظX<ࣰ>اۖ N<وَ>مَW<اۤ> أَN<ن>تَ عَلَیۡهِN<م> بِوَكِJ<ی>لࣲ A<۝>D<١٠٧>",
"وَلَا تَسُبُّوX<ا۟> ٱلَّذِینَ یَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ فَیَسُبُّوX<ا۟> ٱللَّهَ عَQ<دۡ>وَN<ۢ>ا بِغَیۡرِ عِلۡمN<ࣲ>ۗ كَذَT< ٰ>لِكَ زَیَّN<نَّ>ا لِكُلِّ أُN<مَّ>ةٍ عَمَلَهُمۡ ثُN<مَّ> إِلَىٰ R<رَ>بِّهِم N<مَّ>R<رۡ>جِعُهُمۡ فَیُنَبِّئُهُN<م> بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٠٨>",
"وَأَQ<قۡ>سَمُوX<ا۟> بِٱللَّهِ جَهۡدَ أَیۡمَT<ـٰ>نِهِمۡ لَئِN<ن> جَW<اۤ>ءَتۡهُمۡ ءَایَةX<ࣱ> لَّیُؤۡمِنُN<نَّ> بِهَاۚ قُلۡ إِN<نَّ>مَا ٱلۡءَایَT<ـٰ>تُ عِN<ن>دَ ٱللَّهِۖ وَمَا یُشۡعِR<رُ>كُمۡ أَN<نَّ>هَW<اۤ> إِذَا جَW<اۤ>ءَتۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<١٠٩>",
"وَنُقَلِّبُ أَفۡءِدَتَهُمۡ وَأَQ<بۡ>صَT<ـٰ>R<رَ>هُمۡ كَمَا لَمۡ یُؤۡمِنُوX<ا۟> بِهِW<ۦۤ> أَوَّلَ مَR<رَّ>ةX<ࣲ> N<وَ>نَذَR<رُ>هُمۡ فِی طُغۡیَT<ـٰ>نِهِمۡ یَعۡمَهُJ<و>نَ A<۝>D<١١٠>",
"۞ وَلَوۡ أَN<نَّ>نَا نَزَّلۡنَW<اۤ> إِلَیۡهِمُ ٱلۡمَلَW<ـٰۤ>ئِكَةَ وَكَلَّمَهُمُ ٱلۡمَوۡتَىٰ وَحَشَR<رۡ>نَا عَلَیۡهِمۡ كُلَّ شَیۡءN<ࣲ> قُبُلX<ࣰ>ا N<مَّ>ا كَانُوX<ا۟> لِیُؤۡمِنُW<وۤ>X<ا۟> إِلَّW<اۤ> أَX<ن> N<یَ>شَW<اۤ>ءَ ٱللَّهُ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ یَQ<جۡ>هَلُJ<و>نَ A<۝>D<١١١>",
"وَكَذَT< ٰ>لِكَ جَعَلۡنَا لِكُلِّ نَبِیٍّ عَدُوّN<ࣰ>ا شَیَT<ـٰ>طِینَ ٱلۡإِN<ن>سِ وَX<ٱ>لۡجِN<نِّ> یُوحِی بَعۡضُهُمۡ إِلَىٰ بَعۡضN<ࣲ> زُخۡR<رُ>فَ ٱلۡقَوۡلِ غُR<رُ>وR<ر>X<ࣰ>اۚ N<وَ>لَوۡ شَW<اۤ>ءَ R<رَ>بُّكَ مَا فَعَلُوهُۖ فَذَR<رۡ>هُمۡ وَمَا یَفۡتَR<رُ>J<و>نَ A<۝>D<١١٢>",
"وَلِتَصۡغَىW<ٰۤ> إِلَیۡهِ أَفۡءِدَةُ ٱلَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ وَلِیَR<رۡ>ضَوۡهُ وَلِیَQ<قۡ>تَرِفُوX<ا۟> مَا هُم N<مُّ>Q<قۡ>تَرِفُJ<و>نَ A<۝>D<١١٣>",
"أَفَغَیۡR<رَ> ٱللَّهِ أَQ<بۡ>تَغِی حَكَمX<ࣰ>ا N<وَ>هُوَ ٱلَّذِW<یۤ> أَN<ن>زَلَ إِلَیۡكُمُ ٱلۡكِتَT<ـٰ>بَ مُفَصَّلX<ࣰ>اۚ N<وَ>X<ٱ>لَّذِینَ ءَاتَیۡنَT<ـٰ>هُمُ ٱلۡكِتَT<ـٰ>بَ یَعۡلَمُونَ أَN<نَّ>هُT<ۥ> مُنَزَّلX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكَ بِX<ٱ>لۡحَقِّۖ فَلَا تَكُونَN<نَّ> مِنَ ٱلۡمُمۡتَرِJ<ی>نَ A<۝>D<١١٤>",
"وَتَN<مَّ>تۡ كَلِمَتُ R<رَ>بِّكَ صِQ<دۡ>قX<ࣰ>ا N<وَ>عَQ<دۡ>لX<ࣰ>اۚ لَّا مُبَدِّلَ لِكَلِمَT<ـٰ>تِهِT<ۦ>ۚ وَهُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<١١٥>",
"وَإِN<ن> تُطِعۡ أَكۡثَR<رَ> مَN<ن> فِی ٱلۡأَR<رۡ>ضِ یُضِلُّوكَ عَN<ن> سَبِیلِ ٱللَّهِۚ إِX<ن> N<یَ>تَّبِعُونَ إِلَّا ٱX<ل>ظَّN<نَّ> وَإِنۡ هُمۡ إِلَّا یَخۡR<رُ>صُJ<و>نَ A<۝>D<١١٦>",
"إِN<نَّ> R<رَ>بَّكَ هُوَ أَعۡلَمُ مَX<ن> N<یَ>ضِلُّ عَN<ن> سَبِیلِهِT<ۦ>ۖ وَهُوَ أَعۡلَمُ بِX<ٱ>لۡمُهۡتَدِJ<ی>نَ A<۝>D<١١٧>",
"فَكُلُوX<ا۟> مِN<مَّ>ا ذُكِR<رَ> ٱسۡمُ ٱللَّهِ عَلَیۡهِ إِN<ن> كُN<ن>تُN<م> بِءَایَT<ـٰ>تِهِT<ۦ> مُؤۡمِنِJ<ی>نَ A<۝>D<١١٨>",
"وَمَا لَكُمۡ أَلَّا تَأۡكُلُوX<ا۟> مِN<مَّ>ا ذُكِR<رَ> ٱسۡمُ ٱللَّهِ عَلَیۡهِ وَقَQ<دۡ> فَصَّلَ لَكُم N<مَّ>ا حَR<رَّ>مَ عَلَیۡكُمۡ إِلَّا مَا ٱضۡطُرِرۡتُمۡ إِلَیۡهِۗ وَإِN<نَّ> كَثِیR<ر>X<ࣰ>ا لَّیُضِلُّونَ بِأَهۡوَW<اۤ>ئِهِN<م> بِغَیۡرِ عِلۡمٍۚ إِN<نَّ> R<رَ>بَّكَ هُوَ أَعۡلَمُ بِX<ٱ>لۡمُعۡتَدِJ<ی>نَ A<۝>D<١١٩>",
"وَذَR<رُ>وX<ا۟> ظَT<ـٰ>هِR<رَ> ٱلۡإِثۡمِ وَبَاطِنَهُW<ۥۤ>ۚ إِN<نَّ> ٱلَّذِینَ یَكۡسِبُونَ ٱلۡإِثۡمَ سَیُQ<جۡ>زَوۡنَ بِمَا كَانُوX<ا۟> یَQ<قۡ>تَرِفُJ<و>نَ A<۝>D<١٢٠>",
"وَلَا تَأۡكُلُوX<ا۟> مِN<مَّ>ا لَمۡ یُذۡكَرِ ٱسۡمُ ٱللَّهِ عَلَیۡهِ وَإِN<نَّ>هُT<ۥ> لَفِسۡقX<ࣱ>ۗ N<وَ>إِN<نَّ> ٱX<ل>شَّیَT<ـٰ>طِینَ لَیُوحُونَ إِلَىW<ٰۤ> أَوۡلِیَW<اۤ>ئِهِمۡ لِیُجَT<ـٰ>دِلُوكُمۡۖ وَإِنۡ أَطَعۡتُمُوهُمۡ إِN<نَّ>كُمۡ لَمُشۡرِكُJ<و>نَ A<۝>D<١٢١>",
"أَوَ مَN<ن> كَانَ مَیۡتN<ࣰ>ا فَأَحۡیَیۡنَT<ـٰ>هُ وَجَعَلۡنَا لَهُT<ۥ> نُوR<ر>X<ࣰ>ا N<یَ>مۡشِی بِهِT<ۦ> فِی ٱX<ل>N<نَّ>اسِ كَمَX<ن> N<مَّ>ثَلُهُT<ۥ> فِی ٱX<ل>ظُّلُمَT<ـٰ>تِ لَیۡسَ بِخَارِجX<ࣲ> N<مِّ>نۡهَاۚ كَذَT< ٰ>لِكَ زُیِّنَ لِلۡكَT<ـٰ>فِرِینَ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٢٢>",
"وَكَذَT< ٰ>لِكَ جَعَلۡنَا فِی كُلِّ قَR<رۡ>یَةٍ أَكَT<ـٰ>بِR<رَ> مُQ<جۡ>رِمِیهَا لِیَمۡكُR<رُ>وX<ا۟> فِیهَاۖ وَمَا یَمۡكُR<رُ>ونَ إِلَّا بِأَN<ن>فُسِهِمۡ وَمَا یَشۡعُR<رُ>J<و>نَ A<۝>D<١٢٣>",
"وَإِذَا جَW<اۤ>ءَتۡهُمۡ ءَایَةN<ࣱ> قَالُوX<ا۟> لَن N<نُّ>ؤۡمِنَ حَتَّىٰ نُؤۡتَىٰ مِثۡلَ مَW<اۤ> أُوتِیَ R<رُ>سُلُ ٱللَّهِۘ ٱللَّهُ أَعۡلَمُ حَیۡثُ یَQ<جۡ>عَلُ رِسَالَتَهُT<ۥ>ۗ سَیُصِیبُ ٱلَّذِینَ أَQ<جۡ>R<رَ>مُوX<ا۟> صَغَاR<ر>ٌ عِN<ن>دَ ٱللَّهِ وَعَذَابN<ࣱ> شَدِیدُN<ۢ> بِمَا كَانُوX<ا۟> یَمۡكُR<رُ>J<و>نَ A<۝>D<١٢٤>",
"فَمَX<ن> N<یُ>رِدِ ٱللَّهُ أَX<ن> N<یَ>هۡدِیَهُT<ۥ> یَشۡR<رَ>حۡ صَQ<دۡ>R<رَ>هُT<ۥ> لِلۡإِسۡلَT<ـٰ>مِۖ وَمَX<ن> N<یُ>رِQ<دۡ> أَX<ن> N<یُ>ضِلَّهُT<ۥ> یَQ<جۡ>عَلۡ صَQ<دۡ>R<رَ>هُT<ۥ> ضَیِّقًا حَR<رَ>جN<ࣰ>ا كَأَN<نَّ>مَا یَصَّعَّدُ فِی ٱX<ل>سَّمَW<اۤ>ءِۚ كَذَT< ٰ>لِكَ یَQ<جۡ>عَلُ ٱللَّهُ ٱX<ل>رِّQ<جۡ>سَ عَلَى ٱلَّذِینَ لَا یُؤۡمِنُJ<و>نَ A<۝>D<١٢٥>",
"وَهَT<ـٰ>ذَا صِR<رَ>T< ٰ>طُ R<رَ>بِّكَ مُسۡتَقِیمN<ࣰ>اۗ قَQ<دۡ> فَصَّلۡنَا ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یَ>ذَّكَّR<رُ>J<و>نَ A<۝>D<١٢٦>",
"۞ لَهُمۡ دَاR<رُ> ٱX<ل>سَّلَT<ـٰ>مِ عِN<ن>دَ R<رَ>بِّهِمۡۖ وَهُوَ وَلِیُّهُN<م> بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٢٧>",
"وَیَوۡمَ یَحۡشُR<رُ>هُمۡ جَمِیعX<ࣰ>ا N<یَ>T<ـٰ>مَعۡشَR<رَ> ٱلۡجِN<نِّ> قَدِ ٱسۡتَكۡثَR<رۡ>تُم N<مِّ>نَ ٱلۡإِN<ن>سِۖ وَقَالَ أَوۡلِیَW<اۤ>ؤُهُم N<مِّ>نَ ٱلۡإِN<ن>سِ R<رَ>بَّنَا ٱسۡتَمۡتَعَ بَعۡضُنَا بِبَعۡضX<ࣲ> N<وَ>بَلَغۡنَW<اۤ> أَجَلَنَا ٱلَّذِW<یۤ> أَجَّلۡتَ لَنَاۚ قَالَ ٱX<ل>N<نَّ>اR<رُ> مَثۡوَX<ى>T<ٰ>كُمۡ خَT<ـٰ>لِدِینَ فِیهَW<اۤ> إِلَّا مَا شَW<اۤ>ءَ ٱللَّهُۗ إِN<نَّ> R<رَ>بَّكَ حَكِیمٌ عَلِJ<ی>مࣱ A<۝>D<١٢٨>",
"وَكَذَT< ٰ>لِكَ نُوَلِّی بَعۡضَ ٱX<ل>ظَّT<ـٰ>لِمِینَ بَعۡضَN<ۢ>ا بِمَا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<١٢٩>",
"یَT<ـٰ>مَعۡشَR<رَ> ٱلۡجِN<نِّ> وَX<ٱ>لۡإِN<ن>سِ أَلَمۡ یَأۡتِكُمۡ R<رُ>سُلX<ࣱ> N<مِّ>N<ن>كُمۡ یَقُصُّونَ عَلَیۡكُمۡ ءَایَT<ـٰ>تِی وَیُN<ن>ذِR<رُ>ونَكُمۡ لِقَW<اۤ>ءَ یَوۡمِكُمۡ هَT<ـٰ>ذَاۚ قَالُوX<ا۟> شَهِQ<دۡ>نَا عَلَىW<ٰۤ> أَN<ن>فُسِنَاۖ وَغَR<رَّ>تۡهُمُ ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَا وَشَهِدُوX<ا۟> عَلَىW<ٰۤ> أَN<ن>فُسِهِمۡ أَN<نَّ>هُمۡ كَانُوX<ا۟> كَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٣٠>",
"ذَT< ٰ>لِكَ أَX<ن> لَّمۡ یَكُX<ن> R<رَّ>بُّكَ مُهۡلِكَ ٱلۡقُR<رَ>ىٰ بِظُلۡمX<ࣲ> N<وَ>أَهۡلُهَا غَT<ـٰ>فِلُJ<و>نَ A<۝>D<١٣١>",
"وَلِكُلّN<ࣲ> دَR<رَ>جَT<ـٰ>تX<ࣱ> N<مِّ>N<مَّ>ا عَمِلُوX<ا۟>ۚ وَمَا R<رَ>بُّكَ بِغَT<ـٰ>فِلٍ عَN<مَّ>ا یَعۡمَلُJ<و>نَ A<۝>D<١٣٢>",
"وَR<رَ>بُّكَ ٱلۡغَنِیُّ ذُو ٱX<ل>R<رَّ>حۡمَةِۚ إِX<ن> N<یَ>شَأۡ یُذۡهِQ<بۡ>كُمۡ وَیَسۡتَخۡلِفۡ مِX<ن>N<ۢ> بَعۡدِكُم N<مَّ>ا یَشَW<اۤ>ءُ كَمَW<اۤ> أَN<ن>شَأَكُم N<مِّ>N<ن> ذُرِّیَّةِ قَوۡمٍ ءَاخَرِJ<ی>نَ A<۝>D<١٣٣>",
"إِN<نَّ> مَا تُوعَدُونَ لَءَاتX<ࣲ>ۖ N<وَ>مَW<اۤ> أَN<ن>تُN<م> بِمُعۡجِزِJ<ی>نَ A<۝>D<١٣٤>",
"قُلۡ یَT<ـٰ>قَوۡمِ ٱعۡمَلُوX<ا۟> عَلَىٰ مَكَانَتِكُمۡ إِN<نِّ>ی عَامِلN<ࣱ>ۖ فَسَوۡفَ تَعۡلَمُونَ مَN<ن> تَكُونُ لَهُT<ۥ> عَT<ـٰ>قِبَةُ ٱX<ل>دَّارِۚ إِN<نَّ>هُT<ۥ> لَا یُفۡلِحُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<١٣٥>",
"وَجَعَلُوX<ا۟> لِلَّهِ مِN<مَّ>ا ذَR<رَ>أَ مِنَ ٱلۡحَR<رۡ>ثِ وَX<ٱ>لۡأَنۡعَT<ـٰ>مِ نَصِیبN<ࣰ>ا فَقَالُوX<ا۟> هَT<ـٰ>ذَا لِلَّهِ بِزَعۡمِهِمۡ وَهَT<ـٰ>ذَا لِشُR<رَ>كَW<اۤ>ئِنَاۖ فَمَا كَانَ لِشُR<رَ>كَW<اۤ>ئِهِمۡ فَلَا یَصِلُ إِلَى ٱللَّهِۖ وَمَا كَانَ لِلَّهِ فَهُوَ یَصِلُ إِلَىٰ شُR<رَ>كَW<اۤ>ئِهِمۡۗ سَW<اۤ>ءَ مَا یَحۡكُمُJ<و>نَ A<۝>D<١٣٦>",
"وَكَذَT< ٰ>لِكَ زَیَّنَ لِكَثِیرX<ࣲ> N<مِّ>نَ ٱلۡمُشۡرِكِینَ قَتۡلَ أَوۡلَT<ـٰ>دِهِمۡ شُR<رَ>كَW<اۤ>ؤُهُمۡ لِیُR<رۡ>دُوهُمۡ وَلِیَلۡبِسُوX<ا۟> عَلَیۡهِمۡ دِینَهُمۡۖ وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ مَا فَعَلُوهُۖ فَذَR<رۡ>هُمۡ وَمَا یَفۡتَR<رُ>J<و>نَ A<۝>D<١٣٧>",
"وَقَالُوX<ا۟> هَT<ـٰ>ذِهِW<ۦۤ> أَنۡعَT<ـٰ>مX<ࣱ> N<وَ>حَR<رۡ>ثٌ حِQ<جۡ>R<ر>X<ࣱ> لَّا یَQ<طۡ>عَمُهَW<اۤ> إِلَّا مَن N<نَّ>شَW<اۤ>ءُ بِزَعۡمِهِمۡ وَأَنۡعَT<ـٰ>مٌ حُرِّمَتۡ ظُهُوR<رُ>هَا وَأَنۡعَT<ـٰ>مX<ࣱ> لَّا یَذۡكُR<رُ>ونَ ٱسۡمَ ٱللَّهِ عَلَیۡهَا ٱفۡتِR<رَ>W<اۤ>ءً عَلَیۡهِۚ سَیَQ<جۡ>زِیهِN<م> بِمَا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<١٣٨>",
"وَقَالُوX<ا۟> مَا فِی بُطُونِ هَT<ـٰ>ذِهِ ٱلۡأَنۡعَT<ـٰ>مِ خَالِصَةX<ࣱ> لِّذُكُورِنَا وَمُحَR<رَّ>مٌ عَلَىW<ٰۤ> أَزۡوَT< ٰ>جِنَاۖ وَإِX<ن> N<یَ>كُX<ن> N<مَّ>یۡتَةN<ࣰ> فَهُمۡ فِیهِ شُR<رَ>كَW<اۤ>ءُۚ سَیَQ<جۡ>زِیهِمۡ وَصۡفَهُمۡۚ إِN<نَّ>هُT<ۥ> حَكِیمٌ عَلِJ<ی>مࣱ A<۝>D<١٣٩>",
"قَQ<دۡ> خَسِR<رَ> ٱلَّذِینَ قَتَلُW<وۤ>X<ا۟> أَوۡلَT<ـٰ>دَهُمۡ سَفَهَN<ۢ>ا بِغَیۡرِ عِلۡمX<ࣲ> N<وَ>حَR<رَّ>مُوX<ا۟> مَا R<رَ>زَقَهُمُ ٱللَّهُ ٱفۡتِR<رَ>W<اۤ>ءً عَلَى ٱللَّهِۚ قَQ<دۡ> ضَلُّوX<ا۟> وَمَا كَانُوX<ا۟> مُهۡتَدِJ<ی>نَ A<۝>D<١٤٠>",
"۞ وَهُوَ ٱلَّذِW<یۤ> أَN<ن>شَأَ جَN<نَّ>T<ـٰ>تX<ࣲ> N<مَّ>عۡR<رُ>وشَT<ـٰ>تX<ࣲ> N<وَ>غَیۡR<رَ> مَعۡR<رُ>وشَT<ـٰ>تX<ࣲ> N<وَ>X<ٱ>X<ل>N<نَّ>خۡلَ وَX<ٱ>X<ل>زَّR<رۡ>عَ مُخۡتَلِفًا أُكُلُهُT<ۥ> وَX<ٱ>X<ل>زَّیۡتُونَ وَX<ٱ>X<ل>R<رُّ>N<مَّ>انَ مُتَشَT<ـٰ>بِهX<ࣰ>ا N<وَ>غَیۡR<رَ> مُتَشَT<ـٰ>بِهN<ࣲ>ۚ كُلُوX<ا۟> مِN<ن> ثَمَرِهِW<ۦۤ> إِذَW<اۤ> أَثۡمَR<رَ> وَءَاتُوX<ا۟> حَقَّهُT<ۥ> یَوۡمَ حَصَادِهِT<ۦ>ۖ وَلَا تُسۡرِفُW<وۤ>X<ا۟>ۚ إِN<نَّ>هُT<ۥ> لَا یُحِبُّ ٱلۡمُسۡرِفِJ<ی>نَ A<۝>D<١٤١>",
"وَمِنَ ٱلۡأَنۡعَT<ـٰ>مِ حَمُولَةX<ࣰ> N<وَ>فَR<رۡ>شN<ࣰ>اۚ كُلُوX<ا۟> مِN<مَّ>ا R<رَ>زَقَكُمُ ٱللَّهُ وَلَا تَتَّبِعُوX<ا۟> خُطُوَT< ٰ>تِ ٱX<ل>شَّیۡطَT<ـٰ>نِۚ إِN<نَّ>هُT<ۥ> لَكُمۡ عَدُوّX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٤٢>",
"ثَمَT<ـٰ>نِیَةَ أَزۡوَT< ٰ>جX<ࣲ>ۖ N<مِّ>نَ ٱX<ل>ضَّأۡنِ ٱثۡنَیۡنِ وَمِنَ ٱلۡمَعۡزِ ٱثۡنَیۡنِۗ قُلۡ ءَL<اۤ>X<ل>ذَّكَR<رَ>یۡنِ حَR<رَّ>مَ أَمِ ٱلۡأُN<ن>ثَیَیۡنِ أَN<مَّ>ا ٱشۡتَمَلَتۡ عَلَیۡهِ أَR<رۡ>حَامُ ٱلۡأُN<ن>ثَیَیۡنِۖ نَبِّءُونِی بِعِلۡمٍ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٤٣>",
"وَمِنَ ٱلۡإِبِلِ ٱثۡنَیۡنِ وَمِنَ ٱلۡبَقَرِ ٱثۡنَیۡنِۗ قُلۡ ءَL<اۤ>X<ل>ذَّكَR<رَ>یۡنِ حَR<رَّ>مَ أَمِ ٱلۡأُN<ن>ثَیَیۡنِ أَN<مَّ>ا ٱشۡتَمَلَتۡ عَلَیۡهِ أَR<رۡ>حَامُ ٱلۡأُN<ن>ثَیَیۡنِۖ أَمۡ كُN<ن>تُمۡ شُهَدَW<اۤ>ءَ إِذۡ وَصَّX<ى>T<ٰ>كُمُ ٱللَّهُ بِهَT<ـٰ>ذَاۚ فَمَنۡ أَظۡلَمُ مِN<مَّ>نِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبX<ࣰ>ا لِّیُضِلَّ ٱX<ل>N<نَّ>اسَ بِغَیۡرِ عِلۡمٍۚ إِN<نَّ> ٱللَّهَ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٤٤>",
"قُل لَّW<اۤ> أَجِدُ فِی مَW<اۤ> أُوحِیَ إِلَیَّ مُحَR<رَّ>مًا عَلَىٰ طَاعِمX<ࣲ> N<یَ>Q<طۡ>عَمُهُW<ۥۤ> إِلَّW<اۤ> أَX<ن> N<یَ>كُونَ مَیۡتَةً أَوۡ دَمX<ࣰ>ا N<مَّ>سۡفُوحًا أَوۡ لَحۡمَ خِN<ن>زِیرN<ࣲ> فَإِN<نَّ>هُT<ۥ> رِQ<جۡ>سٌ أَوۡ فِسۡقًا أُهِلَّ لِغَیۡرِ ٱللَّهِ بِهِT<ۦ>ۚ فَمَنِ ٱضۡطُR<رَّ> غَیۡR<رَ> بَاغX<ࣲ> N<وَ>لَا عَادN<ࣲ> فَإِN<نَّ> R<رَ>بَّكَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٤٥>",
"وَعَلَى ٱلَّذِینَ هَادُوX<ا۟> حَR<رَّ>مۡنَا كُلَّ ذِی ظُفُرX<ࣲ>ۖ N<وَ>مِنَ ٱلۡبَقَرِ وَX<ٱ>لۡغَنَمِ حَR<رَّ>مۡنَا عَلَیۡهِمۡ شُحُومَهُمَW<اۤ> إِلَّا مَا حَمَلَتۡ ظُهُوR<رُ>هُمَW<اۤ> أَوِ ٱلۡحَوَایَW<اۤ> أَوۡ مَا ٱخۡتَلَطَ بِعَظۡمN<ࣲ>ۚ ذَT< ٰ>لِكَ جَزَیۡنَT<ـٰ>هُN<م> بِبَغۡیِهِمۡۖ وَإِN<نَّ>ا لَصَT<ـٰ>دِقُJ<و>نَ A<۝>D<١٤٦>",
"فَإِN<ن> كَذَّبُوكَ فَقُل R<رَّ>بُّكُمۡ ذُو R<رَ>حۡمَةX<ࣲ> N<وَ>T< ٰ>سِعَةX<ࣲ> N<وَ>لَا یُR<رَ>دُّ بَأۡسُهُT<ۥ> عَنِ ٱلۡقَوۡمِ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<١٤٧>",
"سَیَقُولُ ٱلَّذِینَ أَشۡR<رَ>كُوX<ا۟> لَوۡ شَW<اۤ>ءَ ٱللَّهُ مَW<اۤ> أَشۡR<رَ>كۡنَا وَلَW<اۤ> ءَابَW<اۤ>ؤُنَا وَلَا حَR<رَّ>مۡنَا مِN<ن> شَیۡءN<ࣲ>ۚ كَذَT< ٰ>لِكَ كَذَّبَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ حَتَّىٰ ذَاقُوX<ا۟> بَأۡسَنَاۗ قُلۡ هَلۡ عِN<ن>دَكُم N<مِّ>نۡ عِلۡمN<ࣲ> فَتُخۡرِجُوهُ لَنَW<اۤ>ۖ إِN<ن> تَتَّبِعُونَ إِلَّا ٱX<ل>ظَّN<نَّ> وَإِنۡ أَN<ن>تُمۡ إِلَّا تَخۡR<رُ>صُJ<و>نَ A<۝>D<١٤٨>",
"قُلۡ فَلِلَّهِ ٱلۡحُجَّةُ ٱلۡبَT<ـٰ>لِغَةُۖ فَلَوۡ شَW<اۤ>ءَ لَهَدَX<ى>T<ٰ>كُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<١٤٩>",
"قُلۡ هَلُN<مَّ> شُهَدَW<اۤ>ءَكُمُ ٱلَّذِینَ یَشۡهَدُونَ أَN<نَّ> ٱللَّهَ حَR<رَّ>مَ هَT<ـٰ>ذَاۖ فَإِN<ن> شَهِدُوX<ا۟> فَلَا تَشۡهَQ<دۡ> مَعَهُمۡۚ وَلَا تَتَّبِعۡ أَهۡوَW<اۤ>ءَ ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا وَX<ٱ>لَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ وَهُN<م> بِR<رَ>بِّهِمۡ یَعۡدِلُJ<و>نَ A<۝>D<١٥٠>",
"۞ قُلۡ تَعَالَوۡX<ا۟> أَتۡلُ مَا حَR<رَّ>مَ R<رَ>بُّكُمۡ عَلَیۡكُمۡۖ أَلَّا تُشۡرِكُوX<ا۟> بِهِT<ۦ> شَیۡءX<ࣰ>اۖ N<وَ>بِX<ٱ>لۡوَT< ٰ>لِدَیۡنِ إِحۡسَT<ـٰ>نX<ࣰ>اۖ N<وَ>لَا تَQ<قۡ>تُلُW<وۤ>X<ا۟> أَوۡلَT<ـٰ>دَكُم N<مِّ>نۡ إِمۡلَT<ـٰ>قX<ࣲ> N<نَّ>حۡنُ نَR<رۡ>زُقُكُمۡ وَإِیَّاهُمۡۖ وَلَا تَQ<قۡ>R<رَ>بُوX<ا۟> ٱلۡفَوَT< ٰ>حِشَ مَا ظَهَR<رَ> مِنۡهَا وَمَا بَطَنَۖ وَلَا تَQ<قۡ>تُلُوX<ا۟> ٱX<ل>N<نَّ>فۡسَ ٱلَّتِی حَR<رَّ>مَ ٱللَّهُ إِلَّا بِX<ٱ>لۡحَقِّۚ ذَT< ٰ>لِكُمۡ وَصَّX<ى>T<ٰ>كُN<م> بِهِT<ۦ> لَعَلَّكُمۡ تَعۡقِلُJ<و>نَ A<۝>D<١٥١>",
"وَلَا تَQ<قۡ>R<رَ>بُوX<ا۟> مَالَ ٱلۡیَتِیمِ إِلَّا بِX<ٱ>لَّتِی هِیَ أَحۡسَنُ حَتَّىٰ یَQ<بۡ>لُغَ أَشُدَّهُT<ۥ>ۚ وَأَوۡفُوX<ا۟> ٱلۡكَیۡلَ وَX<ٱ>لۡمِیزَانَ بِX<ٱ>لۡقِسۡطِۖ لَا نُكَلِّفُ نَفۡسًا إِلَّا وُسۡعَهَاۖ وَإِذَا قُلۡتُمۡ فَX<ٱ>عۡدِلُوX<ا۟> وَلَوۡ كَانَ ذَا قُR<رۡ>بَىٰۖ وَبِعَهۡدِ ٱللَّهِ أَوۡفُوX<ا۟>ۚ ذَT< ٰ>لِكُمۡ وَصَّX<ى>T<ٰ>كُN<م> بِهِT<ۦ> لَعَلَّكُمۡ تَذَكَّR<رُ>J<و>نَ A<۝>D<١٥٢>",
"وَأَN<نَّ> هَT<ـٰ>ذَا صِR<رَ>T< ٰ>طِی مُسۡتَقِیمN<ࣰ>ا فَX<ٱ>تَّبِعُوهُۖ وَلَا تَتَّبِعُوX<ا۟> ٱX<ل>سُّبُلَ فَتَفَR<رَّ>قَ بِكُمۡ عَN<ن> سَبِیلِهِT<ۦ>ۚ ذَT< ٰ>لِكُمۡ وَصَّX<ى>T<ٰ>كُN<م> بِهِT<ۦ> لَعَلَّكُمۡ تَتَّقُJ<و>نَ A<۝>D<١٥٣>",
"ثُN<مَّ> ءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ تَمَامًا عَلَى ٱلَّذِW<یۤ> أَحۡسَنَ وَتَفۡصِیلX<ࣰ>ا لِّكُلِّ شَیۡءX<ࣲ> N<وَ>هُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣰ> لَّعَلَّهُN<م> بِلِقَW<اۤ>ءِ R<رَ>بِّهِمۡ یُؤۡمِنُJ<و>نَ A<۝>D<١٥٤>",
"وَهَT<ـٰ>ذَا كِتَT<ـٰ>بٌ أَN<ن>زَلۡنَT<ـٰ>هُ مُبَاR<رَ>كN<ࣱ> فَX<ٱ>تَّبِعُوهُ وَX<ٱ>تَّقُوX<ا۟> لَعَلَّكُمۡ تُR<رۡ>حَمُJ<و>نَ A<۝>D<١٥٥>",
"أَN<ن> تَقُولُW<وۤ>X<ا۟> إِN<نَّ>مَW<اۤ> أُN<ن>زِلَ ٱلۡكِتَT<ـٰ>بُ عَلَىٰ طَW<اۤ>ئِفَتَیۡنِ مِN<ن> قَQ<بۡ>لِنَا وَإِN<ن> كُN<نَّ>ا عَN<ن> دِR<رَ>اسَتِهِمۡ لَغَT<ـٰ>فِلِJ<ی>نَ A<۝>D<١٥٦>",
"أَوۡ تَقُولُوX<ا۟> لَوۡ أَN<نَّ>W<اۤ> أُN<ن>زِلَ عَلَیۡنَا ٱلۡكِتَT<ـٰ>بُ لَكُN<نَّ>W<اۤ> أَهۡدَىٰ مِنۡهُمۡۚ فَقَQ<دۡ> جَW<اۤ>ءَكُN<م> بَیِّنَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ وَهُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةN<ࣱ>ۚ فَمَنۡ أَظۡلَمُ مِN<مَّ>N<ن> كَذَّبَ بِءَایَT<ـٰ>تِ ٱللَّهِ وَصَدَفَ عَنۡهَاۗ سَنَQ<جۡ>زِی ٱلَّذِینَ یَصۡدِفُونَ عَنۡ ءَایَT<ـٰ>تِنَا سُW<وۤ>ءَ ٱلۡعَذَابِ بِمَا كَانُوX<ا۟> یَصۡدِفُJ<و>نَ A<۝>D<١٥٧>",
"هَلۡ یَN<ن>ظُR<رُ>ونَ إِلَّW<اۤ> أَN<ن> تَأۡتِیَهُمُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ أَوۡ یَأۡتِیَ R<رَ>بُّكَ أَوۡ یَأۡتِیَ بَعۡضُ ءَایَT<ـٰ>تِ R<رَ>بِّكَۗ یَوۡمَ یَأۡتِی بَعۡضُ ءَایَT<ـٰ>تِ R<رَ>بِّكَ لَا یَN<ن>فَعُ نَفۡسًا إِیمَT<ـٰ>نُهَا لَمۡ تَكُنۡ ءَامَنَتۡ مِN<ن> قَQ<بۡ>لُ أَوۡ كَسَبَتۡ فِW<یۤ> إِیمَT<ـٰ>نِهَا خَیۡR<ر>N<ࣰ>اۗ قُلِ ٱN<ن>تَظِR<رُ>W<وۤ>X<ا۟> إِN<نَّ>ا مُN<ن>تَظِR<رُ>J<و>نَ A<۝>D<١٥٨>",
"إِN<نَّ> ٱلَّذِینَ فَR<رَّ>قُوX<ا۟> دِینَهُمۡ وَكَانُوX<ا۟> شِیَعX<ࣰ>ا لَّسۡتَ مِنۡهُمۡ فِی شَیۡءٍۚ إِN<نَّ>مَW<اۤ> أَمۡR<رُ>هُمۡ إِلَى ٱللَّهِ ثُN<مَّ> یُنَبِّئُهُN<م> بِمَا كَانُوX<ا۟> یَفۡعَلُJ<و>نَ A<۝>D<١٥٩>",
"مَN<ن> جَW<اۤ>ءَ بِX<ٱ>لۡحَسَنَةِ فَلَهُT<ۥ> عَشۡR<رُ> أَمۡثَالِهَاۖ وَمَN<ن> جَW<اۤ>ءَ بِX<ٱ>X<ل>سَّیِّئَةِ فَلَا یُQ<جۡ>زَىW<ٰۤ> إِلَّا مِثۡلَهَا وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<١٦٠>",
"قُلۡ إِN<نَّ>نِی هَدَX<ى>T<ٰ>نِی R<رَ>بِّW<یۤ> إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِیمN<ࣲ> دِینN<ࣰ>ا قِیَمX<ࣰ>ا N<مِّ>لَّةَ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ حَنِیفX<ࣰ>اۚ N<وَ>مَا كَانَ مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<١٦١>",
"قُلۡ إِN<نَّ> صَلَاتِی وَنُسُكِی وَمَحۡیَایَ وَمَمَاتِی لِلَّهِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٦٢>",
"لَا شَرِیكَ لَهُT<ۥ>ۖ وَبِذَT< ٰ>لِكَ أُمِرۡتُ وَأَنَا۠ أَوَّلُ ٱلۡمُسۡلِمِJ<ی>نَ A<۝>D<١٦٣>",
"قُلۡ أَغَیۡR<رَ> ٱللَّهِ أَQ<بۡ>غِی R<رَ>بّX<ࣰ>ا N<وَ>هُوَ R<رَ>بُّ كُلِّ شَیۡءX<ࣲ>ۚ N<وَ>لَا تَكۡسِبُ كُلُّ نَفۡسٍ إِلَّا عَلَیۡهَاۚ وَلَا تَزِR<رُ> وَازِR<رَ>ةX<ࣱ> N<وِ>زۡR<رَ> أُخۡR<رَ>ىٰۚ ثُN<مَّ> إِلَىٰ R<رَ>بِّكُم N<مَّ>R<رۡ>جِعُكُمۡ فَیُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ فِیهِ تَخۡتَلِفُJ<و>نَ A<۝>D<١٦٤>",
"وَهُوَ ٱلَّذِی جَعَلَكُمۡ خَلَW<ـٰۤ>ئِفَ ٱلۡأَR<رۡ>ضِ وَR<رَ>فَعَ بَعۡضَكُمۡ فَوۡقَ بَعۡضN<ࣲ> دَR<رَ>جَT<ـٰ>تX<ࣲ> لِّیَQ<بۡ>لُوَكُمۡ فِی مَW<اۤ> ءَاتَX<ى>T<ٰ>كُمۡۗ إِN<نَّ> R<رَ>بَّكَ سَرِیعُ ٱلۡعِقَابِ وَإِN<نَّ>هُT<ۥ> لَغَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مُۢ A<۝>D<١٦٥>",
"#۞ اL<لۤ>L<مۤ>L<صۤ> A<۝>D<١>",
"كِتَT<ـٰ>بٌ أُN<ن>زِلَ إِلَیۡكَ فَلَا یَكُN<ن> فِی صَQ<دۡ>رِكَ حَR<رَ>جX<ࣱ> N<مِّ>نۡهُ لِتُN<ن>ذِR<رَ> بِهِT<ۦ> وَذِكۡR<رَ>ىٰ لِلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٢>",
"ٱتَّبِعُوX<ا۟> مَW<اۤ> أُN<ن>زِلَ إِلَیۡكُم N<مِّ>X<ن> R<رَّ>بِّكُمۡ وَلَا تَتَّبِعُوX<ا۟> مِN<ن> دُونِهِW<ۦۤ> أَوۡلِیَW<اۤ>ءَۗ قَلِیلX<ࣰ>ا N<مَّ>ا تَذَكَّR<رُ>J<و>نَ A<۝>D<٣>",
"وَكَم N<مِّ>N<ن> قَR<رۡ>یَةٍ أَهۡلَكۡنَT<ـٰ>هَا فَجَW<اۤ>ءَهَا بَأۡسُنَا بَیَT<ـٰ>تًا أَوۡ هُمۡ قَW<اۤ>ئِلُJ<و>نَ A<۝>D<٤>",
"فَمَا كَانَ دَعۡوَX<ى>T<ٰ>هُمۡ إِذۡ جَW<اۤ>ءَهُN<م> بَأۡسُنَW<اۤ> إِلَّW<اۤ> أَN<ن> قَالُW<وۤ>X<ا۟> إِN<نَّ>ا كُN<نَّ>ا ظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥>",
"فَلَنَسۡءَلَN<نَّ> ٱلَّذِینَ أُR<رۡ>سِلَ إِلَیۡهِمۡ وَلَنَسۡءَلَN<نَّ> ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٦>",
"فَلَنَقُصَّN<نَّ> عَلَیۡهِN<م> بِعِلۡمX<ࣲ>ۖ N<وَ>مَا كُN<نَّ>ا غَW<اۤ>ئِبِJ<ی>نَ A<۝>D<٧>",
"وَX<ٱ>لۡوَزۡنُ یَوۡمَئِذٍ ٱلۡحَقُّۚ فَمَN<ن> ثَقُلَتۡ مَوَT< ٰ>زِینُهُT<ۥ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<٨>",
"وَمَنۡ خَفَّتۡ مَوَT< ٰ>زِینُهُT<ۥ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ خَسِR<رُ>W<وۤ>X<ا۟> أَN<ن>فُسَهُN<م> بِمَا كَانُوX<ا۟> بِءَایَT<ـٰ>تِنَا یَظۡلِمُJ<و>نَ A<۝>D<٩>",
"وَلَقَQ<دۡ> مَكَّN<نَّ>T<ـٰ>كُمۡ فِی ٱلۡأَR<رۡ>ضِ وَجَعَلۡنَا لَكُمۡ فِیهَا مَعَT<ـٰ>یِشَۗ قَلِیلX<ࣰ>ا N<مَّ>ا تَشۡكُR<رُ>J<و>نَ A<۝>D<١٠>",
"وَلَقَQ<دۡ> خَلَQ<قۡ>نَT<ـٰ>كُمۡ ثُN<مَّ> صَوَّR<رۡ>نَT<ـٰ>كُمۡ ثُN<مَّ> قُلۡنَا لِلۡمَلَW<ـٰۤ>ئِكَةِ ٱسۡجُدُوX<ا۟> لِءَادَمَ فَسَجَدُW<وۤ>X<ا۟> إِلَّW<اۤ> إِQ<بۡ>لِیسَ لَمۡ یَكُX<ن> N<مِّ>نَ ٱX<ل>سَّT<ـٰ>جِدِJ<ی>نَ A<۝>D<١١>",
"قَالَ مَا مَنَعَكَ أَلَّا تَسۡجُدَ إِذۡ أَمَR<رۡ>تُكَۖ قَالَ أَنَا۠ خَیۡR<ر>X<ࣱ> N<مِّ>نۡهُ خَلَQ<قۡ>تَنِی مِن N<نَّ>ارX<ࣲ> N<وَ>خَلَQ<قۡ>تَهُT<ۥ> مِN<ن> طِJ<ی>نࣲ A<۝>D<١٢>",
"قَالَ فَX<ٱ>هۡبِQ<طۡ> مِنۡهَا فَمَا یَكُونُ لَكَ أَN<ن> تَتَكَبَّR<رَ> فِیهَا فَX<ٱ>خۡR<رُ>Q<جۡ> إِN<نَّ>كَ مِنَ ٱX<ل>صَّT<ـٰ>غِرِJ<ی>نَ A<۝>D<١٣>",
"قَالَ أَN<ن>ظِرۡنِW<یۤ> إِلَىٰ یَوۡمِ یُQ<بۡ>عَثُJ<و>نَ A<۝>D<١٤>",
"قَالَ إِN<نَّ>كَ مِنَ ٱلۡمُN<ن>ظَرِJ<ی>نَ A<۝>D<١٥>",
"قَالَ فَبِمَW<اۤ> أَغۡوَیۡتَنِی لَأَQ<قۡ>عُدَN<نَّ> لَهُمۡ صِR<رَ>T< ٰ>طَكَ ٱلۡمُسۡتَقِJ<ی>مَ A<۝>D<١٦>",
"ثُN<مَّ> لَءَاتِیَN<نَّ>هُم N<مِّ>X<ن>N<ۢ> بَیۡنِ أَیۡدِیهِمۡ وَمِنۡ خَلۡفِهِمۡ وَعَنۡ أَیۡمَT<ـٰ>نِهِمۡ وَعَN<ن> شَمَW<اۤ>ئِلِهِمۡۖ وَلَا تَجِدُ أَكۡثَR<رَ>هُمۡ شَT<ـٰ>كِرِJ<ی>نَ A<۝>D<١٧>",
"قَالَ ٱخۡR<رُ>Q<جۡ> مِنۡهَا مَذۡءُومX<ࣰ>ا N<مَّ>Q<دۡ>حُوR<ر>X<ࣰ>اۖ لَّمَN<ن> تَبِعَكَ مِنۡهُمۡ لَأَمۡلَأَN<نَّ> جَهَN<نَّ>مَ مِN<ن>كُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<١٨>",
"وَیَW<ـٰۤ>ءَادَمُ ٱسۡكُنۡ أَN<ن>تَ وَزَوۡجُكَ ٱلۡجَN<نَّ>ةَ فَكُلَا مِنۡ حَیۡثُ شِئۡتُمَا وَلَا تَQ<قۡ>R<رَ>بَا هَT<ـٰ>ذِهِ ٱX<ل>شَّجَR<رَ>ةَ فَتَكُونَا مِنَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٩>",
"فَوَسۡوَسَ لَهُمَا ٱX<ل>شَّیۡطَT<ـٰ>نُ لِیُQ<بۡ>دِیَ لَهُمَا مَا وُT<ۥ>رِیَ عَنۡهُمَا مِN<ن> سَوۡءَT< ٰ>تِهِمَا وَقَالَ مَا نَهَX<ى>T<ٰ>كُمَا R<رَ>بُّكُمَا عَنۡ هَT<ـٰ>ذِهِ ٱX<ل>شَّجَR<رَ>ةِ إِلَّW<اۤ> أَN<ن> تَكُونَا مَلَكَیۡنِ أَوۡ تَكُونَا مِنَ ٱلۡخَT<ـٰ>لِدِJ<ی>نَ A<۝>D<٢٠>",
"وَقَاسَمَهُمَW<اۤ> إِN<نِّ>ی لَكُمَا لَمِنَ ٱX<ل>N<نَّ>T<ـٰ>صِحِJ<ی>نَ A<۝>D<٢١>",
"فَدَلَّX<ى>T<ٰ>هُمَا بِغُR<رُ>ورN<ࣲ>ۚ فَلَN<مَّ>ا ذَاقَا ٱX<ل>شَّجَR<رَ>ةَ بَدَتۡ لَهُمَا سَوۡءَT< ٰ>تُهُمَا وَطَفِقَا یَخۡصِفَانِ عَلَیۡهِمَا مِX<ن> N<وَ>R<رَ>قِ ٱلۡجَN<نَّ>ةِۖ وَنَادَX<ى>T<ٰ>هُمَا R<رَ>بُّهُمَW<اۤ> أَلَمۡ أَنۡهَكُمَا عَN<ن> تِلۡكُمَا ٱX<ل>شَّجَR<رَ>ةِ وَأَقُل لَّكُمَW<اۤ> إِN<نَّ> ٱX<ل>شَّیۡطَT<ـٰ>نَ لَكُمَا عَدُوّX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٢٢>",
"قَالَا R<رَ>بَّنَا ظَلَمۡنَW<اۤ> أَN<ن>فُسَنَا وَإِX<ن> لَّمۡ تَغۡفِرۡ لَنَا وَتَR<رۡ>حَمۡنَا لَنَكُونَN<نَّ> مِنَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٢٣>",
"قَالَ ٱهۡبِطُوX<ا۟> بَعۡضُكُمۡ لِبَعۡضٍ عَدُوّX<ࣱ>ۖ N<وَ>لَكُمۡ فِی ٱلۡأَR<رۡ>ضِ مُسۡتَقَR<رّ>X<ࣱ> N<وَ>مَتَT<ـٰ>عٌ إِلَىٰ حِJ<ی>نࣲ A<۝>D<٢٤>",
"قَالَ فِیهَا تَحۡیَوۡنَ وَفِیهَا تَمُوتُونَ وَمِنۡهَا تُخۡR<رَ>جُJ<و>نَ A<۝>D<٢٥>",
"یَT<ـٰ>بَنِW<یۤ> ءَادَمَ قَQ<دۡ> أَN<ن>زَلۡنَا عَلَیۡكُمۡ لِبَاسX<ࣰ>ا N<یُ>وَT< ٰ>رِی سَوۡءَT< ٰ>تِكُمۡ وَرِیشX<ࣰ>اۖ N<وَ>لِبَاسُ ٱX<ل>تَّQ<قۡ>وَىٰ ذَT< ٰ>لِكَ خَیۡR<ر>N<ࣱ>ۚ ذَT< ٰ>لِكَ مِنۡ ءَایَT<ـٰ>تِ ٱللَّهِ لَعَلَّهُمۡ یَذَّكَّR<رُ>J<و>نَ A<۝>D<٢٦>",
"یَT<ـٰ>بَنِW<یۤ> ءَادَمَ لَا یَفۡتِنَN<نَّ>كُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ كَمَW<اۤ> أَخۡR<رَ>جَ أَبَوَیۡكُم N<مِّ>نَ ٱلۡجَN<نَّ>ةِ یَN<ن>زِعُ عَنۡهُمَا لِبَاسَهُمَا لِیُرِیَهُمَا سَوۡءَT< ٰ>تِهِمَW<اۤ>ۚ إِN<نَّ>هُT<ۥ> یَR<رَ>X<ى>T<ٰ>كُمۡ هُوَ وَقَبِیلُهُT<ۥ> مِنۡ حَیۡثُ لَا تَR<رَ>وۡنَهُمۡۗ إِN<نَّ>ا جَعَلۡنَا ٱX<ل>شَّیَT<ـٰ>طِینَ أَوۡلِیَW<اۤ>ءَ لِلَّذِینَ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٢٧>",
"وَإِذَا فَعَلُوX<ا۟> فَT<ـٰ>حِشَةN<ࣰ> قَالُوX<ا۟> وَجَQ<دۡ>نَا عَلَیۡهَW<اۤ> ءَابَW<اۤ>ءَنَا وَٱللَّهُ أَمَR<رَ>نَا بِهَاۗ قُلۡ إِN<نَّ> ٱللَّهَ لَا یَأۡمُR<رُ> بِX<ٱ>لۡفَحۡشَW<اۤ>ءِۖ أَتَقُولُونَ عَلَى ٱللَّهِ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٢٨>",
"قُلۡ أَمَR<رَ> R<رَ>بِّی بِX<ٱ>لۡقِسۡطِۖ وَأَقِیمُوX<ا۟> وُجُوهَكُمۡ عِN<ن>دَ كُلِّ مَسۡجِدX<ࣲ> N<وَ>X<ٱ>Q<دۡ>عُوهُ مُخۡلِصِینَ لَهُ ٱX<ل>دِّینَۚ كَمَا بَدَأَكُمۡ تَعُودُJ<و>نَ A<۝>D<٢٩>",
"فَرِیقًا هَدَىٰ وَفَرِیقًا حَقَّ عَلَیۡهِمُ ٱX<ل>ضَّلَT<ـٰ>لَةُۚ إِN<نَّ>هُمُ ٱتَّخَذُوX<ا۟> ٱX<ل>شَّیَT<ـٰ>طِینَ أَوۡلِیَW<اۤ>ءَ مِN<ن> دُونِ ٱللَّهِ وَیَحۡسَبُونَ أَN<نَّ>هُم N<مُّ>هۡتَدُJ<و>نَ A<۝>D<٣٠>",
"۞ یَT<ـٰ>بَنِW<یۤ> ءَادَمَ خُذُوX<ا۟> زِینَتَكُمۡ عِN<ن>دَ كُلِّ مَسۡجِدX<ࣲ> N<وَ>كُلُوX<ا۟> وَX<ٱ>شۡR<رَ>بُوX<ا۟> وَلَا تُسۡرِفُW<وۤ>X<ا۟>ۚ إِN<نَّ>هُT<ۥ> لَا یُحِبُّ ٱلۡمُسۡرِفِJ<ی>نَ A<۝>D<٣١>",
"قُلۡ مَنۡ حَR<رَّ>مَ زِینَةَ ٱللَّهِ ٱلَّتِW<یۤ> أَخۡR<رَ>جَ لِعِبَادِهِT<ۦ> وَX<ٱ>X<ل>طَّیِّبَT<ـٰ>تِ مِنَ ٱX<ل>رِّزۡقِۚ قُلۡ هِیَ لِلَّذِینَ ءَامَنُوX<ا۟> فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا خَالِصَةX<ࣰ> N<یَ>وۡمَ ٱلۡقِیَT<ـٰ>مَةِۗ كَذَT< ٰ>لِكَ نُفَصِّلُ ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یَ>عۡلَمُJ<و>نَ A<۝>D<٣٢>",
"قُلۡ إِN<نَّ>مَا حَR<رَّ>مَ R<رَ>بِّیَ ٱلۡفَوَT< ٰ>حِشَ مَا ظَهَR<رَ> مِنۡهَا وَمَا بَطَنَ وَX<ٱ>لۡإِثۡمَ وَX<ٱ>لۡبَغۡیَ بِغَیۡرِ ٱلۡحَقِّ وَأَN<ن> تُشۡرِكُوX<ا۟> بِٱللَّهِ مَا لَمۡ یُنَزِّلۡ بِهِT<ۦ> سُلۡطَT<ـٰ>نX<ࣰ>ا N<وَ>أَN<ن> تَقُولُوX<ا۟> عَلَى ٱللَّهِ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٣٣>",
"وَلِكُلِّ أُN<مَّ>ةٍ أَجَلN<ࣱ>ۖ فَإِذَا جَW<اۤ>ءَ أَجَلُهُمۡ لَا یَسۡتَأۡخِR<رُ>ونَ سَاعَةX<ࣰ> N<وَ>لَا یَسۡتَQ<قۡ>دِمُJ<و>نَ A<۝>D<٣٤>",
"یَT<ـٰ>بَنِW<یۤ> ءَادَمَ إِN<مَّ>ا یَأۡتِیَN<نَّ>كُمۡ R<رُ>سُلX<ࣱ> N<مِّ>N<ن>كُمۡ یَقُصُّونَ عَلَیۡكُمۡ ءَایَT<ـٰ>تِی فَمَنِ ٱتَّقَىٰ وَأَصۡلَحَ فَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٣٥>",
"وَX<ٱ>لَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا وَX<ٱ>سۡتَكۡبَR<رُ>وX<ا۟> عَنۡهَW<اۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٣٦>",
"فَمَنۡ أَظۡلَمُ مِN<مَّ>نِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبًا أَوۡ كَذَّبَ بِءَایَT<ـٰ>تِهِW<ۦۤ>ۚ أُX<و۟>لَW<ـٰۤ>ئِكَ یَنَالُهُمۡ نَصِیبُهُم N<مِّ>نَ ٱلۡكِتَT<ـٰ>بِۖ حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءَتۡهُمۡ R<رُ>سُلُنَا یَتَوَفَّوۡنَهُمۡ قَالُW<وۤ>X<ا۟> أَیۡنَ مَا كُN<ن>تُمۡ تَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِۖ قَالُوX<ا۟> ضَلُّوX<ا۟> عَN<نَّ>ا وَشَهِدُوX<ا۟> عَلَىW<ٰۤ> أَN<ن>فُسِهِمۡ أَN<نَّ>هُمۡ كَانُوX<ا۟> كَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٣٧>",
"قَالَ ٱQ<دۡ>خُلُوX<ا۟> فِW<یۤ> أُمَمN<ࣲ> قَQ<دۡ> خَلَتۡ مِN<ن> قَQ<بۡ>لِكُم N<مِّ>نَ ٱلۡجِN<نِّ> وَX<ٱ>لۡإِN<ن>سِ فِی ٱX<ل>N<نَّ>ارِۖ كُلَّمَا دَخَلَتۡ أُN<مَّ>ةX<ࣱ> لَّعَنَتۡ أُخۡتَهَاۖ حَتَّىW<ٰۤ> إِذَا ٱدَّاR<رَ>كُوX<ا۟> فِیهَا جَمِیعN<ࣰ>ا قَالَتۡ أُخۡR<رَ>X<ى>T<ٰ>هُمۡ لِأُولَX<ى>T<ٰ>هُمۡ R<رَ>بَّنَا هَW<ـٰۤ>ؤُلَW<اۤ>ءِ أَضَلُّونَا فَءَاتِهِمۡ عَذَابN<ࣰ>ا ضِعۡفX<ࣰ>ا N<مِّ>نَ ٱX<ل>N<نَّ>ارِۖ قَالَ لِكُلّN<ࣲ> ضِعۡفX<ࣱ> N<وَ>لَT<ـٰ>كِX<ن> لَّا تَعۡلَمُJ<و>نَ A<۝>D<٣٨>",
"وَقَالَتۡ أُولَX<ى>T<ٰ>هُمۡ لِأُخۡR<رَ>X<ى>T<ٰ>هُمۡ فَمَا كَانَ لَكُمۡ عَلَیۡنَا مِN<ن> فَضۡلN<ࣲ> فَذُوقُوX<ا۟> ٱلۡعَذَابَ بِمَا كُN<ن>تُمۡ تَكۡسِبُJ<و>نَ A<۝>D<٣٩>",
"إِN<نَّ> ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا وَX<ٱ>سۡتَكۡبَR<رُ>وX<ا۟> عَنۡهَا لَا تُفَتَّحُ لَهُمۡ أَQ<بۡ>وَT< ٰ>بُ ٱX<ل>سَّمَW<اۤ>ءِ وَلَا یَQ<دۡ>خُلُونَ ٱلۡجَN<نَّ>ةَ حَتَّىٰ یَلِجَ ٱلۡجَمَلُ فِی سَN<مِّ> ٱلۡخِیَاطِۚ وَكَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٤٠>",
"لَهُم N<مِّ>N<ن> جَهَN<نَّ>مَ مِهَادX<ࣱ> N<وَ>مِN<ن> فَوۡقِهِمۡ غَوَاشX<ࣲ>ۚ N<وَ>كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤١>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَا نُكَلِّفُ نَفۡسًا إِلَّا وُسۡعَهَW<اۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡجَN<نَّ>ةِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٤٢>",
"وَنَزَعۡنَا مَا فِی صُدُورِهِم N<مِّ>نۡ غِلّN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهِمُ ٱلۡأَنۡهَT<ـٰ>R<رُ>ۖ وَقَالُوX<ا۟> ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِی هَدَX<ى>T<ٰ>نَا لِهَT<ـٰ>ذَا وَمَا كُN<نَّ>ا لِنَهۡتَدِیَ لَوۡلَW<اۤ> أَنۡ هَدَX<ى>T<ٰ>نَا ٱللَّهُۖ لَقَQ<دۡ> جَW<اۤ>ءَتۡ R<رُ>سُلُ R<رَ>بِّنَا بِX<ٱ>لۡحَقِّۖ وَنُودُW<وۤ>X<ا۟> أَN<ن> تِلۡكُمُ ٱلۡجَN<نَّ>ةُ أُورِثۡتُمُوهَا بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٤٣>",
"وَنَادَىW<ٰۤ> أَصۡحَT<ـٰ>بُ ٱلۡجَN<نَّ>ةِ أَصۡحَT<ـٰ>بَ ٱX<ل>N<نَّ>ارِ أَN<ن> قَQ<دۡ> وَجَQ<دۡ>نَا مَا وَعَدَنَا R<رَ>بُّنَا حَقّN<ࣰ>ا فَهَلۡ وَجَX<د>تُّم N<مَّ>ا وَعَدَ R<رَ>بُّكُمۡ حَقّN<ࣰ>اۖ قَالُوX<ا۟> نَعَمۡۚ فَأَذَّنَ مُؤَذِّنُN<ۢ> بَیۡنَهُمۡ أَX<ن> لَّعۡنَةُ ٱللَّهِ عَلَى ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤٤>",
"ٱلَّذِینَ یَصُدُّونَ عَN<ن> سَبِیلِ ٱللَّهِ وَیَQ<بۡ>غُونَهَا عِوَجX<ࣰ>ا N<وَ>هُN<م> بِX<ٱ>لۡءَاخِR<رَ>ةِ كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٤٥>",
"وَبَیۡنَهُمَا حِجَابX<ࣱ>ۚ N<وَ>عَلَى ٱلۡأَعۡR<رَ>افِ رِجَالX<ࣱ> N<یَ>عۡرِفُونَ كُلَّN<ۢ>ا بِسِیمَX<ى>T<ٰ>هُمۡۚ وَنَادَوۡX<ا۟> أَصۡحَT<ـٰ>بَ ٱلۡجَN<نَّ>ةِ أَN<ن> سَلَT<ـٰ>مٌ عَلَیۡكُمۡۚ لَمۡ یَQ<دۡ>خُلُوهَا وَهُمۡ یَQ<طۡ>مَعُJ<و>نَ A<۝>D<٤٦>",
"۞ وَإِذَا صُرِفَتۡ أَQ<بۡ>صَT<ـٰ>R<رُ>هُمۡ تِلۡقَW<اۤ>ءَ أَصۡحَT<ـٰ>بِ ٱX<ل>N<نَّ>ارِ قَالُوX<ا۟> R<رَ>بَّنَا لَا تَQ<جۡ>عَلۡنَا مَعَ ٱلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤٧>",
"وَنَادَىW<ٰۤ> أَصۡحَT<ـٰ>بُ ٱلۡأَعۡR<رَ>افِ رِجَالX<ࣰ>ا N<یَ>عۡرِفُونَهُN<م> بِسِیمَX<ى>T<ٰ>هُمۡ قَالُوX<ا۟> مَW<اۤ> أَغۡنَىٰ عَN<ن>كُمۡ جَمۡعُكُمۡ وَمَا كُN<ن>تُمۡ تَسۡتَكۡبِR<رُ>J<و>نَ A<۝>D<٤٨>",
"أَهَW<ـٰۤ>ؤُلَW<اۤ>ءِ ٱلَّذِینَ أَQ<قۡ>سَمۡتُمۡ لَا یَنَالُهُمُ ٱللَّهُ بِR<رَ>حۡمَةٍۚ ٱQ<دۡ>خُلُوX<ا۟> ٱلۡجَN<نَّ>ةَ لَا خَوۡفٌ عَلَیۡكُمۡ وَلَW<اۤ> أَN<ن>تُمۡ تَحۡزَنُJ<و>نَ A<۝>D<٤٩>",
"وَنَادَىW<ٰۤ> أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِ أَصۡحَT<ـٰ>بَ ٱلۡجَN<نَّ>ةِ أَنۡ أَفِیضُوX<ا۟> عَلَیۡنَا مِنَ ٱلۡمَW<اۤ>ءِ أَوۡ مِN<مَّ>ا R<رَ>زَقَكُمُ ٱللَّهُۚ قَالُW<وۤ>X<ا۟> إِN<نَّ> ٱللَّهَ حَR<رَّ>مَهُمَا عَلَى ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٥٠>",
"ٱلَّذِینَ ٱتَّخَذُوX<ا۟> دِینَهُمۡ لَهۡوX<ࣰ>ا N<وَ>لَعِبX<ࣰ>ا N<وَ>غَR<رَّ>تۡهُمُ ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَاۚ فَX<ٱ>لۡیَوۡمَ نَN<ن>سَX<ى>T<ٰ>هُمۡ كَمَا نَسُوX<ا۟> لِقَW<اۤ>ءَ یَوۡمِهِمۡ هَT<ـٰ>ذَا وَمَا كَانُوX<ا۟> بِءَایَT<ـٰ>تِنَا یَQ<جۡ>حَدُJ<و>نَ A<۝>D<٥١>",
"وَلَقَQ<دۡ> جِئۡنَT<ـٰ>هُN<م> بِكِتَT<ـٰ>بN<ࣲ> فَصَّلۡنَT<ـٰ>هُ عَلَىٰ عِلۡمٍ هُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٥٢>",
"هَلۡ یَN<ن>ظُR<رُ>ونَ إِلَّا تَأۡوِیلَهُT<ۥ>ۚ یَوۡمَ یَأۡتِی تَأۡوِیلُهُT<ۥ> یَقُولُ ٱلَّذِینَ نَسُوهُ مِN<ن> قَQ<بۡ>لُ قَQ<دۡ> جَW<اۤ>ءَتۡ R<رُ>سُلُ R<رَ>بِّنَا بِX<ٱ>لۡحَقِّ فَهَل لَّنَا مِN<ن> شُفَعَW<اۤ>ءَ فَیَشۡفَعُوX<ا۟> لَنَW<اۤ> أَوۡ نُR<رَ>دُّ فَنَعۡمَلَ غَیۡR<رَ> ٱلَّذِی كُN<نَّ>ا نَعۡمَلُۚ قَQ<دۡ> خَسِR<رُ>W<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ وَضَلَّ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<٥٣>",
"إِN<نَّ> R<رَ>بَّكُمُ ٱللَّهُ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ فِی سِتَّةِ أَیَّامN<ࣲ> ثُN<مَّ> ٱسۡتَوَىٰ عَلَى ٱلۡعَR<رۡ>شِۖ یُغۡشِی ٱلَّیۡلَ ٱX<ل>N<نَّ>هَاR<رَ> یَQ<طۡ>لُبُهُT<ۥ> حَثِیثX<ࣰ>ا N<وَ>X<ٱ>X<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ> وَX<ٱ>X<ل>N<نُّ>جُومَ مُسَخَّR<رَ>T< ٰ>تِN<ۭ> بِأَمۡرِهِW<ۦۤ>ۗ أَلَا لَهُ ٱلۡخَلۡقُ وَX<ٱ>لۡأَمۡR<رُ>ۗ تَبَاR<رَ>كَ ٱللَّهُ R<رَ>بُّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٥٤>",
"ٱQ<دۡ>عُوX<ا۟> R<رَ>بَّكُمۡ تَضَR<رُّ>عX<ࣰ>ا N<وَ>خُفۡیَةًۚ إِN<نَّ>هُT<ۥ> لَا یُحِبُّ ٱلۡمُعۡتَدِJ<ی>نَ A<۝>D<٥٥>",
"وَلَا تُفۡسِدُوX<ا۟> فِی ٱلۡأَR<رۡ>ضِ بَعۡدَ إِصۡلَT<ـٰ>حِهَا وَX<ٱ>Q<دۡ>عُوهُ خَوۡفX<ࣰ>ا N<وَ>طَمَعًاۚ إِN<نَّ> R<رَ>حۡمَتَ ٱللَّهِ قَرِیبX<ࣱ> N<مِّ>نَ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٥٦>",
"وَهُوَ ٱلَّذِی یُR<رۡ>سِلُ ٱX<ل>رِّیَT<ـٰ>حَ بُشۡR<رَ>N<ۢ>ا بَیۡنَ یَدَیۡ R<رَ>حۡمَتِهِT<ۦ>ۖ حَتَّىW<ٰۤ> إِذَW<اۤ> أَقَلَّتۡ سَحَابN<ࣰ>ا ثِقَالN<ࣰ>ا سُQ<قۡ>نَT<ـٰ>هُ لِبَلَدX<ࣲ> N<مَّ>یِّتN<ࣲ> فَأَN<ن>زَلۡنَا بِهِ ٱلۡمَW<اۤ>ءَ فَأَخۡR<رَ>Q<جۡ>نَا بِهِT<ۦ> مِN<ن> كُلِّ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِۚ كَذَT< ٰ>لِكَ نُخۡرِجُ ٱلۡمَوۡتَىٰ لَعَلَّكُمۡ تَذَكَّR<رُ>J<و>نَ A<۝>D<٥٧>",
"وَX<ٱ>لۡبَلَدُ ٱX<ل>طَّیِّبُ یَخۡR<رُ>جُ نَبَاتُهُT<ۥ> بِإِذۡنِ R<رَ>بِّهِT<ۦ>ۖ وَX<ٱ>لَّذِی خَبُثَ لَا یَخۡR<رُ>جُ إِلَّا نَكِدN<ࣰ>اۚ كَذَT< ٰ>لِكَ نُصَرِّفُ ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یَ>شۡكُR<رُ>J<و>نَ A<۝>D<٥٨>",
"لَقَQ<دۡ> أَR<رۡ>سَلۡنَا نُوحًا إِلَىٰ قَوۡمِهِT<ۦ> فَقَالَ یَT<ـٰ>قَوۡمِ ٱعۡبُدُوX<ا۟> ٱللَّهَ مَا لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡR<رُ>هُW<ۥۤ> إِN<نِّ>W<یۤ> أَخَافُ عَلَیۡكُمۡ عَذَابَ یَوۡمٍ عَظِJ<ی>مࣲ A<۝>D<٥٩>",
"قَالَ ٱلۡمَلَأُ مِN<ن> قَوۡمِهِW<ۦۤ> إِN<نَّ>ا لَنَR<رَ>X<ى>T<ٰ>كَ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٦٠>",
"قَالَ یَT<ـٰ>قَوۡمِ لَیۡسَ بِی ضَلَT<ـٰ>لَةX<ࣱ> N<وَ>لَT<ـٰ>كِN<نِّ>ی R<رَ>سُولX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٦١>",
"أُبَلِّغُكُمۡ رِسَT<ـٰ>لَT<ـٰ>تِ R<رَ>بِّی وَأَN<ن>صَحُ لَكُمۡ وَأَعۡلَمُ مِنَ ٱللَّهِ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٦٢>",
"أَوَعَجِQ<بۡ>تُمۡ أَN<ن> جَW<اۤ>ءَكُمۡ ذِكۡR<ر>X<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ عَلَىٰ R<رَ>جُلX<ࣲ> N<مِّ>N<ن>كُمۡ لِیُN<ن>ذِR<رَ>كُمۡ وَلِتَتَّقُوX<ا۟> وَلَعَلَّكُمۡ تُR<رۡ>حَمُJ<و>نَ A<۝>D<٦٣>",
"فَكَذَّبُوهُ فَأَN<ن>جَیۡنَT<ـٰ>هُ وَX<ٱ>لَّذِینَ مَعَهُT<ۥ> فِی ٱلۡفُلۡكِ وَأَغۡR<رَ>Q<قۡ>نَا ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَW<اۤ>ۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> قَوۡمًا عَمِJ<ی>نَ A<۝>D<٦٤>",
"۞ وَإِلَىٰ عَادٍ أَخَاهُمۡ هُودN<ࣰ>اۚ قَالَ یَT<ـٰ>قَوۡمِ ٱعۡبُدُوX<ا۟> ٱللَّهَ مَا لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡR<رُ>هُW<ۥۤ>ۚ أَفَلَا تَتَّقُJ<و>نَ A<۝>D<٦٥>",
"قَالَ ٱلۡمَلَأُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِN<ن> قَوۡمِهِW<ۦۤ> إِN<نَّ>ا لَنَR<رَ>X<ى>T<ٰ>كَ فِی سَفَاهَةX<ࣲ> N<وَ>إِN<نَّ>ا لَنَظُN<نُّ>كَ مِنَ ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٦٦>",
"قَالَ یَT<ـٰ>قَوۡمِ لَیۡسَ بِی سَفَاهَةX<ࣱ> N<وَ>لَT<ـٰ>كِN<نِّ>ی R<رَ>سُولX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٦٧>",
"أُبَلِّغُكُمۡ رِسَT<ـٰ>لَT<ـٰ>تِ R<رَ>بِّی وَأَنَا۠ لَكُمۡ نَاصِحٌ أَمِJ<ی>نٌ A<۝>D<٦٨>",
"أَوَعَجِQ<بۡ>تُمۡ أَN<ن> جَW<اۤ>ءَكُمۡ ذِكۡR<ر>X<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ عَلَىٰ R<رَ>جُلX<ࣲ> N<مِّ>N<ن>كُمۡ لِیُN<ن>ذِR<رَ>كُمۡۚ وَX<ٱ>ذۡكُR<رُ>W<وۤ>X<ا۟> إِذۡ جَعَلَكُمۡ خُلَفَW<اۤ>ءَ مِX<ن>N<ۢ> بَعۡدِ قَوۡمِ نُوحX<ࣲ> N<وَ>زَادَكُمۡ فِی ٱلۡخَلۡقِ بَX<ص>ۜۡطَةN<ࣰ>ۖ فَX<ٱ>ذۡكُR<رُ>W<وۤ>X<ا۟> ءَالَW<اۤ>ءَ ٱللَّهِ لَعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<٦٩>",
"قَالُW<وۤ>X<ا۟> أَجِئۡتَنَا لِنَعۡبُدَ ٱللَّهَ وَحۡدَهُT<ۥ> وَنَذَR<رَ> مَا كَانَ یَعۡبُدُ ءَابَW<اۤ>ؤُنَا فَأۡتِنَا بِمَا تَعِدُنَW<اۤ> إِN<ن> كُN<ن>تَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٧٠>",
"قَالَ قَQ<دۡ> وَقَعَ عَلَیۡكُم N<مِّ>X<ن> R<رَّ>بِّكُمۡ رِQ<جۡ>سX<ࣱ> N<وَ>غَضَبٌۖ أَتُجَT<ـٰ>دِلُونَنِی فِW<یۤ> أَسۡمَW<اۤ>ءN<ࣲ> سَN<مَّ>یۡتُمُوهَW<اۤ> أَN<ن>تُمۡ وَءَابَW<اۤ>ؤُكُم N<مَّ>ا نَزَّلَ ٱللَّهُ بِهَا مِN<ن> سُلۡطَT<ـٰ>نN<ࣲ>ۚ فَX<ٱ>N<ن>تَظِR<رُ>W<وۤ>X<ا۟> إِN<نِّ>ی مَعَكُم N<مِّ>نَ ٱلۡمُN<ن>تَظِرِJ<ی>نَ A<۝>D<٧١>",
"فَأَN<ن>جَیۡنَT<ـٰ>هُ وَX<ٱ>لَّذِینَ مَعَهُT<ۥ> بِR<رَ>حۡمَةX<ࣲ> N<مِّ>N<نَّ>ا وَقَطَعۡنَا دَابِR<رَ> ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَاۖ وَمَا كَانُوX<ا۟> مُؤۡمِنِJ<ی>نَ A<۝>D<٧٢>",
"وإِلَىٰ ثَمُودَ أَخَاهُمۡ صَT<ـٰ>لِحN<ࣰ>اۚ قَالَ یَT<ـٰ>قَوۡمِ ٱعۡبُدُوX<ا۟> ٱللَّهَ مَا لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡR<رُ>هُT<ۥ>ۖ قَQ<دۡ> جَW<اۤ>ءَتۡكُN<م> بَیِّنَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡۖ هَT<ـٰ>ذِهِT<ۦ> نَاقَةُ ٱللَّهِ لَكُمۡ ءَایَةN<ࣰ>ۖ فَذَR<رُ>وهَا تَأۡكُلۡ فِW<یۤ> أَR<رۡ>ضِ ٱللَّهِۖ وَلَا تَمَسُّوهَا بِسُW<وۤ>ءN<ࣲ> فَیَأۡخُذَكُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٧٣>",
"وَX<ٱ>ذۡكُR<رُ>W<وۤ>X<ا۟> إِذۡ جَعَلَكُمۡ خُلَفَW<اۤ>ءَ مِX<ن>N<ۢ> بَعۡدِ عَادX<ࣲ> N<وَ>بَوَّأَكُمۡ فِی ٱلۡأَR<رۡ>ضِ تَتَّخِذُونَ مِN<ن> سُهُولِهَا قُصُوR<ر>X<ࣰ>ا N<وَ>تَنۡحِتُونَ ٱلۡجِبَالَ بُیُوتN<ࣰ>اۖ فَX<ٱ>ذۡكُR<رُ>W<وۤ>X<ا۟> ءَالَW<اۤ>ءَ ٱللَّهِ وَلَا تَعۡثَوۡX<ا۟> فِی ٱلۡأَR<رۡ>ضِ مُفۡسِدِJ<ی>نَ A<۝>D<٧٤>",
"قَالَ ٱلۡمَلَأُ ٱلَّذِینَ ٱسۡتَكۡبَR<رُ>وX<ا۟> مِN<ن> قَوۡمِهِT<ۦ> لِلَّذِینَ ٱسۡتُضۡعِفُوX<ا۟> لِمَنۡ ءَامَنَ مِنۡهُمۡ أَتَعۡلَمُونَ أَN<نَّ> صَT<ـٰ>لِحX<ࣰ>ا N<مُّ>R<رۡ>سَلX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ>ۚ قَالُW<وۤ>X<ا۟> إِN<نَّ>ا بِمَW<اۤ> أُR<رۡ>سِلَ بِهِT<ۦ> مُؤۡمِنُJ<و>نَ A<۝>D<٧٥>",
"قَالَ ٱلَّذِینَ ٱسۡتَكۡبَR<رُ>W<وۤ>X<ا۟> إِN<نَّ>ا بِX<ٱ>لَّذِW<یۤ> ءَامَN<ن>تُN<م> بِهِT<ۦ> كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٧٦>",
"فَعَقَR<رُ>وX<ا۟> ٱX<ل>N<نَّ>اقَةَ وَعَتَوۡX<ا۟> عَنۡ أَمۡرِ R<رَ>بِّهِمۡ وَقَالُوX<ا۟> یَT<ـٰ>صَT<ـٰ>لِحُ ٱئۡتِنَا بِمَا تَعِدُنَW<اۤ> إِN<ن> كُN<ن>تَ مِنَ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٧٧>",
"فَأَخَذَتۡهُمُ ٱX<ل>R<رَّ>Q<جۡ>فَةُ فَأَصۡبَحُوX<ا۟> فِی دَارِهِمۡ جَT<ـٰ>ثِمِJ<ی>نَ A<۝>D<٧٨>",
"فَتَوَلَّىٰ عَنۡهُمۡ وَقَالَ یَT<ـٰ>قَوۡمِ لَقَQ<دۡ> أَQ<بۡ>لَغۡتُكُمۡ رِسَالَةَ R<رَ>بِّی وَنَصَحۡتُ لَكُمۡ وَلَT<ـٰ>كِX<ن> لَّا تُحِبُّونَ ٱX<ل>N<نَّ>T<ـٰ>صِحِJ<ی>نَ A<۝>D<٧٩>",
"وَلُوطًا إِذۡ قَالَ لِقَوۡمِهِW<ۦۤ> أَتَأۡتُونَ ٱلۡفَT<ـٰ>حِشَةَ مَا سَبَقَكُN<م> بِهَا مِنۡ أَحَدX<ࣲ> N<مِّ>نَ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٨٠>",
"إِN<نَّ>كُمۡ لَتَأۡتُونَ ٱX<ل>رِّجَالَ شَهۡوَةX<ࣰ> N<مِّ>N<ن> دُونِ ٱX<ل>N<نِّ>سَW<اۤ>ءِۚ بَلۡ أَN<ن>تُمۡ قَوۡمX<ࣱ> N<مُّ>سۡرِفُJ<و>نَ A<۝>D<٨١>",
"وَمَا كَانَ جَوَابَ قَوۡمِهِW<ۦۤ> إِلَّW<اۤ> أَN<ن> قَالُW<وۤ>X<ا۟> أَخۡرِجُوهُم N<مِّ>N<ن> قَR<رۡ>یَتِكُمۡۖ إِN<نَّ>هُمۡ أُنَاسX<ࣱ> N<یَ>تَطَهَّR<رُ>J<و>نَ A<۝>D<٨٢>",
"فَأَN<ن>جَیۡنَT<ـٰ>هُ وَأَهۡلَهُW<ۥۤ> إِلَّا ٱمۡR<رَ>أَتَهُT<ۥ> كَانَتۡ مِنَ ٱلۡغَT<ـٰ>بِرِJ<ی>نَ A<۝>D<٨٣>",
"وَأَمۡطَR<رۡ>نَا عَلَیۡهِم N<مَّ>طَR<ر>N<ࣰ>اۖ فَX<ٱ>N<ن>ظُR<رۡ> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٨٤>",
"وَإِلَىٰ مَQ<دۡ>یَنَ أَخَاهُمۡ شُعَیۡبN<ࣰ>اۚ قَالَ یَT<ـٰ>قَوۡمِ ٱعۡبُدُوX<ا۟> ٱللَّهَ مَا لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡR<رُ>هُT<ۥ>ۖ قَQ<دۡ> جَW<اۤ>ءَتۡكُN<م> بَیِّنَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡۖ فَأَوۡفُوX<ا۟> ٱلۡكَیۡلَ وَX<ٱ>لۡمِیزَانَ وَلَا تَQ<بۡ>خَسُوX<ا۟> ٱX<ل>N<نَّ>اسَ أَشۡیَW<اۤ>ءَهُمۡ وَلَا تُفۡسِدُوX<ا۟> فِی ٱلۡأَR<رۡ>ضِ بَعۡدَ إِصۡلَT<ـٰ>حِهَاۚ ذَT< ٰ>لِكُمۡ خَیۡR<ر>X<ࣱ> لَّكُمۡ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٨٥>",
"وَلَا تَQ<قۡ>عُدُوX<ا۟> بِكُلِّ صِR<رَ>T< ٰ>طN<ࣲ> تُوعِدُونَ وَتَصُدُّونَ عَN<ن> سَبِیلِ ٱللَّهِ مَنۡ ءَامَنَ بِهِT<ۦ> وَتَQ<بۡ>غُونَهَا عِوَجX<ࣰ>اۚ N<وَ>X<ٱ>ذۡكُR<رُ>W<وۤ>X<ا۟> إِذۡ كُN<ن>تُمۡ قَلِیلN<ࣰ>ا فَكَثَّR<رَ>كُمۡۖ وَX<ٱ>N<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<٨٦>",
"وَإِN<ن> كَانَ طَW<اۤ>ئِفَةX<ࣱ> N<مِّ>N<ن>كُمۡ ءَامَنُوX<ا۟> بِX<ٱ>لَّذِW<یۤ> أُR<رۡ>سِلۡتُ بِهِT<ۦ> وَطَW<اۤ>ئِفَةX<ࣱ> لَّمۡ یُؤۡمِنُوX<ا۟> فَX<ٱ>صۡبِR<رُ>وX<ا۟> حَتَّىٰ یَحۡكُمَ ٱللَّهُ بَیۡنَنَاۚ وَهُوَ خَیۡR<رُ> ٱلۡحَT<ـٰ>كِمِJ<ی>نَ A<۝>D<٨٧>",
"۞ قَالَ ٱلۡمَلَأُ ٱلَّذِینَ ٱسۡتَكۡبَR<رُ>وX<ا۟> مِN<ن> قَوۡمِهِT<ۦ> لَنُخۡرِجَN<نَّ>كَ یَT<ـٰ>شُعَیۡبُ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مَعَكَ مِN<ن> قَR<رۡ>یَتِنَW<اۤ> أَوۡ لَتَعُودُN<نَّ> فِی مِلَّتِنَاۚ قَالَ أَوَ لَوۡ كُN<نَّ>ا كَT<ـٰ>رِهِJ<ی>نَ A<۝>D<٨٨>",
"قَدِ ٱفۡتَR<رَ>یۡنَا عَلَى ٱللَّهِ كَذِبًا إِنۡ عُQ<دۡ>نَا فِی مِلَّتِكُN<م> بَعۡدَ إِذۡ نَجَّX<ى>T<ٰ>نَا ٱللَّهُ مِنۡهَاۚ وَمَا یَكُونُ لَنَW<اۤ> أَن N<نَّ>عُودَ فِیهَW<اۤ> إِلَّW<اۤ> أَX<ن> N<یَ>شَW<اۤ>ءَ ٱللَّهُ R<رَ>بُّنَاۚ وَسِعَ R<رَ>بُّنَا كُلَّ شَیۡءٍ عِلۡمًاۚ عَلَى ٱللَّهِ تَوَكَّلۡنَاۚ R<رَ>بَّنَا ٱفۡتَحۡ بَیۡنَنَا وَبَیۡنَ قَوۡمِنَا بِX<ٱ>لۡحَقِّ وَأَN<ن>تَ خَیۡR<رُ> ٱلۡفَT<ـٰ>تِحِJ<ی>نَ A<۝>D<٨٩>",
"وَقَالَ ٱلۡمَلَأُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِN<ن> قَوۡمِهِT<ۦ> لَئِنِ ٱتَّبَعۡتُمۡ شُعَیۡبًا إِN<نَّ>كُمۡ إِذX<ࣰ>ا لَّخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<٩٠>",
"فَأَخَذَتۡهُمُ ٱX<ل>R<رَّ>Q<جۡ>فَةُ فَأَصۡبَحُوX<ا۟> فِی دَارِهِمۡ جَT<ـٰ>ثِمِJ<ی>نَ A<۝>D<٩١>",
"ٱلَّذِینَ كَذَّبُوX<ا۟> شُعَیۡبN<ࣰ>ا كَأَX<ن> لَّمۡ یَغۡنَوۡX<ا۟> فِیهَاۚ ٱلَّذِینَ كَذَّبُوX<ا۟> شُعَیۡبN<ࣰ>ا كَانُوX<ا۟> هُمُ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٩٢>",
"فَتَوَلَّىٰ عَنۡهُمۡ وَقَالَ یَT<ـٰ>قَوۡمِ لَقَQ<دۡ> أَQ<بۡ>لَغۡتُكُمۡ رِسَT<ـٰ>لَT<ـٰ>تِ R<رَ>بِّی وَنَصَحۡتُ لَكُمۡۖ فَكَیۡفَ ءَاسَىٰ عَلَىٰ قَوۡمN<ࣲ> كَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٩٣>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا فِی قَR<رۡ>یَةX<ࣲ> N<مِّ>ن N<نَّ>بِیٍّ إِلَّW<اۤ> أَخَذۡنَW<اۤ> أَهۡلَهَا بِX<ٱ>لۡبَأۡسَW<اۤ>ءِ وَX<ٱ>X<ل>ضَّR<رَّ>W<اۤ>ءِ لَعَلَّهُمۡ یَضَّR<رَّ>عُJ<و>نَ A<۝>D<٩٤>",
"ثُN<مَّ> بَدَّلۡنَا مَكَانَ ٱX<ل>سَّیِّئَةِ ٱلۡحَسَنَةَ حَتَّىٰ عَفَوX<ا۟> وَّقَالُوX<ا۟> قَQ<دۡ> مَسَّ ءَابَW<اۤ>ءَنَا ٱX<ل>ضَّR<رَّ>W<اۤ>ءُ وَX<ٱ>X<ل>سَّR<رَّ>W<اۤ>ءُ فَأَخَذۡنَT<ـٰ>هُN<م> بَغۡتَةX<ࣰ> N<وَ>هُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٩٥>",
"وَلَوۡ أَN<نَّ> أَهۡلَ ٱلۡقُR<رَ>ىW<ٰۤ> ءَامَنُوX<ا۟> وَX<ٱ>تَّقَوۡX<ا۟> لَفَتَحۡنَا عَلَیۡهِN<م> بَR<رَ>كَT<ـٰ>تX<ࣲ> N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِ وَلَT<ـٰ>كِN<ن> كَذَّبُوX<ا۟> فَأَخَذۡنَT<ـٰ>هُN<م> بِمَا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<٩٦>",
"أَفَأَمِنَ أَهۡلُ ٱلۡقُR<رَ>ىW<ٰۤ> أَX<ن> N<یَ>أۡتِیَهُN<م> بَأۡسُنَا بَیَT<ـٰ>تX<ࣰ>ا N<وَ>هُمۡ نَW<اۤ>ئِمُJ<و>نَ A<۝>D<٩٧>",
"أَوَ أَمِنَ أَهۡلُ ٱلۡقُR<رَ>ىW<ٰۤ> أَX<ن> N<یَ>أۡتِیَهُN<م> بَأۡسُنَا ضُحX<ࣰ>ى N<وَ>هُمۡ یَلۡعَبُJ<و>نَ A<۝>D<٩٨>",
"أَفَأَمِنُوX<ا۟> مَكۡR<رَ> ٱللَّهِۚ فَلَا یَأۡمَنُ مَكۡR<رَ> ٱللَّهِ إِلَّا ٱلۡقَوۡمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<٩٩>",
"أَوَ لَمۡ یَهۡدِ لِلَّذِینَ یَرِثُونَ ٱلۡأَR<رۡ>ضَ مِX<ن>N<ۢ> بَعۡدِ أَهۡلِهَW<اۤ> أَX<ن> لَّوۡ نَشَW<اۤ>ءُ أَصَQ<بۡ>نَT<ـٰ>هُN<م> بِذُنُوبِهِمۡۚ وَنَQ<طۡ>بَعُ عَلَىٰ قُلُوبِهِمۡ فَهُمۡ لَا یَسۡمَعُJ<و>نَ A<۝>D<١٠٠>",
"تِلۡكَ ٱلۡقُR<رَ>ىٰ نَقُصُّ عَلَیۡكَ مِنۡ أَX<ن>N<ۢ>بَW<اۤ>ئِهَاۚ وَلَقَQ<دۡ> جَW<اۤ>ءَتۡهُمۡ R<رُ>سُلُهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَمَا كَانُوX<ا۟> لِیُؤۡمِنُوX<ا۟> بِمَا كَذَّبُوX<ا۟> مِN<ن> قَQ<بۡ>لُۚ كَذَT< ٰ>لِكَ یَQ<طۡ>بَعُ ٱللَّهُ عَلَىٰ قُلُوبِ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٠١>",
"وَمَا وَجَQ<دۡ>نَا لِأَكۡثَرِهِم N<مِّ>نۡ عَهۡدX<ࣲ>ۖ N<وَ>إِX<ن> N<وَ>جَQ<دۡ>نَW<اۤ> أَكۡثَR<رَ>هُمۡ لَفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<١٠٢>",
"ثُN<مَّ> بَعَثۡنَا مِX<ن>N<ۢ> بَعۡدِهِم N<مُّ>وسَىٰ بِءَایَT<ـٰ>تِنَW<اۤ> إِلَىٰ فِرۡعَوۡنَ وَمَلَإِX<ی۟>هِT<ۦ> فَظَلَمُوX<ا۟> بِهَاۖ فَX<ٱ>N<ن>ظُR<رۡ> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<١٠٣>",
"وَقَالَ مُوسَىٰ یَT<ـٰ>فِرۡعَوۡنُ إِN<نِّ>ی R<رَ>سُولX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٠٤>",
"حَقِیقٌ عَلَىW<ٰۤ> أَX<ن> لَّW<اۤ> أَقُولَ عَلَى ٱللَّهِ إِلَّا ٱلۡحَقَّۚ قَQ<دۡ> جِئۡتُكُN<م> بِبَیِّنَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ فَأَR<رۡ>سِلۡ مَعِیَ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِJ<ی>لَ A<۝>D<١٠٥>",
"قَالَ إِN<ن> كُN<ن>تَ جِئۡتَ بِءَایَةN<ࣲ> فَأۡتِ بِهَW<اۤ> إِN<ن> كُN<ن>تَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٠٦>",
"فَأَلۡقَىٰ عَصَاهُ فَإِذَا هِیَ ثُعۡبَانX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٠٧>",
"وَنَزَعَ یَدَهُT<ۥ> فَإِذَا هِیَ بَیۡضَW<اۤ>ءُ لِX<ل>N<نَّ>T<ـٰ>ظِرِJ<ی>نَ A<۝>D<١٠٨>",
"قَالَ ٱلۡمَلَأُ مِN<ن> قَوۡمِ فِرۡعَوۡنَ إِN<نَّ> هَT<ـٰ>ذَا لَسَT<ـٰ>حِR<ر>ٌ عَلِJ<ی>مࣱ A<۝>D<١٠٩>",
"یُرِیدُ أَX<ن> N<یُ>خۡرِجَكُم N<مِّ>نۡ أَR<رۡ>ضِكُمۡۖ فَمَاذَا تَأۡمُR<رُ>J<و>نَ A<۝>D<١١٠>",
"قَالُW<وۤ>X<ا۟> أَR<رۡ>جِهۡ وَأَخَاهُ وَأَR<رۡ>سِلۡ فِی ٱلۡمَدَW<اۤ>ئِنِ حَT<ـٰ>شِرِJ<ی>نَ A<۝>D<١١١>",
"یَأۡتُوكَ بِكُلِّ سَT<ـٰ>حِرٍ عَلِJ<ی>مࣲ A<۝>D<١١٢>",
"وَجَW<اۤ>ءَ ٱX<ل>سَّحَR<رَ>ةُ فِرۡعَوۡنَ قَالُW<وۤ>X<ا۟> إِN<نَّ> لَنَا لَأَQ<جۡ>R<ر>ًا إِN<ن> كُN<نَّ>ا نَحۡنُ ٱلۡغَT<ـٰ>لِبِJ<ی>نَ A<۝>D<١١٣>",
"قَالَ نَعَمۡ وَإِN<نَّ>كُمۡ لَمِنَ ٱلۡمُقَR<رَّ>بِJ<ی>نَ A<۝>D<١١٤>",
"قَالُوX<ا۟> یَT<ـٰ>مُوسَىW<ٰۤ> إِN<مَّ>W<اۤ> أَN<ن> تُلۡقِیَ وَإِN<مَّ>W<اۤ> أَن N<نَّ>كُونَ نَحۡنُ ٱلۡمُلۡقِJ<ی>نَ A<۝>D<١١٥>",
"قَالَ أَلۡقُوX<ا۟>ۖ فَلَN<مَّ>W<اۤ> أَلۡقَوۡX<ا۟> سَحَR<رُ>W<وۤ>X<ا۟> أَعۡیُنَ ٱX<ل>N<نَّ>اسِ وَX<ٱ>سۡتَR<رۡ>هَبُوهُمۡ وَجَW<اۤ>ءُو بِسِحۡرٍ عَظِJ<ی>مࣲ A<۝>D<١١٦>",
"۞ وَأَوۡحَیۡنَW<اۤ> إِلَىٰ مُوسَىW<ٰۤ> أَنۡ أَلۡقِ عَصَاكَۖ فَإِذَا هِیَ تَلۡقَفُ مَا یَأۡفِكُJ<و>نَ A<۝>D<١١٧>",
"فَوَقَعَ ٱلۡحَقُّ وَبَطَلَ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١١٨>",
"فَغُلِبُوX<ا۟> هُنَالِكَ وَX<ٱ>N<ن>قَلَبُوX<ا۟> صَT<ـٰ>غِرِJ<ی>نَ A<۝>D<١١٩>",
"وَأُلۡقِیَ ٱX<ل>سَّحَR<رَ>ةُ سَT<ـٰ>جِدِJ<ی>نَ A<۝>D<١٢٠>",
"قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا بِR<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٢١>",
"R<رَ>بِّ مُوسَىٰ وَهَT<ـٰ>R<رُ>J<و>نَ A<۝>D<١٢٢>",
"قَالَ فِرۡعَوۡنُ ءَامَN<ن>تُN<م> بِهِT<ۦ> قَQ<بۡ>لَ أَنۡ ءَاذَنَ لَكُمۡۖ إِN<نَّ> هَT<ـٰ>ذَا لَمَكۡR<ر>X<ࣱ> N<مَّ>كَR<رۡ>تُمُوهُ فِی ٱلۡمَدِینَةِ لِتُخۡرِجُوX<ا۟> مِنۡهَW<اۤ> أَهۡلَهَاۖ فَسَوۡفَ تَعۡلَمُJ<و>نَ A<۝>D<١٢٣>",
"لَأُقَطِّعَN<نَّ> أَیۡدِیَكُمۡ وَأَR<رۡ>جُلَكُم N<مِّ>نۡ خِلَT<ـٰ>فN<ࣲ> ثُN<مَّ> لَأُصَلِّبَN<نَّ>كُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<١٢٤>",
"قَالُW<وۤ>X<ا۟> إِN<نَّ>W<اۤ> إِلَىٰ R<رَ>بِّنَا مُN<ن>قَلِبُJ<و>نَ A<۝>D<١٢٥>",
"وَمَا تَN<ن>قِمُ مِN<نَّ>W<اۤ> إِلَّW<اۤ> أَنۡ ءَامَN<نَّ>ا بِءَایَT<ـٰ>تِ R<رَ>بِّنَا لَN<مَّ>ا جَW<اۤ>ءَتۡنَاۚ R<رَ>بَّنَW<اۤ> أَفۡرِغۡ عَلَیۡنَا صَQ<بۡ>R<ر>X<ࣰ>ا N<وَ>تَوَفَّنَا مُسۡلِمِJ<ی>نَ A<۝>D<١٢٦>",
"وَقَالَ ٱلۡمَلَأُ مِN<ن> قَوۡمِ فِرۡعَوۡنَ أَتَذَR<رُ> مُوسَىٰ وَقَوۡمَهُT<ۥ> لِیُفۡسِدُوX<ا۟> فِی ٱلۡأَR<رۡ>ضِ وَیَذَR<رَ>كَ وَءَالِهَتَكَۚ قَالَ سَنُقَتِّلُ أَQ<بۡ>نَW<اۤ>ءَهُمۡ وَنَسۡتَحۡیِT<ۦ> نِسَW<اۤ>ءَهُمۡ وَإِN<نَّ>ا فَوۡقَهُمۡ قَT<ـٰ>هِR<رُ>J<و>نَ A<۝>D<١٢٧>",
"قَالَ مُوسَىٰ لِقَوۡمِهِ ٱسۡتَعِینُوX<ا۟> بِٱللَّهِ وَX<ٱ>صۡبِR<رُ>W<وۤ>X<ا۟>ۖ إِN<نَّ> ٱلۡأَR<رۡ>ضَ لِلَّهِ یُورِثُهَا مَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِT<ۦ>ۖ وَX<ٱ>لۡعَT<ـٰ>قِبَةُ لِلۡمُتَّقِJ<ی>نَ A<۝>D<١٢٨>",
"قَالُW<وۤ>X<ا۟> أُوذِینَا مِN<ن> قَQ<بۡ>لِ أَN<ن> تَأۡتِیَنَا وَمِX<ن>N<ۢ> بَعۡدِ مَا جِئۡتَنَاۚ قَالَ عَسَىٰ R<رَ>بُّكُمۡ أَX<ن> N<یُ>هۡلِكَ عَدُوَّكُمۡ وَیَسۡتَخۡلِفَكُمۡ فِی ٱلۡأَR<رۡ>ضِ فَیَN<ن>ظُR<رَ> كَیۡفَ تَعۡمَلُJ<و>نَ A<۝>D<١٢٩>",
"وَلَقَQ<دۡ> أَخَذۡنَW<اۤ> ءَالَ فِرۡعَوۡنَ بِX<ٱ>X<ل>سِّنِینَ وَنَQ<قۡ>صX<ࣲ> N<مِّ>نَ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِ لَعَلَّهُمۡ یَذَّكَّR<رُ>J<و>نَ A<۝>D<١٣٠>",
"فَإِذَا جَW<اۤ>ءَتۡهُمُ ٱلۡحَسَنَةُ قَالُوX<ا۟> لَنَا هَT<ـٰ>ذِهِT<ۦ>ۖ وَإِN<ن> تُصِQ<بۡ>هُمۡ سَیِّئَةX<ࣱ> N<یَ>طَّیَّR<رُ>وX<ا۟> بِمُوسَىٰ وَمَX<ن> N<مَّ>عَهُW<ۥۤ>ۗ أَلَW<اۤ> إِN<نَّ>مَا طَW<ـٰۤ>ئِR<رُ>هُمۡ عِN<ن>دَ ٱللَّهِ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<١٣١>",
"وَقَالُوX<ا۟> مَهۡمَا تَأۡتِنَا بِهِT<ۦ> مِنۡ ءَایَةX<ࣲ> لِّتَسۡحَR<رَ>نَا بِهَا فَمَا نَحۡنُ لَكَ بِمُؤۡمِنِJ<ی>نَ A<۝>D<١٣٢>",
"فَأَR<رۡ>سَلۡنَا عَلَیۡهِمُ ٱX<ل>طُّوفَانَ وَX<ٱ>لۡجَR<رَ>ادَ وَX<ٱ>لۡقُN<مَّ>لَ وَX<ٱ>X<ل>ضَّفَادِعَ وَX<ٱ>X<ل>دَّمَ ءَایَT<ـٰ>تX<ࣲ> N<مُّ>فَصَّلَT<ـٰ>تN<ࣲ> فَX<ٱ>سۡتَكۡبَR<رُ>وX<ا۟> وَكَانُوX<ا۟> قَوۡمX<ࣰ>ا N<مُّ>Q<جۡ>رِمِJ<ی>نَ A<۝>D<١٣٣>",
"وَلَN<مَّ>ا وَقَعَ عَلَیۡهِمُ ٱX<ل>رِّQ<جۡ>زُ قَالُوX<ا۟> یَT<ـٰ>مُوسَى ٱQ<دۡ>عُ لَنَا R<رَ>بَّكَ بِمَا عَهِدَ عِN<ن>دَكَۖ لَئِN<ن> كَشَفۡتَ عَN<نَّ>ا ٱX<ل>رِّQ<جۡ>زَ لَنُؤۡمِنَN<نَّ> لَكَ وَلَنُR<رۡ>سِلَN<نَّ> مَعَكَ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِJ<ی>لَ A<۝>D<١٣٤>",
"فَلَN<مَّ>ا كَشَفۡنَا عَنۡهُمُ ٱX<ل>رِّQ<جۡ>زَ إِلَىW<ٰۤ> أَجَلٍ هُN<م> بَT<ـٰ>لِغُوهُ إِذَا هُمۡ یَN<ن>كُثُJ<و>نَ A<۝>D<١٣٥>",
"فَX<ٱ>N<ن>تَقَمۡنَا مِنۡهُمۡ فَأَغۡR<رَ>Q<قۡ>نَT<ـٰ>هُمۡ فِی ٱلۡیَN<مِّ> بِأَN<نَّ>هُمۡ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا وَكَانُوX<ا۟> عَنۡهَا غَT<ـٰ>فِلِJ<ی>نَ A<۝>D<١٣٦>",
"وَأَوۡR<رَ>ثۡنَا ٱلۡقَوۡمَ ٱلَّذِینَ كَانُوX<ا۟> یُسۡتَضۡعَفُونَ مَشَT<ـٰ>رِقَ ٱلۡأَR<رۡ>ضِ وَمَغَT<ـٰ>رِبَهَا ٱلَّتِی بَT<ـٰ>R<رَ>كۡنَا فِیهَاۖ وَتَN<مَّ>تۡ كَلِمَتُ R<رَ>بِّكَ ٱلۡحُسۡنَىٰ عَلَىٰ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ بِمَا صَبَR<رُ>وX<ا۟>ۖ وَدَN<مَّ>R<رۡ>نَا مَا كَانَ یَصۡنَعُ فِرۡعَوۡنُ وَقَوۡمُهُT<ۥ> وَمَا كَانُوX<ا۟> یَعۡرِشُJ<و>نَ A<۝>D<١٣٧>",
"وَجَT<ـٰ>وَزۡنَا بِبَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ ٱلۡبَحۡR<رَ> فَأَتَوۡX<ا۟> عَلَىٰ قَوۡمX<ࣲ> N<یَ>عۡكُفُونَ عَلَىW<ٰۤ> أَصۡنَامX<ࣲ> لَّهُمۡۚ قَالُوX<ا۟> یَT<ـٰ>مُوسَى ٱQ<جۡ>عَل لَّنَW<اۤ> إِلَT<ـٰ>هN<ࣰ>ا كَمَا لَهُمۡ ءَالِهَةN<ࣱ>ۚ قَالَ إِN<نَّ>كُمۡ قَوۡمN<ࣱ> تَQ<جۡ>هَلُJ<و>نَ A<۝>D<١٣٨>",
"إِN<نَّ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ مُتَبَّR<ر>X<ࣱ> N<مَّ>ا هُمۡ فِیهِ وَبَT<ـٰ>طِلX<ࣱ> N<مَّ>ا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٣٩>",
"قَالَ أَغَیۡR<رَ> ٱللَّهِ أَQ<بۡ>غِیكُمۡ إِلَT<ـٰ>هX<ࣰ>ا N<وَ>هُوَ فَضَّلَكُمۡ عَلَى ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٤٠>",
"وَإِذۡ أَN<ن>جَیۡنَT<ـٰ>كُم N<مِّ>نۡ ءَالِ فِرۡعَوۡنَ یَسُومُونَكُمۡ سُW<وۤ>ءَ ٱلۡعَذَابِ یُقَتِّلُونَ أَQ<بۡ>نَW<اۤ>ءَكُمۡ وَیَسۡتَحۡیُونَ نِسَW<اۤ>ءَكُمۡۚ وَفِی ذَT< ٰ>لِكُN<م> بَلَW<اۤ>ءX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ عَظِJ<ی>مࣱ A<۝>D<١٤١>",
"۞ وَوَT< ٰ>عَQ<دۡ>نَا مُوسَىٰ ثَلَT<ـٰ>ثِینَ لَیۡلَةX<ࣰ> N<وَ>أَتۡمَمۡنَT<ـٰ>هَا بِعَشۡرN<ࣲ> فَتَN<مَّ> مِیقَT<ـٰ>تُ R<رَ>بِّهِW<ۦۤ> أَR<رۡ>بَعِینَ لَیۡلَةX<ࣰ>ۚ N<وَ>قَالَ مُوسَىٰ لِأَخِیهِ هَT<ـٰ>R<رُ>ونَ ٱخۡلُفۡنِی فِی قَوۡمِی وَأَصۡلِحۡ وَلَا تَتَّبِعۡ سَبِیلَ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<١٤٢>",
"وَلَN<مَّ>ا جَW<اۤ>ءَ مُوسَىٰ لِمِیقَT<ـٰ>تِنَا وَكَلَّمَهُT<ۥ> R<رَ>بُّهُT<ۥ> قَالَ R<رَ>بِّ أَرِنِW<یۤ> أَN<ن>ظُR<رۡ> إِلَیۡكَۚ قَالَ لَN<ن> تَR<رَ>X<ى>T<ٰ>نِی وَلَT<ـٰ>كِنِ ٱN<ن>ظُR<رۡ> إِلَى ٱلۡجَبَلِ فَإِنِ ٱسۡتَقَR<رَّ> مَكَانَهُT<ۥ> فَسَوۡفَ تَR<رَ>X<ى>T<ٰ>نِیۚ فَلَN<مَّ>ا تَجَلَّىٰ R<رَ>بُّهُT<ۥ> لِلۡجَبَلِ جَعَلَهُT<ۥ> دَكّX<ࣰ>ا N<وَ>خَR<رَّ> مُوسَىٰ صَعِقN<ࣰ>اۚ فَلَN<مَّ>W<اۤ> أَفَاقَ قَالَ سُQ<بۡ>حَT<ـٰ>نَكَ تُQ<بۡ>تُ إِلَیۡكَ وَأَنَا۠ أَوَّلُ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٤٣>",
"قَالَ یَT<ـٰ>مُوسَىW<ٰۤ> إِN<نِّ>ی ٱصۡطَفَیۡتُكَ عَلَى ٱX<ل>N<نَّ>اسِ بِرِسَT<ـٰ>لَT<ـٰ>تِی وَبِكَلَT<ـٰ>مِی فَخُذۡ مَW<اۤ> ءَاتَیۡتُكَ وَكُX<ن> N<مِّ>نَ ٱX<ل>شَّT<ـٰ>كِرِJ<ی>نَ A<۝>D<١٤٤>",
"وَكَتَQ<بۡ>نَا لَهُT<ۥ> فِی ٱلۡأَلۡوَاحِ مِN<ن> كُلِّ شَیۡءX<ࣲ> N<مَّ>وۡعِظَةX<ࣰ> N<وَ>تَفۡصِیلX<ࣰ>ا لِّكُلِّ شَیۡءN<ࣲ> فَخُذۡهَا بِقُوَّةX<ࣲ> N<وَ>أۡمُR<رۡ> قَوۡمَكَ یَأۡخُذُوX<ا۟> بِأَحۡسَنِهَاۚ سَأُX<و۟>رِیكُمۡ دَاR<رَ> ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<١٤٥>",
"سَأَصۡرِفُ عَنۡ ءَایَT<ـٰ>تِیَ ٱلَّذِینَ یَتَكَبَّR<رُ>ونَ فِی ٱلۡأَR<رۡ>ضِ بِغَیۡرِ ٱلۡحَقِّ وَإِX<ن> N<یَ>R<رَ>وۡX<ا۟> كُلَّ ءَایَةX<ࣲ> لَّا یُؤۡمِنُوX<ا۟> بِهَا وَإِX<ن> N<یَ>R<رَ>وۡX<ا۟> سَبِیلَ ٱX<ل>R<رُّ>شۡدِ لَا یَتَّخِذُوهُ سَبِیلX<ࣰ>ا N<وَ>إِX<ن> N<یَ>R<رَ>وۡX<ا۟> سَبِیلَ ٱلۡغَیِّ یَتَّخِذُوهُ سَبِیلN<ࣰ>اۚ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا وَكَانُوX<ا۟> عَنۡهَا غَT<ـٰ>فِلِJ<ی>نَ A<۝>D<١٤٦>",
"وَX<ٱ>لَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا وَلِقَW<اۤ>ءِ ٱلۡءَاخِR<رَ>ةِ حَبِطَتۡ أَعۡمَT<ـٰ>لُهُمۡۚ هَلۡ یُQ<جۡ>زَوۡنَ إِلَّا مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٤٧>",
"وَX<ٱ>تَّخَذَ قَوۡمُ مُوسَىٰ مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> مِنۡ حُلِیِّهِمۡ عِQ<جۡ>لN<ࣰ>ا جَسَدX<ࣰ>ا لَّهُT<ۥ> خُوَاR<ر>ٌۚ أَلَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ>هُT<ۥ> لَا یُكَلِّمُهُمۡ وَلَا یَهۡدِیهِمۡ سَبِیلًاۘ ٱتَّخَذُوهُ وَكَانُوX<ا۟> ظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٤٨>",
"وَلَN<مَّ>ا سُقِطَ فِW<یۤ> أَیۡدِیهِمۡ وَR<رَ>أَوۡX<ا۟> أَN<نَّ>هُمۡ قَQ<دۡ> ضَلُّوX<ا۟> قَالُوX<ا۟> لَئِX<ن> لَّمۡ یَR<رۡ>حَمۡنَا R<رَ>بُّنَا وَیَغۡفِرۡ لَنَا لَنَكُونَN<نَّ> مِنَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<١٤٩>",
"وَلَN<مَّ>ا R<رَ>جَعَ مُوسَىW<ٰۤ> إِلَىٰ قَوۡمِهِT<ۦ> غَضۡبَT<ـٰ>نَ أَسِفN<ࣰ>ا قَالَ بِئۡسَمَا خَلَفۡتُمُونِی مِX<ن>N<ۢ> بَعۡدِW<یۤ>ۖ أَعَجِلۡتُمۡ أَمۡR<رَ> R<رَ>بِّكُمۡۖ وَأَلۡقَى ٱلۡأَلۡوَاحَ وَأَخَذَ بِR<رَ>أۡسِ أَخِیهِ یَجُR<رُّ>هُW<ۥۤ> إِلَیۡهِۚ قَالَ ٱQ<بۡ>نَ أُN<مَّ> إِN<نَّ> ٱلۡقَوۡمَ ٱسۡتَضۡعَفُونِی وَكَادُوX<ا۟> یَQ<قۡ>تُلُونَنِی فَلَا تُشۡمِتۡ بِیَ ٱلۡأَعۡدَW<اۤ>ءَ وَلَا تَQ<جۡ>عَلۡنِی مَعَ ٱلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٥٠>",
"قَالَ R<رَ>بِّ ٱغۡفِرۡ لِی وَلِأَخِی وَأَQ<دۡ>خِلۡنَا فِی R<رَ>حۡمَتِكَۖ وَأَN<ن>تَ أَR<رۡ>حَمُ ٱX<ل>R<رَّ>T< ٰ>حِمِJ<ی>نَ A<۝>D<١٥١>",
"إِN<نَّ> ٱلَّذِینَ ٱتَّخَذُوX<ا۟> ٱلۡعِQ<جۡ>لَ سَیَنَالُهُمۡ غَضَبX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِمۡ وَذِلَّةN<ࣱ> فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۚ وَكَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُفۡتَرِJ<ی>نَ A<۝>D<١٥٢>",
"وَX<ٱ>لَّذِینَ عَمِلُوX<ا۟> ٱX<ل>سَّیِّءَاتِ ثُN<مَّ> تَابُوX<ا۟> مِX<ن>N<ۢ> بَعۡدِهَا وَءَامَنُW<وۤ>X<ا۟> إِN<نَّ> R<رَ>بَّكَ مِX<ن>N<ۢ> بَعۡدِهَا لَغَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٥٣>",
"وَلَN<مَّ>ا سَكَتَ عَX<ن> N<مُّ>وسَى ٱلۡغَضَبُ أَخَذَ ٱلۡأَلۡوَاحَۖ وَفِی نُسۡخَتِهَا هُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣱ> لِّلَّذِینَ هُمۡ لِR<رَ>بِّهِمۡ یَR<رۡ>هَبُJ<و>نَ A<۝>D<١٥٤>",
"وَX<ٱ>خۡتَاR<رَ> مُوسَىٰ قَوۡمَهُT<ۥ> سَQ<بۡ>عِینَ R<رَ>جُلX<ࣰ>ا لِّمِیقَT<ـٰ>تِنَاۖ فَلَN<مَّ>W<اۤ> أَخَذَتۡهُمُ ٱX<ل>R<رَّ>Q<جۡ>فَةُ قَالَ R<رَ>بِّ لَوۡ شِئۡتَ أَهۡلَكۡتَهُم N<مِّ>N<ن> قَQ<بۡ>لُ وَإِیَّT<ـٰ>یَۖ أَتُهۡلِكُنَا بِمَا فَعَلَ ٱX<ل>سُّفَهَW<اۤ>ءُ مِN<نَّ>W<اۤ>ۖ إِنۡ هِیَ إِلَّا فِتۡنَتُكَ تُضِلُّ بِهَا مَN<ن> تَشَW<اۤ>ءُ وَتَهۡدِی مَN<ن> تَشَW<اۤ>ءُۖ أَN<ن>تَ وَلِیُّنَا فَX<ٱ>غۡفِرۡ لَنَا وَX<ٱ>R<رۡ>حَمۡنَاۖ وَأَN<ن>تَ خَیۡR<رُ> ٱلۡغَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٥٥>",
"۞ وَX<ٱ>كۡتُQ<بۡ> لَنَا فِی هَT<ـٰ>ذِهِ ٱX<ل>دُّنۡیَا حَسَنَةX<ࣰ> N<وَ>فِی ٱلۡءَاخِR<رَ>ةِ إِN<نَّ>ا هُQ<دۡ>نَW<اۤ> إِلَیۡكَۚ قَالَ عَذَابِW<یۤ> أُصِیبُ بِهِT<ۦ> مَنۡ أَشَW<اۤ>ءُۖ وَR<رَ>حۡمَتِی وَسِعَتۡ كُلَّ شَیۡءN<ࣲ>ۚ فَسَأَكۡتُبُهَا لِلَّذِینَ یَتَّقُونَ وَیُؤۡتُونَ ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَX<ٱ>لَّذِینَ هُN<م> بِءَایَT<ـٰ>تِنَا یُؤۡمِنُJ<و>نَ A<۝>D<١٥٦>",
"ٱلَّذِینَ یَتَّبِعُونَ ٱX<ل>R<رَّ>سُولَ ٱX<ل>N<نَّ>بِیَّ ٱلۡأُN<مِّ>یَّ ٱلَّذِی یَجِدُونَهُT<ۥ> مَكۡتُوبًا عِN<ن>دَهُمۡ فِی ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةِ وَX<ٱ>لۡإِN<ن>جِیلِ یَأۡمُR<رُ>هُN<م> بِX<ٱ>لۡمَعۡR<رُ>وفِ وَیَنۡهَX<ى>T<ٰ>هُمۡ عَنِ ٱلۡمُN<ن>كَرِ وَیُحِلُّ لَهُمُ ٱX<ل>طَّیِّبَT<ـٰ>تِ وَیُحَرِّمُ عَلَیۡهِمُ ٱلۡخَبَW<ـٰۤ>ئِثَ وَیَضَعُ عَنۡهُمۡ إِصۡR<رَ>هُمۡ وَX<ٱ>لۡأَغۡلَT<ـٰ>لَ ٱلَّتِی كَانَتۡ عَلَیۡهِمۡۚ فَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> بِهِT<ۦ> وَعَزَّR<رُ>وهُ وَنَصَR<رُ>وهُ وَX<ٱ>تَّبَعُوX<ا۟> ٱX<ل>N<نُّ>وR<رَ> ٱلَّذِW<یۤ> أُN<ن>زِلَ مَعَهُW<ۥۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<١٥٧>",
"قُلۡ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ إِN<نِّ>ی R<رَ>سُولُ ٱللَّهِ إِلَیۡكُمۡ جَمِیعًا ٱلَّذِی لَهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ یُحۡیِT<ۦ> وَیُمِیتُۖ فَءَامِنُوX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِ ٱX<ل>N<نَّ>بِیِّ ٱلۡأُN<مِّ>یِّ ٱلَّذِی یُؤۡمِنُ بِٱللَّهِ وَكَلِمَT<ـٰ>تِهِT<ۦ> وَX<ٱ>تَّبِعُوهُ لَعَلَّكُمۡ تَهۡتَدُJ<و>نَ A<۝>D<١٥٨>",
"وَمِN<ن> قَوۡمِ مُوسَىW<ٰۤ> أُN<مَّ>ةX<ࣱ> N<یَ>هۡدُونَ بِX<ٱ>لۡحَقِّ وَبِهِT<ۦ> یَعۡدِلُJ<و>نَ A<۝>D<١٥٩>",
"وَقَطَّعۡنَT<ـٰ>هُمُ ٱثۡنَتَیۡ عَشۡR<رَ>ةَ أَسۡبَاطًا أُمَمX<ࣰ>اۚ N<وَ>أَوۡحَیۡنَW<اۤ> إِلَىٰ مُوسَىW<ٰۤ> إِذِ ٱسۡتَسۡقَX<ى>T<ٰ>هُ قَوۡمُهُW<ۥۤ> أَنِ ٱضۡرِب بِّعَصَاكَ ٱلۡحَجَR<رَ>ۖ فَX<ٱ>X<ن>N<ۢ>بَجَسَتۡ مِنۡهُ ٱثۡنَتَا عَشۡR<رَ>ةَ عَیۡنN<ࣰ>اۖ قَQ<دۡ> عَلِمَ كُلُّ أُنَاسX<ࣲ> N<مَّ>شۡR<رَ>بَهُمۡۚ وَظَلَّلۡنَا عَلَیۡهِمُ ٱلۡغَمَT<ـٰ>مَ وَأَN<ن>زَلۡنَا عَلَیۡهِمُ ٱلۡمَN<نَّ> وَX<ٱ>X<ل>سَّلۡوَىٰۖ كُلُوX<ا۟> مِN<ن> طَیِّبَT<ـٰ>تِ مَا R<رَ>زَQ<قۡ>نَT<ـٰ>كُمۡۚ وَمَا ظَلَمُونَا وَلَT<ـٰ>كِN<ن> كَانُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ یَظۡلِمُJ<و>نَ A<۝>D<١٦٠>",
"وَإِذۡ قِیلَ لَهُمُ ٱسۡكُنُوX<ا۟> هَT<ـٰ>ذِهِ ٱلۡقَR<رۡ>یَةَ وَكُلُوX<ا۟> مِنۡهَا حَیۡثُ شِئۡتُمۡ وَقُولُوX<ا۟> حِطَّةX<ࣱ> N<وَ>X<ٱ>Q<دۡ>خُلُوX<ا۟> ٱلۡبَابَ سُجَّدX<ࣰ>ا N<نَّ>غۡفِرۡ لَكُمۡ خَطِW<یۤ>ءَT<ـٰ>تِكُمۡۚ سَنَزِیدُ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٦١>",
"فَبَدَّلَ ٱلَّذِینَ ظَلَمُوX<ا۟> مِنۡهُمۡ قَوۡلًا غَیۡR<رَ> ٱلَّذِی قِیلَ لَهُمۡ فَأَR<رۡ>سَلۡنَا عَلَیۡهِمۡ رِQ<جۡ>زX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ بِمَا كَانُوX<ا۟> یَظۡلِمُJ<و>نَ A<۝>D<١٦٢>",
"وَسۡءَلۡهُمۡ عَنِ ٱلۡقَR<رۡ>یَةِ ٱلَّتِی كَانَتۡ حَاضِR<رَ>ةَ ٱلۡبَحۡرِ إِذۡ یَعۡدُونَ فِی ٱX<ل>سَّQ<بۡ>تِ إِذۡ تَأۡتِیهِمۡ حِیتَانُهُمۡ یَوۡمَ سَQ<بۡ>تِهِمۡ شُR<رَّ>عX<ࣰ>ا N<وَ>یَوۡمَ لَا یَسۡبِتُونَ لَا تَأۡتِیهِمۡۚ كَذَT< ٰ>لِكَ نَQ<بۡ>لُوهُN<م> بِمَا كَانُوX<ا۟> یَفۡسُقُJ<و>نَ A<۝>D<١٦٣>",
"وَإِذۡ قَالَتۡ أُN<مَّ>ةX<ࣱ> N<مِّ>نۡهُمۡ لِمَ تَعِظُونَ قَوۡمًا ٱللَّهُ مُهۡلِكُهُمۡ أَوۡ مُعَذِّبُهُمۡ عَذَابN<ࣰ>ا شَدِیدN<ࣰ>اۖ قَالُوX<ا۟> مَعۡذِR<رَ>ةً إِلَىٰ R<رَ>بِّكُمۡ وَلَعَلَّهُمۡ یَتَّقُJ<و>نَ A<۝>D<١٦٤>",
"فَلَN<مَّ>ا نَسُوX<ا۟> مَا ذُكِّR<رُ>وX<ا۟> بِهِW<ۦۤ> أَN<ن>جَیۡنَا ٱلَّذِینَ یَنۡهَوۡنَ عَنِ ٱX<ل>سُّW<وۤ>ءِ وَأَخَذۡنَا ٱلَّذِینَ ظَلَمُوX<ا۟> بِعَذَابِN<ۭ> بَءِیسِN<ۭ> بِمَا كَانُوX<ا۟> یَفۡسُقُJ<و>نَ A<۝>D<١٦٥>",
"فَلَN<مَّ>ا عَتَوۡX<ا۟> عَX<ن> N<مَّ>ا نُهُوX<ا۟> عَنۡهُ قُلۡنَا لَهُمۡ كُونُوX<ا۟> قِR<رَ>دَةً خَT<ـٰ>سِءِJ<ی>نَ A<۝>D<١٦٦>",
"وَإِذۡ تَأَذَّنَ R<رَ>بُّكَ لَیَQ<بۡ>عَثَN<نَّ> عَلَیۡهِمۡ إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ مَX<ن> N<یَ>سُومُهُمۡ سُW<وۤ>ءَ ٱلۡعَذَابِۗ إِN<نَّ> R<رَ>بَّكَ لَسَرِیعُ ٱلۡعِقَابِ وَإِN<نَّ>هُT<ۥ> لَغَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٦٧>",
"وَقَطَّعۡنَT<ـٰ>هُمۡ فِی ٱلۡأَR<رۡ>ضِ أُمَمX<ࣰ>اۖ N<مِّ>نۡهُمُ ٱX<ل>صَّT<ـٰ>لِحُونَ وَمِنۡهُمۡ دُونَ ذَT< ٰ>لِكَۖ وَبَلَوۡنَT<ـٰ>هُN<م> بِX<ٱ>لۡحَسَنَT<ـٰ>تِ وَX<ٱ>X<ل>سَّیِّءَاتِ لَعَلَّهُمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<١٦٨>",
"فَخَلَفَ مِX<ن>N<ۢ> بَعۡدِهِمۡ خَلۡفX<ࣱ> N<وَ>رِثُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ یَأۡخُذُونَ عَR<رَ>ضَ هَT<ـٰ>ذَا ٱلۡأَQ<دۡ>نَىٰ وَیَقُولُونَ سَیُغۡفَR<رُ> لَنَا وَإِX<ن> N<یَ>أۡتِهِمۡ عَR<رَ>ضX<ࣱ> N<مِّ>ثۡلُهُT<ۥ> یَأۡخُذُوهُۚ أَلَمۡ یُؤۡخَذۡ عَلَیۡهِم N<مِّ>یثَT<ـٰ>قُ ٱلۡكِتَT<ـٰ>بِ أَX<ن> لَّا یَقُولُوX<ا۟> عَلَى ٱللَّهِ إِلَّا ٱلۡحَقَّ وَدَR<رَ>سُوX<ا۟> مَا فِیهِۗ وَX<ٱ>X<ل>دَّاR<رُ> ٱلۡءَاخِR<رَ>ةُ خَیۡR<ر>X<ࣱ> لِّلَّذِینَ یَتَّقُونَۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<١٦٩>",
"وَX<ٱ>لَّذِینَ یُمَسِّكُونَ بِX<ٱ>لۡكِتَT<ـٰ>بِ وَأَقَامُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ إِN<نَّ>ا لَا نُضِیعُ أَQ<جۡ>R<رَ> ٱلۡمُصۡلِحِJ<ی>نَ A<۝>D<١٧٠>",
"۞ وَإِذۡ نَتَQ<قۡ>نَا ٱلۡجَبَلَ فَوۡقَهُمۡ كَأَN<نَّ>هُT<ۥ> ظُلَّةX<ࣱ> N<وَ>ظَN<نُّ>W<وۤ>X<ا۟> أَN<نَّ>هُT<ۥ> وَاقِعُN<ۢ> بِهِمۡ خُذُوX<ا۟> مَW<اۤ> ءَاتَیۡنَT<ـٰ>كُN<م> بِقُوَّةX<ࣲ> N<وَ>X<ٱ>ذۡكُR<رُ>وX<ا۟> مَا فِیهِ لَعَلَّكُمۡ تَتَّقُJ<و>نَ A<۝>D<١٧١>",
"وَإِذۡ أَخَذَ R<رَ>بُّكَ مِX<ن>N<ۢ> بَنِW<یۤ> ءَادَمَ مِN<ن> ظُهُورِهِمۡ ذُرِّیَّتَهُمۡ وَأَشۡهَدَهُمۡ عَلَىW<ٰۤ> أَN<ن>فُسِهِمۡ أَلَسۡتُ بِR<رَ>بِّكُمۡۖ قَالُوX<ا۟> بَلَىٰ شَهِQ<دۡ>نَW<اۤ>ۚ أَN<ن> تَقُولُوX<ا۟> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ إِN<نَّ>ا كُN<نَّ>ا عَنۡ هَT<ـٰ>ذَا غَT<ـٰ>فِلِJ<ی>نَ A<۝>D<١٧٢>",
"أَوۡ تَقُولُW<وۤ>X<ا۟> إِN<نَّ>مَW<اۤ> أَشۡR<رَ>كَ ءَابَW<اۤ>ؤُنَا مِN<ن> قَQ<بۡ>لُ وَكُN<نَّ>ا ذُرِّیَّةX<ࣰ> N<مِّ>X<ن>N<ۢ> بَعۡدِهِمۡۖ أَفَتُهۡلِكُنَا بِمَا فَعَلَ ٱلۡمُQ<بۡ>طِلُJ<و>نَ A<۝>D<١٧٣>",
"وَكَذَT< ٰ>لِكَ نُفَصِّلُ ٱلۡءَایَT<ـٰ>تِ وَلَعَلَّهُمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<١٧٤>",
"وَX<ٱ>تۡلُ عَلَیۡهِمۡ نَبَأَ ٱلَّذِW<یۤ> ءَاتَیۡنَT<ـٰ>هُ ءَایَT<ـٰ>تِنَا فَX<ٱ>N<ن>سَلَخَ مِنۡهَا فَأَتۡبَعَهُ ٱX<ل>شَّیۡطَT<ـٰ>نُ فَكَانَ مِنَ ٱلۡغَاوِJ<ی>نَ A<۝>D<١٧٥>",
"وَلَوۡ شِئۡنَا لَR<رَ>فَعۡنَT<ـٰ>هُ بِهَا وَلَT<ـٰ>كِN<نَّ>هُW<ۥۤ> أَخۡلَدَ إِلَى ٱلۡأَR<رۡ>ضِ وَX<ٱ>تَّبَعَ هَوَX<ى>T<ٰ>هُۚ فَمَثَلُهُT<ۥ> كَمَثَلِ ٱلۡكَلۡبِ إِN<ن> تَحۡمِلۡ عَلَیۡهِ یَلۡهَثۡ أَوۡ تَتۡR<رُ>كۡهُ یَلۡهَثۚ ذَّT< ٰ>لِكَ مَثَلُ ٱلۡقَوۡمِ ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَاۚ فَX<ٱ>Q<قۡ>صُصِ ٱلۡقَصَصَ لَعَلَّهُمۡ یَتَفَكَّR<رُ>J<و>نَ A<۝>D<١٧٦>",
"سَW<اۤ>ءَ مَثَلًا ٱلۡقَوۡمُ ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا وَأَN<ن>فُسَهُمۡ كَانُوX<ا۟> یَظۡلِمُJ<و>نَ A<۝>D<١٧٧>",
"مَX<ن> N<یَ>هۡدِ ٱللَّهُ فَهُوَ ٱلۡمُهۡتَدِیۖ وَمَX<ن> N<یُ>ضۡلِلۡ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<١٧٨>",
"وَلَقَQ<دۡ> ذَR<رَ>أۡنَا لِجَهَN<نَّ>مَ كَثِیR<ر>X<ࣰ>ا N<مِّ>نَ ٱلۡجِN<نِّ> وَX<ٱ>لۡإِN<ن>سِۖ لَهُمۡ قُلُوبX<ࣱ> لَّا یَفۡقَهُونَ بِهَا وَلَهُمۡ أَعۡیُنX<ࣱ> لَّا یُQ<بۡ>صِR<رُ>ونَ بِهَا وَلَهُمۡ ءَاذَانX<ࣱ> لَّا یَسۡمَعُونَ بِهَW<اۤ>ۚ أُX<و۟>لَW<ـٰۤ>ئِكَ كَX<ٱ>لۡأَنۡعَT<ـٰ>مِ بَلۡ هُمۡ أَضَلُّۚ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡغَT<ـٰ>فِلُJ<و>نَ A<۝>D<١٧٩>",
"وَلِلَّهِ ٱلۡأَسۡمَW<اۤ>ءُ ٱلۡحُسۡنَىٰ فَX<ٱ>Q<دۡ>عُوهُ بِهَاۖ وَذَR<رُ>وX<ا۟> ٱلَّذِینَ یُلۡحِدُونَ فِW<یۤ> أَسۡمَW<ـٰۤ>ئِهِT<ۦ>ۚ سَیُQ<جۡ>زَوۡنَ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٨٠>",
"وَمِN<مَّ>نۡ خَلَQ<قۡ>نَW<اۤ> أُN<مَّ>ةX<ࣱ> N<یَ>هۡدُونَ بِX<ٱ>لۡحَقِّ وَبِهِT<ۦ> یَعۡدِلُJ<و>نَ A<۝>D<١٨١>",
"وَX<ٱ>لَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا سَنَسۡتَQ<دۡ>رِجُهُم N<مِّ>نۡ حَیۡثُ لَا یَعۡلَمُJ<و>نَ A<۝>D<١٨٢>",
"وَأُمۡلِی لَهُمۡۚ إِN<نَّ> كَیۡدِی مَتِJ<ی>نٌ A<۝>D<١٨٣>",
"أَوَ لَمۡ یَتَفَكَّR<رُ>وX<ا۟>ۗ مَا بِصَاحِبِهِم N<مِّ>N<ن> جِN<نَّ>ةٍۚ إِنۡ هُوَ إِلَّا نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<١٨٤>",
"أَوَ لَمۡ یَN<ن>ظُR<رُ>وX<ا۟> فِی مَلَكُوتِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا خَلَقَ ٱللَّهُ مِN<ن> شَیۡءX<ࣲ> N<وَ>أَنۡ عَسَىW<ٰۤ> أَX<ن> N<یَ>كُونَ قَدِ ٱQ<قۡ>تَR<رَ>بَ أَجَلُهُمۡۖ فَبِأَیِّ حَدِیثِN<ۭ> بَعۡدَهُT<ۥ> یُؤۡمِنُJ<و>نَ A<۝>D<١٨٥>",
"مَX<ن> N<یُ>ضۡلِلِ ٱللَّهُ فَلَا هَادِیَ لَهُT<ۥ>ۚ وَیَذَR<رُ>هُمۡ فِی طُغۡیَT<ـٰ>نِهِمۡ یَعۡمَهُJ<و>نَ A<۝>D<١٨٦>",
"یَسۡءَلُونَكَ عَنِ ٱX<ل>سَّاعَةِ أَیَّانَ مُR<رۡ>سَX<ى>T<ٰ>هَاۖ قُلۡ إِN<نَّ>مَا عِلۡمُهَا عِN<ن>دَ R<رَ>بِّیۖ لَا یُجَلِّیهَا لِوَQ<قۡ>تِهَW<اۤ> إِلَّا هُوَۚ ثَقُلَتۡ فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ لَا تَأۡتِیكُمۡ إِلَّا بَغۡتَةX<ࣰ>ۗ N<یَ>سۡءَلُونَكَ كَأَN<نَّ>كَ حَفِیٌّ عَنۡهَاۖ قُلۡ إِN<نَّ>مَا عِلۡمُهَا عِN<ن>دَ ٱللَّهِ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<١٨٧>",
"قُل لَّW<اۤ> أَمۡلِكُ لِنَفۡسِی نَفۡعX<ࣰ>ا N<وَ>لَا ضَR<رّ>ًا إِلَّا مَا شَW<اۤ>ءَ ٱللَّهُۚ وَلَوۡ كُN<ن>تُ أَعۡلَمُ ٱلۡغَیۡبَ لَX<ٱ>سۡتَكۡثَR<رۡ>تُ مِنَ ٱلۡخَیۡرِ وَمَا مَسَّنِیَ ٱX<ل>سُّW<وۤ>ءُۚ إِنۡ أَنَا۠ إِلَّا نَذِیR<ر>X<ࣱ> N<وَ>بَشِیR<ر>X<ࣱ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<١٨٨>",
"۞ هُوَ ٱلَّذِی خَلَقَكُم N<مِّ>ن N<نَّ>فۡسX<ࣲ> N<وَ>T< ٰ>حِدَةX<ࣲ> N<وَ>جَعَلَ مِنۡهَا زَوۡجَهَا لِیَسۡكُنَ إِلَیۡهَاۖ فَلَN<مَّ>ا تَغَشَّX<ى>T<ٰ>هَا حَمَلَتۡ حَمۡلًا خَفِیفN<ࣰ>ا فَمَR<رَّ>تۡ بِهِT<ۦ>ۖ فَلَN<مَّ>W<اۤ> أَثۡقَلَت دَّعَوَا ٱللَّهَ R<رَ>بَّهُمَا لَئِنۡ ءَاتَیۡتَنَا صَT<ـٰ>لِحX<ࣰ>ا لَّنَكُونَN<نَّ> مِنَ ٱX<ل>شَّT<ـٰ>كِرِJ<ی>نَ A<۝>D<١٨٩>",
"فَلَN<مَّ>W<اۤ> ءَاتَX<ى>T<ٰ>هُمَا صَT<ـٰ>لِحN<ࣰ>ا جَعَلَا لَهُT<ۥ> شُR<رَ>كَW<اۤ>ءَ فِیمَW<اۤ> ءَاتَX<ى>T<ٰ>هُمَاۚ فَتَعَT<ـٰ>لَى ٱللَّهُ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<١٩٠>",
"أَیُشۡرِكُونَ مَا لَا یَخۡلُقُ شَیۡءX<ࣰ>ا N<وَ>هُمۡ یُخۡلَقُJ<و>نَ A<۝>D<١٩١>",
"وَلَا یَسۡتَطِیعُونَ لَهُمۡ نَصۡR<ر>X<ࣰ>ا N<وَ>لَW<اۤ> أَN<ن>فُسَهُمۡ یَN<ن>صُR<رُ>J<و>نَ A<۝>D<١٩٢>",
"وَإِN<ن> تَQ<دۡ>عُوهُمۡ إِلَى ٱلۡهُدَىٰ لَا یَتَّبِعُوكُمۡۚ سَوَW<اۤ>ءٌ عَلَیۡكُمۡ أَدَعَوۡتُمُوهُمۡ أَمۡ أَN<ن>تُمۡ صَT<ـٰ>مِتُJ<و>نَ A<۝>D<١٩٣>",
"إِN<نَّ> ٱلَّذِینَ تَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ عِبَادٌ أَمۡثَالُكُمۡۖ فَX<ٱ>Q<دۡ>عُوهُمۡ فَلۡیَسۡتَجِیبُوX<ا۟> لَكُمۡ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٩٤>",
"أَلَهُمۡ أَR<رۡ>جُلX<ࣱ> N<یَ>مۡشُونَ بِهَW<اۤ>ۖ أَمۡ لَهُمۡ أَیۡدX<ࣲ> N<یَ>Q<بۡ>طِشُونَ بِهَW<اۤ>ۖ أَمۡ لَهُمۡ أَعۡیُنX<ࣱ> N<یُ>Q<بۡ>صِR<رُ>ونَ بِهَW<اۤ>ۖ أَمۡ لَهُمۡ ءَاذَانX<ࣱ> N<یَ>سۡمَعُونَ بِهَاۗ قُلِ ٱQ<دۡ>عُوX<ا۟> شُR<رَ>كَW<اۤ>ءَكُمۡ ثُN<مَّ> كِیدُونِ فَلَا تُN<ن>ظِR<رُ>J<و>نِ A<۝>D<١٩٥>",
"إِN<نَّ> وَلِT<ـۧ>ِّیَ ٱللَّهُ ٱلَّذِی نَزَّلَ ٱلۡكِتَT<ـٰ>بَۖ وَهُوَ یَتَوَلَّى ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<١٩٦>",
"وَX<ٱ>لَّذِینَ تَQ<دۡ>عُونَ مِN<ن> دُونِهِT<ۦ> لَا یَسۡتَطِیعُونَ نَصۡR<رَ>كُمۡ وَلَW<اۤ> أَN<ن>فُسَهُمۡ یَN<ن>صُR<رُ>J<و>نَ A<۝>D<١٩٧>",
"وَإِN<ن> تَQ<دۡ>عُوهُمۡ إِلَى ٱلۡهُدَىٰ لَا یَسۡمَعُوX<ا۟>ۖ وَتَR<رَ>X<ى>T<ٰ>هُمۡ یَN<ن>ظُR<رُ>ونَ إِلَیۡكَ وَهُمۡ لَا یُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<١٩٨>",
"خُذِ ٱلۡعَفۡوَ وَأۡمُR<رۡ> بِX<ٱ>لۡعُR<رۡ>فِ وَأَعۡرِضۡ عَنِ ٱلۡجَT<ـٰ>هِلِJ<ی>نَ A<۝>D<١٩٩>",
"وَإِN<مَّ>ا یَN<ن>زَغَN<نَّ>كَ مِنَ ٱX<ل>شَّیۡطَT<ـٰ>نِ نَزۡغN<ࣱ> فَX<ٱ>سۡتَعِذۡ بِٱللَّهِۚ إِN<نَّ>هُT<ۥ> سَمِیعٌ عَلِJ<ی>مٌ A<۝>D<٢٠٠>",
"إِN<نَّ> ٱلَّذِینَ ٱتَّقَوۡX<ا۟> إِذَا مَسَّهُمۡ طَW<ـٰۤ>ئِفX<ࣱ> N<مِّ>نَ ٱX<ل>شَّیۡطَT<ـٰ>نِ تَذَكَّR<رُ>وX<ا۟> فَإِذَا هُم N<مُّ>Q<بۡ>صِR<رُ>J<و>نَ A<۝>D<٢٠١>",
"وَإِخۡوَT< ٰ>نُهُمۡ یَمُدُّونَهُمۡ فِی ٱلۡغَیِّ ثُN<مَّ> لَا یُQ<قۡ>صِR<رُ>J<و>نَ A<۝>D<٢٠٢>",
"وَإِذَا لَمۡ تَأۡتِهِN<م> بِءَایَةN<ࣲ> قَالُوX<ا۟> لَوۡلَا ٱQ<جۡ>تَبَیۡتَهَاۚ قُلۡ إِN<نَّ>مَW<اۤ> أَتَّبِعُ مَا یُوحَىW<ٰۤ> إِلَیَّ مِX<ن> R<رَّ>بِّیۚ هَT<ـٰ>ذَا بَصَW<اۤ>ئِR<رُ> مِX<ن> R<رَّ>بِّكُمۡ وَهُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣱ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٢٠٣>",
"وَإِذَا قُرِئَ ٱلۡقُR<رۡ>ءَانُ فَX<ٱ>سۡتَمِعُوX<ا۟> لَهُT<ۥ> وَأَN<ن>صِتُوX<ا۟> لَعَلَّكُمۡ تُR<رۡ>حَمُJ<و>نَ A<۝>D<٢٠٤>",
"وَX<ٱ>ذۡكُر R<رَّ>بَّكَ فِی نَفۡسِكَ تَضَR<رُّ>عX<ࣰ>ا N<وَ>خِیفَةX<ࣰ> N<وَ>دُونَ ٱلۡجَهۡرِ مِنَ ٱلۡقَوۡلِ بِX<ٱ>لۡغُدُوِّ وَX<ٱ>لۡءَاصَالِ وَلَا تَكُX<ن> N<مِّ>نَ ٱلۡغَT<ـٰ>فِلِJ<ی>نَ A<۝>D<٢٠٥>",
"إِN<نَّ> ٱلَّذِینَ عِN<ن>دَ R<رَ>بِّكَ لَا یَسۡتَكۡبِR<رُ>ونَ عَنۡ عِبَادَتِهِT<ۦ> وَیُسَبِّحُونَهُT<ۥ> وَلَهُT<ۥ> یَ̅سۡ̅جُ̅دُ̅J<و>̅نَ̅ A<۝>D<٢٠٦> ۩",
"#۞ یَسۡءَلُونَكَ عَنِ ٱلۡأَN<ن>فَالِۖ قُلِ ٱلۡأَN<ن>فَالُ لِلَّهِ وَX<ٱ>X<ل>R<رَّ>سُولِۖ فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَصۡلِحُوX<ا۟> ذَاتَ بَیۡنِكُمۡۖ وَأَطِیعُوX<ا۟> ٱللَّهَ وَR<رَ>سُولَهُW<ۥۤ> إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١>",
"إِN<نَّ>مَا ٱلۡمُؤۡمِنُونَ ٱلَّذِینَ إِذَا ذُكِR<رَ> ٱللَّهُ وَجِلَتۡ قُلُوبُهُمۡ وَإِذَا تُلِیَتۡ عَلَیۡهِمۡ ءَایَT<ـٰ>تُهُT<ۥ> زَادَتۡهُمۡ إِیمَT<ـٰ>نX<ࣰ>ا N<وَ>عَلَىٰ R<رَ>بِّهِمۡ یَتَوَكَّلُJ<و>نَ A<۝>D<٢>",
"ٱلَّذِینَ یُقِیمُونَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَمِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡ یُN<ن>فِقُJ<و>نَ A<۝>D<٣>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُؤۡمِنُونَ حَقّX<ࣰ>اۚ لَّهُمۡ دَR<رَ>جَT<ـٰ>تٌ عِN<ن>دَ R<رَ>بِّهِمۡ وَمَغۡفِR<رَ>ةX<ࣱ> N<وَ>رِزۡقN<ࣱ> كَرِJ<ی>مࣱ A<۝>D<٤>",
"كَمَW<اۤ> أَخۡR<رَ>جَكَ R<رَ>بُّكَ مِX<ن>N<ۢ> بَیۡتِكَ بِX<ٱ>لۡحَقِّ وَإِN<نَّ> فَرِیقX<ࣰ>ا N<مِّ>نَ ٱلۡمُؤۡمِنِینَ لَكَT<ـٰ>رِهُJ<و>نَ A<۝>D<٥>",
"یُجَT<ـٰ>دِلُونَكَ فِی ٱلۡحَقِّ بَعۡدَ مَا تَبَیَّنَ كَأَN<نَّ>مَا یُسَاقُونَ إِلَى ٱلۡمَوۡتِ وَهُمۡ یَN<ن>ظُR<رُ>J<و>نَ A<۝>D<٦>",
"وَإِذۡ یَعِدُكُمُ ٱللَّهُ إِحۡدَى ٱX<ل>طَّW<اۤ>ئِفَتَیۡنِ أَN<نَّ>هَا لَكُمۡ وَتَوَدُّونَ أَN<نَّ> غَیۡR<رَ> ذَاتِ ٱX<ل>شَّوۡكَةِ تَكُونُ لَكُمۡ وَیُرِیدُ ٱللَّهُ أَX<ن> N<یُ>حِقَّ ٱلۡحَقَّ بِكَلِمَT<ـٰ>تِهِT<ۦ> وَیَQ<قۡ>طَعَ دَابِR<رَ> ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٧>",
"لِیُحِقَّ ٱلۡحَقَّ وَیُQ<بۡ>طِلَ ٱلۡبَT<ـٰ>طِلَ وَلَوۡ كَرِهَ ٱلۡمُQ<جۡ>رِمُJ<و>نَ A<۝>D<٨>",
"إِذۡ تَسۡتَغِیثُونَ R<رَ>بَّكُمۡ فَX<ٱ>سۡتَجَابَ لَكُمۡ أَN<نِّ>ی مُمِدُّكُN<م> بِأَلۡفX<ࣲ> N<مِّ>نَ ٱلۡمَلَW<ـٰۤ>ئِكَةِ مُR<رۡ>دِفِJ<ی>نَ A<۝>D<٩>",
"وَمَا جَعَلَهُ ٱللَّهُ إِلَّا بُشۡR<رَ>ىٰ وَلِتَQ<طۡ>مَئِN<نَّ> بِهِT<ۦ> قُلُوبُكُمۡۚ وَمَا ٱX<ل>N<نَّ>صۡR<رُ> إِلَّا مِنۡ عِN<ن>دِ ٱللَّهِۚ إِN<نَّ> ٱللَّهَ عَزِیزٌ حَكِJ<ی>مٌ A<۝>D<١٠>",
"إِذۡ یُغَشِّیكُمُ ٱX<ل>N<نُّ>عَاسَ أَمَنَةX<ࣰ> N<مِّ>نۡهُ وَیُنَزِّلُ عَلَیۡكُم N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءX<ࣰ> لِّیُطَهِّR<رَ>كُN<م> بِهِT<ۦ> وَیُذۡهِبَ عَN<ن>كُمۡ رِQ<جۡ>زَ ٱX<ل>شَّیۡطَT<ـٰ>نِ وَلِیَR<رۡ>بِطَ عَلَىٰ قُلُوبِكُمۡ وَیُثَبِّتَ بِهِ ٱلۡأَQ<قۡ>دَJ<ا>مَ A<۝>D<١١>",
"إِذۡ یُوحِی R<رَ>بُّكَ إِلَى ٱلۡمَلَW<ـٰۤ>ئِكَةِ أَN<نِّ>ی مَعَكُمۡ فَثَبِّتُوX<ا۟> ٱلَّذِینَ ءَامَنُوX<ا۟>ۚ سَأُلۡقِی فِی قُلُوبِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> ٱX<ل>R<رُّ>عۡبَ فَX<ٱ>ضۡرِبُوX<ا۟> فَوۡقَ ٱلۡأَعۡنَاقِ وَX<ٱ>ضۡرِبُوX<ا۟> مِنۡهُمۡ كُلَّ بَنَJ<ا>نࣲ A<۝>D<١٢>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ شَL<اۤ>قُّوX<ا۟> ٱللَّهَ وَR<رَ>سُولَهُT<ۥ>ۚ وَمَX<ن> N<یُ>شَاقِقِ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> فَإِN<نَّ> ٱللَّهَ شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<١٣>",
"ذَT< ٰ>لِكُمۡ فَذُوقُوهُ وَأَN<نَّ> لِلۡكَT<ـٰ>فِرِینَ عَذَابَ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<١٤>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا لَقِیتُمُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> زَحۡفN<ࣰ>ا فَلَا تُوَلُّوهُمُ ٱلۡأَQ<دۡ>بَJ<ا>R<ر>َ A<۝>D<١٥>",
"وَمَX<ن> N<یُ>وَلِّهِمۡ یَوۡمَئِذN<ࣲ> دُبُR<رَ>هُW<ۥۤ> إِلَّا مُتَحَرِّفX<ࣰ>ا لِّقِتَالٍ أَوۡ مُتَحَیِّزًا إِلَىٰ فِئَةN<ࣲ> فَقَQ<دۡ> بَW<اۤ>ءَ بِغَضَبX<ࣲ> N<مِّ>نَ ٱللَّهِ وَمَأۡوَX<ى>T<ٰ>هُ جَهَN<نَّ>مُۖ وَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<١٦>",
"فَلَمۡ تَQ<قۡ>تُلُوهُمۡ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ قَتَلَهُمۡۚ وَمَا R<رَ>مَیۡتَ إِذۡ R<رَ>مَیۡتَ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ R<رَ>مَىٰ وَلِیُQ<بۡ>لِیَ ٱلۡمُؤۡمِنِینَ مِنۡهُ بَلَW<اۤ>ءً حَسَنًاۚ إِN<نَّ> ٱللَّهَ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<١٧>",
"ذَT< ٰ>لِكُمۡ وَأَN<نَّ> ٱللَّهَ مُوهِنُ كَیۡدِ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٨>",
"إِN<ن> تَسۡتَفۡتِحُوX<ا۟> فَقَQ<دۡ> جَW<اۤ>ءَكُمُ ٱلۡفَتۡحُۖ وَإِN<ن> تَN<ن>تَهُوX<ا۟> فَهُوَ خَیۡR<ر>X<ࣱ> لَّكُمۡۖ وَإِN<ن> تَعُودُوX<ا۟> نَعُQ<دۡ> وَلَN<ن> تُغۡنِیَ عَN<ن>كُمۡ فِئَتُكُمۡ شَیۡءX<ࣰ>ا N<وَ>لَوۡ كَثُR<رَ>تۡ وَأَN<نَّ> ٱللَّهَ مَعَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٩>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَطِیعُوX<ا۟> ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> وَلَا تَوَلَّوۡX<ا۟> عَنۡهُ وَأَN<ن>تُمۡ تَسۡمَعُJ<و>نَ A<۝>D<٢٠>",
"وَلَا تَكُونُوX<ا۟> كَX<ٱ>لَّذِینَ قَالُوX<ا۟> سَمِعۡنَا وَهُمۡ لَا یَسۡمَعُJ<و>نَ A<۝>D<٢١>",
"۞ إِN<نَّ> شَR<رَّ> ٱX<ل>دَّوَL<اۤ>بِّ عِN<ن>دَ ٱللَّهِ ٱX<ل>صُّN<مُّ> ٱلۡبُكۡمُ ٱلَّذِینَ لَا یَعۡقِلُJ<و>نَ A<۝>D<٢٢>",
"وَلَوۡ عَلِمَ ٱللَّهُ فِیهِمۡ خَیۡR<ر>X<ࣰ>ا لَّأَسۡمَعَهُمۡۖ وَلَوۡ أَسۡمَعَهُمۡ لَتَوَلَّوX<ا۟> وَّهُم N<مُّ>عۡرِضُJ<و>نَ A<۝>D<٢٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱسۡتَجِیبُوX<ا۟> لِلَّهِ وَلِX<ل>R<رَّ>سُولِ إِذَا دَعَاكُمۡ لِمَا یُحۡیِیكُمۡۖ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ یَحُولُ بَیۡنَ ٱلۡمَR<رۡ>ءِ وَقَلۡبِهِT<ۦ> وَأَN<نَّ>هُW<ۥۤ> إِلَیۡهِ تُحۡشَR<رُ>J<و>نَ A<۝>D<٢٤>",
"وَX<ٱ>تَّقُوX<ا۟> فِتۡنَةX<ࣰ> لَّا تُصِیبَN<نَّ> ٱلَّذِینَ ظَلَمُوX<ا۟> مِN<ن>كُمۡ خَL<اۤ>صَّةX<ࣰ>ۖ N<وَ>X<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<٢٥>",
"وَX<ٱ>ذۡكُR<رُ>W<وۤ>X<ا۟> إِذۡ أَN<ن>تُمۡ قَلِیلX<ࣱ> N<مُّ>سۡتَضۡعَفُونَ فِی ٱلۡأَR<رۡ>ضِ تَخَافُونَ أَX<ن> N<یَ>تَخَطَّفَكُمُ ٱX<ل>N<نَّ>اسُ فَءَاوَX<ى>T<ٰ>كُمۡ وَأَیَّدَكُN<م> بِنَصۡرِهِT<ۦ> وَR<رَ>زَقَكُم N<مِّ>نَ ٱX<ل>طَّیِّبَT<ـٰ>تِ لَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<٢٦>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَخُونُوX<ا۟> ٱللَّهَ وَX<ٱ>X<ل>R<رَّ>سُولَ وَتَخُونُW<وۤ>X<ا۟> أَمَT<ـٰ>نَT<ـٰ>تِكُمۡ وَأَN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٢٧>",
"وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ>مَW<اۤ> أَمۡوَT< ٰ>لُكُمۡ وَأَوۡلَT<ـٰ>دُكُمۡ فِتۡنَةX<ࣱ> N<وَ>أَN<نَّ> ٱللَّهَ عِN<ن>دَهُW<ۥۤ> أَQ<جۡ>R<ر>ٌ عَظِJ<ی>مࣱ A<۝>D<٢٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<ن> تَتَّقُوX<ا۟> ٱللَّهَ یَQ<جۡ>عَل لَّكُمۡ فُR<رۡ>قَانX<ࣰ>ا N<وَ>یُكَفِّرۡ عَN<ن>كُمۡ سَیِّءَاتِكُمۡ وَیَغۡفِرۡ لَكُمۡۗ وَٱللَّهُ ذُو ٱلۡفَضۡلِ ٱلۡعَظِJ<ی>مِ A<۝>D<٢٩>",
"وَإِذۡ یَمۡكُR<رُ> بِكَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لِیُثۡبِتُوكَ أَوۡ یَQ<قۡ>تُلُوكَ أَوۡ یُخۡرِجُوكَۚ وَیَمۡكُR<رُ>ونَ وَیَمۡكُR<رُ> ٱللَّهُۖ وَٱللَّهُ خَیۡR<رُ> ٱلۡمَT<ـٰ>كِرِJ<ی>نَ A<۝>D<٣٠>",
"وَإِذَا تُتۡلَىٰ عَلَیۡهِمۡ ءَایَT<ـٰ>تُنَا قَالُوX<ا۟> قَQ<دۡ> سَمِعۡنَا لَوۡ نَشَW<اۤ>ءُ لَقُلۡنَا مِثۡلَ هَT<ـٰ>ذَW<اۤ> إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّW<اۤ> أَسَT<ـٰ>طِیR<رُ> ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٣١>",
"وَإِذۡ قَالُوX<ا۟> ٱللَّهُN<مَّ> إِN<ن> كَانَ هَT<ـٰ>ذَا هُوَ ٱلۡحَقَّ مِنۡ عِN<ن>دِكَ فَأَمۡطِرۡ عَلَیۡنَا حِجَاR<رَ>ةX<ࣰ> N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ أَوِ ٱئۡتِنَا بِعَذَابٍ أَلِJ<ی>مࣲ A<۝>D<٣٢>",
"وَمَا كَانَ ٱللَّهُ لِیُعَذِّبَهُمۡ وَأَN<ن>تَ فِیهِمۡۚ وَمَا كَانَ ٱللَّهُ مُعَذِّبَهُمۡ وَهُمۡ یَسۡتَغۡفِR<رُ>J<و>نَ A<۝>D<٣٣>",
"وَمَا لَهُمۡ أَلَّا یُعَذِّبَهُمُ ٱللَّهُ وَهُمۡ یَصُدُّونَ عَنِ ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِ وَمَا كَانُW<وۤ>X<ا۟> أَوۡلِیَW<اۤ>ءَهُW<ۥۤ>ۚ إِنۡ أَوۡلِیَW<اۤ>ؤُهُW<ۥۤ> إِلَّا ٱلۡمُتَّقُونَ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٣٤>",
"وَمَا كَانَ صَلَاتُهُمۡ عِN<ن>دَ ٱلۡبَیۡتِ إِلَّا مُكَW<اۤ>ءX<ࣰ> N<وَ>تَصۡدِیَةN<ࣰ>ۚ فَذُوقُوX<ا۟> ٱلۡعَذَابَ بِمَا كُN<ن>تُمۡ تَكۡفُR<رُ>J<و>نَ A<۝>D<٣٥>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> یُN<ن>فِقُونَ أَمۡوَT< ٰ>لَهُمۡ لِیَصُدُّوX<ا۟> عَN<ن> سَبِیلِ ٱللَّهِۚ فَسَیُN<ن>فِقُونَهَا ثُN<مَّ> تَكُونُ عَلَیۡهِمۡ حَسۡR<رَ>ةN<ࣰ> ثُN<مَّ> یُغۡلَبُونَۗ وَX<ٱ>لَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِلَىٰ جَهَN<نَّ>مَ یُحۡشَR<رُ>J<و>نَ A<۝>D<٣٦>",
"لِیَمِیزَ ٱللَّهُ ٱلۡخَبِیثَ مِنَ ٱX<ل>طَّیِّبِ وَیَQ<جۡ>عَلَ ٱلۡخَبِیثَ بَعۡضَهُT<ۥ> عَلَىٰ بَعۡضN<ࣲ> فَیَR<رۡ>كُمَهُT<ۥ> جَمِیعN<ࣰ>ا فَیَQ<جۡ>عَلَهُT<ۥ> فِی جَهَN<نَّ>مَۚ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<٣٧>",
"قُل لِّلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِX<ن> N<یَ>N<ن>تَهُوX<ا۟> یُغۡفَR<رۡ> لَهُم N<مَّ>ا قَQ<دۡ> سَلَفَ وَإِX<ن> N<یَ>عُودُوX<ا۟> فَقَQ<دۡ> مَضَتۡ سُN<نَّ>تُ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٣٨>",
"وَقَT<ـٰ>تِلُوهُمۡ حَتَّىٰ لَا تَكُونَ فِتۡنَةX<ࣱ> N<وَ>یَكُونَ ٱX<ل>دِّینُ كُلُّهُT<ۥ> لِلَّهِۚ فَإِنِ ٱN<ن>تَهَوۡX<ا۟> فَإِN<نَّ> ٱللَّهَ بِمَا یَعۡمَلُونَ بَصِJ<ی>رࣱ A<۝>D<٣٩>",
"وَإِN<ن> تَوَلَّوۡX<ا۟> فَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ مَوۡلَX<ى>T<ٰ>كُمۡۚ نِعۡمَ ٱلۡمَوۡلَىٰ وَنِعۡمَ ٱX<ل>N<نَّ>صِJ<ی>رُ A<۝>D<٤٠>",
"۞ وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ>مَا غَنِمۡتُم N<مِّ>N<ن> شَیۡءN<ࣲ> فَأَN<نَّ> لِلَّهِ خُمُسَهُT<ۥ> وَلِX<ل>R<رَّ>سُولِ وَلِذِی ٱلۡقُR<رۡ>بَىٰ وَX<ٱ>لۡیَتَT<ـٰ>مَىٰ وَX<ٱ>لۡمَسَT<ـٰ>كِینِ وَX<ٱ>Q<بۡ>نِ ٱX<ل>سَّبِیلِ إِN<ن> كُN<ن>تُمۡ ءَامَN<ن>تُN<م> بِٱللَّهِ وَمَW<اۤ> أَN<ن>زَلۡنَا عَلَىٰ عَQ<بۡ>دِنَا یَوۡمَ ٱلۡفُR<رۡ>قَانِ یَوۡمَ ٱلۡتَقَى ٱلۡجَمۡعَانِۗ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<٤١>",
"إِذۡ أَN<ن>تُN<م> بِX<ٱ>لۡعُQ<دۡ>وَةِ ٱX<ل>دُّنۡیَا وَهُN<م> بِX<ٱ>لۡعُQ<دۡ>وَةِ ٱلۡقُصۡوَىٰ وَX<ٱ>X<ل>R<رَّ>كۡبُ أَسۡفَلَ مِN<ن>كُمۡۚ وَلَوۡ تَوَاعَX<د>تُّمۡ لَX<ٱ>خۡتَلَفۡتُمۡ فِی ٱلۡمِیعَT<ـٰ>دِ وَلَT<ـٰ>كِX<ن> لِّیَQ<قۡ>ضِیَ ٱللَّهُ أَمۡR<ر>N<ࣰ>ا كَانَ مَفۡعُولX<ࣰ>ا لِّیَهۡلِكَ مَنۡ هَلَكَ عَX<ن>N<ۢ> بَیِّنَةX<ࣲ> N<وَ>یَحۡیَىٰ مَنۡ حَیَّ عَX<ن>N<ۢ> بَیِّنَةX<ࣲ>ۗ N<وَ>إِN<نَّ> ٱللَّهَ لَسَمِیعٌ عَلِJ<ی>مٌ A<۝>D<٤٢>",
"إِذۡ یُرِیكَهُمُ ٱللَّهُ فِی مَنَامِكَ قَلِیلX<ࣰ>اۖ N<وَ>لَوۡ أَR<رَ>X<ى>T<ٰ>كَهُمۡ كَثِیR<ر>X<ࣰ>ا لَّفَشِلۡتُمۡ وَلَتَنَT<ـٰ>زَعۡتُمۡ فِی ٱلۡأَمۡرِ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ سَلَّمَۚ إِN<نَّ>هُT<ۥ> عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٤٣>",
"وَإِذۡ یُرِیكُمُوهُمۡ إِذِ ٱلۡتَقَیۡتُمۡ فِW<یۤ> أَعۡیُنِكُمۡ قَلِیلX<ࣰ>ا N<وَ>یُقَلِّلُكُمۡ فِW<یۤ> أَعۡیُنِهِمۡ لِیَQ<قۡ>ضِیَ ٱللَّهُ أَمۡR<ر>N<ࣰ>ا كَانَ مَفۡعُولX<ࣰ>اۗ N<وَ>إِلَى ٱللَّهِ تُR<رۡ>جَعُ ٱلۡأُمُJ<و>R<ر>ُ A<۝>D<٤٤>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا لَقِیتُمۡ فِئَةN<ࣰ> فَX<ٱ>ثۡبُتُوX<ا۟> وَX<ٱ>ذۡكُR<رُ>وX<ا۟> ٱللَّهَ كَثِیR<ر>X<ࣰ>ا لَّعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<٤٥>",
"وَأَطِیعُوX<ا۟> ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> وَلَا تَنَT<ـٰ>زَعُوX<ا۟> فَتَفۡشَلُوX<ا۟> وَتَذۡهَبَ رِیحُكُمۡۖ وَX<ٱ>صۡبِR<رُ>W<وۤ>X<ا۟>ۚ إِN<نَّ> ٱللَّهَ مَعَ ٱX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<٤٦>",
"وَلَا تَكُونُوX<ا۟> كَX<ٱ>لَّذِینَ خَR<رَ>جُوX<ا۟> مِN<ن> دِیَT<ـٰ>رِهِN<م> بَطَR<ر>X<ࣰ>ا N<وَ>رِئَW<اۤ>ءَ ٱX<ل>N<نَّ>اسِ وَیَصُدُّونَ عَN<ن> سَبِیلِ ٱللَّهِۚ وَٱللَّهُ بِمَا یَعۡمَلُونَ مُحِJ<ی>Q<ط>ࣱ A<۝>D<٤٧>",
"وَإِذۡ زَیَّنَ لَهُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ أَعۡمَT<ـٰ>لَهُمۡ وَقَالَ لَا غَالِبَ لَكُمُ ٱلۡیَوۡمَ مِنَ ٱX<ل>N<نَّ>اسِ وَإِN<نِّ>ی جَاR<ر>X<ࣱ> لَّكُمۡۖ فَلَN<مَّ>ا تَR<رَ>W<اۤ>ءَتِ ٱلۡفِئَتَانِ نَكَصَ عَلَىٰ عَقِبَیۡهِ وَقَالَ إِN<نِّ>ی بَرِW<یۤ>ءX<ࣱ> N<مِّ>N<ن>كُمۡ إِN<نِّ>W<یۤ> أَR<رَ>ىٰ مَا لَا تَR<رَ>وۡنَ إِN<نِّ>W<یۤ> أَخَافُ ٱللَّهَۚ وَٱللَّهُ شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<٤٨>",
"إِذۡ یَقُولُ ٱلۡمُنَT<ـٰ>فِقُونَ وَX<ٱ>لَّذِینَ فِی قُلُوبِهِم N<مَّ>R<رَ>ضٌ غَR<رَّ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ دِینُهُمۡۗ وَمَX<ن> N<یَ>تَوَكَّلۡ عَلَى ٱللَّهِ فَإِN<نَّ> ٱللَّهَ عَزِیزٌ حَكِJ<ی>مࣱ A<۝>D<٤٩>",
"وَلَوۡ تَR<رَ>ىW<ٰۤ> إِذۡ یَتَوَفَّى ٱلَّذِینَ كَفَR<رُ>وX<ا۟> ٱلۡمَلَW<ـٰۤ>ئِكَةُ یَضۡرِبُونَ وُجُوهَهُمۡ وَأَQ<دۡ>بَT<ـٰ>R<رَ>هُمۡ وَذُوقُوX<ا۟> عَذَابَ ٱلۡحَرِJ<ی>Q<ق>ِ A<۝>D<٥٠>",
"ذَT< ٰ>لِكَ بِمَا قَدَّمَتۡ أَیۡدِیكُمۡ وَأَN<نَّ> ٱللَّهَ لَیۡسَ بِظَلَّT<ـٰ>مX<ࣲ> لِّلۡعَبِJ<ی>Q<د>ِ A<۝>D<٥١>",
"كَدَأۡبِ ءَالِ فِرۡعَوۡنَ وَX<ٱ>لَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۚ كَفَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِ ٱللَّهِ فَأَخَذَهُمُ ٱللَّهُ بِذُنُوبِهِمۡۚ إِN<نَّ> ٱللَّهَ قَوِیّN<ࣱ> شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<٥٢>",
"ذَT< ٰ>لِكَ بِأَN<نَّ> ٱللَّهَ لَمۡ یَكُ مُغَیِّR<ر>X<ࣰ>ا N<نِّ>عۡمَةً أَنۡعَمَهَا عَلَىٰ قَوۡمٍ حَتَّىٰ یُغَیِّR<رُ>وX<ا۟> مَا بِأَN<ن>فُسِهِمۡ وَأَN<نَّ> ٱللَّهَ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<٥٣>",
"كَدَأۡبِ ءَالِ فِرۡعَوۡنَ وَX<ٱ>لَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۚ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِ R<رَ>بِّهِمۡ فَأَهۡلَكۡنَT<ـٰ>هُN<م> بِذُنُوبِهِمۡ وأَغۡR<رَ>Q<قۡ>نَW<اۤ> ءَالَ فِرۡعَوۡنَۚ وَكُلّN<ࣱ> كَانُوX<ا۟> ظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥٤>",
"إِN<نَّ> شَR<رَّ> ٱX<ل>دَّوَL<اۤ>بِّ عِN<ن>دَ ٱللَّهِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فَهُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٥٥>",
"ٱلَّذِینَ عَT<ـٰ>هَX<د>تَّ مِنۡهُمۡ ثُN<مَّ> یَN<ن>قُضُونَ عَهۡدَهُمۡ فِی كُلِّ مَR<رَّ>ةX<ࣲ> N<وَ>هُمۡ لَا یَتَّقُJ<و>نَ A<۝>D<٥٦>",
"فَإِN<مَّ>ا تَثۡقَفَN<نَّ>هُمۡ فِی ٱلۡحَR<رۡ>بِ فَشَرِّQ<دۡ> بِهِم N<مَّ>نۡ خَلۡفَهُمۡ لَعَلَّهُمۡ یَذَّكَّR<رُ>J<و>نَ A<۝>D<٥٧>",
"وَإِN<مَّ>ا تَخَافَN<نَّ> مِN<ن> قَوۡمٍ خِیَانَةN<ࣰ> فَX<ٱ>X<ن>N<ۢ>بِذۡ إِلَیۡهِمۡ عَلَىٰ سَوَW<اۤ>ءٍۚ إِN<نَّ> ٱللَّهَ لَا یُحِبُّ ٱلۡخَW<اۤ>ئِنِJ<ی>نَ A<۝>D<٥٨>",
"وَلَا یَحۡسَبَN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> سَبَقُW<وۤ>X<ا۟>ۚ إِN<نَّ>هُمۡ لَا یُعۡجِزُJ<و>نَ A<۝>D<٥٩>",
"وَأَعِدُّوX<ا۟> لَهُم N<مَّ>ا ٱسۡتَطَعۡتُم N<مِّ>N<ن> قُوَّةX<ࣲ> N<وَ>مِX<ن> رِّبَاطِ ٱلۡخَیۡلِ تُR<رۡ>هِبُونَ بِهِT<ۦ> عَدُوَّ ٱللَّهِ وَعَدُوَّكُمۡ وَءَاخَرِینَ مِN<ن> دُونِهِمۡ لَا تَعۡلَمُونَهُمُ ٱللَّهُ یَعۡلَمُهُمۡۚ وَمَا تُN<ن>فِقُوX<ا۟> مِN<ن> شَیۡءN<ࣲ> فِی سَبِیلِ ٱللَّهِ یُوَفَّ إِلَیۡكُمۡ وَأَN<ن>تُمۡ لَا تُظۡلَمُJ<و>نَ A<۝>D<٦٠>",
"۞ وَإِN<ن> جَنَحُوX<ا۟> لِX<ل>سَّلۡمِ فَX<ٱ>Q<جۡ>نَحۡ لَهَا وَتَوَكَّلۡ عَلَى ٱللَّهِۚ إِN<نَّ>هُT<ۥ> هُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٦١>",
"وَإِX<ن> N<یُ>رِیدُW<وۤ>X<ا۟> أَX<ن> N<یَ>خۡدَعُوكَ فَإِN<نَّ> حَسۡبَكَ ٱللَّهُۚ هُوَ ٱلَّذِW<یۤ> أَیَّدَكَ بِنَصۡرِهِT<ۦ> وَبِX<ٱ>لۡمُؤۡمِنِJ<ی>نَ A<۝>D<٦٢>",
"وَأَلَّفَ بَیۡنَ قُلُوبِهِمۡۚ لَوۡ أَN<ن>فَQ<قۡ>تَ مَا فِی ٱلۡأَR<رۡ>ضِ جَمِیعX<ࣰ>ا N<مَّ>W<اۤ> أَلَّفۡتَ بَیۡنَ قُلُوبِهِمۡ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ أَلَّفَ بَیۡنَهُمۡۚ إِN<نَّ>هُT<ۥ> عَزِیزٌ حَكِJ<ی>مࣱ A<۝>D<٦٣>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ حَسۡبُكَ ٱللَّهُ وَمَنِ ٱتَّبَعَكَ مِنَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٦٤>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ حَرِّضِ ٱلۡمُؤۡمِنِینَ عَلَى ٱلۡقِتَالِۚ إِX<ن> N<یَ>كُX<ن> N<مِّ>N<ن>كُمۡ عِشۡR<رُ>ونَ صَT<ـٰ>بِR<رُ>ونَ یَغۡلِبُوX<ا۟> مِX<ا۟>ئَتَیۡنِۚ وَإِX<ن> N<یَ>كُX<ن> N<مِّ>N<ن>كُم N<مِّ>X<ا۟>ئَةX<ࣱ> N<یَ>غۡلِبُW<وۤ>X<ا۟> أَلۡفX<ࣰ>ا N<مِّ>نَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِأَN<نَّ>هُمۡ قَوۡمX<ࣱ> لَّا یَفۡقَهُJ<و>نَ A<۝>D<٦٥>",
"ٱلۡءَT<ـٰ>نَ خَفَّفَ ٱللَّهُ عَN<ن>كُمۡ وَعَلِمَ أَN<نَّ> فِیكُمۡ ضَعۡفN<ࣰ>اۚ فَإِX<ن> N<یَ>كُX<ن> N<مِّ>N<ن>كُم N<مِّ>X<ا۟>ئَةN<ࣱ> صَابِR<رَ>ةX<ࣱ> N<یَ>غۡلِبُوX<ا۟> مِX<ا۟>ئَتَیۡنِۚ وَإِX<ن> N<یَ>كُX<ن> N<مِّ>N<ن>كُمۡ أَلۡفX<ࣱ> N<یَ>غۡلِبُW<وۤ>X<ا۟> أَلۡفَیۡنِ بِإِذۡنِ ٱللَّهِۗ وَٱللَّهُ مَعَ ٱX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<٦٦>",
"مَا كَانَ لِنَبِیٍّ أَX<ن> N<یَ>كُونَ لَهُW<ۥۤ> أَسۡR<رَ>ىٰ حَتَّىٰ یُثۡخِنَ فِی ٱلۡأَR<رۡ>ضِۚ تُرِیدُونَ عَR<رَ>ضَ ٱX<ل>دُّنۡیَا وَٱللَّهُ یُرِیدُ ٱلۡءَاخِR<رَ>ةَۗ وَٱللَّهُ عَزِیزٌ حَكِJ<ی>مࣱ A<۝>D<٦٧>",
"لَّوۡلَا كِتَT<ـٰ>بX<ࣱ> N<مِّ>نَ ٱللَّهِ سَبَقَ لَمَسَّكُمۡ فِیمَW<اۤ> أَخَذۡتُمۡ عَذَابٌ عَظِJ<ی>مࣱ A<۝>D<٦٨>",
"فَكُلُوX<ا۟> مِN<مَّ>ا غَنِمۡتُمۡ حَلَT<ـٰ>لN<ࣰ>ا طَیِّبX<ࣰ>اۚ N<وَ>X<ٱ>تَّقُوX<ا۟> ٱللَّهَۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٦٩>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ قُل لِّمَN<ن> فِW<یۤ> أَیۡدِیكُم N<مِّ>نَ ٱلۡأَسۡR<رَ>ىW<ٰۤ> إِX<ن> N<یَ>عۡلَمِ ٱللَّهُ فِی قُلُوبِكُمۡ خَیۡR<ر>X<ࣰ>ا N<یُ>ؤۡتِكُمۡ خَیۡR<ر>X<ࣰ>ا N<مِّ>N<مَّ>W<اۤ> أُخِذَ مِN<ن>كُمۡ وَیَغۡفِرۡ لَكُمۡۚ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٧٠>",
"وَإِX<ن> N<یُ>رِیدُوX<ا۟> خِیَانَتَكَ فَقَQ<دۡ> خَانُوX<ا۟> ٱللَّهَ مِN<ن> قَQ<بۡ>لُ فَأَمۡكَنَ مِنۡهُمۡۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مٌ A<۝>D<٧١>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَهَاجَR<رُ>وX<ا۟> وَجَT<ـٰ>هَدُوX<ا۟> بِأَمۡوَT< ٰ>لِهِمۡ وَأَN<ن>فُسِهِمۡ فِی سَبِیلِ ٱللَّهِ وَX<ٱ>لَّذِینَ ءَاوَوX<ا۟> وَّنَصَR<رُ>W<وۤ>X<ا۟> أُX<و۟>لَW<ـٰۤ>ئِكَ بَعۡضُهُمۡ أَوۡلِیَW<اۤ>ءُ بَعۡضX<ࣲ>ۚ N<وَ>X<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَلَمۡ یُهَاجِR<رُ>وX<ا۟> مَا لَكُم N<مِّ>X<ن> N<وَ>لَT<ـٰ>یَتِهِم N<مِّ>N<ن> شَیۡءٍ حَتَّىٰ یُهَاجِR<رُ>وX<ا۟>ۚ وَإِنِ ٱسۡتَN<ن>صَR<رُ>وكُمۡ فِی ٱX<ل>دِّینِ فَعَلَیۡكُمُ ٱX<ل>N<نَّ>صۡR<رُ> إِلَّا عَلَىٰ قَوۡمِN<ۭ> بَیۡنَكُمۡ وَبَیۡنَهُم N<مِّ>یثَT<ـٰ>قX<ࣱ>ۗ N<وَ>ٱللَّهُ بِمَا تَعۡمَلُونَ بَصِJ<ی>رࣱ A<۝>D<٧٢>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> بَعۡضُهُمۡ أَوۡلِیَW<اۤ>ءُ بَعۡضٍۚ إِلَّا تَفۡعَلُوهُ تَكُN<ن> فِتۡنَةN<ࣱ> فِی ٱلۡأَR<رۡ>ضِ وَفَسَادN<ࣱ> كَبِJ<ی>رࣱ A<۝>D<٧٣>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَهَاجَR<رُ>وX<ا۟> وَجَT<ـٰ>هَدُوX<ا۟> فِی سَبِیلِ ٱللَّهِ وَX<ٱ>لَّذِینَ ءَاوَوX<ا۟> وَّنَصَR<رُ>W<وۤ>X<ا۟> أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُؤۡمِنُونَ حَقّX<ࣰ>اۚ لَّهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<وَ>رِزۡقN<ࣱ> كَرِJ<ی>مࣱ A<۝>D<٧٤>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مِX<ن>N<ۢ> بَعۡدُ وَهَاجَR<رُ>وX<ا۟> وَجَT<ـٰ>هَدُوX<ا۟> مَعَكُمۡ فَأُX<و۟>لَW<ـٰۤ>ئِكَ مِN<ن>كُمۡۚ وَأُX<و۟>لُوX<ا۟> ٱلۡأَR<رۡ>حَامِ بَعۡضُهُمۡ أَوۡلَىٰ بِبَعۡضN<ࣲ> فِی كِتَT<ـٰ>بِ ٱللَّهِۚ إِN<نَّ> ٱللَّهَ بِكُلِّ شَیۡءٍ عَلِJ<ی>مُۢ A<۝>D<٧٥>",
"۞ بَR<رَ>W<اۤ>ءَةX<ࣱ> N<مِّ>نَ ٱللَّهِ وَR<رَ>سُولِهِW<ۦۤ> إِلَى ٱلَّذِینَ عَT<ـٰ>هَX<د>تُّم N<مِّ>نَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<١>",
"فَسِیحُوX<ا۟> فِی ٱلۡأَR<رۡ>ضِ أَR<رۡ>بَعَةَ أَشۡهُرX<ࣲ> N<وَ>X<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ>كُمۡ غَیۡR<رُ> مُعۡجِزِی ٱللَّهِ وَأَN<نَّ> ٱللَّهَ مُخۡزِی ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٢>",
"وَأَذَT< ٰ>نX<ࣱ> N<مِّ>نَ ٱللَّهِ وَR<رَ>سُولِهِW<ۦۤ> إِلَى ٱX<ل>N<نَّ>اسِ یَوۡمَ ٱلۡحَجِّ ٱلۡأَكۡبَرِ أَN<نَّ> ٱللَّهَ بَرِW<یۤ>ءX<ࣱ> N<مِّ>نَ ٱلۡمُشۡرِكِینَ وَR<رَ>سُولُهُT<ۥ>ۚ فَإِN<ن> تُQ<بۡ>تُمۡ فَهُوَ خَیۡR<ر>X<ࣱ> لَّكُمۡۖ وَإِN<ن> تَوَلَّیۡتُمۡ فَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ>كُمۡ غَیۡR<رُ> مُعۡجِزِی ٱللَّهِۗ وَبَشِّرِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِعَذَابٍ أَلِJ<ی>مٍ A<۝>D<٣>",
"إِلَّا ٱلَّذِینَ عَT<ـٰ>هَX<د>تُّم N<مِّ>نَ ٱلۡمُشۡرِكِینَ ثُN<مَّ> لَمۡ یَN<ن>قُصُوكُمۡ شَیۡءX<ࣰ>ا N<وَ>لَمۡ یُظَT<ـٰ>هِR<رُ>وX<ا۟> عَلَیۡكُمۡ أَحَدN<ࣰ>ا فَأَتِN<مُّ>W<وۤ>X<ا۟> إِلَیۡهِمۡ عَهۡدَهُمۡ إِلَىٰ مُدَّتِهِمۡۚ إِN<نَّ> ٱللَّهَ یُحِبُّ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٤>",
"فَإِذَا ٱN<ن>سَلَخَ ٱلۡأَشۡهُR<رُ> ٱلۡحُR<رُ>مُ فَX<ٱ>Q<قۡ>تُلُوX<ا۟> ٱلۡمُشۡرِكِینَ حَیۡثُ وَجَX<د>تُّمُوهُمۡ وَخُذُوهُمۡ وَX<ٱ>حۡصُR<رُ>وهُمۡ وَX<ٱ>Q<قۡ>عُدُوX<ا۟> لَهُمۡ كُلَّ مَR<رۡ>صَدN<ࣲ>ۚ فَإِN<ن> تَابُوX<ا۟> وَأَقَامُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتَوُX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ فَخَلُّوX<ا۟> سَبِیلَهُمۡۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٥>",
"وَإِنۡ أَحَدX<ࣱ> N<مِّ>نَ ٱلۡمُشۡرِكِینَ ٱسۡتَجَاR<رَ>كَ فَأَجِرۡهُ حَتَّىٰ یَسۡمَعَ كَلَT<ـٰ>مَ ٱللَّهِ ثُN<مَّ> أَQ<بۡ>لِغۡهُ مَأۡمَنَهُT<ۥ>ۚ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ قَوۡمX<ࣱ> لَّا یَعۡلَمُJ<و>نَ A<۝>D<٦>",
"كَیۡفَ یَكُونُ لِلۡمُشۡرِكِینَ عَهۡدٌ عِN<ن>دَ ٱللَّهِ وَعِN<ن>دَ R<رَ>سُولِهِW<ۦۤ> إِلَّا ٱلَّذِینَ عَT<ـٰ>هَX<د>تُّمۡ عِN<ن>دَ ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِۖ فَمَا ٱسۡتَقَT<ـٰ>مُوX<ا۟> لَكُمۡ فَX<ٱ>سۡتَقِیمُوX<ا۟> لَهُمۡۚ إِN<نَّ> ٱللَّهَ یُحِبُّ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٧>",
"كَیۡفَ وَإِX<ن> N<یَ>ظۡهَR<رُ>وX<ا۟> عَلَیۡكُمۡ لَا یَR<رۡ>قُبُوX<ا۟> فِیكُمۡ إِلّX<ࣰ>ا N<وَ>لَا ذِN<مَّ>ةX<ࣰ>ۚ N<یُ>R<رۡ>ضُونَكُN<م> بِأَفۡوَT< ٰ>هِهِمۡ وَتَأۡبَىٰ قُلُوبُهُمۡ وَأَكۡثَR<رُ>هُمۡ فَT<ـٰ>سِقُJ<و>نَ A<۝>D<٨>",
"ٱشۡتَR<رَ>وۡX<ا۟> بِءَایَT<ـٰ>تِ ٱللَّهِ ثَمَنN<ࣰ>ا قَلِیلN<ࣰ>ا فَصَدُّوX<ا۟> عَN<ن> سَبِیلِهِW<ۦۤ>ۚ إِN<نَّ>هُمۡ سَW<اۤ>ءَ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٩>",
"لَا یَR<رۡ>قُبُونَ فِی مُؤۡمِنٍ إِلّX<ࣰ>ا N<وَ>لَا ذِN<مَّ>ةX<ࣰ>ۚ N<وَ>أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُعۡتَدُJ<و>نَ A<۝>D<١٠>",
"فَإِN<ن> تَابُوX<ا۟> وَأَقَامُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتَوُX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ فَإِخۡوَT< ٰ>نُكُمۡ فِی ٱX<ل>دِّینِۗ وَنُفَصِّلُ ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یَ>عۡلَمُJ<و>نَ A<۝>D<١١>",
"وَإِن N<نَّ>كَثُW<وۤ>X<ا۟> أَیۡمَT<ـٰ>نَهُم N<مِّ>X<ن>N<ۢ> بَعۡدِ عَهۡدِهِمۡ وَطَعَنُوX<ا۟> فِی دِینِكُمۡ فَقَT<ـٰ>تِلُW<وۤ>X<ا۟> أَئِN<مَّ>ةَ ٱلۡكُفۡرِ إِN<نَّ>هُمۡ لَW<اۤ> أَیۡمَT<ـٰ>نَ لَهُمۡ لَعَلَّهُمۡ یَN<ن>تَهُJ<و>نَ A<۝>D<١٢>",
"أَلَا تُقَT<ـٰ>تِلُونَ قَوۡمX<ࣰ>ا N<نَّ>كَثُW<وۤ>X<ا۟> أَیۡمَT<ـٰ>نَهُمۡ وَهَN<مُّ>وX<ا۟> بِإِخۡR<رَ>اجِ ٱX<ل>R<رَّ>سُولِ وَهُN<م> بَدَءُوكُمۡ أَوَّلَ مَR<رَّ>ةٍۚ أَتَخۡشَوۡنَهُمۡۚ فَٱللَّهُ أَحَقُّ أَN<ن> تَخۡشَوۡهُ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٣>",
"قَT<ـٰ>تِلُوهُمۡ یُعَذِّQ<بۡ>هُمُ ٱللَّهُ بِأَیۡدِیكُمۡ وَیُخۡزِهِمۡ وَیَN<ن>صُR<رۡ>كُمۡ عَلَیۡهِمۡ وَیَشۡفِ صُدُوR<رَ> قَوۡمX<ࣲ> N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٤>",
"وَیُذۡهِQ<بۡ> غَیۡظَ قُلُوبِهِمۡۗ وَیَتُوبُ ٱللَّهُ عَلَىٰ مَX<ن> N<یَ>شَW<اۤ>ءُۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مٌ A<۝>D<١٥>",
"أَمۡ حَسِQ<بۡ>تُمۡ أَN<ن> تُتۡR<رَ>كُوX<ا۟> وَلَN<مَّ>ا یَعۡلَمِ ٱللَّهُ ٱلَّذِینَ جَT<ـٰ>هَدُوX<ا۟> مِN<ن>كُمۡ وَلَمۡ یَتَّخِذُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ وَلَا R<رَ>سُولِهِT<ۦ> وَلَا ٱلۡمُؤۡمِنِینَ وَلِیجَةX<ࣰ>ۚ N<وَ>ٱللَّهُ خَبِیR<رُ>N<ۢ> بِمَا تَعۡمَلُJ<و>نَ A<۝>D<١٦>",
"مَا كَانَ لِلۡمُشۡرِكِینَ أَX<ن> N<یَ>عۡمُR<رُ>وX<ا۟> مَسَT<ـٰ>جِدَ ٱللَّهِ شَT<ـٰ>هِدِینَ عَلَىW<ٰۤ> أَN<ن>فُسِهِN<م> بِX<ٱ>لۡكُفۡرِۚ أُX<و۟>لَW<ـٰۤ>ئِكَ حَبِطَتۡ أَعۡمَT<ـٰ>لُهُمۡ وَفِی ٱX<ل>N<نَّ>ارِ هُمۡ خَT<ـٰ>لِدُJ<و>نَ A<۝>D<١٧>",
"إِN<نَّ>مَا یَعۡمُR<رُ> مَسَT<ـٰ>جِدَ ٱللَّهِ مَنۡ ءَامَنَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَأَقَامَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتَى ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَلَمۡ یَخۡشَ إِلَّا ٱللَّهَۖ فَعَسَىW<ٰۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ أَX<ن> N<یَ>كُونُوX<ا۟> مِنَ ٱلۡمُهۡتَدِJ<ی>نَ A<۝>D<١٨>",
"۞ أَجَعَلۡتُمۡ سِقَایَةَ ٱلۡحَL<اۤ>جِّ وَعِمَاR<رَ>ةَ ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِ كَمَنۡ ءَامَنَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَجَT<ـٰ>هَدَ فِی سَبِیلِ ٱللَّهِۚ لَا یَسۡتَوُT<ۥ>نَ عِN<ن>دَ ٱللَّهِۗ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٩>",
"ٱلَّذِینَ ءَامَنُوX<ا۟> وَهَاجَR<رُ>وX<ا۟> وَجَT<ـٰ>هَدُوX<ا۟> فِی سَبِیلِ ٱللَّهِ بِأَمۡوَT< ٰ>لِهِمۡ وَأَN<ن>فُسِهِمۡ أَعۡظَمُ دَR<رَ>جَةً عِN<ن>دَ ٱللَّهِۚ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡفَW<اۤ>ئِزُJ<و>نَ A<۝>D<٢٠>",
"یُبَشِّR<رُ>هُمۡ R<رَ>بُّهُN<م> بِR<رَ>حۡمَةX<ࣲ> N<مِّ>نۡهُ وَرِضۡوَT< ٰ>نX<ࣲ> N<وَ>جَN<نَّ>T<ـٰ>تX<ࣲ> لَّهُمۡ فِیهَا نَعِیمX<ࣱ> N<مُّ>قِJ<ی>مٌ A<۝>D<٢١>",
"خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدًاۚ إِN<نَّ> ٱللَّهَ عِN<ن>دَهُW<ۥۤ> أَQ<جۡ>R<ر>ٌ عَظِJ<ی>مࣱ A<۝>D<٢٢>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَتَّخِذُW<وۤ>X<ا۟> ءَابَW<اۤ>ءَكُمۡ وَإِخۡوَT< ٰ>نَكُمۡ أَوۡلِیَW<اۤ>ءَ إِنِ ٱسۡتَحَبُّوX<ا۟> ٱلۡكُفۡR<رَ> عَلَى ٱلۡإِیمَT<ـٰ>نِۚ وَمَX<ن> N<یَ>تَوَلَّهُم N<مِّ>N<ن>كُمۡ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٢٣>",
"قُلۡ إِN<ن> كَانَ ءَابَW<اۤ>ؤُكُمۡ وَأَQ<بۡ>نَW<اۤ>ؤُكُمۡ وَإِخۡوَT< ٰ>نُكُمۡ وَأَزۡوَT< ٰ>جُكُمۡ وَعَشِیR<رَ>تُكُمۡ وَأَمۡوَT< ٰ>لٌ ٱQ<قۡ>تَR<رَ>فۡتُمُوهَا وَتِجَT<ـٰ>R<رَ>ةN<ࣱ> تَخۡشَوۡنَ كَسَادَهَا وَمَسَT<ـٰ>كِنُ تَR<رۡ>ضَوۡنَهَW<اۤ> أَحَبَّ إِلَیۡكُم N<مِّ>نَ ٱللَّهِ وَR<رَ>سُولِهِT<ۦ> وَجِهَادN<ࣲ> فِی سَبِیلِهِT<ۦ> فَتَR<رَ>بَّصُوX<ا۟> حَتَّىٰ یَأۡتِیَ ٱللَّهُ بِأَمۡرِهِT<ۦ>ۗ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٢٤>",
"لَقَQ<دۡ> نَصَR<رَ>كُمُ ٱللَّهُ فِی مَوَاطِنَ كَثِیR<رَ>ةX<ࣲ> N<وَ>یَوۡمَ حُنَیۡنٍ إِذۡ أَعۡجَبَتۡكُمۡ كَثۡR<رَ>تُكُمۡ فَلَمۡ تُغۡنِ عَN<ن>كُمۡ شَیۡءX<ࣰ>ا N<وَ>ضَاقَتۡ عَلَیۡكُمُ ٱلۡأَR<رۡ>ضُ بِمَا R<رَ>حُبَتۡ ثُN<مَّ> وَلَّیۡتُم N<مُّ>Q<دۡ>بِرِJ<ی>نَ A<۝>D<٢٥>",
"ثُN<مَّ> أَN<ن>زَلَ ٱللَّهُ سَكِینَتَهُT<ۥ> عَلَىٰ R<رَ>سُولِهِT<ۦ> وَعَلَى ٱلۡمُؤۡمِنِینَ وَأَN<ن>زَلَ جُنُودX<ࣰ>ا لَّمۡ تَR<رَ>وۡهَا وَعَذَّبَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟>ۚ وَذَT< ٰ>لِكَ جَزَW<اۤ>ءُ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٢٦>",
"ثُN<مَّ> یَتُوبُ ٱللَّهُ مِX<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَ عَلَىٰ مَX<ن> N<یَ>شَW<اۤ>ءُۗ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٢٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<نَّ>مَا ٱلۡمُشۡرِكُونَ نَجَسN<ࣱ> فَلَا یَQ<قۡ>R<رَ>بُوX<ا۟> ٱلۡمَسۡجِدَ ٱلۡحَR<رَ>امَ بَعۡدَ عَامِهِمۡ هَT<ـٰ>ذَاۚ وَإِنۡ خِفۡتُمۡ عَیۡلَةN<ࣰ> فَسَوۡفَ یُغۡنِیكُمُ ٱللَّهُ مِN<ن> فَضۡلِهِW<ۦۤ> إِN<ن> شَW<اۤ>ءَۚ إِN<نَّ> ٱللَّهَ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<٢٨>",
"قَT<ـٰ>تِلُوX<ا۟> ٱلَّذِینَ لَا یُؤۡمِنُونَ بِٱللَّهِ وَلَا بِX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَلَا یُحَرِّمُونَ مَا حَR<رَّ>مَ ٱللَّهُ وَR<رَ>سُولُهُT<ۥ> وَلَا یَدِینُونَ دِینَ ٱلۡحَقِّ مِنَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ حَتَّىٰ یُعۡطُوX<ا۟> ٱلۡجِزۡیَةَ عَX<ن> N<یَ>دX<ࣲ> N<وَ>هُمۡ صَT<ـٰ>غِR<رُ>J<و>نَ A<۝>D<٢٩>",
"وَقَالَتِ ٱلۡیَهُودُ عُزَیۡR<ر>ٌ ٱQ<بۡ>نُ ٱللَّهِ وَقَالَتِ ٱX<ل>N<نَّ>صَT<ـٰ>R<رَ>ى ٱلۡمَسِیحُ ٱQ<بۡ>نُ ٱللَّهِۖ ذَT< ٰ>لِكَ قَوۡلُهُN<م> بِأَفۡوَT< ٰ>هِهِمۡۖ یُضَT<ـٰ>هِءُونَ قَوۡلَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِN<ن> قَQ<بۡ>لُۚ قَT<ـٰ>تَلَهُمُ ٱللَّهُۖ أَN<نَّ>ىٰ یُؤۡفَكُJ<و>نَ A<۝>D<٣٠>",
"ٱتَّخَذُW<وۤ>X<ا۟> أَحۡبَاR<رَ>هُمۡ وَR<رُ>هۡبَT<ـٰ>نَهُمۡ أَR<رۡ>بَابX<ࣰ>ا N<مِّ>N<ن> دُونِ ٱللَّهِ وَX<ٱ>لۡمَسِیحَ ٱQ<بۡ>نَ مَR<رۡ>یَمَ وَمَW<اۤ> أُمِR<رُ>W<وۤ>X<ا۟> إِلَّا لِیَعۡبُدُW<وۤ>X<ا۟> إِلَT<ـٰ>هX<ࣰ>ا N<وَ>T< ٰ>حِدX<ࣰ>اۖ لَّW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۚ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ> عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٣١>",
"یُرِیدُونَ أَX<ن> N<یُ>Q<طۡ>فِءُوX<ا۟> نُوR<رَ> ٱللَّهِ بِأَفۡوَT< ٰ>هِهِمۡ وَیَأۡبَى ٱللَّهُ إِلَّW<اۤ> أَX<ن> N<یُ>تِN<مَّ> نُوR<رَ>هُT<ۥ> وَلَوۡ كَرِهَ ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٣٢>",
"هُوَ ٱلَّذِW<یۤ> أَR<رۡ>سَلَ R<رَ>سُولَهُT<ۥ> بِX<ٱ>لۡهُدَىٰ وَدِینِ ٱلۡحَقِّ لِیُظۡهِR<رَ>هُT<ۥ> عَلَى ٱX<ل>دِّینِ كُلِّهِT<ۦ> وَلَوۡ كَرِهَ ٱلۡمُشۡرِكُJ<و>نَ A<۝>D<٣٣>",
"۞ یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<نَّ> كَثِیR<ر>X<ࣰ>ا N<مِّ>نَ ٱلۡأَحۡبَارِ وَX<ٱ>X<ل>R<رُّ>هۡبَانِ لَیَأۡكُلُونَ أَمۡوَT< ٰ>لَ ٱX<ل>N<نَّ>اسِ بِX<ٱ>لۡبَT<ـٰ>طِلِ وَیَصُدُّونَ عَN<ن> سَبِیلِ ٱللَّهِۗ وَX<ٱ>لَّذِینَ یَكۡنِزُونَ ٱX<ل>ذَّهَبَ وَX<ٱ>لۡفِضَّةَ وَلَا یُN<ن>فِقُونَهَا فِی سَبِیلِ ٱللَّهِ فَبَشِّرۡهُN<م> بِعَذَابٍ أَلِJ<ی>مࣲ A<۝>D<٣٤>",
"یَوۡمَ یُحۡمَىٰ عَلَیۡهَا فِی نَارِ جَهَN<نَّ>مَ فَتُكۡوَىٰ بِهَا جِبَاهُهُمۡ وَجُنُوبُهُمۡ وَظُهُوR<رُ>هُمۡۖ هَT<ـٰ>ذَا مَا كَنَزۡتُمۡ لِأَN<ن>فُسِكُمۡ فَذُوقُوX<ا۟> مَا كُN<ن>تُمۡ تَكۡنِزُJ<و>نَ A<۝>D<٣٥>",
"إِN<نَّ> عِدَّةَ ٱX<ل>شُّهُورِ عِN<ن>دَ ٱللَّهِ ٱثۡنَا عَشَR<رَ> شَهۡR<ر>N<ࣰ>ا فِی كِتَT<ـٰ>بِ ٱللَّهِ یَوۡمَ خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ مِنۡهَW<اۤ> أَR<رۡ>بَعَةٌ حُR<رُ>مN<ࣱ>ۚ ذَT< ٰ>لِكَ ٱX<ل>دِّینُ ٱلۡقَیِّمُۚ فَلَا تَظۡلِمُوX<ا۟> فِیهِN<نَّ> أَN<ن>فُسَكُمۡۚ وَقَT<ـٰ>تِلُوX<ا۟> ٱلۡمُشۡرِكِینَ كَL<اۤ>فَّةN<ࣰ> كَمَا یُقَT<ـٰ>تِلُونَكُمۡ كَL<اۤ>فَّةX<ࣰ>ۚ N<وَ>X<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ مَعَ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٣٦>",
"إِN<نَّ>مَا ٱX<ل>N<نَّ>سِW<یۤ>ءُ زِیَادَةN<ࣱ> فِی ٱلۡكُفۡرِۖ یُضَلُّ بِهِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> یُحِلُّونَهُT<ۥ> عَامX<ࣰ>ا N<وَ>یُحَرِّمُونَهُT<ۥ> عَامX<ࣰ>ا لِّیُوَاطِءُوX<ا۟> عِدَّةَ مَا حَR<رَّ>مَ ٱللَّهُ فَیُحِلُّوX<ا۟> مَا حَR<رَّ>مَ ٱللَّهُۚ زُیِّنَ لَهُمۡ سُW<وۤ>ءُ أَعۡمَT<ـٰ>لِهِمۡۗ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٣٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> مَا لَكُمۡ إِذَا قِیلَ لَكُمُ ٱN<ن>فِR<رُ>وX<ا۟> فِی سَبِیلِ ٱللَّهِ ٱثَّاقَلۡتُمۡ إِلَى ٱلۡأَR<رۡ>ضِۚ أَR<رَ>ضِیتُN<م> بِX<ٱ>لۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا مِنَ ٱلۡءَاخِR<رَ>ةِۚ فَمَا مَتَT<ـٰ>عُ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا فِی ٱلۡءَاخِR<رَ>ةِ إِلَّا قَلِJ<ی>لٌ A<۝>D<٣٨>",
"إِلَّا تَN<ن>فِR<رُ>وX<ا۟> یُعَذِّQ<بۡ>كُمۡ عَذَابًا أَلِیمX<ࣰ>ا N<وَ>یَسۡتَQ<بۡ>دِلۡ قَوۡمًا غَیۡR<رَ>كُمۡ وَلَا تَضُR<رُّ>وهُ شَیۡءX<ࣰ>اۗ N<وَ>ٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<٣٩>",
"إِلَّا تَN<ن>صُR<رُ>وهُ فَقَQ<دۡ> نَصَR<رَ>هُ ٱللَّهُ إِذۡ أَخۡR<رَ>جَهُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> ثَانِیَ ٱثۡنَیۡنِ إِذۡ هُمَا فِی ٱلۡغَارِ إِذۡ یَقُولُ لِصَT<ـٰ>حِبِهِT<ۦ> لَا تَحۡزَنۡ إِN<نَّ> ٱللَّهَ مَعَنَاۖ فَأَN<ن>زَلَ ٱللَّهُ سَكِینَتَهُT<ۥ> عَلَیۡهِ وَأَیَّدَهُT<ۥ> بِجُنُودX<ࣲ> لَّمۡ تَR<رَ>وۡهَا وَجَعَلَ كَلِمَةَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> ٱX<ل>سُّفۡلَىٰۗ وَكَلِمَةُ ٱللَّهِ هِیَ ٱلۡعُلۡیَاۗ وَٱللَّهُ عَزِیزٌ حَكِJ<ی>مٌ A<۝>D<٤٠>",
"ٱN<ن>فِR<رُ>وX<ا۟> خِفَافX<ࣰ>ا N<وَ>ثِقَالX<ࣰ>ا N<وَ>جَT<ـٰ>هِدُوX<ا۟> بِأَمۡوَT< ٰ>لِكُمۡ وَأَN<ن>فُسِكُمۡ فِی سَبِیلِ ٱللَّهِۚ ذَT< ٰ>لِكُمۡ خَیۡR<ر>X<ࣱ> لَّكُمۡ إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٤١>",
"لَوۡ كَانَ عَR<رَ>ضN<ࣰ>ا قَرِیبX<ࣰ>ا N<وَ>سَفَR<ر>N<ࣰ>ا قَاصِدX<ࣰ>ا لَّX<ٱ>تَّبَعُوكَ وَلَT<ـٰ>كِX<ن>N<ۢ> بَعُدَتۡ عَلَیۡهِمُ ٱX<ل>شُّقَّةُۚ وَسَیَحۡلِفُونَ بِٱللَّهِ لَوِ ٱسۡتَطَعۡنَا لَخَR<رَ>Q<جۡ>نَا مَعَكُمۡ یُهۡلِكُونَ أَN<ن>فُسَهُمۡ وَٱللَّهُ یَعۡلَمُ إِN<نَّ>هُمۡ لَكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<٤٢>",
"عَفَا ٱللَّهُ عَN<ن>كَ لِمَ أَذِN<ن>تَ لَهُمۡ حَتَّىٰ یَتَبَیَّنَ لَكَ ٱلَّذِینَ صَدَقُوX<ا۟> وَتَعۡلَمَ ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٤٣>",
"لَا یَسۡتَءۡذِنُكَ ٱلَّذِینَ یُؤۡمِنُونَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ أَX<ن> N<یُ>جَT<ـٰ>هِدُوX<ا۟> بِأَمۡوَT< ٰ>لِهِمۡ وَأَN<ن>فُسِهِمۡۗ وَٱللَّهُ عَلِیمُN<ۢ> بِX<ٱ>لۡمُتَّقِJ<ی>نَ A<۝>D<٤٤>",
"إِN<نَّ>مَا یَسۡتَءۡذِنُكَ ٱلَّذِینَ لَا یُؤۡمِنُونَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَX<ٱ>R<رۡ>تَابَتۡ قُلُوبُهُمۡ فَهُمۡ فِی R<رَ>یۡبِهِمۡ یَتَR<رَ>دَّدُJ<و>نَ A<۝>D<٤٥>",
"۞ وَلَوۡ أَR<رَ>ادُوX<ا۟> ٱلۡخُR<رُ>وجَ لَأَعَدُّوX<ا۟> لَهُT<ۥ> عُدَّةX<ࣰ> N<وَ>لَT<ـٰ>كِN<ن> كَرِهَ ٱللَّهُ ٱX<ن>N<ۢ>بِعَاثَهُمۡ فَثَبَّطَهُمۡ وَقِیلَ ٱQ<قۡ>عُدُوX<ا۟> مَعَ ٱلۡقَT<ـٰ>عِدِJ<ی>نَ A<۝>D<٤٦>",
"لَوۡ خَR<رَ>جُوX<ا۟> فِیكُم N<مَّ>ا زَادُوكُمۡ إِلَّا خَبَالX<ࣰ>ا N<وَ>لَأَوۡضَعُوX<ا۟> خِلَT<ـٰ>لَكُمۡ یَQ<بۡ>غُونَكُمُ ٱلۡفِتۡنَةَ وَفِیكُمۡ سَN<مَّ>T<ـٰ>عُونَ لَهُمۡۗ وَٱللَّهُ عَلِیمُN<ۢ> بِX<ٱ>X<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤٧>",
"لَقَدِ ٱQ<بۡ>تَغَوُX<ا۟> ٱلۡفِتۡنَةَ مِN<ن> قَQ<بۡ>لُ وَقَلَّبُوX<ا۟> لَكَ ٱلۡأُمُوR<رَ> حَتَّىٰ جَW<اۤ>ءَ ٱلۡحَقُّ وَظَهَR<رَ> أَمۡR<رُ> ٱللَّهِ وَهُمۡ كَT<ـٰ>رِهُJ<و>نَ A<۝>D<٤٨>",
"وَمِنۡهُم N<مَّ>X<ن> N<یَ>قُولُ ٱئۡذَX<ن> لِّی وَلَا تَفۡتِN<نِّ>W<یۤ>ۚ أَلَا فِی ٱلۡفِتۡنَةِ سَقَطُوX<ا۟>ۗ وَإِN<نَّ> جَهَN<نَّ>مَ لَمُحِیطَةُN<ۢ> بِX<ٱ>لۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٤٩>",
"إِN<ن> تُصِQ<بۡ>كَ حَسَنَةN<ࣱ> تَسُؤۡهُمۡۖ وَإِN<ن> تُصِQ<بۡ>كَ مُصِیبَةX<ࣱ> N<یَ>قُولُوX<ا۟> قَQ<دۡ> أَخَذۡنَW<اۤ> أَمۡR<رَ>نَا مِN<ن> قَQ<بۡ>لُ وَیَتَوَلَّوX<ا۟> وَّهُمۡ فَرِحُJ<و>نَ A<۝>D<٥٠>",
"قُل لَّX<ن> N<یُ>صِیبَنَW<اۤ> إِلَّا مَا كَتَبَ ٱللَّهُ لَنَا هُوَ مَوۡلَX<ى>T<ٰ>نَاۚ وَعَلَى ٱللَّهِ فَلۡیَتَوَكَّلِ ٱلۡمُؤۡمِنُJ<و>نَ A<۝>D<٥١>",
"قُلۡ هَلۡ تَR<رَ>بَّصُونَ بِنَW<اۤ> إِلَّW<اۤ> إِحۡدَى ٱلۡحُسۡنَیَیۡنِۖ وَنَحۡنُ نَتَR<رَ>بَّصُ بِكُمۡ أَX<ن> N<یُ>صِیبَكُمُ ٱللَّهُ بِعَذَابX<ࣲ> N<مِّ>نۡ عِN<ن>دِهِW<ۦۤ> أَوۡ بِأَیۡدِینَاۖ فَتَR<رَ>بَّصُW<وۤ>X<ا۟> إِN<نَّ>ا مَعَكُم N<مُّ>تَR<رَ>بِّصُJ<و>نَ A<۝>D<٥٢>",
"قُلۡ أَN<ن>فِقُوX<ا۟> طَوۡعًا أَوۡ كَR<رۡ>هX<ࣰ>ا لَّX<ن> N<یُ>تَقَبَّلَ مِN<ن>كُمۡ إِN<نَّ>كُمۡ كُN<ن>تُمۡ قَوۡمN<ࣰ>ا فَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٥٣>",
"وَمَا مَنَعَهُمۡ أَN<ن> تُQ<قۡ>بَلَ مِنۡهُمۡ نَفَقَT<ـٰ>تُهُمۡ إِلَّW<اۤ> أَN<نَّ>هُمۡ كَفَR<رُ>وX<ا۟> بِٱللَّهِ وَبِR<رَ>سُولِهِT<ۦ> وَلَا یَأۡتُونَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ إِلَّا وَهُمۡ كُسَالَىٰ وَلَا یُN<ن>فِقُونَ إِلَّا وَهُمۡ كَT<ـٰ>رِهُJ<و>نَ A<۝>D<٥٤>",
"فَلَا تُعۡجِQ<بۡ>كَ أَمۡوَT< ٰ>لُهُمۡ وَلَW<اۤ> أَوۡلَT<ـٰ>دُهُمۡۚ إِN<نَّ>مَا یُرِیدُ ٱللَّهُ لِیُعَذِّبَهُN<م> بِهَا فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَتَزۡهَقَ أَN<ن>فُسُهُمۡ وَهُمۡ كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٥٥>",
"وَیَحۡلِفُونَ بِٱللَّهِ إِN<نَّ>هُمۡ لَمِN<ن>كُمۡ وَمَا هُم N<مِّ>N<ن>كُمۡ وَلَT<ـٰ>كِN<نَّ>هُمۡ قَوۡمX<ࣱ> N<یَ>فۡR<رَ>قُJ<و>نَ A<۝>D<٥٦>",
"لَوۡ یَجِدُونَ مَلۡجَءًا أَوۡ مَغَT<ـٰ>R<رَ>T< ٰ>تٍ أَوۡ مُدَّخَلX<ࣰ>ا لَّوَلَّوۡX<ا۟> إِلَیۡهِ وَهُمۡ یَQ<جۡ>مَحُJ<و>نَ A<۝>D<٥٧>",
"وَمِنۡهُم N<مَّ>X<ن> N<یَ>لۡمِزُكَ فِی ٱX<ل>صَّدَقَT<ـٰ>تِ فَإِنۡ أُعۡطُوX<ا۟> مِنۡهَا R<رَ>ضُوX<ا۟> وَإِX<ن> لَّمۡ یُعۡطَوۡX<ا۟> مِنۡهَW<اۤ> إِذَا هُمۡ یَسۡخَطُJ<و>نَ A<۝>D<٥٨>",
"وَلَوۡ أَN<نَّ>هُمۡ R<رَ>ضُوX<ا۟> مَW<اۤ> ءَاتَX<ى>T<ٰ>هُمُ ٱللَّهُ وَR<رَ>سُولُهُT<ۥ> وَقَالُوX<ا۟> حَسۡبُنَا ٱللَّهُ سَیُؤۡتِینَا ٱللَّهُ مِN<ن> فَضۡلِهِT<ۦ> وَR<رَ>سُولُهُW<ۥۤ> إِN<نَّ>W<اۤ> إِلَى ٱللَّهِ R<رَ>T< ٰ>غِبُJ<و>نَ A<۝>D<٥٩>",
"۞ إِN<نَّ>مَا ٱX<ل>صَّدَقَT<ـٰ>تُ لِلۡفُقَR<رَ>W<اۤ>ءِ وَX<ٱ>لۡمَسَT<ـٰ>كِینِ وَX<ٱ>لۡعَT<ـٰ>مِلِینَ عَلَیۡهَا وَX<ٱ>لۡمُؤَلَّفَةِ قُلُوبُهُمۡ وَفِی ٱX<ل>رِّقَابِ وَX<ٱ>لۡغَT<ـٰ>رِمِینَ وَفِی سَبِیلِ ٱللَّهِ وَX<ٱ>Q<بۡ>نِ ٱX<ل>سَّبِیلِۖ فَرِیضَةX<ࣰ> N<مِّ>نَ ٱللَّهِۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<٦٠>",
"وَمِنۡهُمُ ٱلَّذِینَ یُؤۡذُونَ ٱX<ل>N<نَّ>بِیَّ وَیَقُولُونَ هُوَ أُذُنN<ࣱ>ۚ قُلۡ أُذُنُ خَیۡرX<ࣲ> لَّكُمۡ یُؤۡمِنُ بِٱللَّهِ وَیُؤۡمِنُ لِلۡمُؤۡمِنِینَ وَR<رَ>حۡمَةX<ࣱ> لِّلَّذِینَ ءَامَنُوX<ا۟> مِN<ن>كُمۡۚ وَX<ٱ>لَّذِینَ یُؤۡذُونَ R<رَ>سُولَ ٱللَّهِ لَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٦١>",
"یَحۡلِفُونَ بِٱللَّهِ لَكُمۡ لِیُR<رۡ>ضُوكُمۡ وَٱللَّهُ وَR<رَ>سُولُهُW<ۥۤ> أَحَقُّ أَX<ن> N<یُ>R<رۡ>ضُوهُ إِN<ن> كَانُوX<ا۟> مُؤۡمِنِJ<ی>نَ A<۝>D<٦٢>",
"أَلَمۡ یَعۡلَمُW<وۤ>X<ا۟> أَN<نَّ>هُT<ۥ> مَX<ن> N<یُ>حَادِدِ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> فَأَN<نَّ> لَهُT<ۥ> نَاR<رَ> جَهَN<نَّ>مَ خَT<ـٰ>لِدN<ࣰ>ا فِیهَاۚ ذَT< ٰ>لِكَ ٱلۡخِزۡیُ ٱلۡعَظِJ<ی>مُ A<۝>D<٦٣>",
"یَحۡذَR<رُ> ٱلۡمُنَT<ـٰ>فِقُونَ أَN<ن> تُنَزَّلَ عَلَیۡهِمۡ سُوR<رَ>ةN<ࣱ> تُنَبِّئُهُN<م> بِمَا فِی قُلُوبِهِمۡۚ قُلِ ٱسۡتَهۡزِءُW<وۤ>X<ا۟> إِN<نَّ> ٱللَّهَ مُخۡرِجX<ࣱ> N<مَّ>ا تَحۡذَR<رُ>J<و>نَ A<۝>D<٦٤>",
"وَلَئِN<ن> سَأَلۡتَهُمۡ لَیَقُولُN<نَّ> إِN<نَّ>مَا كُN<نَّ>ا نَخُوضُ وَنَلۡعَبُۚ قُلۡ أَبِٱللَّهِ وَءَایَT<ـٰ>تِهِT<ۦ> وَR<رَ>سُولِهِT<ۦ> كُN<ن>تُمۡ تَسۡتَهۡزِءُJ<و>نَ A<۝>D<٦٥>",
"لَا تَعۡتَذِR<رُ>وX<ا۟> قَQ<دۡ> كَفَR<رۡ>تُN<م> بَعۡدَ إِیمَT<ـٰ>نِكُمۡۚ إِن N<نَّ>عۡفُ عَN<ن> طَW<اۤ>ئِفَةX<ࣲ> N<مِّ>N<ن>كُمۡ نُعَذِّQ<بۡ> طَW<اۤ>ئِفَةَN<ۢ> بِأَN<نَّ>هُمۡ كَانُوX<ا۟> مُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٦٦>",
"ٱلۡمُنَT<ـٰ>فِقُونَ وَX<ٱ>لۡمُنَT<ـٰ>فِقَT<ـٰ>تُ بَعۡضُهُم N<مِّ>X<ن>N<ۢ> بَعۡضX<ࣲ>ۚ N<یَ>أۡمُR<رُ>ونَ بِX<ٱ>لۡمُN<ن>كَرِ وَیَنۡهَوۡنَ عَنِ ٱلۡمَعۡR<رُ>وفِ وَیَQ<قۡ>بِضُونَ أَیۡدِیَهُمۡۚ نَسُوX<ا۟> ٱللَّهَ فَنَسِیَهُمۡۚ إِN<نَّ> ٱلۡمُنَT<ـٰ>فِقِینَ هُمُ ٱلۡفَT<ـٰ>سِقُJ<و>نَ A<۝>D<٦٧>",
"وَعَدَ ٱللَّهُ ٱلۡمُنَT<ـٰ>فِقِینَ وَX<ٱ>لۡمُنَT<ـٰ>فِقَT<ـٰ>تِ وَX<ٱ>لۡكُفَّاR<رَ> نَاR<رَ> جَهَN<نَّ>مَ خَT<ـٰ>لِدِینَ فِیهَاۚ هِیَ حَسۡبُهُمۡۚ وَلَعَنَهُمُ ٱللَّهُۖ وَلَهُمۡ عَذَابX<ࣱ> N<مُّ>قِJ<ی>مࣱ A<۝>D<٦٨>",
"كَX<ٱ>لَّذِینَ مِN<ن> قَQ<بۡ>لِكُمۡ كَانُW<وۤ>X<ا۟> أَشَدَّ مِN<ن>كُمۡ قُوَّةX<ࣰ> N<وَ>أَكۡثَR<رَ> أَمۡوَT< ٰ>لX<ࣰ>ا N<وَ>أَوۡلَT<ـٰ>دN<ࣰ>ا فَX<ٱ>سۡتَمۡتَعُوX<ا۟> بِخَلَT<ـٰ>قِهِمۡ فَX<ٱ>سۡتَمۡتَعۡتُN<م> بِخَلَT<ـٰ>قِكُمۡ كَمَا ٱسۡتَمۡتَعَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِكُN<م> بِخَلَT<ـٰ>قِهِمۡ وَخُضۡتُمۡ كَX<ٱ>لَّذِی خَاضُW<وۤ>X<ا۟>ۚ أُX<و۟>لَW<ـٰۤ>ئِكَ حَبِطَتۡ أَعۡمَT<ـٰ>لُهُمۡ فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<٦٩>",
"أَلَمۡ یَأۡتِهِمۡ نَبَأُ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ قَوۡمِ نُوحX<ࣲ> N<وَ>عَادX<ࣲ> N<وَ>ثَمُودَ وَقَوۡمِ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَأَصۡحَT<ـٰ>بِ مَQ<دۡ>یَنَ وَX<ٱ>لۡمُؤۡتَفِكَT<ـٰ>تِۚ أَتَتۡهُمۡ R<رُ>سُلُهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِۖ فَمَا كَانَ ٱللَّهُ لِیَظۡلِمَهُمۡ وَلَT<ـٰ>كِN<ن> كَانُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ یَظۡلِمُJ<و>نَ A<۝>D<٧٠>",
"وَX<ٱ>لۡمُؤۡمِنُونَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تُ بَعۡضُهُمۡ أَوۡلِیَW<اۤ>ءُ بَعۡضX<ࣲ>ۚ N<یَ>أۡمُR<رُ>ونَ بِX<ٱ>لۡمَعۡR<رُ>وفِ وَیَنۡهَوۡنَ عَنِ ٱلۡمُN<ن>كَرِ وَیُقِیمُونَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَیُؤۡتُونَ ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَیُطِیعُونَ ٱللَّهَ وَR<رَ>سُولَهُW<ۥۤ>ۚ أُX<و۟>لَW<ـٰۤ>ئِكَ سَیَR<رۡ>حَمُهُمُ ٱللَّهُۗ إِN<نَّ> ٱللَّهَ عَزِیزٌ حَكِJ<ی>مࣱ A<۝>D<٧١>",
"وَعَدَ ٱللَّهُ ٱلۡمُؤۡمِنِینَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تِ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَا وَمَسَT<ـٰ>كِنَ طَیِّبَةN<ࣰ> فِی جَN<نَّ>T<ـٰ>تِ عَQ<دۡ>نX<ࣲ>ۚ N<وَ>رِضۡوَT< ٰ>نX<ࣱ> N<مِّ>نَ ٱللَّهِ أَكۡبَR<رُ>ۚ ذَT< ٰ>لِكَ هُوَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<٧٢>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ جَT<ـٰ>هِدِ ٱلۡكُفَّاR<رَ> وَX<ٱ>لۡمُنَT<ـٰ>فِقِینَ وَX<ٱ>غۡلُظۡ عَلَیۡهِمۡۚ وَمَأۡوَX<ى>T<ٰ>هُمۡ جَهَN<نَّ>مُۖ وَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<٧٣>",
"یَحۡلِفُونَ بِٱللَّهِ مَا قَالُوX<ا۟> وَلَقَQ<دۡ> قَالُوX<ا۟> كَلِمَةَ ٱلۡكُفۡرِ وَكَفَR<رُ>وX<ا۟> بَعۡدَ إِسۡلَT<ـٰ>مِهِمۡ وَهَN<مُّ>وX<ا۟> بِمَا لَمۡ یَنَالُوX<ا۟>ۚ وَمَا نَقَمُW<وۤ>X<ا۟> إِلَّW<اۤ> أَنۡ أَغۡنَX<ى>T<ٰ>هُمُ ٱللَّهُ وَR<رَ>سُولُهُT<ۥ> مِN<ن> فَضۡلِهِT<ۦ>ۚ فَإِX<ن> N<یَ>تُوبُوX<ا۟> یَكُ خَیۡR<ر>X<ࣰ>ا لَّهُمۡۖ وَإِX<ن> N<یَ>تَوَلَّوۡX<ا۟> یُعَذِّQ<بۡ>هُمُ ٱللَّهُ عَذَابًا أَلِیمN<ࣰ>ا فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِۚ وَمَا لَهُمۡ فِی ٱلۡأَR<رۡ>ضِ مِX<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا نَصِJ<ی>رࣲ A<۝>D<٧٤>",
"۞ وَمِنۡهُم N<مَّ>نۡ عَT<ـٰ>هَدَ ٱللَّهَ لَئِنۡ ءَاتَX<ى>T<ٰ>نَا مِN<ن> فَضۡلِهِT<ۦ> لَنَصَّدَّقَN<نَّ> وَلَنَكُونَN<نَّ> مِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٧٥>",
"فَلَN<مَّ>W<اۤ> ءَاتَX<ى>T<ٰ>هُم N<مِّ>N<ن> فَضۡلِهِT<ۦ> بَخِلُوX<ا۟> بِهِT<ۦ> وَتَوَلَّوX<ا۟> وَّهُم N<مُّ>عۡرِضُJ<و>نَ A<۝>D<٧٦>",
"فَأَعۡقَبَهُمۡ نِفَاقN<ࣰ>ا فِی قُلُوبِهِمۡ إِلَىٰ یَوۡمِ یَلۡقَوۡنَهُT<ۥ> بِمَW<اۤ> أَخۡلَفُوX<ا۟> ٱللَّهَ مَا وَعَدُوهُ وَبِمَا كَانُوX<ا۟> یَكۡذِبُJ<و>نَ A<۝>D<٧٧>",
"أَلَمۡ یَعۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ یَعۡلَمُ سِR<رَّ>هُمۡ وَنَQ<جۡ>وَX<ى>T<ٰ>هُمۡ وَأَN<نَّ> ٱللَّهَ عَلَّT<ـٰ>مُ ٱلۡغُیُJ<و>Q<ب>ِ A<۝>D<٧٨>",
"ٱلَّذِینَ یَلۡمِزُونَ ٱلۡمُطَّوِّعِینَ مِنَ ٱلۡمُؤۡمِنِینَ فِی ٱX<ل>صَّدَقَT<ـٰ>تِ وَX<ٱ>لَّذِینَ لَا یَجِدُونَ إِلَّا جُهۡدَهُمۡ فَیَسۡخَR<رُ>ونَ مِنۡهُمۡ سَخِR<رَ> ٱللَّهُ مِنۡهُمۡ وَلَهُمۡ عَذَابٌ أَلِJ<ی>مٌ A<۝>D<٧٩>",
"ٱسۡتَغۡفِرۡ لَهُمۡ أَوۡ لَا تَسۡتَغۡفِرۡ لَهُمۡ إِN<ن> تَسۡتَغۡفِرۡ لَهُمۡ سَQ<بۡ>عِینَ مَR<رَّ>ةN<ࣰ> فَلَX<ن> N<یَ>غۡفِR<رَ> ٱللَّهُ لَهُمۡۚ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ كَفَR<رُ>وX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ>ۗ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٨٠>",
"فَرِحَ ٱلۡمُخَلَّفُونَ بِمَQ<قۡ>عَدِهِمۡ خِلَT<ـٰ>فَ R<رَ>سُولِ ٱللَّهِ وَكَرِهُW<وۤ>X<ا۟> أَX<ن> N<یُ>جَT<ـٰ>هِدُوX<ا۟> بِأَمۡوَT< ٰ>لِهِمۡ وَأَN<ن>فُسِهِمۡ فِی سَبِیلِ ٱللَّهِ وَقَالُوX<ا۟> لَا تَN<ن>فِR<رُ>وX<ا۟> فِی ٱلۡحَرِّۗ قُلۡ نَاR<رُ> جَهَN<نَّ>مَ أَشَدُّ حَR<رّ>X<ࣰ>اۚ لَّوۡ كَانُوX<ا۟> یَفۡقَهُJ<و>نَ A<۝>D<٨١>",
"فَلۡیَضۡحَكُوX<ا۟> قَلِیلX<ࣰ>ا N<وَ>لۡیَQ<بۡ>كُوX<ا۟> كَثِیR<ر>N<ࣰ>ا جَزَW<اۤ>ءَN<ۢ> بِمَا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<٨٢>",
"فَإِX<ن> R<رَّ>جَعَكَ ٱللَّهُ إِلَىٰ طَW<اۤ>ئِفَةX<ࣲ> N<مِّ>نۡهُمۡ فَX<ٱ>سۡتَءۡذَنُوكَ لِلۡخُR<رُ>وجِ فَقُل لَّN<ن> تَخۡR<رُ>جُوX<ا۟> مَعِیَ أَبَدX<ࣰ>ا N<وَ>لَN<ن> تُقَT<ـٰ>تِلُوX<ا۟> مَعِیَ عَدُوًّاۖ إِN<نَّ>كُمۡ R<رَ>ضِیتُN<م> بِX<ٱ>لۡقُعُودِ أَوَّلَ مَR<رَّ>ةN<ࣲ> فَX<ٱ>Q<قۡ>عُدُوX<ا۟> مَعَ ٱلۡخَT<ـٰ>لِفِJ<ی>نَ A<۝>D<٨٣>",
"وَلَا تُصَلِّ عَلَىW<ٰۤ> أَحَدX<ࣲ> N<مِّ>نۡهُم N<مَّ>اتَ أَبَدX<ࣰ>ا N<وَ>لَا تَقُمۡ عَلَىٰ قَQ<بۡ>رِهِW<ۦۤ>ۖ إِN<نَّ>هُمۡ كَفَR<رُ>وX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ> وَمَاتُوX<ا۟> وَهُمۡ فَT<ـٰ>سِقُJ<و>نَ A<۝>D<٨٤>",
"وَلَا تُعۡجِQ<بۡ>كَ أَمۡوَT< ٰ>لُهُمۡ وَأَوۡلَT<ـٰ>دُهُمۡۚ إِN<نَّ>مَا یُرِیدُ ٱللَّهُ أَX<ن> N<یُ>عَذِّبَهُN<م> بِهَا فِی ٱX<ل>دُّنۡیَا وَتَزۡهَقَ أَN<ن>فُسُهُمۡ وَهُمۡ كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٨٥>",
"وَإِذَW<اۤ> أُN<ن>زِلَتۡ سُوR<رَ>ةٌ أَنۡ ءَامِنُوX<ا۟> بِٱللَّهِ وَجَT<ـٰ>هِدُوX<ا۟> مَعَ R<رَ>سُولِهِ ٱسۡتَءۡذَنَكَ أُX<و۟>لُوX<ا۟> ٱX<ل>طَّوۡلِ مِنۡهُمۡ وَقَالُوX<ا۟> ذَR<رۡ>نَا نَكُX<ن> N<مَّ>عَ ٱلۡقَT<ـٰ>عِدِJ<ی>نَ A<۝>D<٨٦>",
"R<رَ>ضُوX<ا۟> بِأَX<ن> N<یَ>كُونُوX<ا۟> مَعَ ٱلۡخَوَالِفِ وَطُبِعَ عَلَىٰ قُلُوبِهِمۡ فَهُمۡ لَا یَفۡقَهُJ<و>نَ A<۝>D<٨٧>",
"لَT<ـٰ>كِنِ ٱX<ل>R<رَّ>سُولُ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مَعَهُT<ۥ> جَT<ـٰ>هَدُوX<ا۟> بِأَمۡوَT< ٰ>لِهِمۡ وَأَN<ن>فُسِهِمۡۚ وَأُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمُ ٱلۡخَیۡR<رَ>T< ٰ>تُۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<٨٨>",
"أَعَدَّ ٱللَّهُ لَهُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَاۚ ذَT< ٰ>لِكَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<٨٩>",
"وَجَW<اۤ>ءَ ٱلۡمُعَذِّR<رُ>ونَ مِنَ ٱلۡأَعۡR<رَ>ابِ لِیُؤۡذَنَ لَهُمۡ وَقَعَدَ ٱلَّذِینَ كَذَبُوX<ا۟> ٱللَّهَ وَR<رَ>سُولَهُT<ۥ>ۚ سَیُصِیبُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِنۡهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٩٠>",
"لَّیۡسَ عَلَى ٱX<ل>ضُّعَفَW<اۤ>ءِ وَلَا عَلَى ٱلۡمَR<رۡ>ضَىٰ وَلَا عَلَى ٱلَّذِینَ لَا یَجِدُونَ مَا یُN<ن>فِقُونَ حَR<رَ>جٌ إِذَا نَصَحُوX<ا۟> لِلَّهِ وَR<رَ>سُولِهِT<ۦ>ۚ مَا عَلَى ٱلۡمُحۡسِنِینَ مِN<ن> سَبِیلX<ࣲ>ۚ N<وَ>ٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٩١>",
"وَلَا عَلَى ٱلَّذِینَ إِذَا مَW<اۤ> أَتَوۡكَ لِتَحۡمِلَهُمۡ قُلۡتَ لَW<اۤ> أَجِدُ مَW<اۤ> أَحۡمِلُكُمۡ عَلَیۡهِ تَوَلَّوX<ا۟> وَّأَعۡیُنُهُمۡ تَفِیضُ مِنَ ٱX<ل>دَّمۡعِ حَزَنًا أَلَّا یَجِدُوX<ا۟> مَا یُN<ن>فِقُJ<و>نَ A<۝>D<٩٢>",
"۞ إِN<نَّ>مَا ٱX<ل>سَّبِیلُ عَلَى ٱلَّذِینَ یَسۡتَءۡذِنُونَكَ وَهُمۡ أَغۡنِیَW<اۤ>ءُۚ R<رَ>ضُوX<ا۟> بِأَX<ن> N<یَ>كُونُوX<ا۟> مَعَ ٱلۡخَوَالِفِ وَطَبَعَ ٱللَّهُ عَلَىٰ قُلُوبِهِمۡ فَهُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٩٣>",
"یَعۡتَذِR<رُ>ونَ إِلَیۡكُمۡ إِذَا R<رَ>جَعۡتُمۡ إِلَیۡهِمۡۚ قُل لَّا تَعۡتَذِR<رُ>وX<ا۟> لَن N<نُّ>ؤۡمِنَ لَكُمۡ قَQ<دۡ> نَبَّأَنَا ٱللَّهُ مِنۡ أَخۡبَارِكُمۡۚ وَسَیَR<رَ>ى ٱللَّهُ عَمَلَكُمۡ وَR<رَ>سُولُهُT<ۥ> ثُN<مَّ> تُR<رَ>دُّونَ إِلَىٰ عَT<ـٰ>لِمِ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِ فَیُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٩٤>",
"سَیَحۡلِفُونَ بِٱللَّهِ لَكُمۡ إِذَا ٱN<ن>قَلَQ<بۡ>تُمۡ إِلَیۡهِمۡ لِتُعۡرِضُوX<ا۟> عَنۡهُمۡۖ فَأَعۡرِضُوX<ا۟> عَنۡهُمۡۖ إِN<نَّ>هُمۡ رِQ<جۡ>سX<ࣱ>ۖ N<وَ>مَأۡوَX<ى>T<ٰ>هُمۡ جَهَN<نَّ>مُ جَزَW<اۤ>ءَN<ۢ> بِمَا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<٩٥>",
"یَحۡلِفُونَ لَكُمۡ لِتَR<رۡ>ضَوۡX<ا۟> عَنۡهُمۡۖ فَإِN<ن> تَR<رۡ>ضَوۡX<ا۟> عَنۡهُمۡ فَإِN<نَّ> ٱللَّهَ لَا یَR<رۡ>ضَىٰ عَنِ ٱلۡقَوۡمِ ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٩٦>",
"ٱلۡأَعۡR<رَ>ابُ أَشَدُّ كُفۡR<ر>X<ࣰ>ا N<وَ>نِفَاقX<ࣰ>ا N<وَ>أَQ<جۡ>دَR<رُ> أَلَّا یَعۡلَمُوX<ا۟> حُدُودَ مَW<اۤ> أَN<ن>زَلَ ٱللَّهُ عَلَىٰ R<رَ>سُولِهِT<ۦ>ۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<٩٧>",
"وَمِنَ ٱلۡأَعۡR<رَ>ابِ مَX<ن> N<یَ>تَّخِذُ مَا یُN<ن>فِقُ مَغۡR<رَ>مX<ࣰ>ا N<وَ>یَتَR<رَ>بَّصُ بِكُمُ ٱX<ل>دَّوَW<اۤ>ئِR<رَ>ۚ عَلَیۡهِمۡ دَW<اۤ>ئِR<رَ>ةُ ٱX<ل>سَّوۡءِۗ وَٱللَّهُ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<٩٨>",
"وَمِنَ ٱلۡأَعۡR<رَ>ابِ مَX<ن> N<یُ>ؤۡمِنُ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ وَیَتَّخِذُ مَا یُN<ن>فِقُ قُR<رُ>بَT<ـٰ>تٍ عِN<ن>دَ ٱللَّهِ وَصَلَوَT< ٰ>تِ ٱX<ل>R<رَّ>سُولِۚ أَلَW<اۤ> إِN<نَّ>هَا قُR<رۡ>بَةX<ࣱ> لَّهُمۡۚ سَیُQ<دۡ>خِلُهُمُ ٱللَّهُ فِی R<رَ>حۡمَتِهِW<ۦۤ>ۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٩٩>",
"وَX<ٱ>X<ل>سَّT<ـٰ>بِقُونَ ٱلۡأَوَّلُونَ مِنَ ٱلۡمُهَT<ـٰ>جِرِینَ وَX<ٱ>لۡأَN<ن>صَارِ وَX<ٱ>لَّذِینَ ٱتَّبَعُوهُN<م> بِإِحۡسَT<ـٰ>نX<ࣲ> R<رَّ>ضِیَ ٱللَّهُ عَنۡهُمۡ وَR<رَ>ضُوX<ا۟> عَنۡهُ وَأَعَدَّ لَهُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی تَحۡتَهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدN<ࣰ>اۚ ذَT< ٰ>لِكَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<١٠٠>",
"وَمِN<مَّ>نۡ حَوۡلَكُم N<مِّ>نَ ٱلۡأَعۡR<رَ>ابِ مُنَT<ـٰ>فِقُونَۖ وَمِنۡ أَهۡلِ ٱلۡمَدِینَةِ مَR<رَ>دُوX<ا۟> عَلَى ٱX<ل>N<نِّ>فَاقِ لَا تَعۡلَمُهُمۡۖ نَحۡنُ نَعۡلَمُهُمۡۚ سَنُعَذِّبُهُم N<مَّ>R<رَّ>تَیۡنِ ثُN<مَّ> یُR<رَ>دُّونَ إِلَىٰ عَذَابٍ عَظِJ<ی>مࣲ A<۝>D<١٠١>",
"وَءَاخَR<رُ>ونَ ٱعۡتَR<رَ>فُوX<ا۟> بِذُنُوبِهِمۡ خَلَطُوX<ا۟> عَمَلN<ࣰ>ا صَT<ـٰ>لِحX<ࣰ>ا N<وَ>ءَاخَR<رَ> سَیِّئًا عَسَى ٱللَّهُ أَX<ن> N<یَ>تُوبَ عَلَیۡهِمۡۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<١٠٢>",
"خُذۡ مِنۡ أَمۡوَT< ٰ>لِهِمۡ صَدَقَةN<ࣰ> تُطَهِّR<رُ>هُمۡ وَتُزَكِّیهِN<م> بِهَا وَصَلِّ عَلَیۡهِمۡۖ إِN<نَّ> صَلَX<و>T<ٰ>تَكَ سَكَنX<ࣱ> لَّهُمۡۗ وَٱللَّهُ سَمِیعٌ عَلِJ<ی>مٌ A<۝>D<١٠٣>",
"أَلَمۡ یَعۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ هُوَ یَQ<قۡ>بَلُ ٱX<ل>تَّوۡبَةَ عَنۡ عِبَادِهِT<ۦ> وَیَأۡخُذُ ٱX<ل>صَّدَقَT<ـٰ>تِ وَأَN<نَّ> ٱللَّهَ هُوَ ٱX<ل>تَّوَّابُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٠٤>",
"وَقُلِ ٱعۡمَلُوX<ا۟> فَسَیَR<رَ>ى ٱللَّهُ عَمَلَكُمۡ وَR<رَ>سُولُهُT<ۥ> وَX<ٱ>لۡمُؤۡمِنُونَۖ وَسَتُR<رَ>دُّونَ إِلَىٰ عَT<ـٰ>لِمِ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِ فَیُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<١٠٥>",
"وَءَاخَR<رُ>ونَ مُR<رۡ>جَوۡنَ لِأَمۡرِ ٱللَّهِ إِN<مَّ>ا یُعَذِّبُهُمۡ وَإِN<مَّ>ا یَتُوبُ عَلَیۡهِمۡۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<١٠٦>",
"وَX<ٱ>لَّذِینَ ٱتَّخَذُوX<ا۟> مَسۡجِدN<ࣰ>ا ضِR<رَ>اR<ر>X<ࣰ>ا N<وَ>كُفۡR<ر>X<ࣰ>ا N<وَ>تَفۡرِیقَN<ۢ>ا بَیۡنَ ٱلۡمُؤۡمِنِینَ وَإِرۡصَادX<ࣰ>ا لِّمَنۡ حَاR<رَ>بَ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> مِN<ن> قَQ<بۡ>لُۚ وَلَیَحۡلِفُN<نَّ> إِنۡ أَR<رَ>Q<دۡ>نَW<اۤ> إِلَّا ٱلۡحُسۡنَىٰۖ وَٱللَّهُ یَشۡهَدُ إِN<نَّ>هُمۡ لَكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<١٠٧>",
"لَا تَقُمۡ فِیهِ أَبَدX<ࣰ>اۚ لَّمَسۡجِدٌ أُسِّسَ عَلَى ٱX<ل>تَّQ<قۡ>وَىٰ مِنۡ أَوَّلِ یَوۡمٍ أَحَقُّ أَN<ن> تَقُومَ فِیهِۚ فِیهِ رِجَالX<ࣱ> N<یُ>حِبُّونَ أَX<ن> N<یَ>تَطَهَّR<رُ>وX<ا۟>ۚ وَٱللَّهُ یُحِبُّ ٱلۡمُطَّهِّرِJ<ی>نَ A<۝>D<١٠٨>",
"أَفَمَنۡ أَسَّسَ بُنۡیَT<ـٰ>نَهُT<ۥ> عَلَىٰ تَQ<قۡ>وَىٰ مِنَ ٱللَّهِ وَرِضۡوَT< ٰ>نٍ خَیۡR<ر>ٌ أَم N<مَّ>نۡ أَسَّسَ بُنۡیَT<ـٰ>نَهُT<ۥ> عَلَىٰ شَفَا جُR<رُ>فٍ هَارN<ࣲ> فَX<ٱ>نۡهَاR<رَ> بِهِT<ۦ> فِی نَارِ جَهَN<نَّ>مَۗ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٠٩>",
"لَا یَزَالُ بُنۡیَT<ـٰ>نُهُمُ ٱلَّذِی بَنَوۡX<ا۟> رِیبَةN<ࣰ> فِی قُلُوبِهِمۡ إِلَّW<اۤ> أَN<ن> تَقَطَّعَ قُلُوبُهُمۡۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مٌ A<۝>D<١١٠>",
"۞ إِN<نَّ> ٱللَّهَ ٱشۡتَR<رَ>ىٰ مِنَ ٱلۡمُؤۡمِنِینَ أَN<ن>فُسَهُمۡ وَأَمۡوَT< ٰ>لَهُN<م> بِأَN<نَّ> لَهُمُ ٱلۡجَN<نَّ>ةَۚ یُقَT<ـٰ>تِلُونَ فِی سَبِیلِ ٱللَّهِ فَیَQ<قۡ>تُلُونَ وَیُQ<قۡ>تَلُونَۖ وَعۡدًا عَلَیۡهِ حَقّN<ࣰ>ا فِی ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةِ وَX<ٱ>لۡإِN<ن>جِیلِ وَX<ٱ>لۡقُR<رۡ>ءَانِۚ وَمَنۡ أَوۡفَىٰ بِعَهۡدِهِT<ۦ> مِنَ ٱللَّهِۚ فَX<ٱ>سۡتَQ<بۡ>شِR<رُ>وX<ا۟> بِبَیۡعِكُمُ ٱلَّذِی بَایَعۡتُN<م> بِهِT<ۦ>ۚ وَذَT< ٰ>لِكَ هُوَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<١١١>",
"ٱX<ل>تَّW<ـٰۤ>ئِبُونَ ٱلۡعَT<ـٰ>بِدُونَ ٱلۡحَT<ـٰ>مِدُونَ ٱX<ل>سَّW<ـٰۤ>ئِحُونَ ٱX<ل>R<رَّ>T< ٰ>كِعُونَ ٱX<ل>سَّT<ـٰ>جِدُونَ ٱلۡءَامِR<رُ>ونَ بِX<ٱ>لۡمَعۡR<رُ>وفِ وَX<ٱ>X<ل>N<نَّ>اهُونَ عَنِ ٱلۡمُN<ن>كَرِ وَX<ٱ>لۡحَT<ـٰ>فِظُونَ لِحُدُودِ ٱللَّهِۗ وَبَشِّرِ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١١٢>",
"مَا كَانَ لِX<ل>N<نَّ>بِیِّ وَX<ٱ>لَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَX<ن> N<یَ>سۡتَغۡفِR<رُ>وX<ا۟> لِلۡمُشۡرِكِینَ وَلَوۡ كَانُW<وۤ>X<ا۟> أُX<و۟>لِی قُR<رۡ>بَىٰ مِX<ن>N<ۢ> بَعۡدِ مَا تَبَیَّنَ لَهُمۡ أَN<نَّ>هُمۡ أَصۡحَT<ـٰ>بُ ٱلۡجَحِJ<ی>مِ A<۝>D<١١٣>",
"وَمَا كَانَ ٱسۡتِغۡفَاR<رُ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ لِأَبِیهِ إِلَّا عَX<ن> N<مَّ>وۡعِدَةX<ࣲ> N<وَ>عَدَهَW<اۤ> إِیَّاهُ فَلَN<مَّ>ا تَبَیَّنَ لَهُW<ۥۤ> أَN<نَّ>هُT<ۥ> عَدُوّX<ࣱ> لِّلَّهِ تَبَR<رَّ>أَ مِنۡهُۚ إِN<نَّ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ لَأَوَّT< ٰ>هٌ حَلِJ<ی>مࣱ A<۝>D<١١٤>",
"وَمَا كَانَ ٱللَّهُ لِیُضِلَّ قَوۡمَN<ۢ>ا بَعۡدَ إِذۡ هَدَX<ى>T<ٰ>هُمۡ حَتَّىٰ یُبَیِّنَ لَهُم N<مَّ>ا یَتَّقُونَۚ إِN<نَّ> ٱللَّهَ بِكُلِّ شَیۡءٍ عَلِJ<ی>مٌ A<۝>D<١١٥>",
"إِN<نَّ> ٱللَّهَ لَهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ یُحۡیِT<ۦ> وَیُمِیتُۚ وَمَا لَكُم N<مِّ>N<ن> دُونِ ٱللَّهِ مِX<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا نَصِJ<ی>رࣲ A<۝>D<١١٦>",
"لَّقَد تَّابَ ٱللَّهُ عَلَى ٱX<ل>N<نَّ>بِیِّ وَX<ٱ>لۡمُهَT<ـٰ>جِرِینَ وَX<ٱ>لۡأَN<ن>صَارِ ٱلَّذِینَ ٱتَّبَعُوهُ فِی سَاعَةِ ٱلۡعُسۡR<رَ>ةِ مِX<ن>N<ۢ> بَعۡدِ مَا كَادَ یَزِیغُ قُلُوبُ فَرِیقX<ࣲ> N<مِّ>نۡهُمۡ ثُN<مَّ> تَابَ عَلَیۡهِمۡۚ إِN<نَّ>هُT<ۥ> بِهِمۡ R<رَ>ءُوفX<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١١٧>",
"وَعَلَى ٱX<ل>ثَّلَT<ـٰ>ثَةِ ٱلَّذِینَ خُلِّفُوX<ا۟> حَتَّىW<ٰۤ> إِذَا ضَاقَتۡ عَلَیۡهِمُ ٱلۡأَR<رۡ>ضُ بِمَا R<رَ>حُبَتۡ وَضَاقَتۡ عَلَیۡهِمۡ أَN<ن>فُسُهُمۡ وَظَN<نُّ>W<وۤ>X<ا۟> أَX<ن> لَّا مَلۡجَأَ مِنَ ٱللَّهِ إِلَّW<اۤ> إِلَیۡهِ ثُN<مَّ> تَابَ عَلَیۡهِمۡ لِیَتُوبُW<وۤ>X<ا۟>ۚ إِN<نَّ> ٱللَّهَ هُوَ ٱX<ل>تَّوَّابُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١١٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱتَّقُوX<ا۟> ٱللَّهَ وَكُونُوX<ا۟> مَعَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<١١٩>",
"مَا كَانَ لِأَهۡلِ ٱلۡمَدِینَةِ وَمَنۡ حَوۡلَهُم N<مِّ>نَ ٱلۡأَعۡR<رَ>ابِ أَX<ن> N<یَ>تَخَلَّفُوX<ا۟> عَX<ن> R<رَّ>سُولِ ٱللَّهِ وَلَا یَR<رۡ>غَبُوX<ا۟> بِأَN<ن>فُسِهِمۡ عَن N<نَّ>فۡسِهِT<ۦ>ۚ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ لَا یُصِیبُهُمۡ ظَمَأX<ࣱ> N<وَ>لَا نَصَبX<ࣱ> N<وَ>لَا مَخۡمَصَةN<ࣱ> فِی سَبِیلِ ٱللَّهِ وَلَا یَطَءُونَ مَوۡطِئX<ࣰ>ا N<یَ>غِیظُ ٱلۡكُفَّاR<رَ> وَلَا یَنَالُونَ مِنۡ عَدُوّX<ࣲ> N<نَّ>یۡلًا إِلَّا كُتِبَ لَهُN<م> بِهِT<ۦ> عَمَلN<ࣱ> صَT<ـٰ>لِحٌۚ إِN<نَّ> ٱللَّهَ لَا یُضِیعُ أَQ<جۡ>R<رَ> ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٢٠>",
"وَلَا یُN<ن>فِقُونَ نَفَقَةN<ࣰ> صَغِیR<رَ>ةX<ࣰ> N<وَ>لَا كَبِیR<رَ>ةX<ࣰ> N<وَ>لَا یَQ<قۡ>طَعُونَ وَادِیًا إِلَّا كُتِبَ لَهُمۡ لِیَQ<جۡ>زِیَهُمُ ٱللَّهُ أَحۡسَنَ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٢١>",
"۞ وَمَا كَانَ ٱلۡمُؤۡمِنُونَ لِیَN<ن>فِR<رُ>وX<ا۟> كَL<اۤ>فَّةN<ࣰ>ۚ فَلَوۡلَا نَفَR<رَ> مِN<ن> كُلِّ فِرۡقَةX<ࣲ> N<مِّ>نۡهُمۡ طَW<اۤ>ئِفَةX<ࣱ> لِّیَتَفَقَّهُوX<ا۟> فِی ٱX<ل>دِّینِ وَلِیُN<ن>ذِR<رُ>وX<ا۟> قَوۡمَهُمۡ إِذَا R<رَ>جَعُW<وۤ>X<ا۟> إِلَیۡهِمۡ لَعَلَّهُمۡ یَحۡذَR<رُ>J<و>نَ A<۝>D<١٢٢>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> قَT<ـٰ>تِلُوX<ا۟> ٱلَّذِینَ یَلُونَكُم N<مِّ>نَ ٱلۡكُفَّارِ وَلۡیَجِدُوX<ا۟> فِیكُمۡ غِلۡظَةX<ࣰ>ۚ N<وَ>X<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ مَعَ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<١٢٣>",
"وَإِذَا مَW<اۤ> أُN<ن>زِلَتۡ سُوR<رَ>ةN<ࣱ> فَمِنۡهُم N<مَّ>X<ن> N<یَ>قُولُ أَیُّكُمۡ زَادَتۡهُ هَT<ـٰ>ذِهِW<ۦۤ> إِیمَT<ـٰ>نN<ࣰ>اۚ فَأَN<مَّ>ا ٱلَّذِینَ ءَامَنُوX<ا۟> فَزَادَتۡهُمۡ إِیمَT<ـٰ>نX<ࣰ>ا N<وَ>هُمۡ یَسۡتَQ<بۡ>شِR<رُ>J<و>نَ A<۝>D<١٢٤>",
"وَأَN<مَّ>ا ٱلَّذِینَ فِی قُلُوبِهِم N<مَّ>R<رَ>ضN<ࣱ> فَزَادَتۡهُمۡ رِQ<جۡ>سًا إِلَىٰ رِQ<جۡ>سِهِمۡ وَمَاتُوX<ا۟> وَهُمۡ كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<١٢٥>",
"أَوَ لَا یَR<رَ>وۡنَ أَN<نَّ>هُمۡ یُفۡتَنُونَ فِی كُلِّ عَامX<ࣲ> N<مَّ>R<رَّ>ةً أَوۡ مَR<رَّ>تَیۡنِ ثُN<مَّ> لَا یَتُوبُونَ وَلَا هُمۡ یَذَّكَّR<رُ>J<و>نَ A<۝>D<١٢٦>",
"وَإِذَا مَW<اۤ> أُN<ن>زِلَتۡ سُوR<رَ>ةX<ࣱ> N<نَّ>ظَR<رَ> بَعۡضُهُمۡ إِلَىٰ بَعۡضٍ هَلۡ یَR<رَ>X<ى>T<ٰ>كُم N<مِّ>نۡ أَحَدN<ࣲ> ثُN<مَّ> ٱN<ن>صَR<رَ>فُوX<ا۟>ۚ صَR<رَ>فَ ٱللَّهُ قُلُوبَهُN<م> بِأَN<نَّ>هُمۡ قَوۡمX<ࣱ> لَّا یَفۡقَهُJ<و>نَ A<۝>D<١٢٧>",
"لَقَQ<دۡ> جَW<اۤ>ءَكُمۡ R<رَ>سُولX<ࣱ> N<مِّ>نۡ أَN<ن>فُسِكُمۡ عَزِیزٌ عَلَیۡهِ مَا عَنِتُّمۡ حَرِیصٌ عَلَیۡكُN<م> بِX<ٱ>لۡمُؤۡمِنِینَ R<رَ>ءُوفX<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٢٨>",
"فَإِN<ن> تَوَلَّوۡX<ا۟> فَقُلۡ حَسۡبِیَ ٱللَّهُ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ عَلَیۡهِ تَوَكَّلۡتُۖ وَهُوَ R<رَ>بُّ ٱلۡعَR<رۡ>شِ ٱلۡعَظِJ<ی>مِ A<۝>D<١٢٩>",
"#اL<لۤ>رۚ تِلۡكَ ءَایَT<ـٰ>تُ ٱلۡكِتَT<ـٰ>بِ ٱلۡحَكِJ<ی>مِ A<۝>D<١>",
"أَكَانَ لِX<ل>N<نَّ>اسِ عَجَبًا أَنۡ أَوۡحَیۡنَW<اۤ> إِلَىٰ R<رَ>جُلX<ࣲ> N<مِّ>نۡهُمۡ أَنۡ أَN<ن>ذِرِ ٱX<ل>N<نَّ>اسَ وَبَشِّرِ ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَN<نَّ> لَهُمۡ قَدَمَ صِQ<دۡ>قٍ عِN<ن>دَ R<رَ>بِّهِمۡۗ قَالَ ٱلۡكَT<ـٰ>فِR<رُ>ونَ إِN<نَّ> هَT<ـٰ>ذَا لَسَT<ـٰ>حِR<ر>X<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<٢>",
"إِN<نَّ> R<رَ>بَّكُمُ ٱللَّهُ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ فِی سِتَّةِ أَیَّامN<ࣲ> ثُN<مَّ> ٱسۡتَوَىٰ عَلَى ٱلۡعَR<رۡ>شِۖ یُدَبِّR<رُ> ٱلۡأَمۡR<رَ>ۖ مَا مِN<ن> شَفِیعٍ إِلَّا مِX<ن>N<ۢ> بَعۡدِ إِذۡنِهِT<ۦ>ۚ ذَT< ٰ>لِكُمُ ٱللَّهُ R<رَ>بُّكُمۡ فَX<ٱ>عۡبُدُوهُۚ أَفَلَا تَذَكَّR<رُ>J<و>نَ A<۝>D<٣>",
"إِلَیۡهِ مَR<رۡ>جِعُكُمۡ جَمِیعX<ࣰ>اۖ N<وَ>عۡدَ ٱللَّهِ حَقًّاۚ إِN<نَّ>هُT<ۥ> یَQ<بۡ>دَؤُX<ا۟> ٱلۡخَلۡقَ ثُN<مَّ> یُعِیدُهُT<ۥ> لِیَQ<جۡ>زِیَ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ بِX<ٱ>لۡقِسۡطِۚ وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> لَهُمۡ شَR<رَ>ابX<ࣱ> N<مِّ>نۡ حَمِیمX<ࣲ> N<وَ>عَذَابٌ أَلِیمُN<ۢ> بِمَا كَانُوX<ا۟> یَكۡفُR<رُ>J<و>نَ A<۝>D<٤>",
"هُوَ ٱلَّذِی جَعَلَ ٱX<ل>شَّمۡسَ ضِیَW<اۤ>ءX<ࣰ> N<وَ>X<ٱ>لۡقَمَR<رَ> نُوR<ر>X<ࣰ>ا N<وَ>قَدَّR<رَ>هُT<ۥ> مَنَازِلَ لِتَعۡلَمُوX<ا۟> عَدَدَ ٱX<ل>سِّنِینَ وَX<ٱ>لۡحِسَابَۚ مَا خَلَقَ ٱللَّهُ ذَT< ٰ>لِكَ إِلَّا بِX<ٱ>لۡحَقِّۚ یُفَصِّلُ ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یَ>عۡلَمُJ<و>نَ A<۝>D<٥>",
"إِN<نَّ> فِی ٱخۡتِلَT<ـٰ>فِ ٱلَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِ وَمَا خَلَقَ ٱللَّهُ فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>تَّقُJ<و>نَ A<۝>D<٦>",
"إِN<نَّ> ٱلَّذِینَ لَا یَR<رۡ>جُونَ لِقَW<اۤ>ءَنَا وَR<رَ>ضُوX<ا۟> بِX<ٱ>لۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَX<ٱ>Q<طۡ>مَأَN<نُّ>وX<ا۟> بِهَا وَX<ٱ>لَّذِینَ هُمۡ عَنۡ ءَایَT<ـٰ>تِنَا غَT<ـٰ>فِلُJ<و>نَ A<۝>D<٧>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ مَأۡوَX<ى>T<ٰ>هُمُ ٱX<ل>N<نَّ>اR<رُ> بِمَا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<٨>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ یَهۡدِیهِمۡ R<رَ>بُّهُN<م> بِإِیمَT<ـٰ>نِهِمۡۖ تَQ<جۡ>رِی مِN<ن> تَحۡتِهِمُ ٱلۡأَنۡهَT<ـٰ>R<رُ> فِی جَN<نَّ>T<ـٰ>تِ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<٩>",
"دَعۡوَX<ى>T<ٰ>هُمۡ فِیهَا سُQ<بۡ>حَT<ـٰ>نَكَ ٱللَّهُN<مَّ> وَتَحِیَّتُهُمۡ فِیهَا سَلَT<ـٰ>مX<ࣱ>ۚ N<وَ>ءَاخِR<رُ> دَعۡوَX<ى>T<ٰ>هُمۡ أَنِ ٱلۡحَمۡدُ لِلَّهِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٠>",
"۞ وَلَوۡ یُعَجِّلُ ٱللَّهُ لِX<ل>N<نَّ>اسِ ٱX<ل>شَّR<رَّ> ٱسۡتِعۡجَالَهُN<م> بِX<ٱ>لۡخَیۡرِ لَقُضِیَ إِلَیۡهِمۡ أَجَلُهُمۡۖ فَنَذَR<رُ> ٱلَّذِینَ لَا یَR<رۡ>جُونَ لِقَW<اۤ>ءَنَا فِی طُغۡیَT<ـٰ>نِهِمۡ یَعۡمَهُJ<و>نَ A<۝>D<١١>",
"وَإِذَا مَسَّ ٱلۡإِN<ن>سَT<ـٰ>نَ ٱX<ل>ضُّR<رُّ> دَعَانَا لِجَX<ن>N<ۢ>بِهِW<ۦۤ> أَوۡ قَاعِدًا أَوۡ قَW<اۤ>ئِمN<ࣰ>ا فَلَN<مَّ>ا كَشَفۡنَا عَنۡهُ ضُR<رَّ>هُT<ۥ> مَR<رَّ> كَأَX<ن> لَّمۡ یَQ<دۡ>عُنَW<اۤ> إِلَىٰ ضُرّX<ࣲ> N<مَّ>سَّهُT<ۥ>ۚ كَذَT< ٰ>لِكَ زُیِّنَ لِلۡمُسۡرِفِینَ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٢>",
"وَلَقَQ<دۡ> أَهۡلَكۡنَا ٱلۡقُR<رُ>ونَ مِN<ن> قَQ<بۡ>لِكُمۡ لَN<مَّ>ا ظَلَمُوX<ا۟> وَجَW<اۤ>ءَتۡهُمۡ R<رُ>سُلُهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ وَمَا كَانُوX<ا۟> لِیُؤۡمِنُوX<ا۟>ۚ كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡقَوۡمَ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<١٣>",
"ثُN<مَّ> جَعَلۡنَT<ـٰ>كُمۡ خَلَW<ـٰۤ>ئِفَ فِی ٱلۡأَR<رۡ>ضِ مِX<ن>N<ۢ> بَعۡدِهِمۡ لِنَN<ن>ظُR<رَ> كَیۡفَ تَعۡمَلُJ<و>نَ A<۝>D<١٤>",
"وَإِذَا تُتۡلَىٰ عَلَیۡهِمۡ ءَایَاتُنَا بَیِّنَT<ـٰ>تN<ࣲ> قَالَ ٱلَّذِینَ لَا یَR<رۡ>جُونَ لِقَW<اۤ>ءَنَا ٱئۡتِ بِقُR<رۡ>ءَانٍ غَیۡرِ هَT<ـٰ>ذَW<اۤ> أَوۡ بَدِّلۡهُۚ قُلۡ مَا یَكُونُ لِW<یۤ> أَنۡ أُبَدِّلَهُT<ۥ> مِN<ن> تِلۡقَW<اۤ>ئِ نَفۡسِW<یۤ>ۖ إِنۡ أَتَّبِعُ إِلَّا مَا یُوحَىW<ٰۤ> إِلَیَّۖ إِN<نِّ>W<یۤ> أَخَافُ إِنۡ عَصَیۡتُ R<رَ>بِّی عَذَابَ یَوۡمٍ عَظِJ<ی>مࣲ A<۝>D<١٥>",
"قُل لَّوۡ شَW<اۤ>ءَ ٱللَّهُ مَا تَلَوۡتُهُT<ۥ> عَلَیۡكُمۡ وَلَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كُN<م> بِهِT<ۦ>ۖ فَقَQ<دۡ> لَبِثۡتُ فِیكُمۡ عُمُR<ر>X<ࣰ>ا N<مِّ>N<ن> قَQ<بۡ>لِهِW<ۦۤ>ۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<١٦>",
"فَمَنۡ أَظۡلَمُ مِN<مَّ>نِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبًا أَوۡ كَذَّبَ بِءَایَT<ـٰ>تِهِW<ۦۤ>ۚ إِN<نَّ>هُT<ۥ> لَا یُفۡلِحُ ٱلۡمُQ<جۡ>رِمُJ<و>نَ A<۝>D<١٧>",
"وَیَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ مَا لَا یَضُR<رُّ>هُمۡ وَلَا یَN<ن>فَعُهُمۡ وَیَقُولُونَ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ شُفَعَW<ـٰۤ>ؤُنَا عِN<ن>دَ ٱللَّهِۚ قُلۡ أَتُنَبِّءُونَ ٱللَّهَ بِمَا لَا یَعۡلَمُ فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَلَا فِی ٱلۡأَR<رۡ>ضِۚ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ> وَتَعَT<ـٰ>لَىٰ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<١٨>",
"وَمَا كَانَ ٱX<ل>N<نَّ>اسُ إِلَّW<اۤ> أُN<مَّ>ةX<ࣰ> N<وَ>T< ٰ>حِدَةN<ࣰ> فَX<ٱ>خۡتَلَفُوX<ا۟>ۚ وَلَوۡلَا كَلِمَةN<ࣱ> سَبَقَتۡ مِX<ن> R<رَّ>بِّكَ لَقُضِیَ بَیۡنَهُمۡ فِیمَا فِیهِ یَخۡتَلِفُJ<و>نَ A<۝>D<١٩>",
"وَیَقُولُونَ لَوۡلَW<اۤ> أُN<ن>زِلَ عَلَیۡهِ ءَایَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ>ۖ فَقُلۡ إِN<نَّ>مَا ٱلۡغَیۡبُ لِلَّهِ فَX<ٱ>N<ن>تَظِR<رُ>W<وۤ>X<ا۟> إِN<نِّ>ی مَعَكُم N<مِّ>نَ ٱلۡمُN<ن>تَظِرِJ<ی>نَ A<۝>D<٢٠>",
"وَإِذَW<اۤ> أَذَQ<قۡ>نَا ٱX<ل>N<نَّ>اسَ R<رَ>حۡمَةX<ࣰ> N<مِّ>X<ن>N<ۢ> بَعۡدِ ضَR<رَّ>W<اۤ>ءَ مَسَّتۡهُمۡ إِذَا لَهُم N<مَّ>كۡR<ر>N<ࣱ> فِW<یۤ> ءَایَاتِنَاۚ قُلِ ٱللَّهُ أَسۡR<رَ>عُ مَكۡR<ر>ًاۚ إِN<نَّ> R<رُ>سُلَنَا یَكۡتُبُونَ مَا تَمۡكُR<رُ>J<و>نَ A<۝>D<٢١>",
"هُوَ ٱلَّذِی یُسَیِّR<رُ>كُمۡ فِی ٱلۡبَرِّ وَX<ٱ>لۡبَحۡرِۖ حَتَّىW<ٰۤ> إِذَا كُN<ن>تُمۡ فِی ٱلۡفُلۡكِ وَجَR<رَ>یۡنَ بِهِN<م> بِرِیحN<ࣲ> طَیِّبَةX<ࣲ> N<وَ>فَرِحُوX<ا۟> بِهَا جَW<اۤ>ءَتۡهَا رِیحٌ عَاصِفX<ࣱ> N<وَ>جَW<اۤ>ءَهُمُ ٱلۡمَوۡجُ مِN<ن> كُلِّ مَكَانX<ࣲ> N<وَ>ظَN<نُّ>W<وۤ>X<ا۟> أَN<نَّ>هُمۡ أُحِیطَ بِهِمۡ دَعَوُX<ا۟> ٱللَّهَ مُخۡلِصِینَ لَهُ ٱX<ل>دِّینَ لَئِنۡ أَN<ن>جَیۡتَنَا مِنۡ هَT<ـٰ>ذِهِT<ۦ> لَنَكُونَN<نَّ> مِنَ ٱX<ل>شَّT<ـٰ>كِرِJ<ی>نَ A<۝>D<٢٢>",
"فَلَN<مَّ>W<اۤ> أَN<ن>جَX<ى>T<ٰ>هُمۡ إِذَا هُمۡ یَQ<بۡ>غُونَ فِی ٱلۡأَR<رۡ>ضِ بِغَیۡرِ ٱلۡحَقِّۗ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ إِN<نَّ>مَا بَغۡیُكُمۡ عَلَىW<ٰۤ> أَN<ن>فُسِكُمۖ N<مَّ>تَT<ـٰ>عَ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۖ ثُN<مَّ> إِلَیۡنَا مَR<رۡ>جِعُكُمۡ فَنُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٢٣>",
"إِN<نَّ>مَا مَثَلُ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا كَمَW<اۤ>ءٍ أَN<ن>زَلۡنَT<ـٰ>هُ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ فَX<ٱ>خۡتَلَطَ بِهِT<ۦ> نَبَاتُ ٱلۡأَR<رۡ>ضِ مِN<مَّ>ا یَأۡكُلُ ٱX<ل>N<نَّ>اسُ وَX<ٱ>لۡأَنۡعَT<ـٰ>مُ حَتَّىW<ٰۤ> إِذَW<اۤ> أَخَذَتِ ٱلۡأَR<رۡ>ضُ زُخۡR<رُ>فَهَا وَX<ٱ>زَّیَّنَتۡ وَظَN<نَّ> أَهۡلُهَW<اۤ> أَN<نَّ>هُمۡ قَT<ـٰ>دِR<رُ>ونَ عَلَیۡهَW<اۤ> أَتَX<ى>T<ٰ>هَW<اۤ> أَمۡR<رُ>نَا لَیۡلًا أَوۡ نَهَاR<ر>N<ࣰ>ا فَجَعَلۡنَT<ـٰ>هَا حَصِیدN<ࣰ>ا كَأَX<ن> لَّمۡ تَغۡنَ بِX<ٱ>لۡأَمۡسِۚ كَذَT< ٰ>لِكَ نُفَصِّلُ ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یَ>تَفَكَّR<رُ>J<و>نَ A<۝>D<٢٤>",
"وَٱللَّهُ یَQ<دۡ>عُW<وۤ>X<ا۟> إِلَىٰ دَارِ ٱX<ل>سَّلَT<ـٰ>مِ وَیَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُ إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٢٥>",
"۞ لِّلَّذِینَ أَحۡسَنُوX<ا۟> ٱلۡحُسۡنَىٰ وَزِیَادَةX<ࣱ>ۖ N<وَ>لَا یَR<رۡ>هَقُ وُجُوهَهُمۡ قَتَR<ر>X<ࣱ> N<وَ>لَا ذِلَّةٌۚ أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡجَN<نَّ>ةِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٢٦>",
"وَX<ٱ>لَّذِینَ كَسَبُوX<ا۟> ٱX<ل>سَّیِّءَاتِ جَزَW<اۤ>ءُ سَیِّئَةِN<ۭ> بِمِثۡلِهَا وَتَR<رۡ>هَقُهُمۡ ذِلَّةX<ࣱ>ۖ N<مَّ>ا لَهُم N<مِّ>نَ ٱللَّهِ مِنۡ عَاصِمN<ࣲ>ۖ كَأَN<نَّ>مَW<اۤ> أُغۡشِیَتۡ وُجُوهُهُمۡ قِطَعX<ࣰ>ا N<مِّ>نَ ٱلَّیۡلِ مُظۡلِمًاۚ أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٢٧>",
"وَیَوۡمَ نَحۡشُR<رُ>هُمۡ جَمِیعN<ࣰ>ا ثُN<مَّ> نَقُولُ لِلَّذِینَ أَشۡR<رَ>كُوX<ا۟> مَكَانَكُمۡ أَN<ن>تُمۡ وَشُR<رَ>كَW<اۤ>ؤُكُمۡۚ فَزَیَّلۡنَا بَیۡنَهُمۡۖ وَقَالَ شُR<رَ>كَW<اۤ>ؤُهُم N<مَّ>ا كُN<ن>تُمۡ إِیَّانَا تَعۡبُدُJ<و>نَ A<۝>D<٢٨>",
"فَكَفَىٰ بِٱللَّهِ شَهِیدَN<ۢ>ا بَیۡنَنَا وَبَیۡنَكُمۡ إِN<ن> كُN<نَّ>ا عَنۡ عِبَادَتِكُمۡ لَغَT<ـٰ>فِلِJ<ی>نَ A<۝>D<٢٩>",
"هُنَالِكَ تَQ<بۡ>لُوX<ا۟> كُلُّ نَفۡسX<ࣲ> N<مَّ>W<اۤ> أَسۡلَفَتۡۚ وَR<رُ>دُّW<وۤ>X<ا۟> إِلَى ٱللَّهِ مَوۡلَX<ى>T<ٰ>هُمُ ٱلۡحَقِّۖ وَضَلَّ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<٣٠>",
"قُلۡ مَX<ن> N<یَ>R<رۡ>زُقُكُم N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِ أَN<مَّ>X<ن> N<یَ>مۡلِكُ ٱX<ل>سَّمۡعَ وَX<ٱ>لۡأَQ<بۡ>صَT<ـٰ>R<رَ> وَمَX<ن> N<یُ>خۡرِجُ ٱلۡحَیَّ مِنَ ٱلۡمَیِّتِ وَیُخۡرِجُ ٱلۡمَیِّتَ مِنَ ٱلۡحَیِّ وَمَX<ن> N<یُ>دَبِّR<رُ> ٱلۡأَمۡR<رَ>ۚ فَسَیَقُولُونَ ٱللَّهُۚ فَقُلۡ أَفَلَا تَتَّقُJ<و>نَ A<۝>D<٣١>",
"فَذَT< ٰ>لِكُمُ ٱللَّهُ R<رَ>بُّكُمُ ٱلۡحَقُّۖ فَمَاذَا بَعۡدَ ٱلۡحَقِّ إِلَّا ٱX<ل>ضَّلَT<ـٰ>لُۖ فَأَN<نَّ>ىٰ تُصۡR<رَ>فُJ<و>نَ A<۝>D<٣٢>",
"كَذَT< ٰ>لِكَ حَقَّتۡ كَلِمَتُ R<رَ>بِّكَ عَلَى ٱلَّذِینَ فَسَقُW<وۤ>X<ا۟> أَN<نَّ>هُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٣٣>",
"قُلۡ هَلۡ مِN<ن> شُR<رَ>كَW<اۤ>ئِكُم N<مَّ>X<ن> N<یَ>Q<بۡ>دَؤُX<ا۟> ٱلۡخَلۡقَ ثُN<مَّ> یُعِیدُهُT<ۥ>ۚ قُلِ ٱللَّهُ یَQ<بۡ>دَؤُX<ا۟> ٱلۡخَلۡقَ ثُN<مَّ> یُعِیدُهُT<ۥ>ۖ فَأَN<نَّ>ىٰ تُؤۡفَكُJ<و>نَ A<۝>D<٣٤>",
"قُلۡ هَلۡ مِN<ن> شُR<رَ>كَW<اۤ>ئِكُم N<مَّ>X<ن> N<یَ>هۡدِW<یۤ> إِلَى ٱلۡحَقِّۚ قُلِ ٱللَّهُ یَهۡدِی لِلۡحَقِّۗ أَفَمَX<ن> N<یَ>هۡدِW<یۤ> إِلَى ٱلۡحَقِّ أَحَقُّ أَX<ن> N<یُ>تَّبَعَ أَN<مَّ>X<ن> لَّا یَهِدِّW<یۤ> إِلَّW<اۤ> أَX<ن> N<یُ>هۡدَىٰۖ فَمَا لَكُمۡ كَیۡفَ تَحۡكُمُJ<و>نَ A<۝>D<٣٥>",
"وَمَا یَتَّبِعُ أَكۡثَR<رُ>هُمۡ إِلَّا ظَN<نّ>ًاۚ إِN<نَّ> ٱX<ل>ظَّN<نَّ> لَا یُغۡنِی مِنَ ٱلۡحَقِّ شَیۡءًاۚ إِN<نَّ> ٱللَّهَ عَلِیمُN<ۢ> بِمَا یَفۡعَلُJ<و>نَ A<۝>D<٣٦>",
"وَمَا كَانَ هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانُ أَX<ن> N<یُ>فۡتَR<رَ>ىٰ مِN<ن> دُونِ ٱللَّهِ وَلَT<ـٰ>كِN<ن> تَصۡدِیقَ ٱلَّذِی بَیۡنَ یَدَیۡهِ وَتَفۡصِیلَ ٱلۡكِتَT<ـٰ>بِ لَا R<رَ>یۡبَ فِیهِ مِX<ن> R<رَّ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٣٧>",
"أَمۡ یَقُولُونَ ٱفۡتَR<رَ>X<ى>T<ٰ>هُۖ قُلۡ فَأۡتُوX<ا۟> بِسُوR<رَ>ةX<ࣲ> N<مِّ>ثۡلِهِT<ۦ> وَX<ٱ>Q<دۡ>عُوX<ا۟> مَنِ ٱسۡتَطَعۡتُم N<مِّ>N<ن> دُونِ ٱللَّهِ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٣٨>",
"بَلۡ كَذَّبُوX<ا۟> بِمَا لَمۡ یُحِیطُوX<ا۟> بِعِلۡمِهِT<ۦ> وَلَN<مَّ>ا یَأۡتِهِمۡ تَأۡوِیلُهُT<ۥ>ۚ كَذَT< ٰ>لِكَ كَذَّبَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۖ فَX<ٱ>N<ن>ظُR<رۡ> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٣٩>",
"وَمِنۡهُم N<مَّ>X<ن> N<یُ>ؤۡمِنُ بِهِT<ۦ> وَمِنۡهُم N<مَّ>X<ن> لَّا یُؤۡمِنُ بِهِT<ۦ>ۚ وَR<رَ>بُّكَ أَعۡلَمُ بِX<ٱ>لۡمُفۡسِدِJ<ی>نَ A<۝>D<٤٠>",
"وَإِN<ن> كَذَّبُوكَ فَقُل لِّی عَمَلِی وَلَكُمۡ عَمَلُكُمۡۖ أَN<ن>تُN<م> بَرِW<یۤ>ءُونَ مِN<مَّ>W<اۤ> أَعۡمَلُ وَأَنَا۠ بَرِW<یۤ>ءX<ࣱ> N<مِّ>N<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<٤١>",
"وَمِنۡهُم N<مَّ>X<ن> N<یَ>سۡتَمِعُونَ إِلَیۡكَۚ أَفَأَN<ن>تَ تُسۡمِعُ ٱX<ل>صُّN<مَّ> وَلَوۡ كَانُوX<ا۟> لَا یَعۡقِلُJ<و>نَ A<۝>D<٤٢>",
"وَمِنۡهُم N<مَّ>X<ن> N<یَ>N<ن>ظُR<رُ> إِلَیۡكَۚ أَفَأَN<ن>تَ تَهۡدِی ٱلۡعُمۡیَ وَلَوۡ كَانُوX<ا۟> لَا یُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٤٣>",
"إِN<نَّ> ٱللَّهَ لَا یَظۡلِمُ ٱX<ل>N<نَّ>اسَ شَیۡءX<ࣰ>ا N<وَ>لَT<ـٰ>كِN<نَّ> ٱX<ل>N<نَّ>اسَ أَN<ن>فُسَهُمۡ یَظۡلِمُJ<و>نَ A<۝>D<٤٤>",
"وَیَوۡمَ یَحۡشُR<رُ>هُمۡ كَأَX<ن> لَّمۡ یَلۡبَثُW<وۤ>X<ا۟> إِلَّا سَاعَةX<ࣰ> N<مِّ>نَ ٱX<ل>N<نَّ>هَارِ یَتَعَاR<رَ>فُونَ بَیۡنَهُمۡۚ قَQ<دۡ> خَسِR<رَ> ٱلَّذِینَ كَذَّبُوX<ا۟> بِلِقَW<اۤ>ءِ ٱللَّهِ وَمَا كَانُوX<ا۟> مُهۡتَدِJ<ی>نَ A<۝>D<٤٥>",
"وَإِN<مَّ>ا نُرِیَN<نَّ>كَ بَعۡضَ ٱلَّذِی نَعِدُهُمۡ أَوۡ نَتَوَفَّیَN<نَّ>كَ فَإِلَیۡنَا مَR<رۡ>جِعُهُمۡ ثُN<مَّ> ٱللَّهُ شَهِیدٌ عَلَىٰ مَا یَفۡعَلُJ<و>نَ A<۝>D<٤٦>",
"وَلِكُلِّ أُN<مَّ>ةX<ࣲ> R<رَّ>سُولN<ࣱ>ۖ فَإِذَا جَW<اۤ>ءَ R<رَ>سُولُهُمۡ قُضِیَ بَیۡنَهُN<م> بِX<ٱ>لۡقِسۡطِ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<٤٧>",
"وَیَقُولُونَ مَتَىٰ هَT<ـٰ>ذَا ٱلۡوَعۡدُ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٤٨>",
"قُل لَّW<اۤ> أَمۡلِكُ لِنَفۡسِی ضَR<رّ>X<ࣰ>ا N<وَ>لَا نَفۡعًا إِلَّا مَا شَW<اۤ>ءَ ٱللَّهُۗ لِكُلِّ أُN<مَّ>ةٍ أَجَلٌۚ إِذَا جَW<اۤ>ءَ أَجَلُهُمۡ فَلَا یَسۡتَءۡخِR<رُ>ونَ سَاعَةX<ࣰ> N<وَ>لَا یَسۡتَQ<قۡ>دِمُJ<و>نَ A<۝>D<٤٩>",
"قُلۡ أَR<رَ>ءَیۡتُمۡ إِنۡ أَتَX<ى>T<ٰ>كُمۡ عَذَابُهُT<ۥ> بَیَT<ـٰ>تًا أَوۡ نَهَاR<ر>X<ࣰ>ا N<مَّ>اذَا یَسۡتَعۡجِلُ مِنۡهُ ٱلۡمُQ<جۡ>رِمُJ<و>نَ A<۝>D<٥٠>",
"أَثُN<مَّ> إِذَا مَا وَقَعَ ءَامَN<ن>تُN<م> بِهِW<ۦۤ>ۚ ءَL<اۤ>لۡءَT<ـٰ>نَ وَقَQ<دۡ> كُN<ن>تُN<م> بِهِT<ۦ> تَسۡتَعۡجِلُJ<و>نَ A<۝>D<٥١>",
"ثُN<مَّ> قِیلَ لِلَّذِینَ ظَلَمُوX<ا۟> ذُوقُوX<ا۟> عَذَابَ ٱلۡخُلۡدِ هَلۡ تُQ<جۡ>زَوۡنَ إِلَّا بِمَا كُN<ن>تُمۡ تَكۡسِبُJ<و>نَ A<۝>D<٥٢>",
"۞ وَیَسۡتَX<ن>N<ۢ>بِءُونَكَ أَحَقٌّ هُوَۖ قُلۡ إِی وَR<رَ>بِّW<یۤ> إِN<نَّ>هُT<ۥ> لَحَقّX<ࣱ>ۖ N<وَ>مَW<اۤ> أَN<ن>تُN<م> بِمُعۡجِزِJ<ی>نَ A<۝>D<٥٣>",
"وَلَوۡ أَN<نَّ> لِكُلِّ نَفۡسN<ࣲ> ظَلَمَتۡ مَا فِی ٱلۡأَR<رۡ>ضِ لَX<ٱ>فۡتَدَتۡ بِهِT<ۦ>ۗ وَأَسَR<رُّ>وX<ا۟> ٱX<ل>N<نَّ>دَامَةَ لَN<مَّ>ا R<رَ>أَوُX<ا۟> ٱلۡعَذَابَۖ وَقُضِیَ بَیۡنَهُN<م> بِX<ٱ>لۡقِسۡطِ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<٥٤>",
"أَلَW<اۤ> إِN<نَّ> لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۗ أَلَW<اۤ> إِN<نَّ> وَعۡدَ ٱللَّهِ حَقّX<ࣱ> N<وَ>لَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٥٥>",
"هُوَ یُحۡیِT<ۦ> وَیُمِیتُ وَإِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٥٦>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ قَQ<دۡ> جَW<اۤ>ءَتۡكُم N<مَّ>وۡعِظَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ وَشِفَW<اۤ>ءX<ࣱ> لِّمَا فِی ٱX<ل>صُّدُورِ وَهُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣱ> لِّلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٥٧>",
"قُلۡ بِفَضۡلِ ٱللَّهِ وَبِR<رَ>حۡمَتِهِT<ۦ> فَبِذَT< ٰ>لِكَ فَلۡیَفۡR<رَ>حُوX<ا۟> هُوَ خَیۡR<ر>X<ࣱ> N<مِّ>N<مَّ>ا یَQ<جۡ>مَعُJ<و>نَ A<۝>D<٥٨>",
"قُلۡ أَR<رَ>ءَیۡتُم N<مَّ>W<اۤ> أَN<ن>زَلَ ٱللَّهُ لَكُم N<مِّ>X<ن> رِّزۡقN<ࣲ> فَجَعَلۡتُم N<مِّ>نۡهُ حَR<رَ>امX<ࣰ>ا N<وَ>حَلَT<ـٰ>لN<ࣰ>ا قُلۡ ءَL<اۤ>للَّهُ أَذِنَ لَكُمۡۖ أَمۡ عَلَى ٱللَّهِ تَفۡتَR<رُ>J<و>نَ A<۝>D<٥٩>",
"وَمَا ظَN<نُّ> ٱلَّذِینَ یَفۡتَR<رُ>ونَ عَلَى ٱللَّهِ ٱلۡكَذِبَ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۗ إِN<نَّ> ٱللَّهَ لَذُو فَضۡلٍ عَلَى ٱX<ل>N<نَّ>اسِ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَشۡكُR<رُ>J<و>نَ A<۝>D<٦٠>",
"وَمَا تَكُونُ فِی شَأۡنX<ࣲ> N<وَ>مَا تَتۡلُوX<ا۟> مِنۡهُ مِN<ن> قُR<رۡ>ءَانX<ࣲ> N<وَ>لَا تَعۡمَلُونَ مِنۡ عَمَلٍ إِلَّا كُN<نَّ>ا عَلَیۡكُمۡ شُهُودًا إِذۡ تُفِیضُونَ فِیهِۚ وَمَا یَعۡزُبُ عَX<ن> R<رَّ>بِّكَ مِX<ن> N<مِّ>ثۡقَالِ ذَR<رَّ>ةN<ࣲ> فِی ٱلۡأَR<رۡ>ضِ وَلَا فِی ٱX<ل>سَّمَW<اۤ>ءِ وَلَW<اۤ> أَصۡغَR<رَ> مِN<ن> ذَT< ٰ>لِكَ وَلَW<اۤ> أَكۡبَR<رَ> إِلَّا فِی كِتَT<ـٰ>بX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٦١>",
"أَلَW<اۤ> إِN<نَّ> أَوۡلِیَW<اۤ>ءَ ٱللَّهِ لَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٦٢>",
"ٱلَّذِینَ ءَامَنُوX<ا۟> وَكَانُوX<ا۟> یَتَّقُJ<و>نَ A<۝>D<٦٣>",
"لَهُمُ ٱلۡبُشۡR<رَ>ىٰ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَفِی ٱلۡءَاخِR<رَ>ةِۚ لَا تَQ<بۡ>دِیلَ لِكَلِمَT<ـٰ>تِ ٱللَّهِۚ ذَT< ٰ>لِكَ هُوَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<٦٤>",
"وَلَا یَحۡزُN<ن>كَ قَوۡلُهُمۡۘ إِN<نَّ> ٱلۡعِزَّةَ لِلَّهِ جَمِیعًاۚ هُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٦٥>",
"أَلَW<اۤ> إِN<نَّ> لِلَّهِ مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَN<ن> فِی ٱلۡأَR<رۡ>ضِۗ وَمَا یَتَّبِعُ ٱلَّذِینَ یَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ شُR<رَ>كَW<اۤ>ءَۚ إِX<ن> N<یَ>تَّبِعُونَ إِلَّا ٱX<ل>ظَّN<نَّ> وَإِنۡ هُمۡ إِلَّا یَخۡR<رُ>صُJ<و>نَ A<۝>D<٦٦>",
"هُوَ ٱلَّذِی جَعَلَ لَكُمُ ٱلَّیۡلَ لِتَسۡكُنُوX<ا۟> فِیهِ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ> مُQ<بۡ>صِR<ر>ًاۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>سۡمَعُJ<و>نَ A<۝>D<٦٧>",
"قَالُوX<ا۟> ٱتَّخَذَ ٱللَّهُ وَلَدN<ࣰ>اۗ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ>ۖ هُوَ ٱلۡغَنِیُّۖ لَهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۚ إِنۡ عِN<ن>دَكُم N<مِّ>N<ن> سُلۡطَT<ـٰ>نِN<ۭ> بِهَT<ـٰ>ذَW<اۤ>ۚ أَتَقُولُونَ عَلَى ٱللَّهِ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٦٨>",
"قُلۡ إِN<نَّ> ٱلَّذِینَ یَفۡتَR<رُ>ونَ عَلَى ٱللَّهِ ٱلۡكَذِبَ لَا یُفۡلِحُJ<و>نَ A<۝>D<٦٩>",
"مَتَT<ـٰ>عN<ࣱ> فِی ٱX<ل>دُّنۡیَا ثُN<مَّ> إِلَیۡنَا مَR<رۡ>جِعُهُمۡ ثُN<مَّ> نُذِیقُهُمُ ٱلۡعَذَابَ ٱX<ل>شَّدِیدَ بِمَا كَانُوX<ا۟> یَكۡفُR<رُ>J<و>نَ A<۝>D<٧٠>",
"۞ وَX<ٱ>تۡلُ عَلَیۡهِمۡ نَبَأَ نُوحٍ إِذۡ قَالَ لِقَوۡمِهِT<ۦ> یَT<ـٰ>قَوۡمِ إِN<ن> كَانَ كَبُR<رَ> عَلَیۡكُم N<مَّ>قَامِی وَتَذۡكِیرِی بِءَایَT<ـٰ>تِ ٱللَّهِ فَعَلَى ٱللَّهِ تَوَكَّلۡتُ فَأَQ<جۡ>مِعُW<وۤ>X<ا۟> أَمۡR<رَ>كُمۡ وَشُR<رَ>كَW<اۤ>ءَكُمۡ ثُN<مَّ> لَا یَكُنۡ أَمۡR<رُ>كُمۡ عَلَیۡكُمۡ غُN<مَّ>ةN<ࣰ> ثُN<مَّ> ٱQ<قۡ>ضُW<وۤ>X<ا۟> إِلَیَّ وَلَا تُN<ن>ظِR<رُ>J<و>نِ A<۝>D<٧١>",
"فَإِN<ن> تَوَلَّیۡتُمۡ فَمَا سَأَلۡتُكُم N<مِّ>نۡ أَQ<جۡ>رٍۖ إِنۡ أَQ<جۡ>رِیَ إِلَّا عَلَى ٱللَّهِۖ وَأُمِرۡتُ أَنۡ أَكُونَ مِنَ ٱلۡمُسۡلِمِJ<ی>نَ A<۝>D<٧٢>",
"فَكَذَّبُوهُ فَنَجَّیۡنَT<ـٰ>هُ وَمَX<ن> N<مَّ>عَهُT<ۥ> فِی ٱلۡفُلۡكِ وَجَعَلۡنَT<ـٰ>هُمۡ خَلَW<ـٰۤ>ئِفَ وَأَغۡR<رَ>Q<قۡ>نَا ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَاۖ فَX<ٱ>N<ن>ظُR<رۡ> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُN<ن>ذَرِJ<ی>نَ A<۝>D<٧٣>",
"ثُN<مَّ> بَعَثۡنَا مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> R<رُ>سُلًا إِلَىٰ قَوۡمِهِمۡ فَجَW<اۤ>ءُوهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَمَا كَانُوX<ا۟> لِیُؤۡمِنُوX<ا۟> بِمَا كَذَّبُوX<ا۟> بِهِT<ۦ> مِN<ن> قَQ<بۡ>لُۚ كَذَT< ٰ>لِكَ نَQ<طۡ>بَعُ عَلَىٰ قُلُوبِ ٱلۡمُعۡتَدِJ<ی>نَ A<۝>D<٧٤>",
"ثُN<مَّ> بَعَثۡنَا مِX<ن>N<ۢ> بَعۡدِهِم N<مُّ>وسَىٰ وَهَT<ـٰ>R<رُ>ونَ إِلَىٰ فِرۡعَوۡنَ وَمَلَإِX<ی۟>هِT<ۦ> بِءَایَT<ـٰ>تِنَا فَX<ٱ>سۡتَكۡبَR<رُ>وX<ا۟> وَكَانُوX<ا۟> قَوۡمX<ࣰ>ا N<مُّ>Q<جۡ>رِمِJ<ی>نَ A<۝>D<٧٥>",
"فَلَN<مَّ>ا جَW<اۤ>ءَهُمُ ٱلۡحَقُّ مِنۡ عِN<ن>دِنَا قَالُW<وۤ>X<ا۟> إِN<نَّ> هَT<ـٰ>ذَا لَسِحۡR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٧٦>",
"قَالَ مُوسَىW<ٰۤ> أَتَقُولُونَ لِلۡحَقِّ لَN<مَّ>ا جَW<اۤ>ءَكُمۡۖ أَسِحۡR<ر>ٌ هَT<ـٰ>ذَا وَلَا یُفۡلِحُ ٱX<ل>سَّT<ـٰ>حِR<رُ>J<و>نَ A<۝>D<٧٧>",
"قَالُW<وۤ>X<ا۟> أَجِئۡتَنَا لِتَلۡفِتَنَا عَN<مَّ>ا وَجَQ<دۡ>نَا عَلَیۡهِ ءَابَW<اۤ>ءَنَا وَتَكُونَ لَكُمَا ٱلۡكِQ<بۡ>رِیَW<اۤ>ءُ فِی ٱلۡأَR<رۡ>ضِ وَمَا نَحۡنُ لَكُمَا بِمُؤۡمِنِJ<ی>نَ A<۝>D<٧٨>",
"وَقَالَ فِرۡعَوۡنُ ٱئۡتُونِی بِكُلِّ سَT<ـٰ>حِرٍ عَلِJ<ی>مࣲ A<۝>D<٧٩>",
"فَلَN<مَّ>ا جَW<اۤ>ءَ ٱX<ل>سَّحَR<رَ>ةُ قَالَ لَهُم N<مُّ>وسَىW<ٰۤ> أَلۡقُوX<ا۟> مَW<اۤ> أَN<ن>تُم N<مُّ>لۡقُJ<و>نَ A<۝>D<٨٠>",
"فَلَN<مَّ>W<اۤ> أَلۡقَوۡX<ا۟> قَالَ مُوسَىٰ مَا جِئۡتُN<م> بِهِ ٱX<ل>سِّحۡR<رُ>ۖ إِN<نَّ> ٱللَّهَ سَیُQ<بۡ>طِلُهُW<ۥۤ> إِN<نَّ> ٱللَّهَ لَا یُصۡلِحُ عَمَلَ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<٨١>",
"وَیُحِقُّ ٱللَّهُ ٱلۡحَقَّ بِكَلِمَT<ـٰ>تِهِT<ۦ> وَلَوۡ كَرِهَ ٱلۡمُQ<جۡ>رِمُJ<و>نَ A<۝>D<٨٢>",
"فَمَW<اۤ> ءَامَنَ لِمُوسَىW<ٰۤ> إِلَّا ذُرِّیَّةX<ࣱ> N<مِّ>N<ن> قَوۡمِهِT<ۦ> عَلَىٰ خَوۡفX<ࣲ> N<مِّ>N<ن> فِرۡعَوۡنَ وَمَلَإِX<ی۟>هِمۡ أَX<ن> N<یَ>فۡتِنَهُمۡۚ وَإِN<نَّ> فِرۡعَوۡنَ لَعَالN<ࣲ> فِی ٱلۡأَR<رۡ>ضِ وَإِN<نَّ>هُT<ۥ> لَمِنَ ٱلۡمُسۡرِفِJ<ی>نَ A<۝>D<٨٣>",
"وَقَالَ مُوسَىٰ یَT<ـٰ>قَوۡمِ إِN<ن> كُN<ن>تُمۡ ءَامَN<ن>تُN<م> بِٱللَّهِ فَعَلَیۡهِ تَوَكَّلُW<وۤ>X<ا۟> إِN<ن> كُN<ن>تُم N<مُّ>سۡلِمِJ<ی>نَ A<۝>D<٨٤>",
"فَقَالُوX<ا۟> عَلَى ٱللَّهِ تَوَكَّلۡنَا R<رَ>بَّنَا لَا تَQ<جۡ>عَلۡنَا فِتۡنَةX<ࣰ> لِّلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٨٥>",
"وَنَجِّنَا بِR<رَ>حۡمَتِكَ مِنَ ٱلۡقَوۡمِ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٨٦>",
"وَأَوۡحَیۡنَW<اۤ> إِلَىٰ مُوسَىٰ وَأَخِیهِ أَN<ن> تَبَوَّءَا لِقَوۡمِكُمَا بِمِصۡR<رَ> بُیُوتX<ࣰ>ا N<وَ>X<ٱ>Q<جۡ>عَلُوX<ا۟> بُیُوتَكُمۡ قِQ<بۡ>لَةX<ࣰ> N<وَ>أَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَۗ وَبَشِّرِ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٨٧>",
"وَقَالَ مُوسَىٰ R<رَ>بَّنَW<اۤ> إِN<نَّ>كَ ءَاتَیۡتَ فِرۡعَوۡنَ وَمَلَأَهُT<ۥ> زِینَةX<ࣰ> N<وَ>أَمۡوَT< ٰ>لN<ࣰ>ا فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا R<رَ>بَّنَا لِیُضِلُّوX<ا۟> عَN<ن> سَبِیلِكَۖ R<رَ>بَّنَا ٱQ<طۡ>مِسۡ عَلَىW<ٰۤ> أَمۡوَT< ٰ>لِهِمۡ وَX<ٱ>شۡدُQ<دۡ> عَلَىٰ قُلُوبِهِمۡ فَلَا یُؤۡمِنُوX<ا۟> حَتَّىٰ یَR<رَ>وُX<ا۟> ٱلۡعَذَابَ ٱلۡأَلِJ<ی>مَ A<۝>D<٨٨>",
"قَالَ قَQ<دۡ> أُجِیبَت دَّعۡوَتُكُمَا فَX<ٱ>سۡتَقِیمَا وَلَا تَتَّبِعَL<اۤ>N<نِّ> سَبِیلَ ٱلَّذِینَ لَا یَعۡلَمُJ<و>نَ A<۝>D<٨٩>",
"۞ وَجَT<ـٰ>وَزۡنَا بِبَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ ٱلۡبَحۡR<رَ> فَأَتۡبَعَهُمۡ فِرۡعَوۡنُ وَجُنُودُهُT<ۥ> بَغۡیX<ࣰ>ا N<وَ>عَQ<دۡ>وًاۖ حَتَّىW<ٰۤ> إِذَW<اۤ> أَQ<دۡ>R<رَ>كَهُ ٱلۡغَR<رَ>قُ قَالَ ءَامَN<ن>تُ أَN<نَّ>هُT<ۥ> لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا ٱلَّذِW<یۤ> ءَامَنَتۡ بِهِT<ۦ> بَنُW<وۤ>X<ا۟> إِسۡR<رَ>W< ٰۤ>ءِیلَ وَأَنَا۠ مِنَ ٱلۡمُسۡلِمِJ<ی>نَ A<۝>D<٩٠>",
"ءَL<اۤ>لۡءَT<ـٰ>نَ وَقَQ<دۡ> عَصَیۡتَ قَQ<بۡ>لُ وَكُN<ن>تَ مِنَ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<٩١>",
"فَX<ٱ>لۡیَوۡمَ نُنَجِّیكَ بِبَدَنِكَ لِتَكُونَ لِمَنۡ خَلۡفَكَ ءَایَةX<ࣰ>ۚ N<وَ>إِN<نَّ> كَثِیR<ر>X<ࣰ>ا N<مِّ>نَ ٱX<ل>N<نَّ>اسِ عَنۡ ءَایَT<ـٰ>تِنَا لَغَT<ـٰ>فِلُJ<و>نَ A<۝>D<٩٢>",
"وَلَقَQ<دۡ> بَوَّأۡنَا بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ مُبَوَّأَ صِQ<دۡ>قX<ࣲ> N<وَ>R<رَ>زَQ<قۡ>نَT<ـٰ>هُم N<مِّ>نَ ٱX<ل>طَّیِّبَT<ـٰ>تِ فَمَا ٱخۡتَلَفُوX<ا۟> حَتَّىٰ جَW<اۤ>ءَهُمُ ٱلۡعِلۡمُۚ إِN<نَّ> R<رَ>بَّكَ یَQ<قۡ>ضِی بَیۡنَهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ فِیمَا كَانُوX<ا۟> فِیهِ یَخۡتَلِفُJ<و>نَ A<۝>D<٩٣>",
"فَإِN<ن> كُN<ن>تَ فِی شَكّX<ࣲ> N<مِّ>N<مَّ>W<اۤ> أَN<ن>زَلۡنَW<اۤ> إِلَیۡكَ فَسۡءَلِ ٱلَّذِینَ یَQ<قۡ>R<رَ>ءُونَ ٱلۡكِتَT<ـٰ>بَ مِN<ن> قَQ<بۡ>لِكَۚ لَقَQ<دۡ> جَW<اۤ>ءَكَ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّكَ فَلَا تَكُونَN<نَّ> مِنَ ٱلۡمُمۡتَرِJ<ی>نَ A<۝>D<٩٤>",
"وَلَا تَكُونَN<نَّ> مِنَ ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِ ٱللَّهِ فَتَكُونَ مِنَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٩٥>",
"إِN<نَّ> ٱلَّذِینَ حَقَّتۡ عَلَیۡهِمۡ كَلِمَتُ R<رَ>بِّكَ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٩٦>",
"وَلَوۡ جَW<اۤ>ءَتۡهُمۡ كُلُّ ءَایَةٍ حَتَّىٰ یَR<رَ>وُX<ا۟> ٱلۡعَذَابَ ٱلۡأَلِJ<ی>مَ A<۝>D<٩٧>",
"فَلَوۡلَا كَانَتۡ قَR<رۡ>یَةٌ ءَامَنَتۡ فَنَفَعَهَW<اۤ> إِیمَT<ـٰ>نُهَW<اۤ> إِلَّا قَوۡمَ یُونُسَ لَN<مَّ>W<اۤ> ءَامَنُوX<ا۟> كَشَفۡنَا عَنۡهُمۡ عَذَابَ ٱلۡخِزۡیِ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَمَتَّعۡنَT<ـٰ>هُمۡ إِلَىٰ حِJ<ی>نࣲ A<۝>D<٩٨>",
"وَلَوۡ شَW<اۤ>ءَ R<رَ>بُّكَ لَءَامَنَ مَN<ن> فِی ٱلۡأَR<رۡ>ضِ كُلُّهُمۡ جَمِیعًاۚ أَفَأَN<ن>تَ تُكۡرِهُ ٱX<ل>N<نَّ>اسَ حَتَّىٰ یَكُونُوX<ا۟> مُؤۡمِنِJ<ی>نَ A<۝>D<٩٩>",
"وَمَا كَانَ لِنَفۡسٍ أَN<ن> تُؤۡمِنَ إِلَّا بِإِذۡنِ ٱللَّهِۚ وَیَQ<جۡ>عَلُ ٱX<ل>رِّQ<جۡ>سَ عَلَى ٱلَّذِینَ لَا یَعۡقِلُJ<و>نَ A<۝>D<١٠٠>",
"قُلِ ٱN<ن>ظُR<رُ>وX<ا۟> مَاذَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَمَا تُغۡنِی ٱلۡءَایَT<ـٰ>تُ وَX<ٱ>X<ل>N<نُّ>ذُR<رُ> عَN<ن> قَوۡمX<ࣲ> لَّا یُؤۡمِنُJ<و>نَ A<۝>D<١٠١>",
"فَهَلۡ یَN<ن>تَظِR<رُ>ونَ إِلَّا مِثۡلَ أَیَّامِ ٱلَّذِینَ خَلَوۡX<ا۟> مِN<ن> قَQ<بۡ>لِهِمۡۚ قُلۡ فَX<ٱ>N<ن>تَظِR<رُ>W<وۤ>X<ا۟> إِN<نِّ>ی مَعَكُم N<مِّ>نَ ٱلۡمُN<ن>تَظِرِJ<ی>نَ A<۝>D<١٠٢>",
"ثُN<مَّ> نُنَجِّی R<رُ>سُلَنَا وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟>ۚ كَذَT< ٰ>لِكَ حَقًّا عَلَیۡنَا نُN<ن>جِ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٠٣>",
"قُلۡ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ إِN<ن> كُN<ن>تُمۡ فِی شَكّX<ࣲ> N<مِّ>N<ن> دِینِی فَلَW<اۤ> أَعۡبُدُ ٱلَّذِینَ تَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ وَلَT<ـٰ>كِنۡ أَعۡبُدُ ٱللَّهَ ٱلَّذِی یَتَوَفَّX<ى>T<ٰ>كُمۡۖ وَأُمِرۡتُ أَنۡ أَكُونَ مِنَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٠٤>",
"وَأَنۡ أَقِمۡ وَQ<جۡ>هَكَ لِX<ل>دِّینِ حَنِیفX<ࣰ>ا N<وَ>لَا تَكُونَN<نَّ> مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<١٠٥>",
"وَلَا تَQ<دۡ>عُ مِN<ن> دُونِ ٱللَّهِ مَا لَا یَN<ن>فَعُكَ وَلَا یَضُR<رُّ>كَۖ فَإِN<ن> فَعَلۡتَ فَإِN<نَّ>كَ إِذX<ࣰ>ا N<مِّ>نَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٠٦>",
"وَإِX<ن> N<یَ>مۡسَسۡكَ ٱللَّهُ بِضُرّN<ࣲ> فَلَا كَاشِفَ لَهُW<ۥۤ> إِلَّا هُوَۖ وَإِX<ن> N<یُ>رِQ<دۡ>كَ بِخَیۡرN<ࣲ> فَلَا R<رَ>L<اۤ>دَّ لِفَضۡلِهِT<ۦ>ۚ یُصِیبُ بِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِT<ۦ>ۚ وَهُوَ ٱلۡغَفُوR<رُ> ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٠٧>",
"قُلۡ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ قَQ<دۡ> جَW<اۤ>ءَكُمُ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّكُمۡۖ فَمَنِ ٱهۡتَدَىٰ فَإِN<نَّ>مَا یَهۡتَدِی لِنَفۡسِهِT<ۦ>ۖ وَمَN<ن> ضَلَّ فَإِN<نَّ>مَا یَضِلُّ عَلَیۡهَاۖ وَمَW<اۤ> أَنَا۠ عَلَیۡكُN<م> بِوَكِJ<ی>لࣲ A<۝>D<١٠٨>",
"وَX<ٱ>تَّبِعۡ مَا یُوحَىW<ٰۤ> إِلَیۡكَ وَX<ٱ>صۡبِرۡ حَتَّىٰ یَحۡكُمَ ٱللَّهُۚ وَهُوَ خَیۡR<رُ> ٱلۡحَT<ـٰ>كِمِJ<ی>نَ A<۝>D<١٠٩>",
"#اL<لۤ>رۚ كِتَT<ـٰ>بٌ أُحۡكِمَتۡ ءَایَT<ـٰ>تُهُT<ۥ> ثُN<مَّ> فُصِّلَتۡ مِX<ن> لَّدُنۡ حَكِیمٍ خَبِJ<ی>رٍ A<۝>D<١>",
"أَلَّا تَعۡبُدُW<وۤ>X<ا۟> إِلَّا ٱللَّهَۚ إِN<نَّ>نِی لَكُم N<مِّ>نۡهُ نَذِیR<ر>X<ࣱ> N<وَ>بَشِJ<ی>رࣱ A<۝>D<٢>",
"وَأَنِ ٱسۡتَغۡفِR<رُ>وX<ا۟> R<رَ>بَّكُمۡ ثُN<مَّ> تُوبُW<وۤ>X<ا۟> إِلَیۡهِ یُمَتِّعۡكُم N<مَّ>تَT<ـٰ>عًا حَسَنًا إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>X<ࣰ>ى N<وَ>یُؤۡتِ كُلَّ ذِی فَضۡلN<ࣲ> فَضۡلَهُT<ۥ>ۖ وَإِN<ن> تَوَلَّوۡX<ا۟> فَإِN<نِّ>W<یۤ> أَخَافُ عَلَیۡكُمۡ عَذَابَ یَوۡمN<ࣲ> كَبِJ<ی>رٍ A<۝>D<٣>",
"إِلَى ٱللَّهِ مَR<رۡ>جِعُكُمۡۖ وَهُوَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<٤>",
"أَلَW<اۤ> إِN<نَّ>هُمۡ یَثۡنُونَ صُدُوR<رَ>هُمۡ لِیَسۡتَخۡفُوX<ا۟> مِنۡهُۚ أَلَا حِینَ یَسۡتَغۡشُونَ ثِیَابَهُمۡ یَعۡلَمُ مَا یُسِR<رُّ>ونَ وَمَا یُعۡلِنُونَۚ إِN<نَّ>هُT<ۥ> عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٥>",
"۞ وَمَا مِN<ن> دَL<اۤ>بَّةN<ࣲ> فِی ٱلۡأَR<رۡ>ضِ إِلَّا عَلَى ٱللَّهِ رِزۡقُهَا وَیَعۡلَمُ مُسۡتَقَR<رَّ>هَا وَمُسۡتَوۡدَعَهَاۚ كُلّN<ࣱ> فِی كِتَT<ـٰ>بX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٦>",
"وَهُوَ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ فِی سِتَّةِ أَیَّامX<ࣲ> N<وَ>كَانَ عَR<رۡ>شُهُT<ۥ> عَلَى ٱلۡمَW<اۤ>ءِ لِیَQ<بۡ>لُوَكُمۡ أَیُّكُمۡ أَحۡسَنُ عَمَلX<ࣰ>اۗ N<وَ>لَئِN<ن> قُلۡتَ إِN<نَّ>كُم N<مَّ>Q<بۡ>عُوثُونَ مِX<ن>N<ۢ> بَعۡدِ ٱلۡمَوۡتِ لَیَقُولَN<نَّ> ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا سِحۡR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٧>",
"وَلَئِنۡ أَخَّR<رۡ>نَا عَنۡهُمُ ٱلۡعَذَابَ إِلَىW<ٰۤ> أُN<مَّ>ةX<ࣲ> N<مَّ>عۡدُودَةX<ࣲ> لَّیَقُولُN<نَّ> مَا یَحۡبِسُهُW<ۥۤ>ۗ أَلَا یَوۡمَ یَأۡتِیهِمۡ لَیۡسَ مَصۡR<رُ>وفًا عَنۡهُمۡ وَحَاقَ بِهِم N<مَّ>ا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٨>",
"وَلَئِنۡ أَذَQ<قۡ>نَا ٱلۡإِN<ن>سَT<ـٰ>نَ مِN<نَّ>ا R<رَ>حۡمَةN<ࣰ> ثُN<مَّ> نَزَعۡنَT<ـٰ>هَا مِنۡهُ إِN<نَّ>هُT<ۥ> لَیَءُوسN<ࣱ> كَفُJ<و>R<ر>ࣱ A<۝>D<٩>",
"وَلَئِنۡ أَذَQ<قۡ>نَT<ـٰ>هُ نَعۡمَW<اۤ>ءَ بَعۡدَ ضَR<رَّ>W<اۤ>ءَ مَسَّتۡهُ لَیَقُولَN<نَّ> ذَهَبَ ٱX<ل>سَّیِّءَاتُ عَN<نِّ>W<یۤ>ۚ إِN<نَّ>هُT<ۥ> لَفَرِحN<ࣱ> فَخُJ<و>R<ر>ٌ A<۝>D<١٠>",
"إِلَّا ٱلَّذِینَ صَبَR<رُ>وX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<وَ>أَQ<جۡ>R<ر>N<ࣱ> كَبِJ<ی>رࣱ A<۝>D<١١>",
"فَلَعَلَّكَ تَارِكُN<ۢ> بَعۡضَ مَا یُوحَىW<ٰۤ> إِلَیۡكَ وَضَW<اۤ>ئِقُN<ۢ> بِهِT<ۦ> صَQ<دۡ>R<رُ>كَ أَX<ن> N<یَ>قُولُوX<ا۟> لَوۡلَW<اۤ> أُN<ن>زِلَ عَلَیۡهِ كَN<ن>زٌ أَوۡ جَW<اۤ>ءَ مَعَهُT<ۥ> مَلَكٌۚ إِN<نَّ>مَW<اۤ> أَN<ن>تَ نَذِیR<ر>X<ࣱ>ۚ N<وَ>ٱللَّهُ عَلَىٰ كُلِّ شَیۡءX<ࣲ> N<وَ>كِJ<ی>لٌ A<۝>D<١٢>",
"أَمۡ یَقُولُونَ ٱفۡتَR<رَ>X<ى>T<ٰ>هُۖ قُلۡ فَأۡتُوX<ا۟> بِعَشۡرِ سُوَرX<ࣲ> N<مِّ>ثۡلِهِT<ۦ> مُفۡتَR<رَ>یَT<ـٰ>تX<ࣲ> N<وَ>X<ٱ>Q<دۡ>عُوX<ا۟> مَنِ ٱسۡتَطَعۡتُم N<مِّ>N<ن> دُونِ ٱللَّهِ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٣>",
"فَإِلَّمۡ یَسۡتَجِیبُوX<ا۟> لَكُمۡ فَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ>مَW<اۤ> أُN<ن>زِلَ بِعِلۡمِ ٱللَّهِ وَأَX<ن> لَّW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ فَهَلۡ أَN<ن>تُم N<مُّ>سۡلِمُJ<و>نَ A<۝>D<١٤>",
"مَN<ن> كَانَ یُرِیدُ ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا وَزِینَتَهَا نُوَفِّ إِلَیۡهِمۡ أَعۡمَT<ـٰ>لَهُمۡ فِیهَا وَهُمۡ فِیهَا لَا یُQ<بۡ>خَسُJ<و>نَ A<۝>D<١٥>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ لَیۡسَ لَهُمۡ فِی ٱلۡءَاخِR<رَ>ةِ إِلَّا ٱX<ل>N<نَّ>اR<رُ>ۖ وَحَبِطَ مَا صَنَعُوX<ا۟> فِیهَا وَبَT<ـٰ>طِلX<ࣱ> N<مَّ>ا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٦>",
"أَفَمَN<ن> كَانَ عَلَىٰ بَیِّنَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ> وَیَتۡلُوهُ شَاهِدX<ࣱ> N<مِّ>نۡهُ وَمِN<ن> قَQ<بۡ>لِهِT<ۦ> كِتَT<ـٰ>بُ مُوسَىW<ٰۤ> إِمَامX<ࣰ>ا N<وَ>R<رَ>حۡمَةًۚ أُX<و۟>لَW<ـٰۤ>ئِكَ یُؤۡمِنُونَ بِهِT<ۦ>ۚ وَمَX<ن> N<یَ>كۡفُR<رۡ> بِهِT<ۦ> مِنَ ٱلۡأَحۡزَابِ فَX<ٱ>X<ل>N<نَّ>اR<رُ> مَوۡعِدُهُT<ۥ>ۚ فَلَا تَكُ فِی مِرۡیَةX<ࣲ> N<مِّ>نۡهُۚ إِN<نَّ>هُ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّكَ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یُؤۡمِنُJ<و>نَ A<۝>D<١٧>",
"وَمَنۡ أَظۡلَمُ مِN<مَّ>نِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبًاۚ أُX<و۟>لَW<ـٰۤ>ئِكَ یُعۡR<رَ>ضُونَ عَلَىٰ R<رَ>بِّهِمۡ وَیَقُولُ ٱلۡأَشۡهَT<ـٰ>دُ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ ٱلَّذِینَ كَذَبُوX<ا۟> عَلَىٰ R<رَ>بِّهِمۡۚ أَلَا لَعۡنَةُ ٱللَّهِ عَلَى ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٨>",
"ٱلَّذِینَ یَصُدُّونَ عَN<ن> سَبِیلِ ٱللَّهِ وَیَQ<بۡ>غُونَهَا عِوَجX<ࣰ>ا N<وَ>هُN<م> بِX<ٱ>لۡءَاخِR<رَ>ةِ هُمۡ كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<١٩>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ لَمۡ یَكُونُوX<ا۟> مُعۡجِزِینَ فِی ٱلۡأَR<رۡ>ضِ وَمَا كَانَ لَهُم N<مِّ>N<ن> دُونِ ٱللَّهِ مِنۡ أَوۡلِیَW<اۤ>ءَۘ یُضَT<ـٰ>عَفُ لَهُمُ ٱلۡعَذَابُۚ مَا كَانُوX<ا۟> یَسۡتَطِیعُونَ ٱX<ل>سَّمۡعَ وَمَا كَانُوX<ا۟> یُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٢٠>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ خَسِR<رُ>W<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ وَضَلَّ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<٢١>",
"لَا جَR<رَ>مَ أَN<نَّ>هُمۡ فِی ٱلۡءَاخِR<رَ>ةِ هُمُ ٱلۡأَخۡسَR<رُ>J<و>نَ A<۝>D<٢٢>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَأَخۡبَتُW<وۤ>X<ا۟> إِلَىٰ R<رَ>بِّهِمۡ أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡجَN<نَّ>ةِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٢٣>",
"۞ مَثَلُ ٱلۡفَرِیقَیۡنِ كَX<ٱ>لۡأَعۡمَىٰ وَX<ٱ>لۡأَصَN<مِّ> وَX<ٱ>لۡبَصِیرِ وَX<ٱ>X<ل>سَّمِیعِۚ هَلۡ یَسۡتَوِیَانِ مَثَلًاۚ أَفَلَا تَذَكَّR<رُ>J<و>نَ A<۝>D<٢٤>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا نُوحًا إِلَىٰ قَوۡمِهِW<ۦۤ> إِN<نِّ>ی لَكُمۡ نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<٢٥>",
"أَX<ن> لَّا تَعۡبُدُW<وۤ>X<ا۟> إِلَّا ٱللَّهَۖ إِN<نِّ>W<یۤ> أَخَافُ عَلَیۡكُمۡ عَذَابَ یَوۡمٍ أَلِJ<ی>مࣲ A<۝>D<٢٦>",
"فَقَالَ ٱلۡمَلَأُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِN<ن> قَوۡمِهِT<ۦ> مَا نَR<رَ>X<ى>T<ٰ>كَ إِلَّا بَشَR<ر>X<ࣰ>ا N<مِّ>ثۡلَنَا وَمَا نَR<رَ>X<ى>T<ٰ>كَ ٱتَّبَعَكَ إِلَّا ٱلَّذِینَ هُمۡ أَR<رَ>اذِلُنَا بَادِیَ ٱX<ل>R<رَّ>أۡیِ وَمَا نَR<رَ>ىٰ لَكُمۡ عَلَیۡنَا مِN<ن> فَضۡلِN<ۭ> بَلۡ نَظُN<نُّ>كُمۡ كَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٢٧>",
"قَالَ یَT<ـٰ>قَوۡمِ أَR<رَ>ءَیۡتُمۡ إِN<ن> كُN<ن>تُ عَلَىٰ بَیِّنَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّی وَءَاتَX<ى>T<ٰ>نِی R<رَ>حۡمَةX<ࣰ> N<مِّ>نۡ عِN<ن>دِهِT<ۦ> فَعُN<مِّ>یَتۡ عَلَیۡكُمۡ أَنُلۡزِمُكُمُوهَا وَأَN<ن>تُمۡ لَهَا كَT<ـٰ>رِهُJ<و>نَ A<۝>D<٢٨>",
"وَیَT<ـٰ>قَوۡمِ لَW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ مَالًاۖ إِنۡ أَQ<جۡ>رِیَ إِلَّا عَلَى ٱللَّهِۚ وَمَW<اۤ> أَنَا۠ بِطَارِدِ ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟>ۚ إِN<نَّ>هُم N<مُّ>لَT<ـٰ>قُوX<ا۟> R<رَ>بِّهِمۡ وَلَT<ـٰ>كِN<نِّ>W<یۤ> أَR<رَ>X<ى>T<ٰ>كُمۡ قَوۡمN<ࣰ>ا تَQ<جۡ>هَلُJ<و>نَ A<۝>D<٢٩>",
"وَیَT<ـٰ>قَوۡمِ مَX<ن> N<یَ>N<ن>صُR<رُ>نِی مِنَ ٱللَّهِ إِN<ن> طَR<رَ>X<د>تُّهُمۡۚ أَفَلَا تَذَكَّR<رُ>J<و>نَ A<۝>D<٣٠>",
"وَلَW<اۤ> أَقُولُ لَكُمۡ عِN<ن>دِی خَزَW<اۤ>ئِنُ ٱللَّهِ وَلَW<اۤ> أَعۡلَمُ ٱلۡغَیۡبَ وَلَW<اۤ> أَقُولُ إِN<نِّ>ی مَلَكX<ࣱ> N<وَ>لَW<اۤ> أَقُولُ لِلَّذِینَ تَزۡدَرِW<یۤ> أَعۡیُنُكُمۡ لَX<ن> N<یُ>ؤۡتِیَهُمُ ٱللَّهُ خَیۡR<ر>ًاۖ ٱللَّهُ أَعۡلَمُ بِمَا فِW<یۤ> أَN<ن>فُسِهِمۡ إِN<نِّ>W<یۤ> إِذX<ࣰ>ا لَّمِنَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٣١>",
"قَالُوX<ا۟> یَT<ـٰ>نُوحُ قَQ<دۡ> جَT<ـٰ>دَلۡتَنَا فَأَكۡثَR<رۡ>تَ جِدَT< ٰ>لَنَا فَأۡتِنَا بِمَا تَعِدُنَW<اۤ> إِN<ن> كُN<ن>تَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٣٢>",
"قَالَ إِN<نَّ>مَا یَأۡتِیكُN<م> بِهِ ٱللَّهُ إِN<ن> شَW<اۤ>ءَ وَمَW<اۤ> أَN<ن>تُN<م> بِمُعۡجِزِJ<ی>نَ A<۝>D<٣٣>",
"وَلَا یَN<ن>فَعُكُمۡ نُصۡحِW<یۤ> إِنۡ أَR<رَ>X<د>تُّ أَنۡ أَN<ن>صَحَ لَكُمۡ إِN<ن> كَانَ ٱللَّهُ یُرِیدُ أَX<ن> N<یُ>غۡوِیَكُمۡۚ هُوَ R<رَ>بُّكُمۡ وَإِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٣٤>",
"أَمۡ یَقُولُونَ ٱفۡتَR<رَ>X<ى>T<ٰ>هُۖ قُلۡ إِنِ ٱفۡتَR<رَ>یۡتُهُT<ۥ> فَعَلَیَّ إِQ<جۡ>R<رَ>امِی وَأَنَا۠ بَرِW<یۤ>ءX<ࣱ> N<مِّ>N<مَّ>ا تُQ<جۡ>رِمُJ<و>نَ A<۝>D<٣٥>",
"وَأُوحِیَ إِلَىٰ نُوحٍ أَN<نَّ>هُT<ۥ> لَX<ن> N<یُ>ؤۡمِنَ مِN<ن> قَوۡمِكَ إِلَّا مَN<ن> قَQ<دۡ> ءَامَنَ فَلَا تَQ<بۡ>تَئِسۡ بِمَا كَانُوX<ا۟> یَفۡعَلُJ<و>نَ A<۝>D<٣٦>",
"وَX<ٱ>صۡنَعِ ٱلۡفُلۡكَ بِأَعۡیُنِنَا وَوَحۡیِنَا وَلَا تُخَT<ـٰ>طِQ<بۡ>نِی فِی ٱلَّذِینَ ظَلَمُW<وۤ>X<ا۟> إِN<نَّ>هُم N<مُّ>غۡR<رَ>قُJ<و>نَ A<۝>D<٣٧>",
"وَیَصۡنَعُ ٱلۡفُلۡكَ وَكُلَّمَا مَR<رَّ> عَلَیۡهِ مَلَأX<ࣱ> N<مِّ>N<ن> قَوۡمِهِT<ۦ> سَخِR<رُ>وX<ا۟> مِنۡهُۚ قَالَ إِN<ن> تَسۡخَR<رُ>وX<ا۟> مِN<نَّ>ا فَإِN<نَّ>ا نَسۡخَR<رُ> مِN<ن>كُمۡ كَمَا تَسۡخَR<رُ>J<و>نَ A<۝>D<٣٨>",
"فَسَوۡفَ تَعۡلَمُونَ مَX<ن> N<یَ>أۡتِیهِ عَذَابX<ࣱ> N<یُ>خۡزِیهِ وَیَحِلُّ عَلَیۡهِ عَذَابX<ࣱ> N<مُّ>قِJ<ی>مٌ A<۝>D<٣٩>",
"حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءَ أَمۡR<رُ>نَا وَفَاR<رَ> ٱX<ل>تَّN<نُّ>وR<رُ> قُلۡنَا ٱحۡمِلۡ فِیهَا مِN<ن> كُلّN<ࣲ> زَوۡجَیۡنِ ٱثۡنَیۡنِ وَأَهۡلَكَ إِلَّا مَN<ن> سَبَقَ عَلَیۡهِ ٱلۡقَوۡلُ وَمَنۡ ءَامَنَۚ وَمَW<اۤ> ءَامَنَ مَعَهُW<ۥۤ> إِلَّا قَلِJ<ی>لࣱ A<۝>D<٤٠>",
"۞ وَقَالَ ٱR<رۡ>كَبُوX<ا۟> فِیهَا بِسۡمِ ٱللَّهِ مَQ<جۡ>ر۪X<ى>T<ٰ>هَا وَمُR<رۡ>سَX<ى>T<ٰ>هَW<اۤ>ۚ إِN<نَّ> R<رَ>بِّی لَغَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٤١>",
"وَهِیَ تَQ<جۡ>رِی بِهِمۡ فِی مَوۡجN<ࣲ> كَX<ٱ>لۡجِبَالِ وَنَادَىٰ نُوحٌ ٱQ<بۡ>نَهُT<ۥ> وَكَانَ فِی مَعۡزِلX<ࣲ> N<یَ>T<ـٰ>بُنَیَّ ٱR<رۡ>كَب N<مَّ>عَنَا وَلَا تَكُX<ن> N<مَّ>عَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٤٢>",
"قَالَ سَءَاوِW<یۤ> إِلَىٰ جَبَلX<ࣲ> N<یَ>عۡصِمُنِی مِنَ ٱلۡمَW<اۤ>ءِۚ قَالَ لَا عَاصِمَ ٱلۡیَوۡمَ مِنۡ أَمۡرِ ٱللَّهِ إِلَّا مَX<ن> R<رَّ>حِمَۚ وَحَالَ بَیۡنَهُمَا ٱلۡمَوۡجُ فَكَانَ مِنَ ٱلۡمُغۡR<رَ>قِJ<ی>نَ A<۝>D<٤٣>",
"وَقِیلَ یَW<ـٰۤ>أَR<رۡ>ضُ ٱQ<بۡ>لَعِی مَW<اۤ>ءَكِ وَیَT<ـٰ>سَمَW<اۤ>ءُ أَQ<قۡ>لِعِی وَغِیضَ ٱلۡمَW<اۤ>ءُ وَقُضِیَ ٱلۡأَمۡR<رُ> وَX<ٱ>سۡتَوَتۡ عَلَى ٱلۡجُودِیِّۖ وَقِیلَ بُعۡدX<ࣰ>ا لِّلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤٤>",
"وَنَادَىٰ نُوحX<ࣱ> R<رَّ>بَّهُT<ۥ> فَقَالَ R<رَ>بِّ إِN<نَّ> ٱQ<بۡ>نِی مِنۡ أَهۡلِی وَإِN<نَّ> وَعۡدَكَ ٱلۡحَقُّ وَأَN<ن>تَ أَحۡكَمُ ٱلۡحَT<ـٰ>كِمِJ<ی>نَ A<۝>D<٤٥>",
"قَالَ یَT<ـٰ>نُوحُ إِN<نَّ>هُT<ۥ> لَیۡسَ مِنۡ أَهۡلِكَۖ إِN<نَّ>هُT<ۥ> عَمَلٌ غَیۡR<رُ> صَT<ـٰ>لِحN<ࣲ>ۖ فَلَا تَسۡءَلۡنِ مَا لَیۡسَ لَكَ بِهِT<ۦ> عِلۡمٌۖ إِN<نِّ>W<یۤ> أَعِظُكَ أَN<ن> تَكُونَ مِنَ ٱلۡجَT<ـٰ>هِلِJ<ی>نَ A<۝>D<٤٦>",
"قَالَ R<رَ>بِّ إِN<نِّ>W<یۤ> أَعُوذُ بِكَ أَنۡ أَسۡءَلَكَ مَا لَیۡسَ لِی بِهِT<ۦ> عِلۡمX<ࣱ>ۖ N<وَ>إِلَّا تَغۡفِرۡ لِی وَتَR<رۡ>حَمۡنِW<یۤ> أَكُX<ن> N<مِّ>نَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٤٧>",
"قِیلَ یَT<ـٰ>نُوحُ ٱهۡبِQ<طۡ> بِسَلَT<ـٰ>مX<ࣲ> N<مِّ>N<نَّ>ا وَبَR<رَ>كَT<ـٰ>تٍ عَلَیۡكَ وَعَلَىW<ٰۤ> أُمَمX<ࣲ> N<مِّ>N<مَّ>X<ن> N<مَّ>عَكَۚ وَأُمَمN<ࣱ> سَنُمَتِّعُهُمۡ ثُN<مَّ> یَمَسُّهُم N<مِّ>N<نَّ>ا عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٤٨>",
"تِلۡكَ مِنۡ أَX<ن>N<ۢ>بَW<اۤ>ءِ ٱلۡغَیۡبِ نُوحِیهَW<اۤ> إِلَیۡكَۖ مَا كُN<ن>تَ تَعۡلَمُهَW<اۤ> أَN<ن>تَ وَلَا قَوۡمُكَ مِN<ن> قَQ<بۡ>لِ هَT<ـٰ>ذَاۖ فَX<ٱ>صۡبِرۡۖ إِN<نَّ> ٱلۡعَT<ـٰ>قِبَةَ لِلۡمُتَّقِJ<ی>نَ A<۝>D<٤٩>",
"وَإِلَىٰ عَادٍ أَخَاهُمۡ هُودN<ࣰ>اۚ قَالَ یَT<ـٰ>قَوۡمِ ٱعۡبُدُوX<ا۟> ٱللَّهَ مَا لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡR<رُ>هُW<ۥۤ>ۖ إِنۡ أَN<ن>تُمۡ إِلَّا مُفۡتَR<رُ>J<و>نَ A<۝>D<٥٠>",
"یَT<ـٰ>قَوۡمِ لَW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ أَQ<جۡ>R<ر>ًاۖ إِنۡ أَQ<جۡ>رِیَ إِلَّا عَلَى ٱلَّذِی فَطَR<رَ>نِW<یۤ>ۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<٥١>",
"وَیَT<ـٰ>قَوۡمِ ٱسۡتَغۡفِR<رُ>وX<ا۟> R<رَ>بَّكُمۡ ثُN<مَّ> تُوبُW<وۤ>X<ا۟> إِلَیۡهِ یُR<رۡ>سِلِ ٱX<ل>سَّمَW<اۤ>ءَ عَلَیۡكُم N<مِّ>Q<دۡ>R<رَ>اR<ر>X<ࣰ>ا N<وَ>یَزِQ<دۡ>كُمۡ قُوَّةً إِلَىٰ قُوَّتِكُمۡ وَلَا تَتَوَلَّوۡX<ا۟> مُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٥٢>",
"قَالُوX<ا۟> یَT<ـٰ>هُودُ مَا جِئۡتَنَا بِبَیِّنَةX<ࣲ> N<وَ>مَا نَحۡنُ بِتَارِكِW<یۤ> ءَالِهَتِنَا عَN<ن> قَوۡلِكَ وَمَا نَحۡنُ لَكَ بِمُؤۡمِنِJ<ی>نَ A<۝>D<٥٣>",
"إِن N<نَّ>قُولُ إِلَّا ٱعۡتَR<رَ>X<ى>T<ٰ>كَ بَعۡضُ ءَالِهَتِنَا بِسُW<وۤ>ءN<ࣲ>ۗ قَالَ إِN<نِّ>W<یۤ> أُشۡهِدُ ٱللَّهَ وَX<ٱ>شۡهَدُW<وۤ>X<ا۟> أَN<نِّ>ی بَرِW<یۤ>ءX<ࣱ> N<مِّ>N<مَّ>ا تُشۡرِكُJ<و>نَ A<۝>D<٥٤>",
"مِN<ن> دُونِهِT<ۦ>ۖ فَكِیدُونِی جَمِیعN<ࣰ>ا ثُN<مَّ> لَا تُN<ن>ظِR<رُ>J<و>نِ A<۝>D<٥٥>",
"إِN<نِّ>ی تَوَكَّلۡتُ عَلَى ٱللَّهِ R<رَ>بِّی وَR<رَ>بِّكُمۚ N<مَّ>ا مِN<ن> دَL<اۤ>بَّةٍ إِلَّا هُوَ ءَاخِذُN<ۢ> بِنَاصِیَتِهَW<اۤ>ۚ إِN<نَّ> R<رَ>بِّی عَلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٥٦>",
"فَإِN<ن> تَوَلَّوۡX<ا۟> فَقَQ<دۡ> أَQ<بۡ>لَغۡتُكُم N<مَّ>W<اۤ> أُR<رۡ>سِلۡتُ بِهِW<ۦۤ> إِلَیۡكُمۡۚ وَیَسۡتَخۡلِفُ R<رَ>بِّی قَوۡمًا غَیۡR<رَ>كُمۡ وَلَا تَضُR<رُّ>ونَهُT<ۥ> شَیۡءًاۚ إِN<نَّ> R<رَ>بِّی عَلَىٰ كُلِّ شَیۡءٍ حَفِJ<ی>ظࣱ A<۝>D<٥٧>",
"وَلَN<مَّ>ا جَW<اۤ>ءَ أَمۡR<رُ>نَا نَجَّیۡنَا هُودX<ࣰ>ا N<وَ>X<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مَعَهُT<ۥ> بِR<رَ>حۡمَةX<ࣲ> N<مِّ>N<نَّ>ا وَنَجَّیۡنَT<ـٰ>هُم N<مِّ>نۡ عَذَابٍ غَلِJ<ی>ظࣲ A<۝>D<٥٨>",
"وَتِلۡكَ عَادN<ࣱ>ۖ جَحَدُوX<ا۟> بِءَایَT<ـٰ>تِ R<رَ>بِّهِمۡ وَعَصَوۡX<ا۟> R<رُ>سُلَهُT<ۥ> وَX<ٱ>تَّبَعُW<وۤ>X<ا۟> أَمۡR<رَ> كُلِّ جَبَّارٍ عَنِJ<ی>Q<د>ࣲ A<۝>D<٥٩>",
"وَأُتۡبِعُوX<ا۟> فِی هَT<ـٰ>ذِهِ ٱX<ل>دُّنۡیَا لَعۡنَةX<ࣰ> N<وَ>یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۗ أَلَW<اۤ> إِN<نَّ> عَادN<ࣰ>ا كَفَR<رُ>وX<ا۟> R<رَ>بَّهُمۡۗ أَلَا بُعۡدX<ࣰ>ا لِّعَادN<ࣲ> قَوۡمِ هُJ<و>Q<د>ࣲ A<۝>D<٦٠>",
"۞ وَإِلَىٰ ثَمُودَ أَخَاهُمۡ صَT<ـٰ>لِحN<ࣰ>اۚ قَالَ یَT<ـٰ>قَوۡمِ ٱعۡبُدُوX<ا۟> ٱللَّهَ مَا لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡR<رُ>هُT<ۥ>ۖ هُوَ أَN<ن>شَأَكُم N<مِّ>نَ ٱلۡأَR<رۡ>ضِ وَX<ٱ>سۡتَعۡمَR<رَ>كُمۡ فِیهَا فَX<ٱ>سۡتَغۡفِR<رُ>وهُ ثُN<مَّ> تُوبُW<وۤ>X<ا۟> إِلَیۡهِۚ إِN<نَّ> R<رَ>بِّی قَرِیبX<ࣱ> N<مُّ>جِJ<ی>Q<ب>ࣱ A<۝>D<٦١>",
"قَالُوX<ا۟> یَT<ـٰ>صَT<ـٰ>لِحُ قَQ<دۡ> كُN<ن>تَ فِینَا مَR<رۡ>جُوّN<ࣰ>ا قَQ<بۡ>لَ هَT<ـٰ>ذَW<اۤ>ۖ أَتَنۡهَX<ى>T<ٰ>نَW<اۤ> أَن N<نَّ>عۡبُدَ مَا یَعۡبُدُ ءَابَW<اۤ>ؤُنَا وَإِN<نَّ>نَا لَفِی شَكّX<ࣲ> N<مِّ>N<مَّ>ا تَQ<دۡ>عُونَW<اۤ> إِلَیۡهِ مُرِJ<ی>Q<ب>ࣲ A<۝>D<٦٢>",
"قَالَ یَT<ـٰ>قَوۡمِ أَR<رَ>ءَیۡتُمۡ إِN<ن> كُN<ن>تُ عَلَىٰ بَیِّنَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّی وَءَاتَX<ى>T<ٰ>نِی مِنۡهُ R<رَ>حۡمَةN<ࣰ> فَمَX<ن> N<یَ>N<ن>صُR<رُ>نِی مِنَ ٱللَّهِ إِنۡ عَصَیۡتُهُT<ۥ>ۖ فَمَا تَزِیدُونَنِی غَیۡR<رَ> تَخۡسِJ<ی>رࣲ A<۝>D<٦٣>",
"وَیَT<ـٰ>قَوۡمِ هَT<ـٰ>ذِهِT<ۦ> نَاقَةُ ٱللَّهِ لَكُمۡ ءَایَةN<ࣰ>ۖ فَذَR<رُ>وهَا تَأۡكُلۡ فِW<یۤ> أَR<رۡ>ضِ ٱللَّهِۖ وَلَا تَمَسُّوهَا بِسُW<وۤ>ءN<ࣲ> فَیَأۡخُذَكُمۡ عَذَابN<ࣱ> قَرِJ<ی>Q<ب>ࣱ A<۝>D<٦٤>",
"فَعَقَR<رُ>وهَا فَقَالَ تَمَتَّعُوX<ا۟> فِی دَارِكُمۡ ثَلَT<ـٰ>ثَةَ أَیَّامN<ࣲ>ۖ ذَT< ٰ>لِكَ وَعۡدٌ غَیۡR<رُ> مَكۡذُJ<و>Q<ب>ࣲ A<۝>D<٦٥>",
"فَلَN<مَّ>ا جَW<اۤ>ءَ أَمۡR<رُ>نَا نَجَّیۡنَا صَT<ـٰ>لِحX<ࣰ>ا N<وَ>X<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مَعَهُT<ۥ> بِR<رَ>حۡمَةX<ࣲ> N<مِّ>N<نَّ>ا وَمِنۡ خِزۡیِ یَوۡمِئِذٍۚ إِN<نَّ> R<رَ>بَّكَ هُوَ ٱلۡقَوِیُّ ٱلۡعَزِJ<ی>زُ A<۝>D<٦٦>",
"وَأَخَذَ ٱلَّذِینَ ظَلَمُوX<ا۟> ٱX<ل>صَّیۡحَةُ فَأَصۡبَحُوX<ا۟> فِی دِیَT<ـٰ>رِهِمۡ جَT<ـٰ>ثِمِJ<ی>نَ A<۝>D<٦٧>",
"كَأَX<ن> لَّمۡ یَغۡنَوۡX<ا۟> فِیهَW<اۤ>ۗ أَلَW<اۤ> إِN<نَّ> ثَمُودَX<ا۟> كَفَR<رُ>وX<ا۟> R<رَ>بَّهُمۡۗ أَلَا بُعۡدX<ࣰ>ا لِّثَمُJ<و>Q<د>َ A<۝>D<٦٨>",
"وَلَقَQ<دۡ> جَW<اۤ>ءَتۡ R<رُ>سُلُنَW<اۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ بِX<ٱ>لۡبُشۡR<رَ>ىٰ قَالُوX<ا۟> سَلَT<ـٰ>مN<ࣰ>اۖ قَالَ سَلَT<ـٰ>مN<ࣱ>ۖ فَمَا لَبِثَ أَN<ن> جَW<اۤ>ءَ بِعِQ<جۡ>لٍ حَنِJ<ی>ذࣲ A<۝>D<٦٩>",
"فَلَN<مَّ>ا R<رَ>ءَW<اۤ> أَیۡدِیَهُمۡ لَا تَصِلُ إِلَیۡهِ نَكِR<رَ>هُمۡ وَأَوۡجَسَ مِنۡهُمۡ خِیفَةN<ࣰ>ۚ قَالُوX<ا۟> لَا تَخَفۡ إِN<نَّ>W<اۤ> أُR<رۡ>سِلۡنَW<اۤ> إِلَىٰ قَوۡمِ لُJ<و>Q<ط>ࣲ A<۝>D<٧٠>",
"وَX<ٱ>مۡR<رَ>أَتُهُT<ۥ> قَW<اۤ>ئِمَةN<ࣱ> فَضَحِكَتۡ فَبَشَّR<رۡ>نَT<ـٰ>هَا بِإِسۡحَT<ـٰ>قَ وَمِX<ن> N<وَ>R<رَ>W<اۤ>ءِ إِسۡحَT<ـٰ>قَ یَعۡقُJ<و>Q<ب>َ A<۝>D<٧١>",
"قَالَتۡ یَT<ـٰ>وَیۡلَتَىW<ٰۤ> ءَأَلِدُ وَأَنَا۠ عَجُوزX<ࣱ> N<وَ>هَT<ـٰ>ذَا بَعۡلِی شَیۡخًاۖ إِN<نَّ> هَT<ـٰ>ذَا لَشَیۡءٌ عَجِJ<ی>Q<ب>ࣱ A<۝>D<٧٢>",
"قَالُW<وۤ>X<ا۟> أَتَعۡجَبِینَ مِنۡ أَمۡرِ ٱللَّهِۖ R<رَ>حۡمَتُ ٱللَّهِ وَبَR<رَ>كَT<ـٰ>تُهُT<ۥ> عَلَیۡكُمۡ أَهۡلَ ٱلۡبَیۡتِۚ إِN<نَّ>هُT<ۥ> حَمِیدX<ࣱ> N<مَّ>جِJ<ی>Q<د>ࣱ A<۝>D<٧٣>",
"فَلَN<مَّ>ا ذَهَبَ عَنۡ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ ٱX<ل>R<رَّ>وۡعُ وَجَW<اۤ>ءَتۡهُ ٱلۡبُشۡR<رَ>ىٰ یُجَT<ـٰ>دِلُنَا فِی قَوۡمِ لُJ<و>Q<ط>ٍ A<۝>D<٧٤>",
"إِN<نَّ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ لَحَلِیمٌ أَوَّT< ٰ>هX<ࣱ> N<مُّ>نِJ<ی>Q<ب>ࣱ A<۝>D<٧٥>",
"یَW<ـٰۤ>إِQ<بۡ>R<رَ>T< ٰ>هِیمُ أَعۡرِضۡ عَنۡ هَT<ـٰ>ذَW<اۤ>ۖ إِN<نَّ>هُT<ۥ> قَQ<دۡ> جَW<اۤ>ءَ أَمۡR<رُ> R<رَ>بِّكَۖ وَإِN<نَّ>هُمۡ ءَاتِیهِمۡ عَذَابٌ غَیۡR<رُ> مَR<رۡ>دُJ<و>Q<د>ࣲ A<۝>D<٧٦>",
"وَلَN<مَّ>ا جَW<اۤ>ءَتۡ R<رُ>سُلُنَا لُوطN<ࣰ>ا سِW<یۤ>ءَ بِهِمۡ وَضَاقَ بِهِمۡ ذَR<رۡ>عX<ࣰ>ا N<وَ>قَالَ هَT<ـٰ>ذَا یَوۡمٌ عَصِJ<ی>Q<ب>ࣱ A<۝>D<٧٧>",
"وَجَW<اۤ>ءَهُT<ۥ> قَوۡمُهُT<ۥ> یُهۡR<رَ>عُونَ إِلَیۡهِ وَمِN<ن> قَQ<بۡ>لُ كَانُوX<ا۟> یَعۡمَلُونَ ٱX<ل>سَّیِّءَاتِۚ قَالَ یَT<ـٰ>قَوۡمِ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ بَنَاتِی هُN<نَّ> أَQ<طۡ>هَR<رُ> لَكُمۡۖ فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَلَا تُخۡزُونِ فِی ضَیۡفِW<یۤ>ۖ أَلَیۡسَ مِN<ن>كُمۡ R<رَ>جُلX<ࣱ> R<رَّ>شِJ<ی>Q<د>ࣱ A<۝>D<٧٨>",
"قَالُوX<ا۟> لَقَQ<دۡ> عَلِمۡتَ مَا لَنَا فِی بَنَاتِكَ مِنۡ حَقّX<ࣲ> N<وَ>إِN<نَّ>كَ لَتَعۡلَمُ مَا نُرِJ<ی>Q<د>ُ A<۝>D<٧٩>",
"قَالَ لَوۡ أَN<نَّ> لِی بِكُمۡ قُوَّةً أَوۡ ءَاوِW<یۤ> إِلَىٰ R<رُ>كۡنN<ࣲ> شَدِJ<ی>Q<د>ࣲ A<۝>D<٨٠>",
"قَالُوX<ا۟> یَT<ـٰ>لُوطُ إِN<نَّ>ا R<رُ>سُلُ R<رَ>بِّكَ لَX<ن> N<یَ>صِلُW<وۤ>X<ا۟> إِلَیۡكَۖ فَأَسۡرِ بِأَهۡلِكَ بِقِQ<طۡ>عX<ࣲ> N<مِّ>نَ ٱلَّیۡلِ وَلَا یَلۡتَفِتۡ مِN<ن>كُمۡ أَحَدٌ إِلَّا ٱمۡR<رَ>أَتَكَۖ إِN<نَّ>هُT<ۥ> مُصِیبُهَا مَW<اۤ> أَصَابَهُمۡۚ إِN<نَّ> مَوۡعِدَهُمُ ٱX<ل>صُّQ<بۡ>حُۚ أَلَیۡسَ ٱX<ل>صُّQ<بۡ>حُ بِقَرِJ<ی>Q<ب>ࣲ A<۝>D<٨١>",
"فَلَN<مَّ>ا جَW<اۤ>ءَ أَمۡR<رُ>نَا جَعَلۡنَا عَT<ـٰ>لِیَهَا سَافِلَهَا وَأَمۡطَR<رۡ>نَا عَلَیۡهَا حِجَاR<رَ>ةX<ࣰ> N<مِّ>N<ن> سِجِّیلX<ࣲ> N<مَّ>N<ن>ضُJ<و>Q<د>ࣲ A<۝>D<٨٢>",
"مُّسَوَّمَةً عِN<ن>دَ R<رَ>بِّكَۖ وَمَا هِیَ مِنَ ٱX<ل>ظَّT<ـٰ>لِمِینَ بِبَعِJ<ی>Q<د>ࣲ A<۝>D<٨٣>",
"۞ وَإِلَىٰ مَQ<دۡ>یَنَ أَخَاهُمۡ شُعَیۡبN<ࣰ>اۚ قَالَ یَT<ـٰ>قَوۡمِ ٱعۡبُدُوX<ا۟> ٱللَّهَ مَا لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡR<رُ>هُT<ۥ>ۖ وَلَا تَN<ن>قُصُوX<ا۟> ٱلۡمِكۡیَالَ وَX<ٱ>لۡمِیزَانَۖ إِN<نِّ>W<یۤ> أَR<رَ>X<ى>T<ٰ>كُN<م> بِخَیۡرX<ࣲ> N<وَ>إِN<نِّ>W<یۤ> أَخَافُ عَلَیۡكُمۡ عَذَابَ یَوۡمX<ࣲ> N<مُّ>حِJ<ی>Q<ط>ࣲ A<۝>D<٨٤>",
"وَیَT<ـٰ>قَوۡمِ أَوۡفُوX<ا۟> ٱلۡمِكۡیَالَ وَX<ٱ>لۡمِیزَانَ بِX<ٱ>لۡقِسۡطِۖ وَلَا تَQ<بۡ>خَسُوX<ا۟> ٱX<ل>N<نَّ>اسَ أَشۡیَW<اۤ>ءَهُمۡ وَلَا تَعۡثَوۡX<ا۟> فِی ٱلۡأَR<رۡ>ضِ مُفۡسِدِJ<ی>نَ A<۝>D<٨٥>",
"بَقِیَّتُ ٱللَّهِ خَیۡR<ر>X<ࣱ> لَّكُمۡ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِینَۚ وَمَW<اۤ> أَنَا۠ عَلَیۡكُN<م> بِحَفِJ<ی>ظࣲ A<۝>D<٨٦>",
"قَالُوX<ا۟> یَT<ـٰ>شُعَیۡبُ أَصَلَX<و>T<ٰ>تُكَ تَأۡمُR<رُ>كَ أَن N<نَّ>تۡR<رُ>كَ مَا یَعۡبُدُ ءَابَW<اۤ>ؤُنَW<اۤ> أَوۡ أَن N<نَّ>فۡعَلَ فِW<یۤ> أَمۡوَT< ٰ>لِنَا مَا نَشَW<ـٰۤ>ؤُX<ا۟>ۖ إِN<نَّ>كَ لَأَN<ن>تَ ٱلۡحَلِیمُ ٱX<ل>R<رَّ>شِJ<ی>Q<د>ُ A<۝>D<٨٧>",
"قَالَ یَT<ـٰ>قَوۡمِ أَR<رَ>ءَیۡتُمۡ إِN<ن> كُN<ن>تُ عَلَىٰ بَیِّنَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّی وَR<رَ>زَقَنِی مِنۡهُ رِزۡقًا حَسَنX<ࣰ>اۚ N<وَ>مَW<اۤ> أُرِیدُ أَنۡ أُخَالِفَكُمۡ إِلَىٰ مَW<اۤ> أَنۡهَX<ى>T<ٰ>كُمۡ عَنۡهُۚ إِنۡ أُرِیدُ إِلَّا ٱلۡإِصۡلَT<ـٰ>حَ مَا ٱسۡتَطَعۡتُۚ وَمَا تَوۡفِیقِW<یۤ> إِلَّا بِٱللَّهِۚ عَلَیۡهِ تَوَكَّلۡتُ وَإِلَیۡهِ أُنِJ<ی>Q<ب>ُ A<۝>D<٨٨>",
"وَیَT<ـٰ>قَوۡمِ لَا یَQ<جۡ>رِمَN<نَّ>كُمۡ شِقَاقِW<یۤ> أَX<ن> N<یُ>صِیبَكُم N<مِّ>ثۡلُ مَW<اۤ> أَصَابَ قَوۡمَ نُوحٍ أَوۡ قَوۡمَ هُودٍ أَوۡ قَوۡمَ صَT<ـٰ>لِحX<ࣲ>ۚ N<وَ>مَا قَوۡمُ لُوطX<ࣲ> N<مِّ>N<ن>كُN<م> بِبَعِJ<ی>Q<د>ࣲ A<۝>D<٨٩>",
"وَX<ٱ>سۡتَغۡفِR<رُ>وX<ا۟> R<رَ>بَّكُمۡ ثُN<مَّ> تُوبُW<وۤ>X<ا۟> إِلَیۡهِۚ إِN<نَّ> R<رَ>بِّی R<رَ>حِیمX<ࣱ> N<وَ>دُJ<و>Q<د>ࣱ A<۝>D<٩٠>",
"قَالُوX<ا۟> یَT<ـٰ>شُعَیۡبُ مَا نَفۡقَهُ كَثِیR<ر>X<ࣰ>ا N<مِّ>N<مَّ>ا تَقُولُ وَإِN<نَّ>ا لَنَR<رَ>X<ى>T<ٰ>كَ فِینَا ضَعِیفX<ࣰ>اۖ N<وَ>لَوۡلَا R<رَ>هۡطُكَ لَR<رَ>جَمۡنَT<ـٰ>كَۖ وَمَW<اۤ> أَN<ن>تَ عَلَیۡنَا بِعَزِJ<ی>زࣲ A<۝>D<٩١>",
"قَالَ یَT<ـٰ>قَوۡمِ أَR<رَ>هۡطِW<یۤ> أَعَزُّ عَلَیۡكُم N<مِّ>نَ ٱللَّهِ وَX<ٱ>تَّخَذۡتُمُوهُ وَR<رَ>W<اۤ>ءَكُمۡ ظِهۡرِیًّاۖ إِN<نَّ> R<رَ>بِّی بِمَا تَعۡمَلُونَ مُحِJ<ی>Q<ط>ࣱ A<۝>D<٩٢>",
"وَیَT<ـٰ>قَوۡمِ ٱعۡمَلُوX<ا۟> عَلَىٰ مَكَانَتِكُمۡ إِN<نِّ>ی عَT<ـٰ>مِلN<ࣱ>ۖ سَوۡفَ تَعۡلَمُونَ مَX<ن> N<یَ>أۡتِیهِ عَذَابX<ࣱ> N<یُ>خۡزِیهِ وَمَنۡ هُوَ كَT<ـٰ>ذِبX<ࣱ>ۖ N<وَ>X<ٱ>R<رۡ>تَقِبُW<وۤ>X<ا۟> إِN<نِّ>ی مَعَكُمۡ R<رَ>قِJ<ی>Q<ب>ࣱ A<۝>D<٩٣>",
"وَلَN<مَّ>ا جَW<اۤ>ءَ أَمۡR<رُ>نَا نَجَّیۡنَا شُعَیۡبX<ࣰ>ا N<وَ>X<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مَعَهُT<ۥ> بِR<رَ>حۡمَةX<ࣲ> N<مِّ>N<نَّ>ا وَأَخَذَتِ ٱلَّذِینَ ظَلَمُوX<ا۟> ٱX<ل>صَّیۡحَةُ فَأَصۡبَحُوX<ا۟> فِی دِیَT<ـٰ>رِهِمۡ جَT<ـٰ>ثِمِJ<ی>نَ A<۝>D<٩٤>",
"كَأَX<ن> لَّمۡ یَغۡنَوۡX<ا۟> فِیهَW<اۤ>ۗ أَلَا بُعۡدX<ࣰ>ا لِّمَQ<دۡ>یَنَ كَمَا بَعِدَتۡ ثَمُJ<و>Q<د>ُ A<۝>D<٩٥>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا مُوسَىٰ بِءَایَT<ـٰ>تِنَا وَسُلۡطَT<ـٰ>نX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٩٦>",
"إِلَىٰ فِرۡعَوۡنَ وَمَلَإِX<ی۟>هِT<ۦ> فَX<ٱ>تَّبَعُW<وۤ>X<ا۟> أَمۡR<رَ> فِرۡعَوۡنَۖ وَمَW<اۤ> أَمۡR<رُ> فِرۡعَوۡنَ بِR<رَ>شِJ<ی>Q<د>ࣲ A<۝>D<٩٧>",
"یَQ<قۡ>دُمُ قَوۡمَهُT<ۥ> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ فَأَوۡR<رَ>دَهُمُ ٱX<ل>N<نَّ>اR<رَ>ۖ وَبِئۡسَ ٱلۡوِرۡدُ ٱلۡمَوۡR<رُ>J<و>Q<د>ُ A<۝>D<٩٨>",
"وَأُتۡبِعُوX<ا۟> فِی هَT<ـٰ>ذِهِT<ۦ> لَعۡنَةX<ࣰ> N<وَ>یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۚ بِئۡسَ ٱX<ل>رِّفۡدُ ٱلۡمَR<رۡ>فُJ<و>Q<د>ُ A<۝>D<٩٩>",
"ذَT< ٰ>لِكَ مِنۡ أَX<ن>N<ۢ>بَW<اۤ>ءِ ٱلۡقُR<رَ>ىٰ نَقُصُّهُT<ۥ> عَلَیۡكَۖ مِنۡهَا قَW<اۤ>ئِمX<ࣱ> N<وَ>حَصِJ<ی>Q<د>ࣱ A<۝>D<١٠٠>",
"وَمَا ظَلَمۡنَT<ـٰ>هُمۡ وَلَT<ـٰ>كِN<ن> ظَلَمُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡۖ فَمَW<اۤ> أَغۡنَتۡ عَنۡهُمۡ ءَالِهَتُهُمُ ٱلَّتِی یَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ مِN<ن> شَیۡءX<ࣲ> لَّN<مَّ>ا جَW<اۤ>ءَ أَمۡR<رُ> R<رَ>بِّكَۖ وَمَا زَادُوهُمۡ غَیۡR<رَ> تَتۡبِJ<ی>Q<ب>ࣲ A<۝>D<١٠١>",
"وَكَذَT< ٰ>لِكَ أَخۡذُ R<رَ>بِّكَ إِذَW<اۤ> أَخَذَ ٱلۡقُR<رَ>ىٰ وَهِیَ ظَT<ـٰ>لِمَةٌۚ إِN<نَّ> أَخۡذَهُW<ۥۤ> أَلِیمN<ࣱ> شَدِJ<ی>Q<د>ٌ A<۝>D<١٠٢>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّمَنۡ خَافَ عَذَابَ ٱلۡءَاخِR<رَ>ةِۚ ذَT< ٰ>لِكَ یَوۡمX<ࣱ> N<مَّ>Q<جۡ>مُوعX<ࣱ> لَّهُ ٱX<ل>N<نَّ>اسُ وَذَT< ٰ>لِكَ یَوۡمX<ࣱ> N<مَّ>شۡهُJ<و>Q<د>ࣱ A<۝>D<١٠٣>",
"وَمَا نُؤَخِّR<رُ>هُW<ۥۤ> إِلَّا لِأَجَلX<ࣲ> N<مَّ>عۡدُJ<و>Q<د>ࣲ A<۝>D<١٠٤>",
"یَوۡمَ یَأۡتِ لَا تَكَلَّمُ نَفۡسٌ إِلَّا بِإِذۡنِهِT<ۦ>ۚ فَمِنۡهُمۡ شَقِیّX<ࣱ> N<وَ>سَعِJ<ی>Q<د>ࣱ A<۝>D<١٠٥>",
"فَأَN<مَّ>ا ٱلَّذِینَ شَقُوX<ا۟> فَفِی ٱX<ل>N<نَّ>ارِ لَهُمۡ فِیهَا زَفِیR<ر>X<ࣱ> N<وَ>شَهِJ<ی>Q<ق>ٌ A<۝>D<١٠٦>",
"خَT<ـٰ>لِدِینَ فِیهَا مَا دَامَتِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تُ وَX<ٱ>لۡأَR<رۡ>ضُ إِلَّا مَا شَW<اۤ>ءَ R<رَ>بُّكَۚ إِN<نَّ> R<رَ>بَّكَ فَعَّالX<ࣱ> لِّمَا یُرِJ<ی>Q<د>ُ A<۝>D<١٠٧>",
"۞ وَأَN<مَّ>ا ٱلَّذِینَ سُعِدُوX<ا۟> فَفِی ٱلۡجَN<نَّ>ةِ خَT<ـٰ>لِدِینَ فِیهَا مَا دَامَتِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تُ وَX<ٱ>لۡأَR<رۡ>ضُ إِلَّا مَا شَW<اۤ>ءَ R<رَ>بُّكَۖ عَطَW<اۤ>ءً غَیۡR<رَ> مَQ<جۡ>ذُJ<و>ذࣲ A<۝>D<١٠٨>",
"فَلَا تَكُ فِی مِرۡیَةX<ࣲ> N<مِّ>N<مَّ>ا یَعۡبُدُ هَW<ـٰۤ>ؤُلَW<اۤ>ءِۚ مَا یَعۡبُدُونَ إِلَّا كَمَا یَعۡبُدُ ءَابَW<اۤ>ؤُهُم N<مِّ>N<ن> قَQ<بۡ>لُۚ وَإِN<نَّ>ا لَمُوَفُّوهُمۡ نَصِیبَهُمۡ غَیۡR<رَ> مَN<ن>قُJ<و>صࣲ A<۝>D<١٠٩>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ فَX<ٱ>خۡتُلِفَ فِیهِۚ وَلَوۡلَا كَلِمَةN<ࣱ> سَبَقَتۡ مِX<ن> R<رَّ>بِّكَ لَقُضِیَ بَیۡنَهُمۡۚ وَإِN<نَّ>هُمۡ لَفِی شَكّX<ࣲ> N<مِّ>نۡهُ مُرِJ<ی>Q<ب>ࣲ A<۝>D<١١٠>",
"وَإِN<نَّ> كُلّX<ࣰ>ا لَّN<مَّ>ا لَیُوَفِّیَN<نَّ>هُمۡ R<رَ>بُّكَ أَعۡمَT<ـٰ>لَهُمۡۚ إِN<نَّ>هُT<ۥ> بِمَا یَعۡمَلُونَ خَبِJ<ی>رࣱ A<۝>D<١١١>",
"فَX<ٱ>سۡتَقِمۡ كَمَW<اۤ> أُمِرۡتَ وَمَN<ن> تَابَ مَعَكَ وَلَا تَQ<طۡ>غَوۡX<ا۟>ۚ إِN<نَّ>هُT<ۥ> بِمَا تَعۡمَلُونَ بَصِJ<ی>رࣱ A<۝>D<١١٢>",
"وَلَا تَR<رۡ>كَنُW<وۤ>X<ا۟> إِلَى ٱلَّذِینَ ظَلَمُوX<ا۟> فَتَمَسَّكُمُ ٱX<ل>N<نَّ>اR<رُ> وَمَا لَكُم N<مِّ>N<ن> دُونِ ٱللَّهِ مِنۡ أَوۡلِیَW<اۤ>ءَ ثُN<مَّ> لَا تُN<ن>صَR<رُ>J<و>نَ A<۝>D<١١٣>",
"وَأَقِمِ ٱX<ل>صَّلَX<و>T<ٰ>ةَ طَR<رَ>فَیِ ٱX<ل>N<نَّ>هَارِ وَزُلَفX<ࣰ>ا N<مِّ>نَ ٱلَّیۡلِۚ إِN<نَّ> ٱلۡحَسَنَT<ـٰ>تِ یُذۡهِQ<بۡ>نَ ٱX<ل>سَّیِّءَاتِۚ ذَT< ٰ>لِكَ ذِكۡR<رَ>ىٰ لِX<ل>ذَّT< ٰ>كِرِJ<ی>نَ A<۝>D<١١٤>",
"وَX<ٱ>صۡبِرۡ فَإِN<نَّ> ٱللَّهَ لَا یُضِیعُ أَQ<جۡ>R<رَ> ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١١٥>",
"فَلَوۡلَا كَانَ مِنَ ٱلۡقُR<رُ>ونِ مِN<ن> قَQ<بۡ>لِكُمۡ أُX<و۟>لُوX<ا۟> بَقِیَّةX<ࣲ> N<یَ>نۡهَوۡنَ عَنِ ٱلۡفَسَادِ فِی ٱلۡأَR<رۡ>ضِ إِلَّا قَلِیلX<ࣰ>ا N<مِّ>N<مَّ>نۡ أَN<ن>جَیۡنَا مِنۡهُمۡۗ وَX<ٱ>تَّبَعَ ٱلَّذِینَ ظَلَمُوX<ا۟> مَW<اۤ> أُتۡرِفُوX<ا۟> فِیهِ وَكَانُوX<ا۟> مُQ<جۡ>رِمِJ<ی>نَ A<۝>D<١١٦>",
"وَمَا كَانَ R<رَ>بُّكَ لِیُهۡلِكَ ٱلۡقُR<رَ>ىٰ بِظُلۡمX<ࣲ> N<وَ>أَهۡلُهَا مُصۡلِحُJ<و>نَ A<۝>D<١١٧>",
"وَلَوۡ شَW<اۤ>ءَ R<رَ>بُّكَ لَجَعَلَ ٱX<ل>N<نَّ>اسَ أُN<مَّ>ةX<ࣰ> N<وَ>T< ٰ>حِدَةX<ࣰ>ۖ N<وَ>لَا یَزَالُونَ مُخۡتَلِفِJ<ی>نَ A<۝>D<١١٨>",
"إِلَّا مَX<ن> R<رَّ>حِمَ R<رَ>بُّكَۚ وَلِذَT< ٰ>لِكَ خَلَقَهُمۡۗ وَتَN<مَّ>تۡ كَلِمَةُ R<رَ>بِّكَ لَأَمۡلَأَN<نَّ> جَهَN<نَّ>مَ مِنَ ٱلۡجِN<نَّ>ةِ وَX<ٱ>X<ل>N<نَّ>اسِ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<١١٩>",
"وَكُلّX<ࣰ>ا N<نَّ>قُصُّ عَلَیۡكَ مِنۡ أَX<ن>N<ۢ>بَW<اۤ>ءِ ٱX<ل>R<رُّ>سُلِ مَا نُثَبِّتُ بِهِT<ۦ> فُؤَادَكَۚ وَجَW<اۤ>ءَكَ فِی هَT<ـٰ>ذِهِ ٱلۡحَقُّ وَمَوۡعِظَةX<ࣱ> N<وَ>ذِكۡR<رَ>ىٰ لِلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٢٠>",
"وَقُل لِّلَّذِینَ لَا یُؤۡمِنُونَ ٱعۡمَلُوX<ا۟> عَلَىٰ مَكَانَتِكُمۡ إِN<نَّ>ا عَT<ـٰ>مِلُJ<و>نَ A<۝>D<١٢١>",
"وَX<ٱ>N<ن>تَظِR<رُ>W<وۤ>X<ا۟> إِN<نَّ>ا مُN<ن>تَظِR<رُ>J<و>نَ A<۝>D<١٢٢>",
"وَلِلَّهِ غَیۡبُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَإِلَیۡهِ یُR<رۡ>جَعُ ٱلۡأَمۡR<رُ> كُلُّهُT<ۥ> فَX<ٱ>عۡبُQ<دۡ>هُ وَتَوَكَّلۡ عَلَیۡهِۚ وَمَا R<رَ>بُّكَ بِغَT<ـٰ>فِلٍ عَN<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<١٢٣>",
"#اL<لۤ>رۚ تِلۡكَ ءَایَT<ـٰ>تُ ٱلۡكِتَT<ـٰ>بِ ٱلۡمُبِJ<ی>نِ A<۝>D<١>",
"إِN<نَّ>W<اۤ> أَN<ن>زَلۡنَT<ـٰ>هُ قُR<رۡ>ءَT< ٰ>نًا عَR<رَ>بِیّX<ࣰ>ا لَّعَلَّكُمۡ تَعۡقِلُJ<و>نَ A<۝>D<٢>",
"نَحۡنُ نَقُصُّ عَلَیۡكَ أَحۡسَنَ ٱلۡقَصَصِ بِمَW<اۤ> أَوۡحَیۡنَW<اۤ> إِلَیۡكَ هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانَ وَإِN<ن> كُN<ن>تَ مِN<ن> قَQ<بۡ>لِهِT<ۦ> لَمِنَ ٱلۡغَT<ـٰ>فِلِJ<ی>نَ A<۝>D<٣>",
"إِذۡ قَالَ یُوسُفُ لِأَبِیهِ یَW<ـٰۤ>أَبَتِ إِN<نِّ>ی R<رَ>أَیۡتُ أَحَدَ عَشَR<رَ> كَوۡكَبX<ࣰ>ا N<وَ>X<ٱ>X<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ> R<رَ>أَیۡتُهُمۡ لِی سَT<ـٰ>جِدِJ<ی>نَ A<۝>D<٤>",
"قَالَ یَT<ـٰ>بُنَیَّ لَا تَQ<قۡ>صُصۡ R<رُ>ءۡیَاكَ عَلَىW<ٰۤ> إِخۡوَتِكَ فَیَكِیدُوX<ا۟> لَكَ كَیۡدًاۖ إِN<نَّ> ٱX<ل>شَّیۡطَT<ـٰ>نَ لِلۡإِN<ن>سَT<ـٰ>نِ عَدُوّX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٥>",
"وَكَذَT< ٰ>لِكَ یَQ<جۡ>تَبِیكَ R<رَ>بُّكَ وَیُعَلِّمُكَ مِN<ن> تَأۡوِیلِ ٱلۡأَحَادِیثِ وَیُتِN<مُّ> نِعۡمَتَهُT<ۥ> عَلَیۡكَ وَعَلَىW<ٰۤ> ءَالِ یَعۡقُوبَ كَمَW<اۤ> أَتَN<مَّ>هَا عَلَىW<ٰۤ> أَبَوَیۡكَ مِN<ن> قَQ<بۡ>لُ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَإِسۡحَT<ـٰ>قَۚ إِN<نَّ> R<رَ>بَّكَ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<٦>",
"۞ لَّقَQ<دۡ> كَانَ فِی یُوسُفَ وَإِخۡوَتِهِW<ۦۤ> ءَایَT<ـٰ>تX<ࣱ> لِّX<ل>سَّW<اۤ>ئِلِJ<ی>نَ A<۝>D<٧>",
"إِذۡ قَالُوX<ا۟> لَیُوسُفُ وَأَخُوهُ أَحَبُّ إِلَىW<ٰۤ> أَبِینَا مِN<نَّ>ا وَنَحۡنُ عُصۡبَةٌ إِN<نَّ> أَبَانَا لَفِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٨>",
"ٱQ<قۡ>تُلُوX<ا۟> یُوسُفَ أَوِ ٱQ<طۡ>R<رَ>حُوهُ أَR<رۡ>ضX<ࣰ>ا N<یَ>خۡلُ لَكُمۡ وَQ<جۡ>هُ أَبِیكُمۡ وَتَكُونُوX<ا۟> مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> قَوۡمN<ࣰ>ا صَT<ـٰ>لِحِJ<ی>نَ A<۝>D<٩>",
"قَالَ قَW<اۤ>ئِلX<ࣱ> N<مِّ>نۡهُمۡ لَا تَQ<قۡ>تُلُوX<ا۟> یُوسُفَ وَأَلۡقُوهُ فِی غَیَT<ـٰ>بَتِ ٱلۡجُبِّ یَلۡتَقِQ<طۡ>هُ بَعۡضُ ٱX<ل>سَّیَّاR<رَ>ةِ إِN<ن> كُN<ن>تُمۡ فَT<ـٰ>عِلِJ<ی>نَ A<۝>D<١٠>",
"قَالُوX<ا۟> یَW<ـٰۤ>أَبَانَا مَالَكَ لَا تَأۡمَـ۫N<نَّ>ا عَلَىٰ یُوسُفَ وَإِN<نَّ>ا لَهُT<ۥ> لَنَT<ـٰ>صِحُJ<و>نَ A<۝>D<١١>",
"أَR<رۡ>سِلۡهُ مَعَنَا غَدX<ࣰ>ا N<یَ>R<رۡ>تَعۡ وَیَلۡعَQ<بۡ> وَإِN<نَّ>ا لَهُT<ۥ> لَحَT<ـٰ>فِظُJ<و>نَ A<۝>D<١٢>",
"قَالَ إِN<نِّ>ی لَیَحۡزُنُنِW<یۤ> أَN<ن> تَذۡهَبُوX<ا۟> بِهِT<ۦ> وَأَخَافُ أَX<ن> N<یَ>أۡكُلَهُ ٱX<ل>ذِّئۡبُ وَأَN<ن>تُمۡ عَنۡهُ غَT<ـٰ>فِلُJ<و>نَ A<۝>D<١٣>",
"قَالُوX<ا۟> لَئِنۡ أَكَلَهُ ٱX<ل>ذِّئۡبُ وَنَحۡنُ عُصۡبَةٌ إِN<نَّ>W<اۤ> إِذX<ࣰ>ا لَّخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<١٤>",
"فَلَN<مَّ>ا ذَهَبُوX<ا۟> بِهِT<ۦ> وَأَQ<جۡ>مَعُW<وۤ>X<ا۟> أَX<ن> N<یَ>Q<جۡ>عَلُوهُ فِی غَیَT<ـٰ>بَتِ ٱلۡجُبِّۚ وَأَوۡحَیۡنَW<اۤ> إِلَیۡهِ لَتُنَبِّئَN<نَّ>هُN<م> بِأَمۡرِهِمۡ هَT<ـٰ>ذَا وَهُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<١٥>",
"وَجَW<اۤ>ءُW<وۤ> أَبَاهُمۡ عِشَW<اۤ>ءX<ࣰ> N<یَ>Q<بۡ>كُJ<و>نَ A<۝>D<١٦>",
"قَالُوX<ا۟> یَW<ـٰۤ>أَبَانَW<اۤ> إِN<نَّ>ا ذَهَQ<بۡ>نَا نَسۡتَبِقُ وَتَR<رَ>كۡنَا یُوسُفَ عِN<ن>دَ مَتَT<ـٰ>عِنَا فَأَكَلَهُ ٱX<ل>ذِّئۡبُۖ وَمَW<اۤ> أَN<ن>تَ بِمُؤۡمِنX<ࣲ> لَّنَا وَلَوۡ كُN<نَّ>ا صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٧>",
"وَجَW<اۤ>ءُو عَلَىٰ قَمِیصِهِT<ۦ> بِدَمN<ࣲ> كَذِبN<ࣲ>ۚ قَالَ بَلۡ سَوَّلَتۡ لَكُمۡ أَN<ن>فُسُكُمۡ أَمۡR<ر>N<ࣰ>اۖ فَصَQ<بۡ>R<ر>N<ࣱ> جَمِیلX<ࣱ>ۖ N<وَ>ٱللَّهُ ٱلۡمُسۡتَعَانُ عَلَىٰ مَا تَصِفُJ<و>نَ A<۝>D<١٨>",
"وَجَW<اۤ>ءَتۡ سَیَّاR<رَ>ةN<ࣱ> فَأَR<رۡ>سَلُوX<ا۟> وَارِدَهُمۡ فَأَQ<دۡ>لَىٰ دَلۡوَهُT<ۥ>ۖ قَالَ یَT<ـٰ>بُشۡR<رَ>ىٰ هَT<ـٰ>ذَا غُلَT<ـٰ>مX<ࣱ>ۚ N<وَ>أَسَR<رُّ>وهُ بِضَT<ـٰ>عَةX<ࣰ>ۚ N<وَ>ٱللَّهُ عَلِیمُN<ۢ> بِمَا یَعۡمَلُJ<و>نَ A<۝>D<١٩>",
"وَشَR<رَ>وۡهُ بِثَمَنِN<ۭ> بَخۡسN<ࣲ> دَR<رَ>T< ٰ>هِمَ مَعۡدُودَةX<ࣲ> N<وَ>كَانُوX<ا۟> فِیهِ مِنَ ٱX<ل>زَّT< ٰ>هِدِJ<ی>نَ A<۝>D<٢٠>",
"وَقَالَ ٱلَّذِی ٱشۡتَR<رَ>X<ى>T<ٰ>هُ مِX<ن> N<مِّ>صۡR<رَ> لِX<ٱ>مۡR<رَ>أَتِهِW<ۦۤ> أَكۡرِمِی مَثۡوَX<ى>T<ٰ>هُ عَسَىW<ٰۤ> أَX<ن> N<یَ>N<ن>فَعَنَW<اۤ> أَوۡ نَتَّخِذَهُT<ۥ> وَلَدX<ࣰ>اۚ N<وَ>كَذَT< ٰ>لِكَ مَكَّN<نَّ>ا لِیُوسُفَ فِی ٱلۡأَR<رۡ>ضِ وَلِنُعَلِّمَهُT<ۥ> مِN<ن> تَأۡوِیلِ ٱلۡأَحَادِیثِۚ وَٱللَّهُ غَالِبٌ عَلَىW<ٰۤ> أَمۡرِهِT<ۦ> وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٢١>",
"وَلَN<مَّ>ا بَلَغَ أَشُدَّهُW<ۥۤ> ءَاتَیۡنَT<ـٰ>هُ حُكۡمX<ࣰ>ا N<وَ>عِلۡمX<ࣰ>اۚ N<وَ>كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٢٢>",
"وَR<رَ>T< ٰ>وَدَتۡهُ ٱلَّتِی هُوَ فِی بَیۡتِهَا عَن N<نَّ>فۡسِهِT<ۦ> وَغَلَّقَتِ ٱلۡأَQ<بۡ>وَT< ٰ>بَ وَقَالَتۡ هَیۡتَ لَكَۚ قَالَ مَعَاذَ ٱللَّهِۖ إِN<نَّ>هُT<ۥ> R<رَ>بِّW<یۤ> أَحۡسَنَ مَثۡوَایَۖ إِN<نَّ>هُT<ۥ> لَا یُفۡلِحُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٢٣>",
"وَلَقَQ<دۡ> هَN<مَّ>تۡ بِهِT<ۦ>ۖ وَهَN<مَّ> بِهَا لَوۡلَW<اۤ> أَX<ن> R<رَّ>ءَا بُR<رۡ>هَT<ـٰ>نَ R<رَ>بِّهِT<ۦ>ۚ كَذَT< ٰ>لِكَ لِنَصۡرِفَ عَنۡهُ ٱX<ل>سُّW<وۤ>ءَ وَX<ٱ>لۡفَحۡشَW<اۤ>ءَۚ إِN<نَّ>هُT<ۥ> مِنۡ عِبَادِنَا ٱلۡمُخۡلَصِJ<ی>نَ A<۝>D<٢٤>",
"وَX<ٱ>سۡتَبَقَا ٱلۡبَابَ وَقَدَّتۡ قَمِیصَهُT<ۥ> مِN<ن> دُبُرX<ࣲ> N<وَ>أَلۡفَیَا سَیِّدَهَا لَدَا ٱلۡبَابِۚ قَالَتۡ مَا جَزَW<اۤ>ءُ مَنۡ أَR<رَ>ادَ بِأَهۡلِكَ سُW<وۤ>ءًا إِلَّW<اۤ> أَX<ن> N<یُ>سۡجَنَ أَوۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٢٥>",
"قَالَ هِیَ R<رَ>T< ٰ>وَدَتۡنِی عَن N<نَّ>فۡسِیۚ وَشَهِدَ شَاهِدX<ࣱ> N<مِّ>نۡ أَهۡلِهَW<اۤ> إِN<ن> كَانَ قَمِیصُهُT<ۥ> قُدَّ مِN<ن> قُبُلN<ࣲ> فَصَدَقَتۡ وَهُوَ مِنَ ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٢٦>",
"وَإِN<ن> كَانَ قَمِیصُهُT<ۥ> قُدَّ مِN<ن> دُبُرN<ࣲ> فَكَذَبَتۡ وَهُوَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٢٧>",
"فَلَN<مَّ>ا R<رَ>ءَا قَمِیصَهُT<ۥ> قُدَّ مِN<ن> دُبُرN<ࣲ> قَالَ إِN<نَّ>هُT<ۥ> مِN<ن> كَیۡدِكُN<نَّ>ۖ إِN<نَّ> كَیۡدَكُN<نَّ> عَظِJ<ی>مࣱ A<۝>D<٢٨>",
"یُوسُفُ أَعۡرِضۡ عَنۡ هَT<ـٰ>ذَاۚ وَX<ٱ>سۡتَغۡفِرِی لِذَX<ن>N<ۢ>بِكِۖ إِN<نَّ>كِ كُN<ن>تِ مِنَ ٱلۡخَاطِءِJ<ی>نَ A<۝>D<٢٩>",
"۞ وَقَالَ نِسۡوَةN<ࣱ> فِی ٱلۡمَدِینَةِ ٱمۡR<رَ>أَتُ ٱلۡعَزِیزِ تُR<رَ>T< ٰ>وِدُ فَتَX<ى>T<ٰ>هَا عَن N<نَّ>فۡسِهِT<ۦ>ۖ قَQ<دۡ> شَغَفَهَا حُبًّاۖ إِN<نَّ>ا لَنَR<رَ>X<ى>T<ٰ>هَا فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٣٠>",
"فَلَN<مَّ>ا سَمِعَتۡ بِمَكۡرِهِN<نَّ> أَR<رۡ>سَلَتۡ إِلَیۡهِN<نَّ> وَأَعۡتَدَتۡ لَهُN<نَّ> مُتَّكَءX<ࣰ>ا N<وَ>ءَاتَتۡ كُلَّ وَT< ٰ>حِدَةX<ࣲ> N<مِّ>نۡهُN<نَّ> سِكِّینX<ࣰ>ا N<وَ>قَالَتِ ٱخۡR<رُ>Q<جۡ> عَلَیۡهِN<نَّ>ۖ فَلَN<مَّ>ا R<رَ>أَیۡنَهُW<ۥۤ> أَكۡبَR<رۡ>نَهُT<ۥ> وَقَطَّعۡنَ أَیۡدِیَهُN<نَّ> وَقُلۡنَ حَT<ـٰ>شَ لِلَّهِ مَا هَT<ـٰ>ذَا بَشَR<ر>ًا إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا مَلَكN<ࣱ> كَرِJ<ی>مࣱ A<۝>D<٣١>",
"قَالَتۡ فَذَT< ٰ>لِكُN<نَّ> ٱلَّذِی لُمۡتُN<نَّ>نِی فِیهِۖ وَلَقَQ<دۡ> R<رَ>T< ٰ>وَX<د>تُّهُT<ۥ> عَن N<نَّ>فۡسِهِT<ۦ> فَX<ٱ>سۡتَعۡصَمَۖ وَلَئِX<ن> لَّمۡ یَفۡعَلۡ مَW<اۤ> ءَامُR<رُ>هُT<ۥ> لَیُسۡجَنَN<نَّ> وَلَیَكُونX<ࣰ>ا N<مِّ>نَ ٱX<ل>صَّT<ـٰ>غِرِJ<ی>نَ A<۝>D<٣٢>",
"قَالَ R<رَ>بِّ ٱX<ل>سِّQ<جۡ>نُ أَحَبُّ إِلَیَّ مِN<مَّ>ا یَQ<دۡ>عُونَنِW<یۤ> إِلَیۡهِۖ وَإِلَّا تَصۡرِفۡ عَN<نِّ>ی كَیۡدَهُN<نَّ> أَصۡبُ إِلَیۡهِN<نَّ> وَأَكُX<ن> N<مِّ>نَ ٱلۡجَT<ـٰ>هِلِJ<ی>نَ A<۝>D<٣٣>",
"فَX<ٱ>سۡتَجَابَ لَهُT<ۥ> R<رَ>بُّهُT<ۥ> فَصَR<رَ>فَ عَنۡهُ كَیۡدَهُN<نَّ>ۚ إِN<نَّ>هُT<ۥ> هُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٣٤>",
"ثُN<مَّ> بَدَا لَهُم N<مِّ>X<ن>N<ۢ> بَعۡدِ مَا R<رَ>أَوُX<ا۟> ٱلۡءَایَT<ـٰ>تِ لَیَسۡجُنُN<نَّ>هُT<ۥ> حَتَّىٰ حِJ<ی>نࣲ A<۝>D<٣٥>",
"وَدَخَلَ مَعَهُ ٱX<ل>سِّQ<جۡ>نَ فَتَیَانِۖ قَالَ أَحَدُهُمَW<اۤ> إِN<نِّ>W<یۤ> أَR<رَ>X<ى>T<ٰ>نِW<یۤ> أَعۡصِR<رُ> خَمۡR<ر>X<ࣰ>اۖ N<وَ>قَالَ ٱلۡءَاخَR<رُ> إِN<نِّ>W<یۤ> أَR<رَ>X<ى>T<ٰ>نِW<یۤ> أَحۡمِلُ فَوۡقَ R<رَ>أۡسِی خُQ<بۡ>زN<ࣰ>ا تَأۡكُلُ ٱX<ل>طَّیۡR<رُ> مِنۡهُۖ نَبِّئۡنَا بِتَأۡوِیلِهِW<ۦۤ>ۖ إِN<نَّ>ا نَR<رَ>X<ى>T<ٰ>كَ مِنَ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٣٦>",
"قَالَ لَا یَأۡتِیكُمَا طَعَامN<ࣱ> تُR<رۡ>زَقَانِهِW<ۦۤ> إِلَّا نَبَّأۡتُكُمَا بِتَأۡوِیلِهِT<ۦ> قَQ<بۡ>لَ أَX<ن> N<یَ>أۡتِیَكُمَاۚ ذَT< ٰ>لِكُمَا مِN<مَّ>ا عَلَّمَنِی R<رَ>بِّW<یۤ>ۚ إِN<نِّ>ی تَR<رَ>كۡتُ مِلَّةَ قَوۡمX<ࣲ> لَّا یُؤۡمِنُونَ بِٱللَّهِ وَهُN<م> بِX<ٱ>لۡءَاخِR<رَ>ةِ هُمۡ كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٣٧>",
"وَX<ٱ>تَّبَعۡتُ مِلَّةَ ءَابَW<اۤ>ءِW<یۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَإِسۡحَT<ـٰ>قَ وَیَعۡقُوبَۚ مَا كَانَ لَنَW<اۤ> أَن N<نُّ>شۡرِكَ بِٱللَّهِ مِN<ن> شَیۡءN<ࣲ>ۚ ذَT< ٰ>لِكَ مِN<ن> فَضۡلِ ٱللَّهِ عَلَیۡنَا وَعَلَى ٱX<ل>N<نَّ>اسِ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَشۡكُR<رُ>J<و>نَ A<۝>D<٣٨>",
"یَT<ـٰ>صَT<ـٰ>حِبَیِ ٱX<ل>سِّQ<جۡ>نِ ءَأَR<رۡ>بَابX<ࣱ> N<مُّ>تَفَرِّقُونَ خَیۡR<ر>ٌ أَمِ ٱللَّهُ ٱلۡوَT< ٰ>حِدُ ٱلۡقَهَّJ<ا>R<ر>ُ A<۝>D<٣٩>",
"مَا تَعۡبُدُونَ مِN<ن> دُونِهِW<ۦۤ> إِلَّW<اۤ> أَسۡمَW<اۤ>ءN<ࣰ> سَN<مَّ>یۡتُمُوهَW<اۤ> أَN<ن>تُمۡ وَءَابَW<اۤ>ؤُكُم N<مَّ>W<اۤ> أَN<ن>زَلَ ٱللَّهُ بِهَا مِN<ن> سُلۡطَT<ـٰ>نٍۚ إِنِ ٱلۡحُكۡمُ إِلَّا لِلَّهِ أَمَR<رَ> أَلَّا تَعۡبُدُW<وۤ>X<ا۟> إِلَّW<اۤ> إِیَّاهُۚ ذَT< ٰ>لِكَ ٱX<ل>دِّینُ ٱلۡقَیِّمُ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٤٠>",
"یَT<ـٰ>صَT<ـٰ>حِبَیِ ٱX<ل>سِّQ<جۡ>نِ أَN<مَّ>W<اۤ> أَحَدُكُمَا فَیَسۡقِی R<رَ>بَّهُT<ۥ> خَمۡR<ر>X<ࣰ>اۖ N<وَ>أَN<مَّ>ا ٱلۡءَاخَR<رُ> فَیُصۡلَبُ فَتَأۡكُلُ ٱX<ل>طَّیۡR<رُ> مِX<ن> R<رَّ>أۡسِهِT<ۦ>ۚ قُضِیَ ٱلۡأَمۡR<رُ> ٱلَّذِی فِیهِ تَسۡتَفۡتِیَJ<ا>نِ A<۝>D<٤١>",
"وَقَالَ لِلَّذِی ظَN<نَّ> أَN<نَّ>هُT<ۥ> نَاجX<ࣲ> N<مِّ>نۡهُمَا ٱذۡكُR<رۡ>نِی عِN<ن>دَ R<رَ>بِّكَ فَأَN<ن>سَX<ى>T<ٰ>هُ ٱX<ل>شَّیۡطَT<ـٰ>نُ ذِكۡR<رَ> R<رَ>بِّهِT<ۦ> فَلَبِثَ فِی ٱX<ل>سِّQ<جۡ>نِ بِضۡعَ سِنِJ<ی>نَ A<۝>D<٤٢>",
"وَقَالَ ٱلۡمَلِكُ إِN<نِّ>W<یۤ> أَR<رَ>ىٰ سَQ<بۡ>عَ بَقَR<رَ>T< ٰ>تN<ࣲ> سِمَانX<ࣲ> N<یَ>أۡكُلُهُN<نَّ> سَQ<بۡ>عٌ عِجَافX<ࣱ> N<وَ>سَQ<بۡ>عَ سُX<ن>N<ۢ>بُلَT<ـٰ>تٍ خُضۡرX<ࣲ> N<وَ>أُخَR<رَ> یَابِسَT<ـٰ>تX<ࣲ>ۖ N<یَ>W<ـٰۤ>أَیُّهَا ٱلۡمَلَأُ أَفۡتُونِی فِی R<رُ>ءۡیَT<ـٰ>یَ إِN<ن> كُN<ن>تُمۡ لِX<ل>R<رُّ>ءۡیَا تَعۡبُR<رُ>J<و>نَ A<۝>D<٤٣>",
"قَالُW<وۤ>X<ا۟> أَضۡغَT<ـٰ>ثُ أَحۡلَT<ـٰ>مX<ࣲ>ۖ N<وَ>مَا نَحۡنُ بِتَأۡوِیلِ ٱلۡأَحۡلَT<ـٰ>مِ بِعَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤٤>",
"وَقَالَ ٱلَّذِی نَجَا مِنۡهُمَا وَX<ٱ>دَّكَR<رَ> بَعۡدَ أُN<مَّ>ةٍ أَنَا۠ أُنَبِّئُكُN<م> بِتَأۡوِیلِهِT<ۦ> فَأَR<رۡ>سِلُJ<و>نِ A<۝>D<٤٥>",
"یُوسُفُ أَیُّهَا ٱX<ل>صِّدِّیقُ أَفۡتِنَا فِی سَQ<بۡ>عِ بَقَR<رَ>T< ٰ>تN<ࣲ> سِمَانX<ࣲ> N<یَ>أۡكُلُهُN<نَّ> سَQ<بۡ>عٌ عِجَافX<ࣱ> N<وَ>سَQ<بۡ>عِ سُX<ن>N<ۢ>بُلَT<ـٰ>تٍ خُضۡرX<ࣲ> N<وَ>أُخَR<رَ> یَابِسَT<ـٰ>تX<ࣲ> لَّعَلِّW<یۤ> أَR<رۡ>جِعُ إِلَى ٱX<ل>N<نَّ>اسِ لَعَلَّهُمۡ یَعۡلَمُJ<و>نَ A<۝>D<٤٦>",
"قَالَ تَزۡR<رَ>عُونَ سَQ<بۡ>عَ سِنِینَ دَأَبN<ࣰ>ا فَمَا حَصَX<د>تُّمۡ فَذَR<رُ>وهُ فِی سُX<ن>N<ۢ>بُلِهِW<ۦۤ> إِلَّا قَلِیلX<ࣰ>ا N<مِّ>N<مَّ>ا تَأۡكُلُJ<و>نَ A<۝>D<٤٧>",
"ثُN<مَّ> یَأۡتِی مِX<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَ سَQ<بۡ>عN<ࣱ> شِدَادX<ࣱ> N<یَ>أۡكُلۡنَ مَا قَدَّمۡتُمۡ لَهُN<نَّ> إِلَّا قَلِیلX<ࣰ>ا N<مِّ>N<مَّ>ا تُحۡصِنُJ<و>نَ A<۝>D<٤٨>",
"ثُN<مَّ> یَأۡتِی مِX<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَ عَامN<ࣱ> فِیهِ یُغَاثُ ٱX<ل>N<نَّ>اسُ وَفِیهِ یَعۡصِR<رُ>J<و>نَ A<۝>D<٤٩>",
"وَقَالَ ٱلۡمَلِكُ ٱئۡتُونِی بِهِT<ۦ>ۖ فَلَN<مَّ>ا جَW<اۤ>ءَهُ ٱX<ل>R<رَّ>سُولُ قَالَ ٱR<رۡ>جِعۡ إِلَىٰ R<رَ>بِّكَ فَسۡءَلۡهُ مَا بَالُ ٱX<ل>N<نِّ>سۡوَةِ ٱلَّT<ـٰ>تِی قَطَّعۡنَ أَیۡدِیَهُN<نَّ>ۚ إِN<نَّ> R<رَ>بِّی بِكَیۡدِهِN<نَّ> عَلِJ<ی>مࣱ A<۝>D<٥٠>",
"قَالَ مَا خَQ<طۡ>بُكُN<نَّ> إِذۡ R<رَ>T< ٰ>وَX<د>تُّN<نَّ> یُوسُفَ عَن N<نَّ>فۡسِهِT<ۦ>ۚ قُلۡنَ حَT<ـٰ>شَ لِلَّهِ مَا عَلِمۡنَا عَلَیۡهِ مِN<ن> سُW<وۤ>ءN<ࣲ>ۚ قَالَتِ ٱمۡR<رَ>أَتُ ٱلۡعَزِیزِ ٱلۡءَT<ـٰ>نَ حَصۡحَصَ ٱلۡحَقُّ أَنَا۠ R<رَ>T< ٰ>وَX<د>تُّهُT<ۥ> عَن N<نَّ>فۡسِهِT<ۦ> وَإِN<نَّ>هُT<ۥ> لَمِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٥١>",
"ذَT< ٰ>لِكَ لِیَعۡلَمَ أَN<نِّ>ی لَمۡ أَخُنۡهُ بِX<ٱ>لۡغَیۡبِ وَأَN<نَّ> ٱللَّهَ لَا یَهۡدِی كَیۡدَ ٱلۡخَW<اۤ>ئِنِJ<ی>نَ A<۝>D<٥٢>",
"۞ وَمَW<اۤ> أُبَرِّئُ نَفۡسِW<یۤ>ۚ إِN<نَّ> ٱX<ل>N<نَّ>فۡسَ لَأَN<مَّ>اR<رَ>ةُN<ۢ> بِX<ٱ>X<ل>سُّW<وۤ>ءِ إِلَّا مَا R<رَ>حِمَ R<رَ>بِّW<یۤ>ۚ إِN<نَّ> R<رَ>بِّی غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٥٣>",
"وَقَالَ ٱلۡمَلِكُ ٱئۡتُونِی بِهِW<ۦۤ> أَسۡتَخۡلِصۡهُ لِنَفۡسِیۖ فَلَN<مَّ>ا كَلَّمَهُT<ۥ> قَالَ إِN<نَّ>كَ ٱلۡیَوۡمَ لَدَیۡنَا مَكِینٌ أَمِJ<ی>نࣱ A<۝>D<٥٤>",
"قَالَ ٱQ<جۡ>عَلۡنِی عَلَىٰ خَزَW<اۤ>ئِنِ ٱلۡأَR<رۡ>ضِۖ إِN<نِّ>ی حَفِیظٌ عَلِJ<ی>مࣱ A<۝>D<٥٥>",
"وَكَذَT< ٰ>لِكَ مَكَّN<نَّ>ا لِیُوسُفَ فِی ٱلۡأَR<رۡ>ضِ یَتَبَوَّأُ مِنۡهَا حَیۡثُ یَشَW<اۤ>ءُۚ نُصِیبُ بِR<رَ>حۡمَتِنَا مَن N<نَّ>شَW<اۤ>ءُۖ وَلَا نُضِیعُ أَQ<جۡ>R<رَ> ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٥٦>",
"وَلَأَQ<جۡ>R<رُ> ٱلۡءَاخِR<رَ>ةِ خَیۡR<ر>X<ࣱ> لِّلَّذِینَ ءَامَنُوX<ا۟> وَكَانُوX<ا۟> یَتَّقُJ<و>نَ A<۝>D<٥٧>",
"وَجَW<اۤ>ءَ إِخۡوَةُ یُوسُفَ فَدَخَلُوX<ا۟> عَلَیۡهِ فَعَR<رَ>فَهُمۡ وَهُمۡ لَهُT<ۥ> مُN<ن>كِR<رُ>J<و>نَ A<۝>D<٥٨>",
"وَلَN<مَّ>ا جَهَّزَهُN<م> بِجَهَازِهِمۡ قَالَ ٱئۡتُونِی بِأَخX<ࣲ> لَّكُم N<مِّ>نۡ أَبِیكُمۡۚ أَلَا تَR<رَ>وۡنَ أَN<نِّ>W<یۤ> أُوفِی ٱلۡكَیۡلَ وَأَنَا۠ خَیۡR<رُ> ٱلۡمُN<ن>زِلِJ<ی>نَ A<۝>D<٥٩>",
"فَإِX<ن> لَّمۡ تَأۡتُونِی بِهِT<ۦ> فَلَا كَیۡلَ لَكُمۡ عِN<ن>دِی وَلَا تَQ<قۡ>R<رَ>بُJ<و>نِ A<۝>D<٦٠>",
"قَالُوX<ا۟> سَنُR<رَ>T< ٰ>وِدُ عَنۡهُ أَبَاهُ وَإِN<نَّ>ا لَفَT<ـٰ>عِلُJ<و>نَ A<۝>D<٦١>",
"وَقَالَ لِفِتۡیَT<ـٰ>نِهِ ٱQ<جۡ>عَلُوX<ا۟> بِضَT<ـٰ>عَتَهُمۡ فِی رِحَالِهِمۡ لَعَلَّهُمۡ یَعۡرِفُونَهَW<اۤ> إِذَا ٱN<ن>قَلَبُW<وۤ>X<ا۟> إِلَىW<ٰۤ> أَهۡلِهِمۡ لَعَلَّهُمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<٦٢>",
"فَلَN<مَّ>ا R<رَ>جَعُW<وۤ>X<ا۟> إِلَىW<ٰۤ> أَبِیهِمۡ قَالُوX<ا۟> یَW<ـٰۤ>أَبَانَا مُنِعَ مِN<نَّ>ا ٱلۡكَیۡلُ فَأَR<رۡ>سِلۡ مَعَنَW<اۤ> أَخَانَا نَكۡتَلۡ وَإِN<نَّ>ا لَهُT<ۥ> لَحَT<ـٰ>فِظُJ<و>نَ A<۝>D<٦٣>",
"قَالَ هَلۡ ءَامَنُكُمۡ عَلَیۡهِ إِلَّا كَمَW<اۤ> أَمِN<ن>تُكُمۡ عَلَىW<ٰۤ> أَخِیهِ مِN<ن> قَQ<بۡ>لُ فَٱللَّهُ خَیۡR<ر>ٌ حَT<ـٰ>فِظX<ࣰ>اۖ N<وَ>هُوَ أَR<رۡ>حَمُ ٱX<ل>R<رَّ>T< ٰ>حِمِJ<ی>نَ A<۝>D<٦٤>",
"وَلَN<مَّ>ا فَتَحُوX<ا۟> مَتَT<ـٰ>عَهُمۡ وَجَدُوX<ا۟> بِضَT<ـٰ>عَتَهُمۡ R<رُ>دَّتۡ إِلَیۡهِمۡۖ قَالُوX<ا۟> یَW<ـٰۤ>أَبَانَا مَا نَQ<بۡ>غِیۖ هَT<ـٰ>ذِهِT<ۦ> بِضَT<ـٰ>عَتُنَا R<رُ>دَّتۡ إِلَیۡنَاۖ وَنَمِیR<رُ> أَهۡلَنَا وَنَحۡفَظُ أَخَانَا وَنَزۡدَادُ كَیۡلَ بَعِیرN<ࣲ>ۖ ذَT< ٰ>لِكَ كَیۡلX<ࣱ> N<یَ>سِJ<ی>رࣱ A<۝>D<٦٥>",
"قَالَ لَنۡ أُR<رۡ>سِلَهُT<ۥ> مَعَكُمۡ حَتَّىٰ تُؤۡتُونِ مَوۡثِقX<ࣰ>ا N<مِّ>نَ ٱللَّهِ لَتَأۡتُN<نَّ>نِی بِهِW<ۦۤ> إِلَّW<اۤ> أَX<ن> N<یُ>حَاطَ بِكُمۡۖ فَلَN<مَّ>W<اۤ> ءَاتَوۡهُ مَوۡثِقَهُمۡ قَالَ ٱللَّهُ عَلَىٰ مَا نَقُولُ وَكِJ<ی>لࣱ A<۝>D<٦٦>",
"وَقَالَ یَT<ـٰ>بَنِیَّ لَا تَQ<دۡ>خُلُوX<ا۟> مِX<ن>N<ۢ> بَابX<ࣲ> N<وَ>T< ٰ>حِدX<ࣲ> N<وَ>X<ٱ>Q<دۡ>خُلُوX<ا۟> مِنۡ أَQ<بۡ>وَT< ٰ>بX<ࣲ> N<مُّ>تَفَرِّقَةX<ࣲ>ۖ N<وَ>مَW<اۤ> أُغۡنِی عَN<ن>كُم N<مِّ>نَ ٱللَّهِ مِN<ن> شَیۡءٍۖ إِنِ ٱلۡحُكۡمُ إِلَّا لِلَّهِۖ عَلَیۡهِ تَوَكَّلۡتُۖ وَعَلَیۡهِ فَلۡیَتَوَكَّلِ ٱلۡمُتَوَكِّلُJ<و>نَ A<۝>D<٦٧>",
"وَلَN<مَّ>ا دَخَلُوX<ا۟> مِنۡ حَیۡثُ أَمَR<رَ>هُمۡ أَبُوهُم N<مَّ>ا كَانَ یُغۡنِی عَنۡهُم N<مِّ>نَ ٱللَّهِ مِN<ن> شَیۡءٍ إِلَّا حَاجَةN<ࣰ> فِی نَفۡسِ یَعۡقُوبَ قَضَX<ى>T<ٰ>هَاۚ وَإِN<نَّ>هُT<ۥ> لَذُو عِلۡمX<ࣲ> لِّمَا عَلَّمۡنَT<ـٰ>هُ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٦٨>",
"وَلَN<مَّ>ا دَخَلُوX<ا۟> عَلَىٰ یُوسُفَ ءَاوَىW<ٰۤ> إِلَیۡهِ أَخَاهُۖ قَالَ إِN<نِّ>W<یۤ> أَنَا۠ أَخُوكَ فَلَا تَQ<بۡ>تَئِسۡ بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٦٩>",
"فَلَN<مَّ>ا جَهَّزَهُN<م> بِجَهَازِهِمۡ جَعَلَ ٱX<ل>سِّقَایَةَ فِی R<رَ>حۡلِ أَخِیهِ ثُN<مَّ> أَذَّنَ مُؤَذِّنٌ أَیَّتُهَا ٱلۡعِیR<رُ> إِN<نَّ>كُمۡ لَسَT<ـٰ>رِقُJ<و>نَ A<۝>D<٧٠>",
"قَالُوX<ا۟> وَأَQ<قۡ>بَلُوX<ا۟> عَلَیۡهِم N<مَّ>اذَا تَفۡقِدُJ<و>نَ A<۝>D<٧١>",
"قَالُوX<ا۟> نَفۡقِدُ صُوَاعَ ٱلۡمَلِكِ وَلِمَN<ن> جَW<اۤ>ءَ بِهِT<ۦ> حِمۡلُ بَعِیرX<ࣲ> N<وَ>أَنَا۠ بِهِT<ۦ> زَعِJ<ی>مࣱ A<۝>D<٧٢>",
"قَالُوX<ا۟> تَٱللَّهِ لَقَQ<دۡ> عَلِمۡتُم N<مَّ>ا جِئۡنَا لِنُفۡسِدَ فِی ٱلۡأَR<رۡ>ضِ وَمَا كُN<نَّ>ا سَT<ـٰ>رِقِJ<ی>نَ A<۝>D<٧٣>",
"قَالُوX<ا۟> فَمَا جَزَW< ٰۤ>ؤُهُW<ۥۤ> إِN<ن> كُN<ن>تُمۡ كَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٧٤>",
"قَالُوX<ا۟> جَزَW< ٰۤ>ؤُهُT<ۥ> مَX<ن> N<وُ>جِدَ فِی R<رَ>حۡلِهِT<ۦ> فَهُوَ جَزَW< ٰۤ>ؤُهُT<ۥ>ۚ كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٧٥>",
"فَبَدَأَ بِأَوۡعِیَتِهِمۡ قَQ<بۡ>لَ وِعَW<اۤ>ءِ أَخِیهِ ثُN<مَّ> ٱسۡتَخۡR<رَ>جَهَا مِX<ن> N<وِ>عَW<اۤ>ءِ أَخِیهِۚ كَذَT< ٰ>لِكَ كِQ<دۡ>نَا لِیُوسُفَۖ مَا كَانَ لِیَأۡخُذَ أَخَاهُ فِی دِینِ ٱلۡمَلِكِ إِلَّW<اۤ> أَX<ن> N<یَ>شَW<اۤ>ءَ ٱللَّهُۚ نَR<رۡ>فَعُ دَR<رَ>جَT<ـٰ>تX<ࣲ> N<مَّ>ن N<نَّ>شَW<اۤ>ءُۗ وَفَوۡقَ كُلِّ ذِی عِلۡمٍ عَلِJ<ی>مࣱ A<۝>D<٧٦>",
"۞ قَالُW<وۤ>X<ا۟> إِX<ن> N<یَ>سۡرِQ<قۡ> فَقَQ<دۡ> سَR<رَ>قَ أَخX<ࣱ> لَّهُT<ۥ> مِN<ن> قَQ<بۡ>لُۚ فَأَسَR<رَّ>هَا یُوسُفُ فِی نَفۡسِهِT<ۦ> وَلَمۡ یُQ<بۡ>دِهَا لَهُمۡۚ قَالَ أَN<ن>تُمۡ شَR<رّ>X<ࣱ> N<مَّ>كَانX<ࣰ>اۖ N<وَ>ٱللَّهُ أَعۡلَمُ بِمَا تَصِفُJ<و>نَ A<۝>D<٧٧>",
"قَالُوX<ا۟> یَW<ـٰۤ>أَیُّهَا ٱلۡعَزِیزُ إِN<نَّ> لَهُW<ۥۤ> أَبN<ࣰ>ا شَیۡخN<ࣰ>ا كَبِیR<ر>N<ࣰ>ا فَخُذۡ أَحَدَنَا مَكَانَهُW<ۥۤ>ۖ إِN<نَّ>ا نَR<رَ>X<ى>T<ٰ>كَ مِنَ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٧٨>",
"قَالَ مَعَاذَ ٱللَّهِ أَن N<نَّ>أۡخُذَ إِلَّا مَX<ن> N<وَ>جَQ<دۡ>نَا مَتَT<ـٰ>عَنَا عِN<ن>دَهُW<ۥۤ> إِN<نَّ>W<اۤ> إِذX<ࣰ>ا لَّظَT<ـٰ>لِمُJ<و>نَ A<۝>D<٧٩>",
"فَلَN<مَّ>ا ٱسۡتَیۡءَسُوX<ا۟> مِنۡهُ خَلَصُوX<ا۟> نَجِیّN<ࣰ>اۖ قَالَ كَبِیR<رُ>هُمۡ أَلَمۡ تَعۡلَمُW<وۤ>X<ا۟> أَN<نَّ> أَبَاكُمۡ قَQ<دۡ> أَخَذَ عَلَیۡكُم N<مَّ>وۡثِقX<ࣰ>ا N<مِّ>نَ ٱللَّهِ وَمِN<ن> قَQ<بۡ>لُ مَا فَR<رَّ>طتُمۡ فِی یُوسُفَۖ فَلَنۡ أَQ<بۡ>R<رَ>حَ ٱلۡأَR<رۡ>ضَ حَتَّىٰ یَأۡذَنَ لِW<یۤ> أَبِW<یۤ> أَوۡ یَحۡكُمَ ٱللَّهُ لِیۖ وَهُوَ خَیۡR<رُ> ٱلۡحَT<ـٰ>كِمِJ<ی>نَ A<۝>D<٨٠>",
"ٱR<رۡ>جِعُW<وۤ>X<ا۟> إِلَىW<ٰۤ> أَبِیكُمۡ فَقُولُوX<ا۟> یَW<ـٰۤ>أَبَانَW<اۤ> إِN<نَّ> ٱQ<بۡ>نَكَ سَR<رَ>قَ وَمَا شَهِQ<دۡ>نَW<اۤ> إِلَّا بِمَا عَلِمۡنَا وَمَا كُN<نَّ>ا لِلۡغَیۡبِ حَT<ـٰ>فِظِJ<ی>نَ A<۝>D<٨١>",
"وَسۡءَلِ ٱلۡقَR<رۡ>یَةَ ٱلَّتِی كُN<نَّ>ا فِیهَا وَX<ٱ>لۡعِیR<رَ> ٱلَّتِW<یۤ> أَQ<قۡ>بَلۡنَا فِیهَاۖ وَإِN<نَّ>ا لَصَT<ـٰ>دِقُJ<و>نَ A<۝>D<٨٢>",
"قَالَ بَلۡ سَوَّلَتۡ لَكُمۡ أَN<ن>فُسُكُمۡ أَمۡR<ر>N<ࣰ>اۖ فَصَQ<بۡ>R<ر>N<ࣱ> جَمِیلٌۖ عَسَى ٱللَّهُ أَX<ن> N<یَ>أۡتِیَنِی بِهِمۡ جَمِیعًاۚ إِN<نَّ>هُT<ۥ> هُوَ ٱلۡعَلِیمُ ٱلۡحَكِJ<ی>مُ A<۝>D<٨٣>",
"وَتَوَلَّىٰ عَنۡهُمۡ وَقَالَ یَW<ـٰۤ>أَسَفَىٰ عَلَىٰ یُوسُفَ وَX<ٱ>Q<بۡ>یَضَّتۡ عَیۡنَاهُ مِنَ ٱلۡحُزۡنِ فَهُوَ كَظِJ<ی>مࣱ A<۝>D<٨٤>",
"قَالُوX<ا۟> تَٱللَّهِ تَفۡتَؤُX<ا۟> تَذۡكُR<رُ> یُوسُفَ حَتَّىٰ تَكُونَ حَR<رَ>ضًا أَوۡ تَكُونَ مِنَ ٱلۡهَT<ـٰ>لِكِJ<ی>نَ A<۝>D<٨٥>",
"قَالَ إِN<نَّ>مَW<اۤ> أَشۡكُوX<ا۟> بَثِّی وَحُزۡنِW<یۤ> إِلَى ٱللَّهِ وَأَعۡلَمُ مِنَ ٱللَّهِ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٨٦>",
"یَT<ـٰ>بَنِیَّ ٱذۡهَبُوX<ا۟> فَتَحَسَّسُوX<ا۟> مِX<ن> N<یُ>وسُفَ وَأَخِیهِ وَلَا تَX<ا۟>یۡءَسُوX<ا۟> مِX<ن> R<رَّ>وۡحِ ٱللَّهِۖ إِN<نَّ>هُT<ۥ> لَا یَX<ا۟>یۡءَسُ مِX<ن> R<رَّ>وۡحِ ٱللَّهِ إِلَّا ٱلۡقَوۡمُ ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٨٧>",
"فَلَN<مَّ>ا دَخَلُوX<ا۟> عَلَیۡهِ قَالُوX<ا۟> یَW<ـٰۤ>أَیُّهَا ٱلۡعَزِیزُ مَسَّنَا وَأَهۡلَنَا ٱX<ل>ضُّR<رُّ> وَجِئۡنَا بِبِضَT<ـٰ>عَةX<ࣲ> N<مُّ>زۡجَX<ى>T<ٰ>ةN<ࣲ> فَأَوۡفِ لَنَا ٱلۡكَیۡلَ وَتَصَدَّQ<قۡ> عَلَیۡنَW<اۤ>ۖ إِN<نَّ> ٱللَّهَ یَQ<جۡ>زِی ٱلۡمُتَصَدِّقِJ<ی>نَ A<۝>D<٨٨>",
"قَالَ هَلۡ عَلِمۡتُم N<مَّ>ا فَعَلۡتُN<م> بِیُوسُفَ وَأَخِیهِ إِذۡ أَN<ن>تُمۡ جَT<ـٰ>هِلُJ<و>نَ A<۝>D<٨٩>",
"قَالُW<وۤ>X<ا۟> أَءِN<نَّ>كَ لَأَN<ن>تَ یُوسُفُۖ قَالَ أَنَا۠ یُوسُفُ وَهَT<ـٰ>ذَW<اۤ> أَخِیۖ قَQ<دۡ> مَN<نَّ> ٱللَّهُ عَلَیۡنَW<اۤ>ۖ إِN<نَّ>هُT<ۥ> مَX<ن> N<یَ>تَّقِ وَیَصۡبِرۡ فَإِN<نَّ> ٱللَّهَ لَا یُضِیعُ أَQ<جۡ>R<رَ> ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٩٠>",
"قَالُوX<ا۟> تَٱللَّهِ لَقَQ<دۡ> ءَاثَR<رَ>كَ ٱللَّهُ عَلَیۡنَا وَإِN<ن> كُN<نَّ>ا لَخَT<ـٰ>طِءِJ<ی>نَ A<۝>D<٩١>",
"قَالَ لَا تَثۡرِیبَ عَلَیۡكُمُ ٱلۡیَوۡمَۖ یَغۡفِR<رُ> ٱللَّهُ لَكُمۡۖ وَهُوَ أَR<رۡ>حَمُ ٱX<ل>R<رَّ>T< ٰ>حِمِJ<ی>نَ A<۝>D<٩٢>",
"ٱذۡهَبُوX<ا۟> بِقَمِیصِی هَT<ـٰ>ذَا فَأَلۡقُوهُ عَلَىٰ وَQ<جۡ>هِ أَبِی یَأۡتِ بَصِیR<ر>X<ࣰ>ا N<وَ>أۡتُونِی بِأَهۡلِكُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٩٣>",
"وَلَN<مَّ>ا فَصَلَتِ ٱلۡعِیR<رُ> قَالَ أَبُوهُمۡ إِN<نِّ>ی لَأَجِدُ رِیحَ یُوسُفَۖ لَوۡلَW<اۤ> أَN<ن> تُفَN<نِّ>دُJ<و>نِ A<۝>D<٩٤>",
"قَالُوX<ا۟> تَٱللَّهِ إِN<نَّ>كَ لَفِی ضَلَT<ـٰ>لِكَ ٱلۡقَدِJ<ی>مِ A<۝>D<٩٥>",
"فَلَN<مَّ>W<اۤ> أَN<ن> جَW<اۤ>ءَ ٱلۡبَشِیR<رُ> أَلۡقَX<ى>T<ٰ>هُ عَلَىٰ وَQ<جۡ>هِهِT<ۦ> فَX<ٱ>R<رۡ>تَدَّ بَصِیR<ر>N<ࣰ>اۖ قَالَ أَلَمۡ أَقُل لَّكُمۡ إِN<نِّ>W<یۤ> أَعۡلَمُ مِنَ ٱللَّهِ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٩٦>",
"قَالُوX<ا۟> یَW<ـٰۤ>أَبَانَا ٱسۡتَغۡفِرۡ لَنَا ذُنُوبَنَW<اۤ> إِN<نَّ>ا كُN<نَّ>ا خَT<ـٰ>طِءِJ<ی>نَ A<۝>D<٩٧>",
"قَالَ سَوۡفَ أَسۡتَغۡفِR<رُ> لَكُمۡ R<رَ>بِّW<یۤ>ۖ إِN<نَّ>هُT<ۥ> هُوَ ٱلۡغَفُوR<رُ> ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٩٨>",
"فَلَN<مَّ>ا دَخَلُوX<ا۟> عَلَىٰ یُوسُفَ ءَاوَىW<ٰۤ> إِلَیۡهِ أَبَوَیۡهِ وَقَالَ ٱQ<دۡ>خُلُوX<ا۟> مِصۡR<رَ> إِN<ن> شَW<اۤ>ءَ ٱللَّهُ ءَامِنِJ<ی>نَ A<۝>D<٩٩>",
"وَR<رَ>فَعَ أَبَوَیۡهِ عَلَى ٱلۡعَR<رۡ>شِ وَخَR<رُّ>وX<ا۟> لَهُT<ۥ> سُجَّدX<ࣰ>اۖ N<وَ>قَالَ یَW<ـٰۤ>أَبَتِ هَT<ـٰ>ذَا تَأۡوِیلُ R<رُ>ءۡیَT<ـٰ>یَ مِN<ن> قَQ<بۡ>لُ قَQ<دۡ> جَعَلَهَا R<رَ>بِّی حَقّX<ࣰ>اۖ N<وَ>قَQ<دۡ> أَحۡسَنَ بِW<یۤ> إِذۡ أَخۡR<رَ>جَنِی مِنَ ٱX<ل>سِّQ<جۡ>نِ وَجَW<اۤ>ءَ بِكُم N<مِّ>نَ ٱلۡبَQ<دۡ>وِ مِX<ن>N<ۢ> بَعۡدِ أَن N<نَّ>زَغَ ٱX<ل>شَّیۡطَT<ـٰ>نُ بَیۡنِی وَبَیۡنَ إِخۡوَتِW<یۤ>ۚ إِN<نَّ> R<رَ>بِّی لَطِیفX<ࣱ> لِّمَا یَشَW<اۤ>ءُۚ إِN<نَّ>هُT<ۥ> هُوَ ٱلۡعَلِیمُ ٱلۡحَكِJ<ی>مُ A<۝>D<١٠٠>",
"۞ R<رَ>بِّ قَQ<دۡ> ءَاتَیۡتَنِی مِنَ ٱلۡمُلۡكِ وَعَلَّمۡتَنِی مِN<ن> تَأۡوِیلِ ٱلۡأَحَادِیثِۚ فَاطِR<رَ> ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ أَN<ن>تَ وَلِیِّT<ۦ> فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِۖ تَوَفَّنِی مُسۡلِمX<ࣰ>ا N<وَ>أَلۡحِQ<قۡ>نِی بِX<ٱ>X<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<١٠١>",
"ذَT< ٰ>لِكَ مِنۡ أَX<ن>N<ۢ>بَW<اۤ>ءِ ٱلۡغَیۡبِ نُوحِیهِ إِلَیۡكَۖ وَمَا كُN<ن>تَ لَدَیۡهِمۡ إِذۡ أَQ<جۡ>مَعُW<وۤ>X<ا۟> أَمۡR<رَ>هُمۡ وَهُمۡ یَمۡكُR<رُ>J<و>نَ A<۝>D<١٠٢>",
"وَمَW<اۤ> أَكۡثَR<رُ> ٱX<ل>N<نَّ>اسِ وَلَوۡ حَR<رَ>صۡتَ بِمُؤۡمِنِJ<ی>نَ A<۝>D<١٠٣>",
"وَمَا تَسۡءَلُهُمۡ عَلَیۡهِ مِنۡ أَQ<جۡ>رٍۚ إِنۡ هُوَ إِلَّا ذِكۡR<ر>X<ࣱ> لِّلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٠٤>",
"وَكَأَیِّX<ن> N<مِّ>نۡ ءَایَةN<ࣲ> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ یَمُR<رُّ>ونَ عَلَیۡهَا وَهُمۡ عَنۡهَا مُعۡرِضُJ<و>نَ A<۝>D<١٠٥>",
"وَمَا یُؤۡمِنُ أَكۡثَR<رُ>هُN<م> بِٱللَّهِ إِلَّا وَهُم N<مُّ>شۡرِكُJ<و>نَ A<۝>D<١٠٦>",
"أَفَأَمِنُW<وۤ>X<ا۟> أَN<ن> تَأۡتِیَهُمۡ غَT<ـٰ>شِیَةX<ࣱ> N<مِّ>نۡ عَذَابِ ٱللَّهِ أَوۡ تَأۡتِیَهُمُ ٱX<ل>سَّاعَةُ بَغۡتَةX<ࣰ> N<وَ>هُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<١٠٧>",
"قُلۡ هَT<ـٰ>ذِهِT<ۦ> سَبِیلِW<یۤ> أَQ<دۡ>عُW<وۤ>X<ا۟> إِلَى ٱللَّهِۚ عَلَىٰ بَصِیR<رَ>ةٍ أَنَا۠ وَمَنِ ٱتَّبَعَنِیۖ وَسُQ<بۡ>حَT<ـٰ>نَ ٱللَّهِ وَمَW<اۤ> أَنَا۠ مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<١٠٨>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا مِN<ن> قَQ<بۡ>لِكَ إِلَّا رِجَالX<ࣰ>ا N<نُّ>وحِW<یۤ> إِلَیۡهِم N<مِّ>نۡ أَهۡلِ ٱلۡقُR<رَ>ىW<ٰۤ>ۗ أَفَلَمۡ یَسِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَیَN<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۗ وَلَدَاR<رُ> ٱلۡءَاخِR<رَ>ةِ خَیۡR<ر>X<ࣱ> لِّلَّذِینَ ٱتَّقَوۡX<ا۟>ۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<١٠٩>",
"حَتَّىW<ٰۤ> إِذَا ٱسۡتَیۡءَسَ ٱX<ل>R<رُّ>سُلُ وَظَN<نُّ>W<وۤ>X<ا۟> أَN<نَّ>هُمۡ قَQ<دۡ> كُذِبُوX<ا۟> جَW<اۤ>ءَهُمۡ نَصۡR<رُ>نَا فَنُجِّیَ مَن N<نَّ>شَW<اۤ>ءُۖ وَلَا یُR<رَ>دُّ بَأۡسُنَا عَنِ ٱلۡقَوۡمِ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<١١٠>",
"لَقَQ<دۡ> كَانَ فِی قَصَصِهِمۡ عِQ<بۡ>R<رَ>ةX<ࣱ> لِّأُX<و۟>لِی ٱلۡأَلۡبَT<ـٰ>بِۗ مَا كَانَ حَدِیثX<ࣰ>ا N<یُ>فۡتَR<رَ>ىٰ وَلَT<ـٰ>كِN<ن> تَصۡدِیقَ ٱلَّذِی بَیۡنَ یَدَیۡهِ وَتَفۡصِیلَ كُلِّ شَیۡءX<ࣲ> N<وَ>هُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<١١١>",
"#اL<لۤ>L<مۤ>رۚ تِلۡكَ ءَایَT<ـٰ>تُ ٱلۡكِتَT<ـٰ>بِۗ وَX<ٱ>لَّذِW<یۤ> أُN<ن>زِلَ إِلَیۡكَ مِX<ن> R<رَّ>بِّكَ ٱلۡحَقُّ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یُؤۡمِنُJ<و>نَ A<۝>D<١>",
"ٱللَّهُ ٱلَّذِی R<رَ>فَعَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ بِغَیۡرِ عَمَدN<ࣲ> تَR<رَ>وۡنَهَاۖ ثُN<مَّ> ٱسۡتَوَىٰ عَلَى ٱلۡعَR<رۡ>شِۖ وَسَخَّR<رَ> ٱX<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ>ۖ كُلّX<ࣱ> N<یَ>Q<جۡ>رِی لِأَجَلX<ࣲ> N<مُّ>سَN<مّ>X<ࣰ>ىۚ N<یُ>دَبِّR<رُ> ٱلۡأَمۡR<رَ> یُفَصِّلُ ٱلۡءَایَT<ـٰ>تِ لَعَلَّكُN<م> بِلِقَW<اۤ>ءِ R<رَ>بِّكُمۡ تُوقِنُJ<و>نَ A<۝>D<٢>",
"وَهُوَ ٱلَّذِی مَدَّ ٱلۡأَR<رۡ>ضَ وَجَعَلَ فِیهَا R<رَ>وَT< ٰ>سِیَ وَأَنۡهَT<ـٰ>R<ر>X<ࣰ>اۖ N<وَ>مِN<ن> كُلِّ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِ جَعَلَ فِیهَا زَوۡجَیۡنِ ٱثۡنَیۡنِۖ یُغۡشِی ٱلَّیۡلَ ٱX<ل>N<نَّ>هَاR<رَ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>تَفَكَّR<رُ>J<و>نَ A<۝>D<٣>",
"وَفِی ٱلۡأَR<رۡ>ضِ قِطَعX<ࣱ> N<مُّ>تَجَT<ـٰ>وِR<رَ>T< ٰ>تX<ࣱ> N<وَ>جَN<نَّ>T<ـٰ>تX<ࣱ> N<مِّ>نۡ أَعۡنَT<ـٰ>بX<ࣲ> N<وَ>زَR<رۡ>عX<ࣱ> N<وَ>نَخِیلN<ࣱ> صِنۡوَانX<ࣱ> N<وَ>غَیۡR<رُ> صِنۡوَانX<ࣲ> N<یُ>سۡقَىٰ بِمَW<اۤ>ءX<ࣲ> N<وَ>T< ٰ>حِدX<ࣲ> N<وَ>نُفَضِّلُ بَعۡضَهَا عَلَىٰ بَعۡضN<ࣲ> فِی ٱلۡأُكُلِۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>عۡقِلُJ<و>نَ A<۝>D<٤>",
"۞ وَإِN<ن> تَعۡجَQ<بۡ> فَعَجَبN<ࣱ> قَوۡلُهُمۡ أَءِذَا كُN<نَّ>ا تُR<رَ>T< ٰ>بًا أَءِN<نَّ>ا لَفِی خَلۡقN<ࣲ> جَدِیدٍۗ أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِR<رَ>بِّهِمۡۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ ٱلۡأَغۡلَT<ـٰ>لُ فِW<یۤ> أَعۡنَاقِهِمۡۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٥>",
"وَیَسۡتَعۡجِلُونَكَ بِX<ٱ>X<ل>سَّیِّئَةِ قَQ<بۡ>لَ ٱلۡحَسَنَةِ وَقَQ<دۡ> خَلَتۡ مِN<ن> قَQ<بۡ>لِهِمُ ٱلۡمَثُلَT<ـٰ>تُۗ وَإِN<نَّ> R<رَ>بَّكَ لَذُو مَغۡفِR<رَ>ةX<ࣲ> لِّX<ل>N<نَّ>اسِ عَلَىٰ ظُلۡمِهِمۡۖ وَإِN<نَّ> R<رَ>بَّكَ لَشَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<٦>",
"وَیَقُولُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَوۡلَW<اۤ> أُN<ن>زِلَ عَلَیۡهِ ءَایَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِW<ۦۤ>ۗ إِN<نَّ>مَW<اۤ> أَN<ن>تَ مُN<ن>ذِR<ر>X<ࣱ>ۖ N<وَ>لِكُلِّ قَوۡمٍ هَJ<ا>Q<د>ٍ A<۝>D<٧>",
"ٱللَّهُ یَعۡلَمُ مَا تَحۡمِلُ كُلُّ أُN<ن>ثَىٰ وَمَا تَغِیضُ ٱلۡأَR<رۡ>حَامُ وَمَا تَزۡدَادُۚ وَكُلُّ شَیۡءٍ عِN<ن>دَهُT<ۥ> بِمِQ<قۡ>دَJ<ا>R<ر>ٍ A<۝>D<٨>",
"عَT<ـٰ>لِمُ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِ ٱلۡكَبِیR<رُ> ٱلۡمُتَعَJ<ا>لِ A<۝>D<٩>",
"سَوَW<اۤ>ءX<ࣱ> N<مِّ>N<ن>كُم N<مَّ>نۡ أَسَR<رَّ> ٱلۡقَوۡلَ وَمَN<ن> جَهَR<رَ> بِهِT<ۦ> وَمَنۡ هُوَ مُسۡتَخۡفِN<ۭ> بِX<ٱ>لَّیۡلِ وَسَارِبُN<ۢ> بِX<ٱ>X<ل>N<نَّ>هَJ<ا>R<ر>ِ A<۝>D<١٠>",
"لَهُT<ۥ> مُعَقِّبَT<ـٰ>تX<ࣱ> N<مِّ>X<ن>N<ۢ> بَیۡنِ یَدَیۡهِ وَمِنۡ خَلۡفِهِT<ۦ> یَحۡفَظُونَهُT<ۥ> مِنۡ أَمۡرِ ٱللَّهِۗ إِN<نَّ> ٱللَّهَ لَا یُغَیِّR<رُ> مَا بِقَوۡمٍ حَتَّىٰ یُغَیِّR<رُ>وX<ا۟> مَا بِأَN<ن>فُسِهِمۡۗ وَإِذَW<اۤ> أَR<رَ>ادَ ٱللَّهُ بِقَوۡمN<ࣲ> سُW<وۤ>ءN<ࣰ>ا فَلَا مَR<رَ>دَّ لَهُT<ۥ>ۚ وَمَا لَهُم N<مِّ>N<ن> دُونِهِT<ۦ> مِX<ن> N<وَ>J<ا>لٍ A<۝>D<١١>",
"هُوَ ٱلَّذِی یُرِیكُمُ ٱلۡبَR<رۡ>قَ خَوۡفX<ࣰ>ا N<وَ>طَمَعX<ࣰ>ا N<وَ>یُN<ن>شِئُ ٱX<ل>سَّحَابَ ٱX<ل>ثِّقَJ<ا>لَ A<۝>D<١٢>",
"وَیُسَبِّحُ ٱX<ل>R<رَّ>عۡدُ بِحَمۡدِهِT<ۦ> وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ مِنۡ خِیفَتِهِT<ۦ> وَیُR<رۡ>سِلُ ٱX<ل>صَّوَT< ٰ>عِقَ فَیُصِیبُ بِهَا مَX<ن> N<یَ>شَW<اۤ>ءُ وَهُمۡ یُجَT<ـٰ>دِلُونَ فِی ٱللَّهِ وَهُوَ شَدِیدُ ٱلۡمِحَJ<ا>لِ A<۝>D<١٣>",
"لَهُT<ۥ> دَعۡوَةُ ٱلۡحَقِّۚ وَX<ٱ>لَّذِینَ یَQ<دۡ>عُونَ مِN<ن> دُونِهِT<ۦ> لَا یَسۡتَجِیبُونَ لَهُN<م> بِشَیۡءٍ إِلَّا كَبَT<ـٰ>سِطِ كَفَّیۡهِ إِلَى ٱلۡمَW<اۤ>ءِ لِیَQ<بۡ>لُغَ فَاهُ وَمَا هُوَ بِبَT<ـٰ>لِغِهِT<ۦ>ۚ وَمَا دُعَW<اۤ>ءُ ٱلۡكَT<ـٰ>فِرِینَ إِلَّا فِی ضَلَT<ـٰ>لࣲ A<۝>D<١٤>",
"وَ̅لِ̅لَّ̅هِ̅ ̅یَ̅سۡ̅جُ̅دُ̅ مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ طَوۡعX<ࣰ>ا N<وَ>كَR<رۡ>هX<ࣰ>ا N<وَ>ظِلَT<ـٰ>لُهُN<م> بِX<ٱ>لۡغُدُوِّ وَX<ٱ>لۡءَاصَJ<ا>لِ A<۝>D<١٥> ۩",
"قُلۡ مَX<ن> R<رَّ>بُّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ قُلِ ٱللَّهُۚ قُلۡ أَفَX<ٱ>تَّخَذۡتُم N<مِّ>N<ن> دُونِهِW<ۦۤ> أَوۡلِیَW<اۤ>ءَ لَا یَمۡلِكُونَ لِأَN<ن>فُسِهِمۡ نَفۡعX<ࣰ>ا N<وَ>لَا ضَR<رّ>N<ࣰ>اۚ قُلۡ هَلۡ یَسۡتَوِی ٱلۡأَعۡمَىٰ وَX<ٱ>لۡبَصِیR<رُ> أَمۡ هَلۡ تَسۡتَوِی ٱX<ل>ظُّلُمَT<ـٰ>تُ وَX<ٱ>X<ل>N<نُّ>وR<رُ>ۗ أَمۡ جَعَلُوX<ا۟> لِلَّهِ شُR<رَ>كَW<اۤ>ءَ خَلَقُوX<ا۟> كَخَلۡقِهِT<ۦ> فَتَشَT<ـٰ>بَهَ ٱلۡخَلۡقُ عَلَیۡهِمۡۚ قُلِ ٱللَّهُ خَT<ـٰ>لِقُ كُلِّ شَیۡءX<ࣲ> N<وَ>هُوَ ٱلۡوَT< ٰ>حِدُ ٱلۡقَهَّT<ـٰ>R<ر>ُ A<۝>D<١٦>",
"أَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَسَالَتۡ أَوۡدِیَةُN<ۢ> بِقَدَرِهَا فَX<ٱ>حۡتَمَلَ ٱX<ل>سَّیۡلُ زَبَدX<ࣰ>ا R<رَّ>ابِیX<ࣰ>اۖ N<وَ>مِN<مَّ>ا یُوقِدُونَ عَلَیۡهِ فِی ٱX<ل>N<نَّ>ارِ ٱQ<بۡ>تِغَW<اۤ>ءَ حِلۡیَةٍ أَوۡ مَتَT<ـٰ>عN<ࣲ> زَبَدX<ࣱ> N<مِّ>ثۡلُهُT<ۥ>ۚ كَذَT< ٰ>لِكَ یَضۡرِبُ ٱللَّهُ ٱلۡحَقَّ وَX<ٱ>لۡبَT<ـٰ>طِلَۚ فَأَN<مَّ>ا ٱX<ل>زَّبَدُ فَیَذۡهَبُ جُفَW<اۤ>ءX<ࣰ>ۖ N<وَ>أَN<مَّ>ا مَا یَN<ن>فَعُ ٱX<ل>N<نَّ>اسَ فَیَمۡكُثُ فِی ٱلۡأَR<رۡ>ضِۚ كَذَT< ٰ>لِكَ یَضۡرِبُ ٱللَّهُ ٱلۡأَمۡثَJ<ا>لَ A<۝>D<١٧>",
"لِلَّذِینَ ٱسۡتَجَابُوX<ا۟> لِR<رَ>بِّهِمُ ٱلۡحُسۡنَىٰۚ وَX<ٱ>لَّذِینَ لَمۡ یَسۡتَجِیبُوX<ا۟> لَهُT<ۥ> لَوۡ أَN<نَّ> لَهُم N<مَّ>ا فِی ٱلۡأَR<رۡ>ضِ جَمِیعX<ࣰ>ا N<وَ>مِثۡلَهُT<ۥ> مَعَهُT<ۥ> لَX<ٱ>فۡتَدَوۡX<ا۟> بِهِW<ۦۤ>ۚ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ سُW<وۤ>ءُ ٱلۡحِسَابِ وَمَأۡوَX<ى>T<ٰ>هُمۡ جَهَN<نَّ>مُۖ وَبِئۡسَ ٱلۡمِهَJ<ا>Q<د>ُ A<۝>D<١٨>",
"۞ أَفَمَX<ن> N<یَ>عۡلَمُ أَN<نَّ>مَW<اۤ> أُN<ن>زِلَ إِلَیۡكَ مِX<ن> R<رَّ>بِّكَ ٱلۡحَقُّ كَمَنۡ هُوَ أَعۡمَىW<ٰۤ>ۚ إِN<نَّ>مَا یَتَذَكَّR<رُ> أُX<و۟>لُوX<ا۟> ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<١٩>",
"ٱلَّذِینَ یُوفُونَ بِعَهۡدِ ٱللَّهِ وَلَا یَN<ن>قُضُونَ ٱلۡمِیثَT<ـٰ>Q<ق>َ A<۝>D<٢٠>",
"وَX<ٱ>لَّذِینَ یَصِلُونَ مَW<اۤ> أَمَR<رَ> ٱللَّهُ بِهِW<ۦۤ> أَX<ن> N<یُ>وصَلَ وَیَخۡشَوۡنَ R<رَ>بَّهُمۡ وَیَخَافُونَ سُW<وۤ>ءَ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<٢١>",
"وَX<ٱ>لَّذِینَ صَبَR<رُ>وX<ا۟> ٱQ<بۡ>تِغَW<اۤ>ءَ وَQ<جۡ>هِ R<رَ>بِّهِمۡ وَأَقَامُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَأَN<ن>فَقُوX<ا۟> مِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡ سِR<رّ>X<ࣰ>ا N<وَ>عَلَانِیَةX<ࣰ> N<وَ>یَQ<دۡ>R<رَ>ءُونَ بِX<ٱ>لۡحَسَنَةِ ٱX<ل>سَّیِّئَةَ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ عُQ<قۡ>بَى ٱX<ل>دَّJ<ا>R<ر>ِ A<۝>D<٢٢>",
"جَN<نَّ>T<ـٰ>تُ عَQ<دۡ>نX<ࣲ> N<یَ>Q<دۡ>خُلُونَهَا وَمَN<ن> صَلَحَ مِنۡ ءَابَW<اۤ>ئِهِمۡ وَأَزۡوَT< ٰ>جِهِمۡ وَذُرِّیَّT<ـٰ>تِهِمۡۖ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ یَQ<دۡ>خُلُونَ عَلَیۡهِم N<مِّ>N<ن> كُلِّ بَJ<ا>Q<ب>ࣲ A<۝>D<٢٣>",
"سَلَT<ـٰ>مٌ عَلَیۡكُN<م> بِمَا صَبَR<رۡ>تُمۡۚ فَنِعۡمَ عُQ<قۡ>بَى ٱX<ل>دَّJ<ا>R<ر>ِ A<۝>D<٢٤>",
"وَX<ٱ>لَّذِینَ یَN<ن>قُضُونَ عَهۡدَ ٱللَّهِ مِX<ن>N<ۢ> بَعۡدِ مِیثَT<ـٰ>قِهِT<ۦ> وَیَQ<قۡ>طَعُونَ مَW<اۤ> أَمَR<رَ> ٱللَّهُ بِهِW<ۦۤ> أَX<ن> N<یُ>وصَلَ وَیُفۡسِدُونَ فِی ٱلۡأَR<رۡ>ضِ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمُ ٱX<ل>لَّعۡنَةُ وَلَهُمۡ سُW<وۤ>ءُ ٱX<ل>دَّJ<ا>R<ر>ِ A<۝>D<٢٥>",
"ٱللَّهُ یَQ<بۡ>سُطُ ٱX<ل>رِّزۡقَ لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیَQ<قۡ>دِR<رُ>ۚ وَفَرِحُوX<ا۟> بِX<ٱ>لۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَمَا ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَا فِی ٱلۡءَاخِR<رَ>ةِ إِلَّا مَتَT<ـٰ>عࣱ A<۝>D<٢٦>",
"وَیَقُولُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَوۡلَW<اۤ> أُN<ن>زِلَ عَلَیۡهِ ءَایَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ>ۚ قُلۡ إِN<نَّ> ٱللَّهَ یُضِلُّ مَX<ن> N<یَ>شَW<اۤ>ءُ وَیَهۡدِW<یۤ> إِلَیۡهِ مَنۡ أَنَJ<ا>Q<ب>َ A<۝>D<٢٧>",
"ٱلَّذِینَ ءَامَنُوX<ا۟> وَتَQ<طۡ>مَئِN<نُّ> قُلُوبُهُN<م> بِذِكۡرِ ٱللَّهِۗ أَلَا بِذِكۡرِ ٱللَّهِ تَQ<طۡ>مَئِN<نُّ> ٱلۡقُلُJ<و>Q<ب>ُ A<۝>D<٢٨>",
"ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ طُوبَىٰ لَهُمۡ وَحُسۡنُ مَءَJ<ا>Q<ب>ࣲ A<۝>D<٢٩>",
"كَذَT< ٰ>لِكَ أَR<رۡ>سَلۡنَT<ـٰ>كَ فِW<یۤ> أُN<مَّ>ةN<ࣲ> قَQ<دۡ> خَلَتۡ مِN<ن> قَQ<بۡ>لِهَW<اۤ> أُمَمX<ࣱ> لِّتَتۡلُوَX<ا۟> عَلَیۡهِمُ ٱلَّذِW<یۤ> أَوۡحَیۡنَW<اۤ> إِلَیۡكَ وَهُمۡ یَكۡفُR<رُ>ونَ بِX<ٱ>X<ل>R<رَّ>حۡمَT<ـٰ>نِۚ قُلۡ هُوَ R<رَ>بِّی لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ عَلَیۡهِ تَوَكَّلۡتُ وَإِلَیۡهِ مَتَJ<ا>Q<ب>ِ A<۝>D<٣٠>",
"وَلَوۡ أَN<نَّ> قُR<رۡ>ءَانN<ࣰ>ا سُیِّR<رَ>تۡ بِهِ ٱلۡجِبَالُ أَوۡ قُطِّعَتۡ بِهِ ٱلۡأَR<رۡ>ضُ أَوۡ كُلِّمَ بِهِ ٱلۡمَوۡتَىٰۗ بَل لِّلَّهِ ٱلۡأَمۡR<رُ> جَمِیعًاۗ أَفَلَمۡ یَX<ا۟>یۡءَسِ ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَX<ن> لَّوۡ یَشَW<اۤ>ءُ ٱللَّهُ لَهَدَى ٱX<ل>N<نَّ>اسَ جَمِیعX<ࣰ>اۗ N<وَ>لَا یَزَالُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> تُصِیبُهُN<م> بِمَا صَنَعُوX<ا۟> قَارِعَةٌ أَوۡ تَحُلُّ قَرِیبX<ࣰ>ا N<مِّ>N<ن> دَارِهِمۡ حَتَّىٰ یَأۡتِیَ وَعۡدُ ٱللَّهِۚ إِN<نَّ> ٱللَّهَ لَا یُخۡلِفُ ٱلۡمِیعَJ<ا>Q<د>َ A<۝>D<٣١>",
"وَلَقَدِ ٱسۡتُهۡزِئَ بِR<رُ>سُلX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِكَ فَأَمۡلَیۡتُ لِلَّذِینَ كَفَR<رُ>وX<ا۟> ثُN<مَّ> أَخَذۡتُهُمۡۖ فَكَیۡفَ كَانَ عِقَJ<ا>Q<ب>ِ A<۝>D<٣٢>",
"أَفَمَنۡ هُوَ قَW<اۤ>ئِمٌ عَلَىٰ كُلِّ نَفۡسِN<ۭ> بِمَا كَسَبَتۡۗ وَجَعَلُوX<ا۟> لِلَّهِ شُR<رَ>كَW<اۤ>ءَ قُلۡ سَN<مُّ>وهُمۡۚ أَمۡ تُنَبِّءُونَهُT<ۥ> بِمَا لَا یَعۡلَمُ فِی ٱلۡأَR<رۡ>ضِ أَN<م> بِظَT<ـٰ>هِرX<ࣲ> N<مِّ>نَ ٱلۡقَوۡلِۗ بَلۡ زُیِّنَ لِلَّذِینَ كَفَR<رُ>وX<ا۟> مَكۡR<رُ>هُمۡ وَصُدُّوX<ا۟> عَنِ ٱX<ل>سَّبِیلِۗ وَمَX<ن> N<یُ>ضۡلِلِ ٱللَّهُ فَمَا لَهُT<ۥ> مِنۡ هَJ<ا>Q<د>ࣲ A<۝>D<٣٣>",
"لَّهُمۡ عَذَابN<ࣱ> فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۖ وَلَعَذَابُ ٱلۡءَاخِR<رَ>ةِ أَشَقُّۖ وَمَا لَهُم N<مِّ>نَ ٱللَّهِ مِX<ن> N<وَ>J<ا>Q<ق>ࣲ A<۝>D<٣٤>",
"۞ N<مَّ>ثَلُ ٱلۡجَN<نَّ>ةِ ٱلَّتِی وُعِدَ ٱلۡمُتَّقُونَۖ تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ>ۖ أُكُلُهَا دَW<اۤ>ئِمX<ࣱ> N<وَ>ظِلُّهَاۚ تِلۡكَ عُQ<قۡ>بَى ٱلَّذِینَ ٱتَّقَوX<ا۟>ۚ وَّعُQ<قۡ>بَى ٱلۡكَT<ـٰ>فِرِینَ ٱX<ل>N<نَّ>J<ا>R<ر>ُ A<۝>D<٣٥>",
"وَX<ٱ>لَّذِینَ ءَاتَیۡنَT<ـٰ>هُمُ ٱلۡكِتَT<ـٰ>بَ یَفۡR<رَ>حُونَ بِمَW<اۤ> أُN<ن>زِلَ إِلَیۡكَۖ وَمِنَ ٱلۡأَحۡزَابِ مَX<ن> N<یُ>N<ن>كِR<رُ> بَعۡضَهُT<ۥ>ۚ قُلۡ إِN<نَّ>مَW<اۤ> أُمِرۡتُ أَنۡ أَعۡبُدَ ٱللَّهَ وَلَW<اۤ> أُشۡرِكَ بِهِW<ۦۤ>ۚ إِلَیۡهِ أَQ<دۡ>عُوX<ا۟> وَإِلَیۡهِ مَءَJ<ا>Q<ب>ِ A<۝>D<٣٦>",
"وَكَذَT< ٰ>لِكَ أَN<ن>زَلۡنَT<ـٰ>هُ حُكۡمًا عَR<رَ>بِیّX<ࣰ>اۚ N<وَ>لَئِنِ ٱتَّبَعۡتَ أَهۡوَW<اۤ>ءَهُN<م> بَعۡدَ مَا جَW<اۤ>ءَكَ مِنَ ٱلۡعِلۡمِ مَا لَكَ مِنَ ٱللَّهِ مِX<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا وَJ<ا>Q<ق>ࣲ A<۝>D<٣٧>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا R<رُ>سُلX<ࣰ>ا N<مِّ>N<ن> قَQ<بۡ>لِكَ وَجَعَلۡنَا لَهُمۡ أَزۡوَT< ٰ>جX<ࣰ>ا N<وَ>ذُرِّیَّةX<ࣰ>ۚ N<وَ>مَا كَانَ لِR<رَ>سُولٍ أَX<ن> N<یَ>أۡتِیَ بِءَایَةٍ إِلَّا بِإِذۡنِ ٱللَّهِۗ لِكُلِّ أَجَلN<ࣲ> كِتَJ<ا>Q<ب>ࣱ A<۝>D<٣٨>",
"یَمۡحُوX<ا۟> ٱللَّهُ مَا یَشَW<اۤ>ءُ وَیُثۡبِتُۖ وَعِN<ن>دَهُW<ۥۤ> أُN<مُّ> ٱلۡكِتَT<ـٰ>Q<ب>ِ A<۝>D<٣٩>",
"وَإِX<ن> N<مَّ>ا نُرِیَN<نَّ>كَ بَعۡضَ ٱلَّذِی نَعِدُهُمۡ أَوۡ نَتَوَفَّیَN<نَّ>كَ فَإِN<نَّ>مَا عَلَیۡكَ ٱلۡبَلَT<ـٰ>غُ وَعَلَیۡنَا ٱلۡحِسَJ<ا>Q<ب>ُ A<۝>D<٤٠>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ>ا نَأۡتِی ٱلۡأَR<رۡ>ضَ نَN<ن>قُصُهَا مِنۡ أَQ<طۡ>R<رَ>افِهَاۚ وَٱللَّهُ یَحۡكُمُ لَا مُعَقِّبَ لِحُكۡمِهِT<ۦ>ۚ وَهُوَ سَرِیعُ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<٤١>",
"وَقَQ<دۡ> مَكَR<رَ> ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ فَلِلَّهِ ٱلۡمَكۡR<رُ> جَمِیعX<ࣰ>اۖ N<یَ>عۡلَمُ مَا تَكۡسِبُ كُلُّ نَفۡسX<ࣲ>ۗ N<وَ>سَیَعۡلَمُ ٱلۡكُفَّT<ـٰ>R<رُ> لِمَنۡ عُQ<قۡ>بَى ٱX<ل>دَّJ<ا>R<ر>ِ A<۝>D<٤٢>",
"وَیَقُولُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَسۡتَ مُR<رۡ>سَلN<ࣰ>اۚ قُلۡ كَفَىٰ بِٱللَّهِ شَهِیدَN<ۢ>ا بَیۡنِی وَبَیۡنَكُمۡ وَمَنۡ عِN<ن>دَهُT<ۥ> عِلۡمُ ٱلۡكِتَT<ـٰ>Q<ب>ِ A<۝>D<٤٣>",
"#اL<لۤ>رۚ كِتَT<ـٰ>بٌ أَN<ن>زَلۡنَT<ـٰ>هُ إِلَیۡكَ لِتُخۡرِجَ ٱX<ل>N<نَّ>اسَ مِنَ ٱX<ل>ظُّلُمَT<ـٰ>تِ إِلَى ٱX<ل>N<نُّ>ورِ بِإِذۡنِ R<رَ>بِّهِمۡ إِلَىٰ صِR<رَ>T< ٰ>طِ ٱلۡعَزِیزِ ٱلۡحَمِJ<ی>Q<د>ِ A<۝>D<١>",
"ٱللَّهِ ٱلَّذِی لَهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۗ وَوَیۡلX<ࣱ> لِّلۡكَT<ـٰ>فِرِینَ مِنۡ عَذَابN<ࣲ> شَدِJ<ی>Q<د>ٍ A<۝>D<٢>",
"ٱلَّذِینَ یَسۡتَحِبُّونَ ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا عَلَى ٱلۡءَاخِR<رَ>ةِ وَیَصُدُّونَ عَN<ن> سَبِیلِ ٱللَّهِ وَیَQ<بۡ>غُونَهَا عِوَجًاۚ أُX<و۟>لَW<ـٰۤ>ئِكَ فِی ضَلَT<ـٰ>لِN<ۭ> بَعِJ<ی>Q<د>ࣲ A<۝>D<٣>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا مِX<ن> R<رَّ>سُولٍ إِلَّا بِلِسَانِ قَوۡمِهِT<ۦ> لِیُبَیِّنَ لَهُمۡۖ فَیُضِلُّ ٱللَّهُ مَX<ن> N<یَ>شَW<اۤ>ءُ وَیَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٤>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا مُوسَىٰ بِءَایَT<ـٰ>تِنَW<اۤ> أَنۡ أَخۡرِQ<جۡ> قَوۡمَكَ مِنَ ٱX<ل>ظُّلُمَT<ـٰ>تِ إِلَى ٱX<ل>N<نُّ>ورِ وَذَكِّرۡهُN<م> بِأَیَّX<ى>T<ٰ>مِ ٱللَّهِۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّكُلِّ صَبَّارN<ࣲ> شَكُJ<و>R<ر>ࣲ A<۝>D<٥>",
"وَإِذۡ قَالَ مُوسَىٰ لِقَوۡمِهِ ٱذۡكُR<رُ>وX<ا۟> نِعۡمَةَ ٱللَّهِ عَلَیۡكُمۡ إِذۡ أَN<ن>جَX<ى>T<ٰ>كُم N<مِّ>نۡ ءَالِ فِرۡعَوۡنَ یَسُومُونَكُمۡ سُW<وۤ>ءَ ٱلۡعَذَابِ وَیُذَبِّحُونَ أَQ<بۡ>نَW<اۤ>ءَكُمۡ وَیَسۡتَحۡیُونَ نِسَW<اۤ>ءَكُمۡۚ وَفِی ذَT< ٰ>لِكُN<م> بَلَW<اۤ>ءX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ عَظِJ<ی>مࣱ A<۝>D<٦>",
"وَإِذۡ تَأَذَّنَ R<رَ>بُّكُمۡ لَئِN<ن> شَكَR<رۡ>تُمۡ لَأَزِیدَN<نَّ>كُمۡۖ وَلَئِN<ن> كَفَR<رۡ>تُمۡ إِN<نَّ> عَذَابِی لَشَدِJ<ی>Q<د>ࣱ A<۝>D<٧>",
"وَقَالَ مُوسَىW<ٰۤ> إِN<ن> تَكۡفُR<رُ>W<وۤ>X<ا۟> أَN<ن>تُمۡ وَمَN<ن> فِی ٱلۡأَR<رۡ>ضِ جَمِیعN<ࣰ>ا فَإِN<نَّ> ٱللَّهَ لَغَنِیٌّ حَمِJ<ی>Q<د>ٌ A<۝>D<٨>",
"أَلَمۡ یَأۡتِكُمۡ نَبَؤُX<ا۟> ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِكُمۡ قَوۡمِ نُوحX<ࣲ> N<وَ>عَادX<ࣲ> N<وَ>ثَمُودَ وَX<ٱ>لَّذِینَ مِX<ن>N<ۢ> بَعۡدِهِمۡ لَا یَعۡلَمُهُمۡ إِلَّا ٱللَّهُۚ جَW<اۤ>ءَتۡهُمۡ R<رُ>سُلُهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَR<رَ>دُّW<وۤ>X<ا۟> أَیۡدِیَهُمۡ فِW<یۤ> أَفۡوَT< ٰ>هِهِمۡ وَقَالُW<وۤ>X<ا۟> إِN<نَّ>ا كَفَR<رۡ>نَا بِمَW<اۤ> أُR<رۡ>سِلۡتُN<م> بِهِT<ۦ> وَإِN<نَّ>ا لَفِی شَكّX<ࣲ> N<مِّ>N<مَّ>ا تَQ<دۡ>عُونَنَW<اۤ> إِلَیۡهِ مُرِJ<ی>Q<ب>ࣲ A<۝>D<٩>",
"۞ قَالَتۡ R<رُ>سُلُهُمۡ أَفِی ٱللَّهِ شَكّN<ࣱ> فَاطِرِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ یَQ<دۡ>عُوكُمۡ لِیَغۡفِR<رَ> لَكُم N<مِّ>N<ن> ذُنُوبِكُمۡ وَیُؤَخِّR<رَ>كُمۡ إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>N<ࣰ>ىۚ قَالُW<وۤ>X<ا۟> إِنۡ أَN<ن>تُمۡ إِلَّا بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُنَا تُرِیدُونَ أَN<ن> تَصُدُّونَا عَN<مَّ>ا كَانَ یَعۡبُدُ ءَابَW<اۤ>ؤُنَا فَأۡتُونَا بِسُلۡطَT<ـٰ>نX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<١٠>",
"قَالَتۡ لَهُمۡ R<رُ>سُلُهُمۡ إِن N<نَّ>حۡنُ إِلَّا بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُكُمۡ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ یَمُN<نُّ> عَلَىٰ مَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِT<ۦ>ۖ وَمَا كَانَ لَنَW<اۤ> أَن N<نَّ>أۡتِیَكُN<م> بِسُلۡطَT<ـٰ>نٍ إِلَّا بِإِذۡنِ ٱللَّهِۚ وَعَلَى ٱللَّهِ فَلۡیَتَوَكَّلِ ٱلۡمُؤۡمِنُJ<و>نَ A<۝>D<١١>",
"وَمَا لَنَW<اۤ> أَلَّا نَتَوَكَّلَ عَلَى ٱللَّهِ وَقَQ<دۡ> هَدَX<ى>T<ٰ>نَا سُبُلَنَاۚ وَلَنَصۡبِR<رَ>N<نَّ> عَلَىٰ مَW<اۤ> ءَاذَیۡتُمُونَاۚ وَعَلَى ٱللَّهِ فَلۡیَتَوَكَّلِ ٱلۡمُتَوَكِّلُJ<و>نَ A<۝>D<١٢>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لِR<رُ>سُلِهِمۡ لَنُخۡرِجَN<نَّ>كُم N<مِّ>نۡ أَR<رۡ>ضِنَW<اۤ> أَوۡ لَتَعُودُN<نَّ> فِی مِلَّتِنَاۖ فَأَوۡحَىW<ٰۤ> إِلَیۡهِمۡ R<رَ>بُّهُمۡ لَنُهۡلِكَN<نَّ> ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٣>",
"وَلَنُسۡكِنَN<نَّ>كُمُ ٱلۡأَR<رۡ>ضَ مِX<ن>N<ۢ> بَعۡدِهِمۡۚ ذَT< ٰ>لِكَ لِمَنۡ خَافَ مَقَامِی وَخَافَ وَعِJ<ی>Q<د>ِ A<۝>D<١٤>",
"وَX<ٱ>سۡتَفۡتَحُوX<ا۟> وَخَابَ كُلُّ جَبَّارٍ عَنِJ<ی>Q<د>ࣲ A<۝>D<١٥>",
"مِّX<ن> N<وَ>R<رَ>W<اۤ>ئِهِT<ۦ> جَهَN<نَّ>مُ وَیُسۡقَىٰ مِX<ن> N<مَّ>W<اۤ>ءN<ࣲ> صَدِJ<ی>Q<د>ࣲ A<۝>D<١٦>",
"یَتَجَR<رَّ>عُهُT<ۥ> وَلَا یَكَادُ یُسِیغُهُT<ۥ> وَیَأۡتِیهِ ٱلۡمَوۡتُ مِN<ن> كُلِّ مَكَانX<ࣲ> N<وَ>مَا هُوَ بِمَیِّتX<ࣲ>ۖ N<وَ>مِX<ن> N<وَ>R<رَ>W<اۤ>ئِهِT<ۦ> عَذَابٌ غَلِJ<ی>ظࣱ A<۝>D<١٧>",
"مَّثَلُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِR<رَ>بِّهِمۡۖ أَعۡمَT<ـٰ>لُهُمۡ كَR<رَ>مَادٍ ٱشۡتَدَّتۡ بِهِ ٱX<ل>رِّیحُ فِی یَوۡمٍ عَاصِفX<ࣲ>ۖ لَّا یَQ<قۡ>دِR<رُ>ونَ مِN<مَّ>ا كَسَبُوX<ا۟> عَلَىٰ شَیۡءN<ࣲ>ۚ ذَT< ٰ>لِكَ هُوَ ٱX<ل>ضَّلَT<ـٰ>لُ ٱلۡبَعِJ<ی>Q<د>ُ A<۝>D<١٨>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ بِX<ٱ>لۡحَقِّۚ إِX<ن> N<یَ>شَأۡ یُذۡهِQ<بۡ>كُمۡ وَیَأۡتِ بِخَلۡقN<ࣲ> جَدِJ<ی>Q<د>ࣲ A<۝>D<١٩>",
"وَمَا ذَT< ٰ>لِكَ عَلَى ٱللَّهِ بِعَزِJ<ی>زࣲ A<۝>D<٢٠>",
"وَبَR<رَ>زُوX<ا۟> لِلَّهِ جَمِیعN<ࣰ>ا فَقَالَ ٱX<ل>ضُّعَفَW<ـٰۤ>ؤُX<ا۟> لِلَّذِینَ ٱسۡتَكۡبَR<رُ>W<وۤ>X<ا۟> إِN<نَّ>ا كُN<نَّ>ا لَكُمۡ تَبَعN<ࣰ>ا فَهَلۡ أَN<ن>تُم N<مُّ>غۡنُونَ عَN<نَّ>ا مِنۡ عَذَابِ ٱللَّهِ مِN<ن> شَیۡءN<ࣲ>ۚ قَالُوX<ا۟> لَوۡ هَدَX<ى>T<ٰ>نَا ٱللَّهُ لَهَدَیۡنَT<ـٰ>كُمۡۖ سَوَW<اۤ>ءٌ عَلَیۡنَW<اۤ> أَجَزِعۡنَW<اۤ> أَمۡ صَبَR<رۡ>نَا مَا لَنَا مِX<ن> N<مَّ>حِJ<ی>صࣲ A<۝>D<٢١>",
"وَقَالَ ٱX<ل>شَّیۡطَT<ـٰ>نُ لَN<مَّ>ا قُضِیَ ٱلۡأَمۡR<رُ> إِN<نَّ> ٱللَّهَ وَعَدَكُمۡ وَعۡدَ ٱلۡحَقِّ وَوَعَX<د>تُّكُمۡ فَأَخۡلَفۡتُكُمۡۖ وَمَا كَانَ لِیَ عَلَیۡكُم N<مِّ>N<ن> سُلۡطَT<ـٰ>نٍ إِلَّW<اۤ> أَN<ن> دَعَوۡتُكُمۡ فَX<ٱ>سۡتَجَQ<بۡ>تُمۡ لِیۖ فَلَا تَلُومُونِی وَلُومُW<وۤ>X<ا۟> أَN<ن>فُسَكُمۖ N<مَّ>W<اۤ> أَنَا۠ بِمُصۡرِخِكُمۡ وَمَW<اۤ> أَN<ن>تُN<م> بِمُصۡرِخِیَّ إِN<نِّ>ی كَفَR<رۡ>تُ بِمَW<اۤ> أَشۡR<رَ>كۡتُمُونِ مِN<ن> قَQ<بۡ>لُۗ إِN<نَّ> ٱX<ل>ظَّT<ـٰ>لِمِینَ لَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٢٢>",
"وَأُQ<دۡ>خِلَ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَا بِإِذۡنِ R<رَ>بِّهِمۡۖ تَحِیَّتُهُمۡ فِیهَا سَلَT<ـٰ>مٌ A<۝>D<٢٣>",
"أَلَمۡ تَR<رَ> كَیۡفَ ضَR<رَ>بَ ٱللَّهُ مَثَلN<ࣰ>ا كَلِمَةN<ࣰ> طَیِّبَةN<ࣰ> كَشَجَR<رَ>ةN<ࣲ> طَیِّبَةٍ أَصۡلُهَا ثَابِتX<ࣱ> N<وَ>فَR<رۡ>عُهَا فِی ٱX<ل>سَّمَW<اۤ>ءِ A<۝>D<٢٤>",
"تُؤۡتِW<یۤ> أُكُلَهَا كُلَّ حِینِN<ۭ> بِإِذۡنِ R<رَ>بِّهَاۗ وَیَضۡرِبُ ٱللَّهُ ٱلۡأَمۡثَالَ لِX<ل>N<نَّ>اسِ لَعَلَّهُمۡ یَتَذَكَّR<رُ>J<و>نَ A<۝>D<٢٥>",
"وَمَثَلُ كَلِمَةٍ خَبِیثَةN<ࣲ> كَشَجَR<رَ>ةٍ خَبِیثَةٍ ٱQ<جۡ>تُثَّتۡ مِN<ن> فَوۡقِ ٱلۡأَR<رۡ>ضِ مَا لَهَا مِN<ن> قَR<رَ>J<ا>R<ر>ࣲ A<۝>D<٢٦>",
"یُثَبِّتُ ٱللَّهُ ٱلَّذِینَ ءَامَنُوX<ا۟> بِX<ٱ>لۡقَوۡلِ ٱX<ل>ثَّابِتِ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَفِی ٱلۡءَاخِR<رَ>ةِۖ وَیُضِلُّ ٱللَّهُ ٱX<ل>ظَّT<ـٰ>لِمِینَۚ وَیَفۡعَلُ ٱللَّهُ مَا یَشَW<اۤ>ءُ A<۝>D<٢٧>",
"۞ أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ بَدَّلُوX<ا۟> نِعۡمَتَ ٱللَّهِ كُفۡR<ر>X<ࣰ>ا N<وَ>أَحَلُّوX<ا۟> قَوۡمَهُمۡ دَاR<رَ> ٱلۡبَوَJ<ا>R<ر>ِ A<۝>D<٢٨>",
"جَهَN<نَّ>مَ یَصۡلَوۡنَهَاۖ وَبِئۡسَ ٱلۡقَR<رَ>J<ا>R<ر>ُ A<۝>D<٢٩>",
"وَجَعَلُوX<ا۟> لِلَّهِ أَN<ن>دَادX<ࣰ>ا لِّیُضِلُّوX<ا۟> عَN<ن> سَبِیلِهِT<ۦ>ۗ قُلۡ تَمَتَّعُوX<ا۟> فَإِN<نَّ> مَصِیR<رَ>كُمۡ إِلَى ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٣٠>",
"قُل لِّعِبَادِیَ ٱلَّذِینَ ءَامَنُوX<ا۟> یُقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَیُN<ن>فِقُوX<ا۟> مِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡ سِR<رّ>X<ࣰ>ا N<وَ>عَلَانِیَةX<ࣰ> N<مِّ>N<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>أۡتِیَ یَوۡمX<ࣱ> لَّا بَیۡعN<ࣱ> فِیهِ وَلَا خِلَT<ـٰ>لٌ A<۝>D<٣١>",
"ٱللَّهُ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَأَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَخۡR<رَ>جَ بِهِT<ۦ> مِنَ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِ رِزۡقX<ࣰ>ا لَّكُمۡۖ وَسَخَّR<رَ> لَكُمُ ٱلۡفُلۡكَ لِتَQ<جۡ>رِیَ فِی ٱلۡبَحۡرِ بِأَمۡرِهِT<ۦ>ۖ وَسَخَّR<رَ> لَكُمُ ٱلۡأَنۡهَT<ـٰ>R<ر>َ A<۝>D<٣٢>",
"وَسَخَّR<رَ> لَكُمُ ٱX<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ> دَW<اۤ>ئِبَیۡنِۖ وَسَخَّR<رَ> لَكُمُ ٱلَّیۡلَ وَX<ٱ>X<ل>N<نَّ>هَJ<ا>R<ر>َ A<۝>D<٣٣>",
"وَءَاتَX<ى>T<ٰ>كُم N<مِّ>N<ن> كُلِّ مَا سَأَلۡتُمُوهُۚ وَإِN<ن> تَعُدُّوX<ا۟> نِعۡمَتَ ٱللَّهِ لَا تُحۡصُوهَW<اۤ>ۗ إِN<نَّ> ٱلۡإِN<ن>سَT<ـٰ>نَ لَظَلُومN<ࣱ> كَفَّJ<ا>R<ر>ࣱ A<۝>D<٣٤>",
"وَإِذۡ قَالَ إِQ<بۡ>R<رَ>T< ٰ>هِیمُ R<رَ>بِّ ٱQ<جۡ>عَلۡ هَT<ـٰ>ذَا ٱلۡبَلَدَ ءَامِنX<ࣰ>ا N<وَ>X<ٱ>Q<جۡ>نُQ<بۡ>نِی وَبَنِیَّ أَن N<نَّ>عۡبُدَ ٱلۡأَصۡنَJ<ا>مَ A<۝>D<٣٥>",
"R<رَ>بِّ إِN<نَّ>هُN<نَّ> أَضۡلَلۡنَ كَثِیR<ر>X<ࣰ>ا N<مِّ>نَ ٱX<ل>N<نَّ>اسِۖ فَمَN<ن> تَبِعَنِی فَإِN<نَّ>هُT<ۥ> مِN<نِّ>یۖ وَمَنۡ عَصَانِی فَإِN<نَّ>كَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٣٦>",
"R<رَّ>بَّنَW<اۤ> إِN<نِّ>W<یۤ> أَسۡكَN<ن>تُ مِN<ن> ذُرِّیَّتِی بِوَادٍ غَیۡرِ ذِی زَR<رۡ>عٍ عِN<ن>دَ بَیۡتِكَ ٱلۡمُحَR<رَّ>مِ R<رَ>بَّنَا لِیُقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ فَX<ٱ>Q<جۡ>عَلۡ أَفۡءِدَةX<ࣰ> N<مِّ>نَ ٱX<ل>N<نَّ>اسِ تَهۡوِW<یۤ> إِلَیۡهِمۡ وَX<ٱ>R<رۡ>زُQ<قۡ>هُم N<مِّ>نَ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِ لَعَلَّهُمۡ یَشۡكُR<رُ>J<و>نَ A<۝>D<٣٧>",
"R<رَ>بَّنَW<اۤ> إِN<نَّ>كَ تَعۡلَمُ مَا نُخۡفِی وَمَا نُعۡلِنُۗ وَمَا یَخۡفَىٰ عَلَى ٱللَّهِ مِN<ن> شَیۡءN<ࣲ> فِی ٱلۡأَR<رۡ>ضِ وَلَا فِی ٱX<ل>سَّمَW<اۤ>ءِ A<۝>D<٣٨>",
"ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِی وَهَبَ لِی عَلَى ٱلۡكِبَرِ إِسۡمَT<ـٰ>عِیلَ وَإِسۡحَT<ـٰ>قَۚ إِN<نَّ> R<رَ>بِّی لَسَمِیعُ ٱX<ل>دُّعَW<اۤ>ءِ A<۝>D<٣٩>",
"R<رَ>بِّ ٱQ<جۡ>عَلۡنِی مُقِیمَ ٱX<ل>صَّلَX<و>T<ٰ>ةِ وَمِN<ن> ذُرِّیَّتِیۚ R<رَ>بَّنَا وَتَقَبَّلۡ دُعَW<اۤ>ءِ A<۝>D<٤٠>",
"R<رَ>بَّنَا ٱغۡفِرۡ لِی وَلِوَT< ٰ>لِدَیَّ وَلِلۡمُؤۡمِنِینَ یَوۡمَ یَقُومُ ٱلۡحِسَJ<ا>Q<ب>ُ A<۝>D<٤١>",
"وَلَا تَحۡسَبَN<نَّ> ٱللَّهَ غَT<ـٰ>فِلًا عَN<مَّ>ا یَعۡمَلُ ٱX<ل>ظَّT<ـٰ>لِمُونَۚ إِN<نَّ>مَا یُؤَخِّR<رُ>هُمۡ لِیَوۡمN<ࣲ> تَشۡخَصُ فِیهِ ٱلۡأَQ<بۡ>صَT<ـٰ>R<ر>ُ A<۝>D<٤٢>",
"مُهۡطِعِینَ مُQ<قۡ>نِعِی R<رُ>ءُوسِهِمۡ لَا یَR<رۡ>تَدُّ إِلَیۡهِمۡ طَR<رۡ>فُهُمۡۖ وَأَفۡءِدَتُهُمۡ هَوَW<اۤ>ءࣱ A<۝>D<٤٣>",
"وَأَN<ن>ذِرِ ٱX<ل>N<نَّ>اسَ یَوۡمَ یَأۡتِیهِمُ ٱلۡعَذَابُ فَیَقُولُ ٱلَّذِینَ ظَلَمُوX<ا۟> R<رَ>بَّنَW<اۤ> أَخِّرۡنَW<اۤ> إِلَىW<ٰۤ> أَجَلN<ࣲ> قَرِیبX<ࣲ> N<نُّ>جِQ<بۡ> دَعۡوَتَكَ وَنَتَّبِعِ ٱX<ل>R<رُّ>سُلَۗ أَوَ لَمۡ تَكُونُW<وۤ>X<ا۟> أَQ<قۡ>سَمۡتُم N<مِّ>N<ن> قَQ<بۡ>لُ مَا لَكُم N<مِّ>N<ن> زَوَJ<ا>لࣲ A<۝>D<٤٤>",
"وَسَكَN<ن>تُمۡ فِی مَسَT<ـٰ>كِنِ ٱلَّذِینَ ظَلَمُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ وَتَبَیَّنَ لَكُمۡ كَیۡفَ فَعَلۡنَا بِهِمۡ وَضَR<رَ>Q<بۡ>نَا لَكُمُ ٱلۡأَمۡثَJ<ا>لَ A<۝>D<٤٥>",
"وَقَQ<دۡ> مَكَR<رُ>وX<ا۟> مَكۡR<رَ>هُمۡ وَعِN<ن>دَ ٱللَّهِ مَكۡR<رُ>هُمۡ وَإِN<ن> كَانَ مَكۡR<رُ>هُمۡ لِتَزُولَ مِنۡهُ ٱلۡجِبَJ<ا>لُ A<۝>D<٤٦>",
"فَلَا تَحۡسَبَN<نَّ> ٱللَّهَ مُخۡلِفَ وَعۡدِهِT<ۦ> R<رُ>سُلَهُW<ۥۤ>ۚ إِN<نَّ> ٱللَّهَ عَزِیزN<ࣱ> ذُو ٱN<ن>تِقَJ<ا>مࣲ A<۝>D<٤٧>",
"یَوۡمَ تُبَدَّلُ ٱلۡأَR<رۡ>ضُ غَیۡR<رَ> ٱلۡأَR<رۡ>ضِ وَX<ٱ>X<ل>سَّمَT<ـٰ>وَT< ٰ>تُۖ وَبَR<رَ>زُوX<ا۟> لِلَّهِ ٱلۡوَT< ٰ>حِدِ ٱلۡقَهَّJ<ا>R<ر>ِ A<۝>D<٤٨>",
"وَتَR<رَ>ى ٱلۡمُQ<جۡ>رِمِینَ یَوۡمَئِذX<ࣲ> N<مُّ>قَR<رَّ>نِینَ فِی ٱلۡأَصۡفَJ<ا>Q<د>ِ A<۝>D<٤٩>",
"سَR<رَ>ابِیلُهُم N<مِّ>N<ن> قَطِR<رَ>انX<ࣲ> N<وَ>تَغۡشَىٰ وُجُوهَهُمُ ٱX<ل>N<نَّ>J<ا>R<ر>ُ A<۝>D<٥٠>",
"لِیَQ<جۡ>زِیَ ٱللَّهُ كُلَّ نَفۡسX<ࣲ> N<مَّ>ا كَسَبَتۡۚ إِN<نَّ> ٱللَّهَ سَرِیعُ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<٥١>",
"هَT<ـٰ>ذَا بَلَT<ـٰ>غX<ࣱ> لِّX<ل>N<نَّ>اسِ وَلِیُN<ن>ذَR<رُ>وX<ا۟> بِهِT<ۦ> وَلِیَعۡلَمُW<وۤ>X<ا۟> أَN<نَّ>مَا هُوَ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدX<ࣱ> N<وَ>لِیَذَّكَّR<رَ> أُX<و۟>لُوX<ا۟> ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<٥٢>",
"#۞ اL<لۤ>رۚ تِلۡكَ ءَایَT<ـٰ>تُ ٱلۡكِتَT<ـٰ>بِ وَقُR<رۡ>ءَانX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<١>",
"R<رُّ>بَمَا یَوَدُّ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَوۡ كَانُوX<ا۟> مُسۡلِمِJ<ی>نَ A<۝>D<٢>",
"ذَR<رۡ>هُمۡ یَأۡكُلُوX<ا۟> وَیَتَمَتَّعُوX<ا۟> وَیُلۡهِهِمُ ٱلۡأَمَلُۖ فَسَوۡفَ یَعۡلَمُJ<و>نَ A<۝>D<٣>",
"وَمَW<اۤ> أَهۡلَكۡنَا مِN<ن> قَR<رۡ>یَةٍ إِلَّا وَلَهَا كِتَابX<ࣱ> N<مَّ>عۡلُJ<و>مࣱ A<۝>D<٤>",
"مَّا تَسۡبِقُ مِنۡ أُN<مَّ>ةٍ أَجَلَهَا وَمَا یَسۡتَءۡخِR<رُ>J<و>نَ A<۝>D<٥>",
"وَقَالُوX<ا۟> یَW<ـٰۤ>أَیُّهَا ٱلَّذِی نُزِّلَ عَلَیۡهِ ٱX<ل>ذِّكۡR<رُ> إِN<نَّ>كَ لَمَQ<جۡ>نُJ<و>نࣱ A<۝>D<٦>",
"لَّوۡ مَا تَأۡتِینَا بِX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةِ إِN<ن> كُN<ن>تَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٧>",
"مَا نُنَزِّلُ ٱلۡمَلَW<ـٰۤ>ئِكَةَ إِلَّا بِX<ٱ>لۡحَقِّ وَمَا كَانُW<وۤ>X<ا۟> إِذX<ࣰ>ا N<مُّ>N<ن>ظَرِJ<ی>نَ A<۝>D<٨>",
"إِN<نَّ>ا نَحۡنُ نَزَّلۡنَا ٱX<ل>ذِّكۡR<رَ> وَإِN<نَّ>ا لَهُT<ۥ> لَحَT<ـٰ>فِظُJ<و>نَ A<۝>D<٩>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا مِN<ن> قَQ<بۡ>لِكَ فِی شِیَعِ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٠>",
"وَمَا یَأۡتِیهِم N<مِّ>X<ن> R<رَّ>سُولٍ إِلَّا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<١١>",
"كَذَT< ٰ>لِكَ نَسۡلُكُهُT<ۥ> فِی قُلُوبِ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<١٢>",
"لَا یُؤۡمِنُونَ بِهِT<ۦ> وَقَQ<دۡ> خَلَتۡ سُN<نَّ>ةُ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٣>",
"وَلَوۡ فَتَحۡنَا عَلَیۡهِN<م> بَابX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ فَظَلُّوX<ا۟> فِیهِ یَعۡR<رُ>جُJ<و>نَ A<۝>D<١٤>",
"لَقَالُW<وۤ>X<ا۟> إِN<نَّ>مَا سُكِّR<رَ>تۡ أَQ<بۡ>صَT<ـٰ>R<رُ>نَا بَلۡ نَحۡنُ قَوۡمX<ࣱ> N<مَّ>سۡحُوR<رُ>J<و>نَ A<۝>D<١٥>",
"وَلَقَQ<دۡ> جَعَلۡنَا فِی ٱX<ل>سَّمَW<اۤ>ءِ بُR<رُ>وجX<ࣰ>ا N<وَ>زَیَّN<نَّ>T<ـٰ>هَا لِX<ل>N<نَّ>T<ـٰ>ظِرِJ<ی>نَ A<۝>D<١٦>",
"وَحَفِظۡنَT<ـٰ>هَا مِN<ن> كُلِّ شَیۡطَT<ـٰ>نX<ࣲ> R<رَّ>جِJ<ی>مٍ A<۝>D<١٧>",
"إِلَّا مَنِ ٱسۡتَR<رَ>قَ ٱX<ل>سَّمۡعَ فَأَتۡبَعَهُT<ۥ> شِهَابX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٨>",
"وَX<ٱ>لۡأَR<رۡ>ضَ مَدَQ<دۡ>نَT<ـٰ>هَا وَأَلۡقَیۡنَا فِیهَا R<رَ>وَT< ٰ>سِیَ وَأَX<ن>N<ۢ>بَتۡنَا فِیهَا مِN<ن> كُلِّ شَیۡءX<ࣲ> N<مَّ>وۡزُJ<و>نࣲ A<۝>D<١٩>",
"وَجَعَلۡنَا لَكُمۡ فِیهَا مَعَT<ـٰ>یِشَ وَمَX<ن> لَّسۡتُمۡ لَهُT<ۥ> بِR<رَ>T< ٰ>زِقِJ<ی>نَ A<۝>D<٢٠>",
"وَإِX<ن> N<مِّ>N<ن> شَیۡءٍ إِلَّا عِN<ن>دَنَا خَزَW<اۤ>ئِنُهُT<ۥ> وَمَا نُنَزِّلُهُW<ۥۤ> إِلَّا بِقَدَرX<ࣲ> N<مَّ>عۡلُJ<و>مࣲ A<۝>D<٢١>",
"وَأَR<رۡ>سَلۡنَا ٱX<ل>رِّیَT<ـٰ>حَ لَوَT< ٰ>قِحَ فَأَN<ن>زَلۡنَا مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَسۡقَیۡنَT<ـٰ>كُمُوهُ وَمَW<اۤ> أَN<ن>تُمۡ لَهُT<ۥ> بِخَT<ـٰ>زِنِJ<ی>نَ A<۝>D<٢٢>",
"وَإِN<نَّ>ا لَنَحۡنُ نُحۡیِT<ۦ> وَنُمِیتُ وَنَحۡنُ ٱلۡوَT< ٰ>رِثُJ<و>نَ A<۝>D<٢٣>",
"وَلَقَQ<دۡ> عَلِمۡنَا ٱلۡمُسۡتَQ<قۡ>دِمِینَ مِN<ن>كُمۡ وَلَقَQ<دۡ> عَلِمۡنَا ٱلۡمُسۡتَءۡخِرِJ<ی>نَ A<۝>D<٢٤>",
"وَإِN<نَّ> R<رَ>بَّكَ هُوَ یَحۡشُR<رُ>هُمۡۚ إِN<نَّ>هُT<ۥ> حَكِیمٌ عَلِJ<ی>مࣱ A<۝>D<٢٥>",
"وَلَقَQ<دۡ> خَلَQ<قۡ>نَا ٱلۡإِN<ن>سَT<ـٰ>نَ مِN<ن> صَلۡصَT<ـٰ>لX<ࣲ> N<مِّ>نۡ حَمَإX<ࣲ> N<مَّ>سۡنُJ<و>نࣲ A<۝>D<٢٦>",
"وَX<ٱ>لۡجَL<اۤ>N<نَّ> خَلَQ<قۡ>نَT<ـٰ>هُ مِN<ن> قَQ<بۡ>لُ مِن N<نَّ>ارِ ٱX<ل>سَّمُJ<و>مِ A<۝>D<٢٧>",
"وَإِذۡ قَالَ R<رَ>بُّكَ لِلۡمَلَW<ـٰۤ>ئِكَةِ إِN<نِّ>ی خَT<ـٰ>لِقُN<ۢ> بَشَR<ر>X<ࣰ>ا N<مِّ>N<ن> صَلۡصَT<ـٰ>لX<ࣲ> N<مِّ>نۡ حَمَإX<ࣲ> N<مَّ>سۡنُJ<و>نࣲ A<۝>D<٢٨>",
"فَإِذَا سَوَّیۡتُهُT<ۥ> وَنَفَخۡتُ فِیهِ مِX<ن> R<رُّ>وحِی فَقَعُوX<ا۟> لَهُT<ۥ> سَT<ـٰ>جِدِJ<ی>نَ A<۝>D<٢٩>",
"فَسَجَدَ ٱلۡمَلَW<ـٰۤ>ئِكَةُ كُلُّهُمۡ أَQ<جۡ>مَعُJ<و>نَ A<۝>D<٣٠>",
"إِلَّW<اۤ> إِQ<بۡ>لِیسَ أَبَىW<ٰۤ> أَX<ن> N<یَ>كُونَ مَعَ ٱX<ل>سَّT<ـٰ>جِدِJ<ی>نَ A<۝>D<٣١>",
"قَالَ یَW<ـٰۤ>إِQ<بۡ>لِیسُ مَا لَكَ أَلَّا تَكُونَ مَعَ ٱX<ل>سَّT<ـٰ>جِدِJ<ی>نَ A<۝>D<٣٢>",
"قَالَ لَمۡ أَكُX<ن> لِّأَسۡجُدَ لِبَشَرٍ خَلَQ<قۡ>تَهُT<ۥ> مِN<ن> صَلۡصَT<ـٰ>لX<ࣲ> N<مِّ>نۡ حَمَإX<ࣲ> N<مَّ>سۡنُJ<و>نࣲ A<۝>D<٣٣>",
"قَالَ فَX<ٱ>خۡR<رُ>Q<جۡ> مِنۡهَا فَإِN<نَّ>كَ R<رَ>جِJ<ی>مࣱ A<۝>D<٣٤>",
"وَإِN<نَّ> عَلَیۡكَ ٱX<ل>لَّعۡنَةَ إِلَىٰ یَوۡمِ ٱX<ل>دِّJ<ی>نِ A<۝>D<٣٥>",
"قَالَ R<رَ>بِّ فَأَN<ن>ظِرۡنِW<یۤ> إِلَىٰ یَوۡمِ یُQ<بۡ>عَثُJ<و>نَ A<۝>D<٣٦>",
"قَالَ فَإِN<نَّ>كَ مِنَ ٱلۡمُN<ن>ظَرِJ<ی>نَ A<۝>D<٣٧>",
"إِلَىٰ یَوۡمِ ٱلۡوَQ<قۡ>تِ ٱلۡمَعۡلُJ<و>مِ A<۝>D<٣٨>",
"قَالَ R<رَ>بِّ بِمَW<اۤ> أَغۡوَیۡتَنِی لَأُزَیِّنَN<نَّ> لَهُمۡ فِی ٱلۡأَR<رۡ>ضِ وَلَأُغۡوِیَN<نَّ>هُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٣٩>",
"إِلَّا عِبَادَكَ مِنۡهُمُ ٱلۡمُخۡلَصِJ<ی>نَ A<۝>D<٤٠>",
"قَالَ هَT<ـٰ>ذَا صِR<رَ>T< ٰ>طٌ عَلَیَّ مُسۡتَقِJ<ی>مٌ A<۝>D<٤١>",
"إِN<نَّ> عِبَادِی لَیۡسَ لَكَ عَلَیۡهِمۡ سُلۡطَT<ـٰ>نٌ إِلَّا مَنِ ٱتَّبَعَكَ مِنَ ٱلۡغَاوِJ<ی>نَ A<۝>D<٤٢>",
"وَإِN<نَّ> جَهَN<نَّ>مَ لَمَوۡعِدُهُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٤٣>",
"لَهَا سَQ<بۡ>عَةُ أَQ<بۡ>وَT< ٰ>بX<ࣲ> لِّكُلِّ بَابX<ࣲ> N<مِّ>نۡهُمۡ جُزۡءX<ࣱ> N<مَّ>Q<قۡ>سُJ<و>مٌ A<۝>D<٤٤>",
"إِN<نَّ> ٱلۡمُتَّقِینَ فِی جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>عُیُJ<و>نٍ A<۝>D<٤٥>",
"ٱQ<دۡ>خُلُوهَا بِسَلَT<ـٰ>مٍ ءَامِنِJ<ی>نَ A<۝>D<٤٦>",
"وَنَزَعۡنَا مَا فِی صُدُورِهِم N<مِّ>نۡ غِلٍّ إِخۡوَT< ٰ>نًا عَلَىٰ سُR<رُ>رX<ࣲ> N<مُّ>تَقَT<ـٰ>بِلِJ<ی>نَ A<۝>D<٤٧>",
"لَا یَمَسُّهُمۡ فِیهَا نَصَبX<ࣱ> N<وَ>مَا هُم N<مِّ>نۡهَا بِمُخۡR<رَ>جِJ<ی>نَ A<۝>D<٤٨>",
"۞ نَبِّئۡ عِبَادِW<یۤ> أَN<نِّ>W<یۤ> أَنَا ٱلۡغَفُوR<رُ> ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٤٩>",
"وَأَN<نَّ> عَذَابِی هُوَ ٱلۡعَذَابُ ٱلۡأَلِJ<ی>مُ A<۝>D<٥٠>",
"وَنَبِّئۡهُمۡ عَN<ن> ضَیۡفِ إِQ<بۡ>R<رَ>T< ٰ>هِJ<ی>مَ A<۝>D<٥١>",
"إِذۡ دَخَلُوX<ا۟> عَلَیۡهِ فَقَالُوX<ا۟> سَلَT<ـٰ>مN<ࣰ>ا قَالَ إِN<نَّ>ا مِN<ن>كُمۡ وَجِلُJ<و>نَ A<۝>D<٥٢>",
"قَالُوX<ا۟> لَا تَوۡجَلۡ إِN<نَّ>ا نُبَشِّR<رُ>كَ بِغُلَT<ـٰ>مٍ عَلِJ<ی>مࣲ A<۝>D<٥٣>",
"قَالَ أَبَشَّR<رۡ>تُمُونِی عَلَىW<ٰۤ> أَX<ن> N<مَّ>سَّنِیَ ٱلۡكِبَR<رُ> فَبِمَ تُبَشِّR<رُ>J<و>نَ A<۝>D<٥٤>",
"قَالُوX<ا۟> بَشَّR<رۡ>نَT<ـٰ>كَ بِX<ٱ>لۡحَقِّ فَلَا تَكُX<ن> N<مِّ>نَ ٱلۡقَT<ـٰ>نِطِJ<ی>نَ A<۝>D<٥٥>",
"قَالَ وَمَX<ن> N<یَ>Q<قۡ>نَطُ مِX<ن> R<رَّ>حۡمَةِ R<رَ>بِّهِW<ۦۤ> إِلَّا ٱX<ل>ضَّL<اۤ>لُّJ<و>نَ A<۝>D<٥٦>",
"قَالَ فَمَا خَQ<طۡ>بُكُمۡ أَیُّهَا ٱلۡمُR<رۡ>سَلُJ<و>نَ A<۝>D<٥٧>",
"قَالُW<وۤ>X<ا۟> إِN<نَّ>W<اۤ> أُR<رۡ>سِلۡنَW<اۤ> إِلَىٰ قَوۡمX<ࣲ> N<مُّ>Q<جۡ>رِمِJ<ی>نَ A<۝>D<٥٨>",
"إِلَّW<اۤ> ءَالَ لُوطٍ إِN<نَّ>ا لَمُنَجُّوهُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٥٩>",
"إِلَّا ٱمۡR<رَ>أَتَهُT<ۥ> قَدَّR<رۡ>نَW<اۤ> إِN<نَّ>هَا لَمِنَ ٱلۡغَT<ـٰ>بِرِJ<ی>نَ A<۝>D<٦٠>",
"فَلَN<مَّ>ا جَW<اۤ>ءَ ءَالَ لُوطٍ ٱلۡمُR<رۡ>سَلُJ<و>نَ A<۝>D<٦١>",
"قَالَ إِN<نَّ>كُمۡ قَوۡمX<ࣱ> N<مُّ>N<ن>كَR<رُ>J<و>نَ A<۝>D<٦٢>",
"قَالُوX<ا۟> بَلۡ جِئۡنَT<ـٰ>كَ بِمَا كَانُوX<ا۟> فِیهِ یَمۡتَR<رُ>J<و>نَ A<۝>D<٦٣>",
"وَأَتَیۡنَT<ـٰ>كَ بِX<ٱ>لۡحَقِّ وَإِN<نَّ>ا لَصَT<ـٰ>دِقُJ<و>نَ A<۝>D<٦٤>",
"فَأَسۡرِ بِأَهۡلِكَ بِقِQ<طۡ>عX<ࣲ> N<مِّ>نَ ٱلَّیۡلِ وَX<ٱ>تَّبِعۡ أَQ<دۡ>بَT<ـٰ>R<رَ>هُمۡ وَلَا یَلۡتَفِتۡ مِN<ن>كُمۡ أَحَدX<ࣱ> N<وَ>X<ٱ>مۡضُوX<ا۟> حَیۡثُ تُؤۡمَR<رُ>J<و>نَ A<۝>D<٦٥>",
"وَقَضَیۡنَW<اۤ> إِلَیۡهِ ذَT< ٰ>لِكَ ٱلۡأَمۡR<رَ> أَN<نَّ> دَابِR<رَ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ مَQ<قۡ>طُوعX<ࣱ> N<مُّ>صۡبِحِJ<ی>نَ A<۝>D<٦٦>",
"وَجَW<اۤ>ءَ أَهۡلُ ٱلۡمَدِینَةِ یَسۡتَQ<بۡ>شِR<رُ>J<و>نَ A<۝>D<٦٧>",
"قَالَ إِN<نَّ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ ضَیۡفِی فَلَا تَفۡضَحُJ<و>نِ A<۝>D<٦٨>",
"وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَلَا تُخۡزُJ<و>نِ A<۝>D<٦٩>",
"قَالُW<وۤ>X<ا۟> أَوَ لَمۡ نَنۡهَكَ عَنِ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٧٠>",
"قَالَ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ بَنَاتِW<یۤ> إِN<ن> كُN<ن>تُمۡ فَT<ـٰ>عِلِJ<ی>نَ A<۝>D<٧١>",
"لَعَمۡR<رُ>كَ إِN<نَّ>هُمۡ لَفِی سَكۡR<رَ>تِهِمۡ یَعۡمَهُJ<و>نَ A<۝>D<٧٢>",
"فَأَخَذَتۡهُمُ ٱX<ل>صَّیۡحَةُ مُشۡرِقِJ<ی>نَ A<۝>D<٧٣>",
"فَجَعَلۡنَا عَT<ـٰ>لِیَهَا سَافِلَهَا وَأَمۡطَR<رۡ>نَا عَلَیۡهِمۡ حِجَاR<رَ>ةX<ࣰ> N<مِّ>N<ن> سِجِّJ<ی>لٍ A<۝>D<٧٤>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّلۡمُتَوَسِّمِJ<ی>نَ A<۝>D<٧٥>",
"وَإِN<نَّ>هَا لَبِسَبِیلX<ࣲ> N<مُّ>قِJ<ی>مٍ A<۝>D<٧٦>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٧٧>",
"وَإِN<ن> كَانَ أَصۡحَT<ـٰ>بُ ٱلۡأَیۡكَةِ لَظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٧٨>",
"فَX<ٱ>N<ن>تَقَمۡنَا مِنۡهُمۡ وَإِN<نَّ>هُمَا لَبِإِمَامX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٧٩>",
"وَلَقَQ<دۡ> كَذَّبَ أَصۡحَT<ـٰ>بُ ٱلۡحِQ<جۡ>رِ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٨٠>",
"وَءَاتَیۡنَT<ـٰ>هُمۡ ءَایَT<ـٰ>تِنَا فَكَانُوX<ا۟> عَنۡهَا مُعۡرِضِJ<ی>نَ A<۝>D<٨١>",
"وَكَانُوX<ا۟> یَنۡحِتُونَ مِنَ ٱلۡجِبَالِ بُیُوتًا ءَامِنِJ<ی>نَ A<۝>D<٨٢>",
"فَأَخَذَتۡهُمُ ٱX<ل>صَّیۡحَةُ مُصۡبِحِJ<ی>نَ A<۝>D<٨٣>",
"فَمَW<اۤ> أَغۡنَىٰ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<٨٤>",
"وَمَا خَلَQ<قۡ>نَا ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَمَا بَیۡنَهُمَW<اۤ> إِلَّا بِX<ٱ>لۡحَقِّۗ وَإِN<نَّ> ٱX<ل>سَّاعَةَ لَءَاتِیَةN<ࣱ>ۖ فَX<ٱ>صۡفَحِ ٱX<ل>صَّفۡحَ ٱلۡجَمِJ<ی>لَ A<۝>D<٨٥>",
"إِN<نَّ> R<رَ>بَّكَ هُوَ ٱلۡخَلَّT<ـٰ>قُ ٱلۡعَلِJ<ی>مُ A<۝>D<٨٦>",
"وَلَقَQ<دۡ> ءَاتَیۡنَT<ـٰ>كَ سَQ<بۡ>عX<ࣰ>ا N<مِّ>نَ ٱلۡمَثَانِی وَX<ٱ>لۡقُR<رۡ>ءَانَ ٱلۡعَظِJ<ی>مَ A<۝>D<٨٧>",
"لَا تَمُدَّN<نَّ> عَیۡنَیۡكَ إِلَىٰ مَا مَتَّعۡنَا بِهِW<ۦۤ> أَزۡوَT< ٰ>جX<ࣰ>ا N<مِّ>نۡهُمۡ وَلَا تَحۡزَنۡ عَلَیۡهِمۡ وَX<ٱ>خۡفِضۡ جَنَاحَكَ لِلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٨٨>",
"وَقُلۡ إِN<نِّ>W<یۤ> أَنَا ٱX<ل>N<نَّ>ذِیR<رُ> ٱلۡمُبِJ<ی>نُ A<۝>D<٨٩>",
"كَمَW<اۤ> أَN<ن>زَلۡنَا عَلَى ٱلۡمُQ<قۡ>تَسِمِJ<ی>نَ A<۝>D<٩٠>",
"ٱلَّذِینَ جَعَلُوX<ا۟> ٱلۡقُR<رۡ>ءَانَ عِضِJ<ی>نَ A<۝>D<٩١>",
"فَوَR<رَ>بِّكَ لَنَسۡءَلَN<نَّ>هُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٩٢>",
"عَN<مَّ>ا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٩٣>",
"فَX<ٱ>صۡدَعۡ بِمَا تُؤۡمَR<رُ> وَأَعۡرِضۡ عَنِ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<٩٤>",
"إِN<نَّ>ا كَفَیۡنَT<ـٰ>كَ ٱلۡمُسۡتَهۡزِءِJ<ی>نَ A<۝>D<٩٥>",
"ٱلَّذِینَ یَQ<جۡ>عَلُونَ مَعَ ٱللَّهِ إِلَT<ـٰ>هًا ءَاخَR<رَ>ۚ فَسَوۡفَ یَعۡلَمُJ<و>نَ A<۝>D<٩٦>",
"وَلَقَQ<دۡ> نَعۡلَمُ أَN<نَّ>كَ یَضِیقُ صَQ<دۡ>R<رُ>كَ بِمَا یَقُولُJ<و>نَ A<۝>D<٩٧>",
"فَسَبِّحۡ بِحَمۡدِ R<رَ>بِّكَ وَكُX<ن> N<مِّ>نَ ٱX<ل>سَّT<ـٰ>جِدِJ<ی>نَ A<۝>D<٩٨>",
"وَX<ٱ>عۡبُQ<دۡ> R<رَ>بَّكَ حَتَّىٰ یَأۡتِیَكَ ٱلۡیَقِJ<ی>نُ A<۝>D<٩٩>",
"#۞ أَتَىW<ٰۤ> أَمۡR<رُ> ٱللَّهِ فَلَا تَسۡتَعۡجِلُوهُۚ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ> وَتَعَT<ـٰ>لَىٰ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<١>",
"یُنَزِّلُ ٱلۡمَلَW<ـٰۤ>ئِكَةَ بِX<ٱ>X<ل>R<رُّ>وحِ مِنۡ أَمۡرِهِT<ۦ> عَلَىٰ مَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِW<ۦۤ> أَنۡ أَN<ن>ذِR<رُ>W<وۤ>X<ا۟> أَN<نَّ>هُT<ۥ> لَW<اۤ> إِلَT<ـٰ>هَ إِلَّW<اۤ> أَنَا۠ فَX<ٱ>تَّقُJ<و>نِ A<۝>D<٢>",
"خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ بِX<ٱ>لۡحَقِّۚ تَعَT<ـٰ>لَىٰ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٣>",
"خَلَقَ ٱلۡإِN<ن>سَT<ـٰ>نَ مِن N<نُّ>Q<طۡ>فَةN<ࣲ> فَإِذَا هُوَ خَصِیمX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٤>",
"وَX<ٱ>لۡأَنۡعَT<ـٰ>مَ خَلَقَهَاۖ لَكُمۡ فِیهَا دِفۡءX<ࣱ> N<وَ>مَنَT<ـٰ>فِعُ وَمِنۡهَا تَأۡكُلُJ<و>نَ A<۝>D<٥>",
"وَلَكُمۡ فِیهَا جَمَالٌ حِینَ تُرِیحُونَ وَحِینَ تَسۡR<رَ>حُJ<و>نَ A<۝>D<٦>",
"وَتَحۡمِلُ أَثۡقَالَكُمۡ إِلَىٰ بَلَدX<ࣲ> لَّمۡ تَكُونُوX<ا۟> بَT<ـٰ>لِغِیهِ إِلَّا بِشِقِّ ٱلۡأَN<ن>فُسِۚ إِN<نَّ> R<رَ>بَّكُمۡ لَR<رَ>ءُوفX<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٧>",
"وَX<ٱ>لۡخَیۡلَ وَX<ٱ>لۡبِغَالَ وَX<ٱ>لۡحَمِیR<رَ> لِتَR<رۡ>كَبُوهَا وَزِینَةX<ࣰ>ۚ N<وَ>یَخۡلُقُ مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٨>",
"وَعَلَى ٱللَّهِ قَصۡدُ ٱX<ل>سَّبِیلِ وَمِنۡهَا جَW<اۤ>ئِR<ر>X<ࣱ>ۚ N<وَ>لَوۡ شَW<اۤ>ءَ لَهَدَX<ى>T<ٰ>كُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٩>",
"هُوَ ٱلَّذِW<یۤ> أَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءX<ࣰ>ۖ لَّكُم N<مِّ>نۡهُ شَR<رَ>ابX<ࣱ> N<وَ>مِنۡهُ شَجَR<ر>N<ࣱ> فِیهِ تُسِیمُJ<و>نَ A<۝>D<١٠>",
"یُX<ن>N<ۢ>بِتُ لَكُN<م> بِهِ ٱX<ل>زَّR<رۡ>عَ وَX<ٱ>X<ل>زَّیۡتُونَ وَX<ٱ>X<ل>N<نَّ>خِیلَ وَX<ٱ>لۡأَعۡنَT<ـٰ>بَ وَمِN<ن> كُلِّ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یَ>تَفَكَّR<رُ>J<و>نَ A<۝>D<١١>",
"وَسَخَّR<رَ> لَكُمُ ٱلَّیۡلَ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ> وَX<ٱ>X<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ>ۖ وَX<ٱ>X<ل>N<نُّ>جُومُ مُسَخَّR<رَ>T< ٰ>تُN<ۢ> بِأَمۡرِهِW<ۦۤ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>عۡقِلُJ<و>نَ A<۝>D<١٢>",
"وَمَا ذَR<رَ>أَ لَكُمۡ فِی ٱلۡأَR<رۡ>ضِ مُخۡتَلِفًا أَلۡوَT< ٰ>نُهُW<ۥۤ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یَ>ذَّكَّR<رُ>J<و>نَ A<۝>D<١٣>",
"وَهُوَ ٱلَّذِی سَخَّR<رَ> ٱلۡبَحۡR<رَ> لِتَأۡكُلُوX<ا۟> مِنۡهُ لَحۡمN<ࣰ>ا طَرِیّX<ࣰ>ا N<وَ>تَسۡتَخۡرِجُوX<ا۟> مِنۡهُ حِلۡیَةN<ࣰ> تَلۡبَسُونَهَاۖ وَتَR<رَ>ى ٱلۡفُلۡكَ مَوَاخِR<رَ> فِیهِ وَلِتَQ<بۡ>تَغُوX<ا۟> مِN<ن> فَضۡلِهِT<ۦ> وَلَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<١٤>",
"وَأَلۡقَىٰ فِی ٱلۡأَR<رۡ>ضِ R<رَ>وَT< ٰ>سِیَ أَN<ن> تَمِیدَ بِكُمۡ وَأَنۡهَT<ـٰ>R<ر>X<ࣰ>ا N<وَ>سُبُلX<ࣰ>ا لَّعَلَّكُمۡ تَهۡتَدُJ<و>نَ A<۝>D<١٥>",
"وَعَلَT<ـٰ>مَT<ـٰ>تX<ࣲ>ۚ N<وَ>بِX<ٱ>X<ل>N<نَّ>Q<جۡ>مِ هُمۡ یَهۡتَدُJ<و>نَ A<۝>D<١٦>",
"أَفَمَX<ن> N<یَ>خۡلُقُ كَمَX<ن> لَّا یَخۡلُقُۚ أَفَلَا تَذَكَّR<رُ>J<و>نَ A<۝>D<١٧>",
"وَإِN<ن> تَعُدُّوX<ا۟> نِعۡمَةَ ٱللَّهِ لَا تُحۡصُوهَW<اۤ>ۗ إِN<نَّ> ٱللَّهَ لَغَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٨>",
"وَٱللَّهُ یَعۡلَمُ مَا تُسِR<رُّ>ونَ وَمَا تُعۡلِنُJ<و>نَ A<۝>D<١٩>",
"وَX<ٱ>لَّذِینَ یَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ لَا یَخۡلُقُونَ شَیۡءX<ࣰ>ا N<وَ>هُمۡ یُخۡلَقُJ<و>نَ A<۝>D<٢٠>",
"أَمۡوَT< ٰ>تٌ غَیۡR<رُ> أَحۡیَW<اۤ>ءX<ࣲ>ۖ N<وَ>مَا یَشۡعُR<رُ>ونَ أَیَّانَ یُQ<بۡ>عَثُJ<و>نَ A<۝>D<٢١>",
"إِلَT<ـٰ>هُكُمۡ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدN<ࣱ>ۚ فَX<ٱ>لَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ قُلُوبُهُم N<مُّ>N<ن>كِR<رَ>ةX<ࣱ> N<وَ>هُم N<مُّ>سۡتَكۡبِR<رُ>J<و>نَ A<۝>D<٢٢>",
"لَا جَR<رَ>مَ أَN<نَّ> ٱللَّهَ یَعۡلَمُ مَا یُسِR<رُّ>ونَ وَمَا یُعۡلِنُونَۚ إِN<نَّ>هُT<ۥ> لَا یُحِبُّ ٱلۡمُسۡتَكۡبِرِJ<ی>نَ A<۝>D<٢٣>",
"وَإِذَا قِیلَ لَهُم N<مَّ>اذَW<اۤ> أَN<ن>زَلَ R<رَ>بُّكُمۡ قَالُW<وۤ>X<ا۟> أَسَT<ـٰ>طِیR<رُ> ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٢٤>",
"لِیَحۡمِلُW<وۤ>X<ا۟> أَوۡزَاR<رَ>هُمۡ كَامِلَةX<ࣰ> N<یَ>وۡمَ ٱلۡقِیَT<ـٰ>مَةِ وَمِنۡ أَوۡزَارِ ٱلَّذِینَ یُضِلُّونَهُN<م> بِغَیۡرِ عِلۡمٍۗ أَلَا سَW<اۤ>ءَ مَا یَزِR<رُ>J<و>نَ A<۝>D<٢٥>",
"قَQ<دۡ> مَكَR<رَ> ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ فَأَتَى ٱللَّهُ بُنۡیَT<ـٰ>نَهُم N<مِّ>نَ ٱلۡقَوَاعِدِ فَخَR<رَّ> عَلَیۡهِمُ ٱX<ل>سَّQ<قۡ>فُ مِN<ن> فَوۡقِهِمۡ وَأَتَX<ى>T<ٰ>هُمُ ٱلۡعَذَابُ مِنۡ حَیۡثُ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٢٦>",
"ثُN<مَّ> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ یُخۡزِیهِمۡ وَیَقُولُ أَیۡنَ شُR<رَ>كَW<اۤ>ءِیَ ٱلَّذِینَ كُN<ن>تُمۡ تُشَL<ـٰۤ>قُّونَ فِیهِمۡۚ قَالَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡعِلۡمَ إِN<نَّ> ٱلۡخِزۡیَ ٱلۡیَوۡمَ وَX<ٱ>X<ل>سُّW<وۤ>ءَ عَلَى ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٢٧>",
"ٱلَّذِینَ تَتَوَفَّX<ى>T<ٰ>هُمُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ ظَالِمِW<یۤ> أَN<ن>فُسِهِمۡۖ فَأَلۡقَوُX<ا۟> ٱX<ل>سَّلَمَ مَا كُN<نَّ>ا نَعۡمَلُ مِN<ن> سُW<وۤ>ءِN<ۭ>ۚ بَلَىW<ٰۤ>ۚ إِN<نَّ> ٱللَّهَ عَلِیمُN<ۢ> بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٢٨>",
"فَX<ٱ>Q<دۡ>خُلُW<وۤ>X<ا۟> أَQ<بۡ>وَT< ٰ>بَ جَهَN<نَّ>مَ خَT<ـٰ>لِدِینَ فِیهَاۖ فَلَبِئۡسَ مَثۡوَى ٱلۡمُتَكَبِّرِJ<ی>نَ A<۝>D<٢٩>",
"۞ وَقِیلَ لِلَّذِینَ ٱتَّقَوۡX<ا۟> مَاذَW<اۤ> أَN<ن>زَلَ R<رَ>بُّكُمۡۚ قَالُوX<ا۟> خَیۡR<ر>X<ࣰ>اۗ لِّلَّذِینَ أَحۡسَنُوX<ا۟> فِی هَT<ـٰ>ذِهِ ٱX<ل>دُّنۡیَا حَسَنَةX<ࣱ>ۚ N<وَ>لَدَاR<رُ> ٱلۡءَاخِR<رَ>ةِ خَیۡR<ر>X<ࣱ>ۚ N<وَ>لَنِعۡمَ دَاR<رُ> ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٣٠>",
"جَN<نَّ>T<ـٰ>تُ عَQ<دۡ>نX<ࣲ> N<یَ>Q<دۡ>خُلُونَهَا تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ>ۖ لَهُمۡ فِیهَا مَا یَشَW<اۤ>ءُونَۚ كَذَT< ٰ>لِكَ یَQ<جۡ>زِی ٱللَّهُ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٣١>",
"ٱلَّذِینَ تَتَوَفَّX<ى>T<ٰ>هُمُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ طَیِّبِینَ یَقُولُونَ سَلَT<ـٰ>مٌ عَلَیۡكُمُ ٱQ<دۡ>خُلُوX<ا۟> ٱلۡجَN<نَّ>ةَ بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٣٢>",
"هَلۡ یَN<ن>ظُR<رُ>ونَ إِلَّW<اۤ> أَN<ن> تَأۡتِیَهُمُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ أَوۡ یَأۡتِیَ أَمۡR<رُ> R<رَ>بِّكَۚ كَذَT< ٰ>لِكَ فَعَلَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۚ وَمَا ظَلَمَهُمُ ٱللَّهُ وَلَT<ـٰ>كِN<ن> كَانُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ یَظۡلِمُJ<و>نَ A<۝>D<٣٣>",
"فَأَصَابَهُمۡ سَیِّءَاتُ مَا عَمِلُوX<ا۟> وَحَاقَ بِهِم N<مَّ>ا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٣٤>",
"وَقَالَ ٱلَّذِینَ أَشۡR<رَ>كُوX<ا۟> لَوۡ شَW<اۤ>ءَ ٱللَّهُ مَا عَبَQ<دۡ>نَا مِN<ن> دُونِهِT<ۦ> مِN<ن> شَیۡءX<ࣲ> N<نَّ>حۡنُ وَلَW<اۤ> ءَابَW<اۤ>ؤُنَا وَلَا حَR<رَّ>مۡنَا مِN<ن> دُونِهِT<ۦ> مِN<ن> شَیۡءN<ࣲ>ۚ كَذَT< ٰ>لِكَ فَعَلَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۚ فَهَلۡ عَلَى ٱX<ل>R<رُّ>سُلِ إِلَّا ٱلۡبَلَT<ـٰ>غُ ٱلۡمُبِJ<ی>نُ A<۝>D<٣٥>",
"وَلَقَQ<دۡ> بَعَثۡنَا فِی كُلِّ أُN<مَّ>ةX<ࣲ> R<رَّ>سُولًا أَنِ ٱعۡبُدُوX<ا۟> ٱللَّهَ وَX<ٱ>Q<جۡ>تَنِبُوX<ا۟> ٱX<ل>طَّT<ـٰ>غُوتَۖ فَمِنۡهُم N<مَّ>نۡ هَدَى ٱللَّهُ وَمِنۡهُم N<مَّ>نۡ حَقَّتۡ عَلَیۡهِ ٱX<ل>ضَّلَT<ـٰ>لَةُۚ فَسِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَX<ٱ>N<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُكَذِّبِJ<ی>نَ A<۝>D<٣٦>",
"إِN<ن> تَحۡرِصۡ عَلَىٰ هُدَX<ى>T<ٰ>هُمۡ فَإِN<نَّ> ٱللَّهَ لَا یَهۡدِی مَX<ن> N<یُ>ضِلُّۖ وَمَا لَهُم N<مِّ>ن N<نَّ>T<ـٰ>صِرِJ<ی>نَ A<۝>D<٣٧>",
"وَأَQ<قۡ>سَمُوX<ا۟> بِٱللَّهِ جَهۡدَ أَیۡمَT<ـٰ>نِهِمۡ لَا یَQ<بۡ>عَثُ ٱللَّهُ مَX<ن> N<یَ>مُوتُۚ بَلَىٰ وَعۡدًا عَلَیۡهِ حَقّX<ࣰ>ا N<وَ>لَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٣٨>",
"لِیُبَیِّنَ لَهُمُ ٱلَّذِی یَخۡتَلِفُونَ فِیهِ وَلِیَعۡلَمَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَN<نَّ>هُمۡ كَانُوX<ا۟> كَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٣٩>",
"إِN<نَّ>مَا قَوۡلُنَا لِشَیۡءٍ إِذَW<اۤ> أَR<رَ>Q<دۡ>نَT<ـٰ>هُ أَن N<نَّ>قُولَ لَهُT<ۥ> كُN<ن> فَیَكُJ<و>نُ A<۝>D<٤٠>",
"وَX<ٱ>لَّذِینَ هَاجَR<رُ>وX<ا۟> فِی ٱللَّهِ مِX<ن>N<ۢ> بَعۡدِ مَا ظُلِمُوX<ا۟> لَنُبَوِّئَN<نَّ>هُمۡ فِی ٱX<ل>دُّنۡیَا حَسَنَةX<ࣰ>ۖ N<وَ>لَأَQ<جۡ>R<رُ> ٱلۡءَاخِR<رَ>ةِ أَكۡبَR<رُ>ۚ لَوۡ كَانُوX<ا۟> یَعۡلَمُJ<و>نَ A<۝>D<٤١>",
"ٱلَّذِینَ صَبَR<رُ>وX<ا۟> وَعَلَىٰ R<رَ>بِّهِمۡ یَتَوَكَّلُJ<و>نَ A<۝>D<٤٢>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا مِN<ن> قَQ<بۡ>لِكَ إِلَّا رِجَالX<ࣰ>ا N<نُّ>وحِW<یۤ> إِلَیۡهِمۡۖ فَسۡءَلُW<وۤ>X<ا۟> أَهۡلَ ٱX<ل>ذِّكۡرِ إِN<ن> كُN<ن>تُمۡ لَا تَعۡلَمُJ<و>نَ A<۝>D<٤٣>",
"بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ وَX<ٱ>X<ل>زُّبُرِۗ وَأَN<ن>زَلۡنَW<اۤ> إِلَیۡكَ ٱX<ل>ذِّكۡR<رَ> لِتُبَیِّنَ لِX<ل>N<نَّ>اسِ مَا نُزِّلَ إِلَیۡهِمۡ وَلَعَلَّهُمۡ یَتَفَكَّR<رُ>J<و>نَ A<۝>D<٤٤>",
"أَفَأَمِنَ ٱلَّذِینَ مَكَR<رُ>وX<ا۟> ٱX<ل>سَّیِّءَاتِ أَX<ن> N<یَ>خۡسِفَ ٱللَّهُ بِهِمُ ٱلۡأَR<رۡ>ضَ أَوۡ یَأۡتِیَهُمُ ٱلۡعَذَابُ مِنۡ حَیۡثُ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٤٥>",
"أَوۡ یَأۡخُذَهُمۡ فِی تَقَلُّبِهِمۡ فَمَا هُN<م> بِمُعۡجِزِJ<ی>نَ A<۝>D<٤٦>",
"أَوۡ یَأۡخُذَهُمۡ عَلَىٰ تَخَوُّفN<ࣲ> فَإِN<نَّ> R<رَ>بَّكُمۡ لَR<رَ>ءُوفX<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<٤٧>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> إِلَىٰ مَا خَلَقَ ٱللَّهُ مِN<ن> شَیۡءX<ࣲ> N<یَ>تَفَیَّؤُX<ا۟> ظِلَT<ـٰ>لُهُT<ۥ> عَنِ ٱلۡیَمِینِ وَX<ٱ>X<ل>شَّمَW<اۤ>ئِلِ سُجَّدX<ࣰ>ا لِّلَّهِ وَهُمۡ دَT< ٰ>خِR<رُ>J<و>نَ A<۝>D<٤٨>",
"وَ̅لِ̅لَّ̅هِ̅ ̅یَ̅سۡ̅جُ̅دُ̅ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِ مِN<ن> دَL<اۤ>بَّةX<ࣲ> N<وَ>X<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ وَهُمۡ لَا یَسۡتَكۡبِR<رُ>J<و>نَ A<۝>D<٤٩>",
"یَخَافُونَ R<رَ>بَّهُم N<مِّ>N<ن> فَوۡقِهِمۡ وَیَفۡعَلُونَ مَا یُؤۡمَR<رُ>J<و>نَ A<۝>D<٥٠> ۩",
"۞ وَقَالَ ٱللَّهُ لَا تَتَّخِذُW<وۤ>X<ا۟> إِلَT<ـٰ>هَیۡنِ ٱثۡنَیۡنِۖ إِN<نَّ>مَا هُوَ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدN<ࣱ> فَإِیَّT<ـٰ>یَ فَX<ٱ>R<رۡ>هَبُJ<و>نِ A<۝>D<٥١>",
"وَلَهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَلَهُ ٱX<ل>دِّینُ وَاصِبًاۚ أَفَغَیۡR<رَ> ٱللَّهِ تَتَّقُJ<و>نَ A<۝>D<٥٢>",
"وَمَا بِكُم N<مِّ>ن N<نِّ>عۡمَةN<ࣲ> فَمِنَ ٱللَّهِۖ ثُN<مَّ> إِذَا مَسَّكُمُ ٱX<ل>ضُّR<رُّ> فَإِلَیۡهِ تَQ<جۡ>ءَR<رُ>J<و>نَ A<۝>D<٥٣>",
"ثُN<مَّ> إِذَا كَشَفَ ٱX<ل>ضُّR<رَّ> عَN<ن>كُمۡ إِذَا فَرِیقX<ࣱ> N<مِّ>N<ن>كُN<م> بِR<رَ>بِّهِمۡ یُشۡرِكُJ<و>نَ A<۝>D<٥٤>",
"لِیَكۡفُR<رُ>وX<ا۟> بِمَW<اۤ> ءَاتَیۡنَT<ـٰ>هُمۡۚ فَتَمَتَّعُوX<ا۟> فَسَوۡفَ تَعۡلَمُJ<و>نَ A<۝>D<٥٥>",
"وَیَQ<جۡ>عَلُونَ لِمَا لَا یَعۡلَمُونَ نَصِیبX<ࣰ>ا N<مِّ>N<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡۗ تَٱللَّهِ لَتُسۡءَلُN<نَّ> عَN<مَّ>ا كُN<ن>تُمۡ تَفۡتَR<رُ>J<و>نَ A<۝>D<٥٦>",
"وَیَQ<جۡ>عَلُونَ لِلَّهِ ٱلۡبَنَT<ـٰ>تِ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ> وَلَهُم N<مَّ>ا یَشۡتَهُJ<و>نَ A<۝>D<٥٧>",
"وَإِذَا بُشِّR<رَ> أَحَدُهُN<م> بِX<ٱ>لۡأُN<ن>ثَىٰ ظَلَّ وَQ<جۡ>هُهُT<ۥ> مُسۡوَدّX<ࣰ>ا N<وَ>هُوَ كَظِJ<ی>مࣱ A<۝>D<٥٨>",
"یَتَوَT< ٰ>R<رَ>ىٰ مِنَ ٱلۡقَوۡمِ مِN<ن> سُW<وۤ>ءِ مَا بُشِّR<رَ> بِهِW<ۦۤ>ۚ أَیُمۡسِكُهُT<ۥ> عَلَىٰ هُونٍ أَمۡ یَدُسُّهُT<ۥ> فِی ٱX<ل>تُّR<رَ>ابِۗ أَلَا سَW<اۤ>ءَ مَا یَحۡكُمُJ<و>نَ A<۝>D<٥٩>",
"لِلَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ مَثَلُ ٱX<ل>سَّوۡءِۖ وَلِلَّهِ ٱلۡمَثَلُ ٱلۡأَعۡلَىٰۚ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٦٠>",
"وَلَوۡ یُؤَاخِذُ ٱللَّهُ ٱX<ل>N<نَّ>اسَ بِظُلۡمِهِم N<مَّ>ا تَR<رَ>كَ عَلَیۡهَا مِN<ن> دَL<اۤ>بَّةX<ࣲ> N<وَ>لَT<ـٰ>كِX<ن> N<یُ>ؤَخِّR<رُ>هُمۡ إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>N<ࣰ>ىۖ فَإِذَا جَW<اۤ>ءَ أَجَلُهُمۡ لَا یَسۡتَءۡخِR<رُ>ونَ سَاعَةX<ࣰ> N<وَ>لَا یَسۡتَQ<قۡ>دِمُJ<و>نَ A<۝>D<٦١>",
"وَیَQ<جۡ>عَلُونَ لِلَّهِ مَا یَكۡR<رَ>هُونَۚ وَتَصِفُ أَلۡسِنَتُهُمُ ٱلۡكَذِبَ أَN<نَّ> لَهُمُ ٱلۡحُسۡنَىٰۚ لَا جَR<رَ>مَ أَN<نَّ> لَهُمُ ٱX<ل>N<نَّ>اR<رَ> وَأَN<نَّ>هُم N<مُّ>فۡR<رَ>طُJ<و>نَ A<۝>D<٦٢>",
"تَٱللَّهِ لَقَQ<دۡ> أَR<رۡ>سَلۡنَW<اۤ> إِلَىW<ٰۤ> أُمَمX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِكَ فَزَیَّنَ لَهُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ أَعۡمَT<ـٰ>لَهُمۡ فَهُوَ وَلِیُّهُمُ ٱلۡیَوۡمَ وَلَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٦٣>",
"وَمَW<اۤ> أَN<ن>زَلۡنَا عَلَیۡكَ ٱلۡكِتَT<ـٰ>بَ إِلَّا لِتُبَیِّنَ لَهُمُ ٱلَّذِی ٱخۡتَلَفُوX<ا۟> فِیهِ وَهُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٦٤>",
"وَٱللَّهُ أَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَحۡیَا بِهِ ٱلۡأَR<رۡ>ضَ بَعۡدَ مَوۡتِهَW<اۤ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یَ>سۡمَعُJ<و>نَ A<۝>D<٦٥>",
"وَإِN<نَّ> لَكُمۡ فِی ٱلۡأَنۡعَT<ـٰ>مِ لَعِQ<بۡ>R<رَ>ةX<ࣰ>ۖ N<نُّ>سۡقِیكُم N<مِّ>N<مَّ>ا فِی بُطُونِهِT<ۦ> مِX<ن>N<ۢ> بَیۡنِ فَR<رۡ>ثX<ࣲ> N<وَ>دَمX<ࣲ> لَّبَنًا خَالِصN<ࣰ>ا سَW<اۤ>ئِغX<ࣰ>ا لِّX<ل>شَّT<ـٰ>رِبِJ<ی>نَ A<۝>D<٦٦>",
"وَمِN<ن> ثَمَR<رَ>T< ٰ>تِ ٱX<ل>N<نَّ>خِیلِ وَX<ٱ>لۡأَعۡنَT<ـٰ>بِ تَتَّخِذُونَ مِنۡهُ سَكَR<ر>X<ࣰ>ا N<وَ>رِزۡقًا حَسَنًاۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یَ>عۡقِلُJ<و>نَ A<۝>D<٦٧>",
"وَأَوۡحَىٰ R<رَ>بُّكَ إِلَى ٱX<ل>N<نَّ>حۡلِ أَنِ ٱتَّخِذِی مِنَ ٱلۡجِبَالِ بُیُوتX<ࣰ>ا N<وَ>مِنَ ٱX<ل>شَّجَرِ وَمِN<مَّ>ا یَعۡرِشُJ<و>نَ A<۝>D<٦٨>",
"ثُN<مَّ> كُلِی مِN<ن> كُلِّ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِ فَX<ٱ>سۡلُكِی سُبُلَ R<رَ>بِّكِ ذُلُلX<ࣰ>اۚ N<یَ>خۡR<رُ>جُ مِX<ن>N<ۢ> بُطُونِهَا شَR<رَ>ابX<ࣱ> N<مُّ>خۡتَلِفٌ أَلۡوَT< ٰ>نُهُT<ۥ> فِیهِ شِفَW<اۤ>ءX<ࣱ> لِّX<ل>N<نَّ>اسِۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یَ>تَفَكَّR<رُ>J<و>نَ A<۝>D<٦٩>",
"وَٱللَّهُ خَلَقَكُمۡ ثُN<مَّ> یَتَوَفَّX<ى>T<ٰ>كُمۡۚ وَمِN<ن>كُم N<مَّ>X<ن> N<یُ>R<رَ>دُّ إِلَىW<ٰۤ> أَR<رۡ>ذَلِ ٱلۡعُمُرِ لِكَیۡ لَا یَعۡلَمَ بَعۡدَ عِلۡمN<ࣲ> شَیۡءًاۚ إِN<نَّ> ٱللَّهَ عَلِیمN<ࣱ> قَدِJ<ی>رࣱ A<۝>D<٧٠>",
"وَٱللَّهُ فَضَّلَ بَعۡضَكُمۡ عَلَىٰ بَعۡضN<ࣲ> فِی ٱX<ل>رِّزۡقِۚ فَمَا ٱلَّذِینَ فُضِّلُوX<ا۟> بِR<رَ>L<اۤ>دِّی رِزۡقِهِمۡ عَلَىٰ مَا مَلَكَتۡ أَیۡمَT<ـٰ>نُهُمۡ فَهُمۡ فِیهِ سَوَW<اۤ>ءٌۚ أَفَبِنِعۡمَةِ ٱللَّهِ یَQ<جۡ>حَدُJ<و>نَ A<۝>D<٧١>",
"وَٱللَّهُ جَعَلَ لَكُم N<مِّ>نۡ أَN<ن>فُسِكُمۡ أَزۡوَT< ٰ>جX<ࣰ>ا N<وَ>جَعَلَ لَكُم N<مِّ>نۡ أَزۡوَT< ٰ>جِكُN<م> بَنِینَ وَحَفَدَةX<ࣰ> N<وَ>R<رَ>زَقَكُم N<مِّ>نَ ٱX<ل>طَّیِّبَT<ـٰ>تِۚ أَفَبِX<ٱ>لۡبَT<ـٰ>طِلِ یُؤۡمِنُونَ وَبِنِعۡمَتِ ٱللَّهِ هُمۡ یَكۡفُR<رُ>J<و>نَ A<۝>D<٧٢>",
"وَیَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ مَا لَا یَمۡلِكُ لَهُمۡ رِزۡقX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ شَیۡءX<ࣰ>ا N<وَ>لَا یَسۡتَطِیعُJ<و>نَ A<۝>D<٧٣>",
"فَلَا تَضۡرِبُوX<ا۟> لِلَّهِ ٱلۡأَمۡثَالَۚ إِN<نَّ> ٱللَّهَ یَعۡلَمُ وَأَN<ن>تُمۡ لَا تَعۡلَمُJ<و>نَ A<۝>D<٧٤>",
"۞ ضَR<رَ>بَ ٱللَّهُ مَثَلًا عَQ<بۡ>دX<ࣰ>ا N<مَّ>مۡلُوكX<ࣰ>ا لَّا یَQ<قۡ>دِR<رُ> عَلَىٰ شَیۡءX<ࣲ> N<وَ>مَX<ن> R<رَّ>زَQ<قۡ>نَT<ـٰ>هُ مِN<نَّ>ا رِزۡقًا حَسَنN<ࣰ>ا فَهُوَ یُN<ن>فِقُ مِنۡهُ سِR<رّ>X<ࣰ>ا N<وَ>جَهۡR<ر>ًاۖ هَلۡ یَسۡتَوُT<ۥ>نَۚ ٱلۡحَمۡدُ لِلَّهِۚ بَلۡ أَكۡثَR<رُ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٧٥>",
"وَضَR<رَ>بَ ٱللَّهُ مَثَلX<ࣰ>ا R<رَّ>جُلَیۡنِ أَحَدُهُمَW<اۤ> أَQ<بۡ>كَمُ لَا یَQ<قۡ>دِR<رُ> عَلَىٰ شَیۡءX<ࣲ> N<وَ>هُوَ كَلٌّ عَلَىٰ مَوۡلَX<ى>T<ٰ>هُ أَیۡنَمَا یُوَجِّX<ه>هُّ لَا یَأۡتِ بِخَیۡرٍ هَلۡ یَسۡتَوِی هُوَ وَمَX<ن> N<یَ>أۡمُR<رُ> بِX<ٱ>لۡعَQ<دۡ>لِ وَهُوَ عَلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٧٦>",
"وَلِلَّهِ غَیۡبُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَمَW<اۤ> أَمۡR<رُ> ٱX<ل>سَّاعَةِ إِلَّا كَلَمۡحِ ٱلۡبَصَرِ أَوۡ هُوَ أَQ<قۡ>R<رَ>بُۚ إِN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٧٧>",
"وَٱللَّهُ أَخۡR<رَ>جَكُم N<مِّ>X<ن>N<ۢ> بُطُونِ أُN<مَّ>هَT<ـٰ>تِكُمۡ لَا تَعۡلَمُونَ شَیۡءX<ࣰ>ا N<وَ>جَعَلَ لَكُمُ ٱX<ل>سَّمۡعَ وَX<ٱ>لۡأَQ<بۡ>صَT<ـٰ>R<رَ> وَX<ٱ>لۡأَفۡءِدَةَ لَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<٧٨>",
"أَلَمۡ یَR<رَ>وۡX<ا۟> إِلَى ٱX<ل>طَّیۡرِ مُسَخَّR<رَ>T< ٰ>تN<ࣲ> فِی جَوِّ ٱX<ل>سَّمَW<اۤ>ءِ مَا یُمۡسِكُهُN<نَّ> إِلَّا ٱللَّهُۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٧٩>",
"وَٱللَّهُ جَعَلَ لَكُم N<مِّ>X<ن>N<ۢ> بُیُوتِكُمۡ سَكَنX<ࣰ>ا N<وَ>جَعَلَ لَكُم N<مِّ>N<ن> جُلُودِ ٱلۡأَنۡعَT<ـٰ>مِ بُیُوتN<ࣰ>ا تَسۡتَخِفُّونَهَا یَوۡمَ ظَعۡنِكُمۡ وَیَوۡمَ إِقَامَتِكُمۡ وَمِنۡ أَصۡوَافِهَا وَأَوۡبَارِهَا وَأَشۡعَارِهَW<اۤ> أَثَT<ـٰ>ثX<ࣰ>ا N<وَ>مَتَT<ـٰ>عًا إِلَىٰ حِJ<ی>نࣲ A<۝>D<٨٠>",
"وَٱللَّهُ جَعَلَ لَكُم N<مِّ>N<مَّ>ا خَلَقَ ظِلَT<ـٰ>لX<ࣰ>ا N<وَ>جَعَلَ لَكُم N<مِّ>نَ ٱلۡجِبَالِ أَكۡنَT<ـٰ>نX<ࣰ>ا N<وَ>جَعَلَ لَكُمۡ سَR<رَ>T< ٰ>بِیلَ تَقِیكُمُ ٱلۡحَR<رَّ> وَسَR<رَ>T< ٰ>بِیلَ تَقِیكُN<م> بَأۡسَكُمۡۚ كَذَT< ٰ>لِكَ یُتِN<مُّ> نِعۡمَتَهُT<ۥ> عَلَیۡكُمۡ لَعَلَّكُمۡ تُسۡلِمُJ<و>نَ A<۝>D<٨١>",
"فَإِN<ن> تَوَلَّوۡX<ا۟> فَإِN<نَّ>مَا عَلَیۡكَ ٱلۡبَلَT<ـٰ>غُ ٱلۡمُبِJ<ی>نُ A<۝>D<٨٢>",
"یَعۡرِفُونَ نِعۡمَتَ ٱللَّهِ ثُN<مَّ> یُN<ن>كِR<رُ>ونَهَا وَأَكۡثَR<رُ>هُمُ ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٨٣>",
"وَیَوۡمَ نَQ<بۡ>عَثُ مِN<ن> كُلِّ أُN<مَّ>ةN<ࣲ> شَهِیدN<ࣰ>ا ثُN<مَّ> لَا یُؤۡذَنُ لِلَّذِینَ كَفَR<رُ>وX<ا۟> وَلَا هُمۡ یُسۡتَعۡتَبُJ<و>نَ A<۝>D<٨٤>",
"وَإِذَا R<رَ>ءَا ٱلَّذِینَ ظَلَمُوX<ا۟> ٱلۡعَذَابَ فَلَا یُخَفَّفُ عَنۡهُمۡ وَلَا هُمۡ یُN<ن>ظَR<رُ>J<و>نَ A<۝>D<٨٥>",
"وَإِذَا R<رَ>ءَا ٱلَّذِینَ أَشۡR<رَ>كُوX<ا۟> شُR<رَ>كَW<اۤ>ءَهُمۡ قَالُوX<ا۟> R<رَ>بَّنَا هَW<ـٰۤ>ؤُلَW<اۤ>ءِ شُR<رَ>كَW<اۤ>ؤُنَا ٱلَّذِینَ كُN<نَّ>ا نَQ<دۡ>عُوX<ا۟> مِN<ن> دُونِكَۖ فَأَلۡقَوۡX<ا۟> إِلَیۡهِمُ ٱلۡقَوۡلَ إِN<نَّ>كُمۡ لَكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<٨٦>",
"وَأَلۡقَوۡX<ا۟> إِلَى ٱللَّهِ یَوۡمَئِذٍ ٱX<ل>سَّلَمَۖ وَضَلَّ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<٨٧>",
"ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَصَدُّوX<ا۟> عَN<ن> سَبِیلِ ٱللَّهِ زِQ<دۡ>نَT<ـٰ>هُمۡ عَذَابN<ࣰ>ا فَوۡقَ ٱلۡعَذَابِ بِمَا كَانُوX<ا۟> یُفۡسِدُJ<و>نَ A<۝>D<٨٨>",
"وَیَوۡمَ نَQ<بۡ>عَثُ فِی كُلِّ أُN<مَّ>ةN<ࣲ> شَهِیدًا عَلَیۡهِم N<مِّ>نۡ أَN<ن>فُسِهِمۡۖ وَجِئۡنَا بِكَ شَهِیدًا عَلَىٰ هَW<ـٰۤ>ؤُلَW<اۤ>ءِۚ وَنَزَّلۡنَا عَلَیۡكَ ٱلۡكِتَT<ـٰ>بَ تِQ<بۡ>یَT<ـٰ>نX<ࣰ>ا لِّكُلِّ شَیۡءX<ࣲ> N<وَ>هُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣰ> N<وَ>بُشۡR<رَ>ىٰ لِلۡمُسۡلِمِJ<ی>نَ A<۝>D<٨٩>",
"۞ إِN<نَّ> ٱللَّهَ یَأۡمُR<رُ> بِX<ٱ>لۡعَQ<دۡ>لِ وَX<ٱ>لۡإِحۡسَT<ـٰ>نِ وَإِیتَW<اۤ>ئِ ذِی ٱلۡقُR<رۡ>بَىٰ وَیَنۡهَىٰ عَنِ ٱلۡفَحۡشَW<اۤ>ءِ وَX<ٱ>لۡمُN<ن>كَرِ وَX<ٱ>لۡبَغۡیِۚ یَعِظُكُمۡ لَعَلَّكُمۡ تَذَكَّR<رُ>J<و>نَ A<۝>D<٩٠>",
"وَأَوۡفُوX<ا۟> بِعَهۡدِ ٱللَّهِ إِذَا عَT<ـٰ>هَX<د>تُّمۡ وَلَا تَN<ن>قُضُوX<ا۟> ٱلۡأَیۡمَT<ـٰ>نَ بَعۡدَ تَوۡكِیدِهَا وَقَQ<دۡ> جَعَلۡتُمُ ٱللَّهَ عَلَیۡكُمۡ كَفِیلًاۚ إِN<نَّ> ٱللَّهَ یَعۡلَمُ مَا تَفۡعَلُJ<و>نَ A<۝>D<٩١>",
"وَلَا تَكُونُوX<ا۟> كَX<ٱ>لَّتِی نَقَضَتۡ غَزۡلَهَا مِX<ن>N<ۢ> بَعۡدِ قُوَّةٍ أَN<ن>كَT<ـٰ>ثN<ࣰ>ا تَتَّخِذُونَ أَیۡمَT<ـٰ>نَكُمۡ دَخَلَN<ۢ>ا بَیۡنَكُمۡ أَN<ن> تَكُونَ أُN<مَّ>ةٌ هِیَ أَR<رۡ>بَىٰ مِنۡ أُN<مَّ>ةٍۚ إِN<نَّ>مَا یَQ<بۡ>لُوكُمُ ٱللَّهُ بِهِT<ۦ>ۚ وَلَیُبَیِّنَN<نَّ> لَكُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ مَا كُN<ن>تُمۡ فِیهِ تَخۡتَلِفُJ<و>نَ A<۝>D<٩٢>",
"وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ لَجَعَلَكُمۡ أُN<مَّ>ةX<ࣰ> N<وَ>T< ٰ>حِدَةX<ࣰ> N<وَ>لَT<ـٰ>كِX<ن> N<یُ>ضِلُّ مَX<ن> N<یَ>شَW<اۤ>ءُ وَیَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَلَتُسۡءَلُN<نَّ> عَN<مَّ>ا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٩٣>",
"وَلَا تَتَّخِذُW<وۤ>X<ا۟> أَیۡمَT<ـٰ>نَكُمۡ دَخَلَN<ۢ>ا بَیۡنَكُمۡ فَتَزِلَّ قَدَمُN<ۢ> بَعۡدَ ثُبُوتِهَا وَتَذُوقُوX<ا۟> ٱX<ل>سُّW<وۤ>ءَ بِمَا صَدَX<د>تُّمۡ عَN<ن> سَبِیلِ ٱللَّهِ وَلَكُمۡ عَذَابٌ عَظِJ<ی>مࣱ A<۝>D<٩٤>",
"وَلَا تَشۡتَR<رُ>وX<ا۟> بِعَهۡدِ ٱللَّهِ ثَمَنN<ࣰ>ا قَلِیلًاۚ إِN<نَّ>مَا عِN<ن>دَ ٱللَّهِ هُوَ خَیۡR<ر>X<ࣱ> لَّكُمۡ إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٩٥>",
"مَا عِN<ن>دَكُمۡ یَN<ن>فَدُ وَمَا عِN<ن>دَ ٱللَّهِ بَاقX<ࣲ>ۗ N<وَ>لَنَQ<جۡ>زِیَN<نَّ> ٱلَّذِینَ صَبَR<رُ>W<وۤ>X<ا۟> أَQ<جۡ>R<رَ>هُN<م> بِأَحۡسَنِ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٩٦>",
"مَنۡ عَمِلَ صَT<ـٰ>لِحX<ࣰ>ا N<مِّ>N<ن> ذَكَرٍ أَوۡ أُN<ن>ثَىٰ وَهُوَ مُؤۡمِنN<ࣱ> فَلَنُحۡیِیَN<نَّ>هُT<ۥ> حَیَX<و>T<ٰ>ةN<ࣰ> طَیِّبَةX<ࣰ>ۖ N<وَ>لَنَQ<جۡ>زِیَN<نَّ>هُمۡ أَQ<جۡ>R<رَ>هُN<م> بِأَحۡسَنِ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٩٧>",
"فَإِذَا قَR<رَ>أۡتَ ٱلۡقُR<رۡ>ءَانَ فَX<ٱ>سۡتَعِذۡ بِٱللَّهِ مِنَ ٱX<ل>شَّیۡطَT<ـٰ>نِ ٱX<ل>R<رَّ>جِJ<ی>مِ A<۝>D<٩٨>",
"إِN<نَّ>هُT<ۥ> لَیۡسَ لَهُT<ۥ> سُلۡطَT<ـٰ>نٌ عَلَى ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَلَىٰ R<رَ>بِّهِمۡ یَتَوَكَّلُJ<و>نَ A<۝>D<٩٩>",
"إِN<نَّ>مَا سُلۡطَT<ـٰ>نُهُT<ۥ> عَلَى ٱلَّذِینَ یَتَوَلَّوۡنَهُT<ۥ> وَX<ٱ>لَّذِینَ هُN<م> بِهِT<ۦ> مُشۡرِكُJ<و>نَ A<۝>D<١٠٠>",
"وَإِذَا بَدَّلۡنَW<اۤ> ءَایَةX<ࣰ> N<مَّ>كَانَ ءَایَةX<ࣲ> N<وَ>ٱللَّهُ أَعۡلَمُ بِمَا یُنَزِّلُ قَالُW<وۤ>X<ا۟> إِN<نَّ>مَW<اۤ> أَN<ن>تَ مُفۡتَرِN<ۭ>ۚ بَلۡ أَكۡثَR<رُ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<١٠١>",
"قُلۡ نَزَّلَهُT<ۥ> R<رُ>وحُ ٱلۡقُدُسِ مِX<ن> R<رَّ>بِّكَ بِX<ٱ>لۡحَقِّ لِیُثَبِّتَ ٱلَّذِینَ ءَامَنُوX<ا۟> وَهُدX<ࣰ>ى N<وَ>بُشۡR<رَ>ىٰ لِلۡمُسۡلِمِJ<ی>نَ A<۝>D<١٠٢>",
"وَلَقَQ<دۡ> نَعۡلَمُ أَN<نَّ>هُمۡ یَقُولُونَ إِN<نَّ>مَا یُعَلِّمُهُT<ۥ> بَشَR<ر>X<ࣱ>ۗ لِّسَانُ ٱلَّذِی یُلۡحِدُونَ إِلَیۡهِ أَعۡجَمِیّX<ࣱ> N<وَ>هَT<ـٰ>ذَا لِسَانٌ عَR<رَ>بِیّX<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<١٠٣>",
"إِN<نَّ> ٱلَّذِینَ لَا یُؤۡمِنُونَ بِءَایَT<ـٰ>تِ ٱللَّهِ لَا یَهۡدِیهِمُ ٱللَّهُ وَلَهُمۡ عَذَابٌ أَلِJ<ی>مٌ A<۝>D<١٠٤>",
"إِN<نَّ>مَا یَفۡتَرِی ٱلۡكَذِبَ ٱلَّذِینَ لَا یُؤۡمِنُونَ بِءَایَT<ـٰ>تِ ٱللَّهِۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<١٠٥>",
"مَN<ن> كَفَR<رَ> بِٱللَّهِ مِX<ن>N<ۢ> بَعۡدِ إِیمَT<ـٰ>نِهِW<ۦۤ> إِلَّا مَنۡ أُكۡرِهَ وَقَلۡبُهُT<ۥ> مُQ<طۡ>مَئِN<نُّ>N<ۢ> بِX<ٱ>لۡإِیمَT<ـٰ>نِ وَلَT<ـٰ>كِX<ن> N<مَّ>N<ن> شَR<رَ>حَ بِX<ٱ>لۡكُفۡرِ صَQ<دۡ>R<ر>N<ࣰ>ا فَعَلَیۡهِمۡ غَضَبX<ࣱ> N<مِّ>نَ ٱللَّهِ وَلَهُمۡ عَذَابٌ عَظِJ<ی>مࣱ A<۝>D<١٠٦>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُمُ ٱسۡتَحَبُّوX<ا۟> ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا عَلَى ٱلۡءَاخِR<رَ>ةِ وَأَN<نَّ> ٱللَّهَ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٠٧>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ طَبَعَ ٱللَّهُ عَلَىٰ قُلُوبِهِمۡ وَسَمۡعِهِمۡ وَأَQ<بۡ>صَT<ـٰ>رِهِمۡۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡغَT<ـٰ>فِلُJ<و>نَ A<۝>D<١٠٨>",
"لَا جَR<رَ>مَ أَN<نَّ>هُمۡ فِی ٱلۡءَاخِR<رَ>ةِ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<١٠٩>",
"ثُN<مَّ> إِN<نَّ> R<رَ>بَّكَ لِلَّذِینَ هَاجَR<رُ>وX<ا۟> مِX<ن>N<ۢ> بَعۡدِ مَا فُتِنُوX<ا۟> ثُN<مَّ> جَT<ـٰ>هَدُوX<ا۟> وَصَبَR<رُ>W<وۤ>X<ا۟> إِN<نَّ> R<رَ>بَّكَ مِX<ن>N<ۢ> بَعۡدِهَا لَغَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١١٠>",
"۞ یَوۡمَ تَأۡتِی كُلُّ نَفۡسN<ࣲ> تُجَT<ـٰ>دِلُ عَن N<نَّ>فۡسِهَا وَتُوَفَّىٰ كُلُّ نَفۡسX<ࣲ> N<مَّ>ا عَمِلَتۡ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<١١١>",
"وَضَR<رَ>بَ ٱللَّهُ مَثَلN<ࣰ>ا قَR<رۡ>یَةN<ࣰ> كَانَتۡ ءَامِنَةX<ࣰ> N<مُّ>Q<طۡ>مَئِN<نَّ>ةX<ࣰ> N<یَ>أۡتِیهَا رِزۡقُهَا R<رَ>غَدX<ࣰ>ا N<مِّ>N<ن> كُلِّ مَكَانN<ࣲ> فَكَفَR<رَ>تۡ بِأَنۡعُمِ ٱللَّهِ فَأَذَT< ٰ>قَهَا ٱللَّهُ لِبَاسَ ٱلۡجُوعِ وَX<ٱ>لۡخَوۡفِ بِمَا كَانُوX<ا۟> یَصۡنَعُJ<و>نَ A<۝>D<١١٢>",
"وَلَقَQ<دۡ> جَW<اۤ>ءَهُمۡ R<رَ>سُولX<ࣱ> N<مِّ>نۡهُمۡ فَكَذَّبُوهُ فَأَخَذَهُمُ ٱلۡعَذَابُ وَهُمۡ ظَT<ـٰ>لِمُJ<و>نَ A<۝>D<١١٣>",
"فَكُلُوX<ا۟> مِN<مَّ>ا R<رَ>زَقَكُمُ ٱللَّهُ حَلَT<ـٰ>لN<ࣰ>ا طَیِّبX<ࣰ>ا N<وَ>X<ٱ>شۡكُR<رُ>وX<ا۟> نِعۡمَتَ ٱللَّهِ إِN<ن> كُN<ن>تُمۡ إِیَّاهُ تَعۡبُدُJ<و>نَ A<۝>D<١١٤>",
"إِN<نَّ>مَا حَR<رَّ>مَ عَلَیۡكُمُ ٱلۡمَیۡتَةَ وَX<ٱ>X<ل>دَّمَ وَلَحۡمَ ٱلۡخِN<ن>زِیرِ وَمَW<اۤ> أُهِلَّ لِغَیۡرِ ٱللَّهِ بِهِT<ۦ>ۖ فَمَنِ ٱضۡطُR<رَّ> غَیۡR<رَ> بَاغX<ࣲ> N<وَ>لَا عَادN<ࣲ> فَإِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١١٥>",
"وَلَا تَقُولُوX<ا۟> لِمَا تَصِفُ أَلۡسِنَتُكُمُ ٱلۡكَذِبَ هَT<ـٰ>ذَا حَلَT<ـٰ>لX<ࣱ> N<وَ>هَT<ـٰ>ذَا حَR<رَ>امX<ࣱ> لِّتَفۡتَR<رُ>وX<ا۟> عَلَى ٱللَّهِ ٱلۡكَذِبَۚ إِN<نَّ> ٱلَّذِینَ یَفۡتَR<رُ>ونَ عَلَى ٱللَّهِ ٱلۡكَذِبَ لَا یُفۡلِحُJ<و>نَ A<۝>D<١١٦>",
"مَتَT<ـٰ>عN<ࣱ> قَلِیلX<ࣱ> N<وَ>لَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<١١٧>",
"وَعَلَى ٱلَّذِینَ هَادُوX<ا۟> حَR<رَّ>مۡنَا مَا قَصَصۡنَا عَلَیۡكَ مِN<ن> قَQ<بۡ>لُۖ وَمَا ظَلَمۡنَT<ـٰ>هُمۡ وَلَT<ـٰ>كِN<ن> كَانُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ یَظۡلِمُJ<و>نَ A<۝>D<١١٨>",
"ثُN<مَّ> إِN<نَّ> R<رَ>بَّكَ لِلَّذِینَ عَمِلُوX<ا۟> ٱX<ل>سُّW<وۤ>ءَ بِجَهَT<ـٰ>لَةN<ࣲ> ثُN<مَّ> تَابُوX<ا۟> مِX<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَ وَأَصۡلَحُW<وۤ>X<ا۟> إِN<نَّ> R<رَ>بَّكَ مِX<ن>N<ۢ> بَعۡدِهَا لَغَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<١١٩>",
"إِN<نَّ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ كَانَ أُN<مَّ>ةN<ࣰ> قَانِتX<ࣰ>ا لِّلَّهِ حَنِیفX<ࣰ>ا N<وَ>لَمۡ یَكُ مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<١٢٠>",
"شَاكِR<ر>X<ࣰ>ا لِّأَنۡعُمِهِۚ ٱQ<جۡ>تَبَX<ى>T<ٰ>هُ وَهَدَX<ى>T<ٰ>هُ إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<١٢١>",
"وَءَاتَیۡنَT<ـٰ>هُ فِی ٱX<ل>دُّنۡیَا حَسَنَةX<ࣰ>ۖ N<وَ>إِN<نَّ>هُT<ۥ> فِی ٱلۡءَاخِR<رَ>ةِ لَمِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<١٢٢>",
"ثُN<مَّ> أَوۡحَیۡنَW<اۤ> إِلَیۡكَ أَنِ ٱتَّبِعۡ مِلَّةَ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ حَنِیفX<ࣰ>اۖ N<وَ>مَا كَانَ مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<١٢٣>",
"إِN<نَّ>مَا جُعِلَ ٱX<ل>سَّQ<بۡ>تُ عَلَى ٱلَّذِینَ ٱخۡتَلَفُوX<ا۟> فِیهِۚ وَإِN<نَّ> R<رَ>بَّكَ لَیَحۡكُمُ بَیۡنَهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ فِیمَا كَانُوX<ا۟> فِیهِ یَخۡتَلِفُJ<و>نَ A<۝>D<١٢٤>",
"ٱQ<دۡ>عُ إِلَىٰ سَبِیلِ R<رَ>بِّكَ بِX<ٱ>لۡحِكۡمَةِ وَX<ٱ>لۡمَوۡعِظَةِ ٱلۡحَسَنَةِۖ وَجَT<ـٰ>دِلۡهُN<م> بِX<ٱ>لَّتِی هِیَ أَحۡسَنُۚ إِN<نَّ> R<رَ>بَّكَ هُوَ أَعۡلَمُ بِمَN<ن> ضَلَّ عَN<ن> سَبِیلِهِT<ۦ> وَهُوَ أَعۡلَمُ بِX<ٱ>لۡمُهۡتَدِJ<ی>نَ A<۝>D<١٢٥>",
"وَإِنۡ عَاقَQ<بۡ>تُمۡ فَعَاقِبُوX<ا۟> بِمِثۡلِ مَا عُوقِQ<بۡ>تُN<م> بِهِT<ۦ>ۖ وَلَئِN<ن> صَبَR<رۡ>تُمۡ لَهُوَ خَیۡR<ر>X<ࣱ> لِّX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<١٢٦>",
"وَX<ٱ>صۡبِرۡ وَمَا صَQ<بۡ>R<رُ>كَ إِلَّا بِٱللَّهِۚ وَلَا تَحۡزَنۡ عَلَیۡهِمۡ وَلَا تَكُ فِی ضَیۡقX<ࣲ> N<مِّ>N<مَّ>ا یَمۡكُR<رُ>J<و>نَ A<۝>D<١٢٧>",
"إِN<نَّ> ٱللَّهَ مَعَ ٱلَّذِینَ ٱتَّقَوX<ا۟> وَّX<ٱ>لَّذِینَ هُم N<مُّ>حۡسِنُJ<و>نَ A<۝>D<١٢٨>",
"#۞ سُQ<بۡ>حَT<ـٰ>نَ ٱلَّذِW<یۤ> أَسۡR<رَ>ىٰ بِعَQ<بۡ>دِهِT<ۦ> لَیۡلX<ࣰ>ا N<مِّ>نَ ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِ إِلَى ٱلۡمَسۡجِدِ ٱلۡأَQ<قۡ>صَا ٱلَّذِی بَT<ـٰ>R<رَ>كۡنَا حَوۡلَهُT<ۥ> لِنُرِیَهُT<ۥ> مِنۡ ءَایَT<ـٰ>تِنَW<اۤ>ۚ إِN<نَّ>هُT<ۥ> هُوَ ٱX<ل>سَّمِیعُ ٱلۡبَصِJ<ی>رُ A<۝>D<١>",
"وَءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ وَجَعَلۡنَT<ـٰ>هُ هُدX<ࣰ>ى لِّبَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ أَلَّا تَتَّخِذُوX<ا۟> مِN<ن> دُونِی وَكِیلࣰا A<۝>D<٢>",
"ذُرِّیَّةَ مَنۡ حَمَلۡنَا مَعَ نُوحٍۚ إِN<نَّ>هُT<ۥ> كَانَ عَQ<بۡ>دN<ࣰ>ا شَكُوR<ر>ࣰا A<۝>D<٣>",
"وَقَضَیۡنَW<اۤ> إِلَىٰ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ فِی ٱلۡكِتَT<ـٰ>بِ لَتُفۡسِدُN<نَّ> فِی ٱلۡأَR<رۡ>ضِ مَR<رَّ>تَیۡنِ وَلَتَعۡلُN<نَّ> عُلُوّN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٤>",
"فَإِذَا جَW<اۤ>ءَ وَعۡدُ أُولَX<ى>T<ٰ>هُمَا بَعَثۡنَا عَلَیۡكُمۡ عِبَادX<ࣰ>ا لَّنَW<اۤ> أُX<و۟>لِی بَأۡسN<ࣲ> شَدِیدN<ࣲ> فَجَاسُوX<ا۟> خِلَT<ـٰ>لَ ٱX<ل>دِّیَارِۚ وَكَانَ وَعۡدX<ࣰ>ا N<مَّ>فۡعُولࣰا A<۝>D<٥>",
"ثُN<مَّ> R<رَ>دَQ<دۡ>نَا لَكُمُ ٱلۡكَR<رَّ>ةَ عَلَیۡهِمۡ وَأَمۡدَQ<دۡ>نَT<ـٰ>كُN<م> بِأَمۡوَT< ٰ>لX<ࣲ> N<وَ>بَنِینَ وَجَعَلۡنَT<ـٰ>كُمۡ أَكۡثَR<رَ> نَفِیR<ر>ًا A<۝>D<٦>",
"إِنۡ أَحۡسَN<ن>تُمۡ أَحۡسَN<ن>تُمۡ لِأَN<ن>فُسِكُمۡۖ وَإِنۡ أَسَأۡتُمۡ فَلَهَاۚ فَإِذَا جَW<اۤ>ءَ وَعۡدُ ٱلۡءَاخِR<رَ>ةِ لِیَسُW<ـࣳۤ>ءُوX<ا۟> وُجُوهَكُمۡ وَلِیَQ<دۡ>خُلُوX<ا۟> ٱلۡمَسۡجِدَ كَمَا دَخَلُوهُ أَوَّلَ مَR<رَّ>ةX<ࣲ> N<وَ>لِیُتَبِّR<رُ>وX<ا۟> مَا عَلَوۡX<ا۟> تَتۡبِیR<ر>ًا A<۝>D<٧>",
"عَسَىٰ R<رَ>بُّكُمۡ أَX<ن> N<یَ>R<رۡ>حَمَكُمۡۚ وَإِنۡ عُX<د>تُّمۡ عُQ<دۡ>نَاۚ وَجَعَلۡنَا جَهَN<نَّ>مَ لِلۡكَT<ـٰ>فِرِینَ حَصِیR<ر>ًا A<۝>D<٨>",
"إِN<نَّ> هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانَ یَهۡدِی لِلَّتِی هِیَ أَQ<قۡ>وَمُ وَیُبَشِّR<رُ> ٱلۡمُؤۡمِنِینَ ٱلَّذِینَ یَعۡمَلُونَ ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ أَN<نَّ> لَهُمۡ أَQ<جۡ>R<ر>N<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٩>",
"وَأَN<نَّ> ٱلَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ أَعۡتَQ<دۡ>نَا لَهُمۡ عَذَابًا أَلِیمࣰا A<۝>D<١٠>",
"وَیَQ<دۡ>عُ ٱلۡإِN<ن>سَT<ـٰ>نُ بِX<ٱ>X<ل>شَّرِّ دُعَW<اۤ>ءَهُT<ۥ> بِX<ٱ>لۡخَیۡرِۖ وَكَانَ ٱلۡإِN<ن>سَT<ـٰ>نُ عَجُولࣰا A<۝>D<١١>",
"وَجَعَلۡنَا ٱلَّیۡلَ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ> ءَایَتَیۡنِۖ فَمَحَوۡنَW<اۤ> ءَایَةَ ٱلَّیۡلِ وَجَعَلۡنَW<اۤ> ءَایَةَ ٱX<ل>N<نَّ>هَارِ مُQ<بۡ>صِR<رَ>ةX<ࣰ> لِّتَQ<بۡ>تَغُوX<ا۟> فَضۡلX<ࣰ>ا N<مِّ>X<ن> R<رَّ>بِّكُمۡ وَلِتَعۡلَمُوX<ا۟> عَدَدَ ٱX<ل>سِّنِینَ وَX<ٱ>لۡحِسَابَۚ وَكُلَّ شَیۡءN<ࣲ> فَصَّلۡنَT<ـٰ>هُ تَفۡصِیلࣰا A<۝>D<١٢>",
"وَكُلَّ إِN<ن>سَT<ـٰ>نٍ أَلۡزَمۡنَT<ـٰ>هُ طَW<ـٰۤ>ئِR<رَ>هُT<ۥ> فِی عُنُقِهِT<ۦ>ۖ وَنُخۡرِجُ لَهُT<ۥ> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ كِتَT<ـٰ>بX<ࣰ>ا N<یَ>لۡقَX<ى>T<ٰ>هُ مَN<ن>شُوR<ر>ًا A<۝>D<١٣>",
"ٱQ<قۡ>R<رَ>أۡ كِتَT<ـٰ>بَكَ كَفَىٰ بِنَفۡسِكَ ٱلۡیَوۡمَ عَلَیۡكَ حَسِیبࣰا A<۝>D<١٤>",
"مَّنِ ٱهۡتَدَىٰ فَإِN<نَّ>مَا یَهۡتَدِی لِنَفۡسِهِT<ۦ>ۖ وَمَN<ن> ضَلَّ فَإِN<نَّ>مَا یَضِلُّ عَلَیۡهَاۚ وَلَا تَزِR<رُ> وَازِR<رَ>ةX<ࣱ> N<وِ>زۡR<رَ> أُخۡR<رَ>ىٰۗ وَمَا كُN<نَّ>ا مُعَذِّبِینَ حَتَّىٰ نَQ<بۡ>عَثَ R<رَ>سُولࣰا A<۝>D<١٥>",
"وَإِذَW<اۤ> أَR<رَ>Q<دۡ>نَW<اۤ> أَن N<نُّ>هۡلِكَ قَR<رۡ>یَةً أَمَR<رۡ>نَا مُتۡR<رَ>فِیهَا فَفَسَقُوX<ا۟> فِیهَا فَحَقَّ عَلَیۡهَا ٱلۡقَوۡلُ فَدَN<مَّ>R<رۡ>نَT<ـٰ>هَا تَQ<دۡ>مِیR<ر>ࣰا A<۝>D<١٦>",
"وَكَمۡ أَهۡلَكۡنَا مِنَ ٱلۡقُR<رُ>ونِ مِX<ن>N<ۢ> بَعۡدِ نُوحX<ࣲ>ۗ N<وَ>كَفَىٰ بِR<رَ>بِّكَ بِذُنُوبِ عِبَادِهِT<ۦ> خَبِیR<رَ>N<ۢ>ا بَصِیR<ر>ࣰا A<۝>D<١٧>",
"مَّN<ن> كَانَ یُرِیدُ ٱلۡعَاجِلَةَ عَجَّلۡنَا لَهُT<ۥ> فِیهَا مَا نَشَW<اۤ>ءُ لِمَن N<نُّ>رِیدُ ثُN<مَّ> جَعَلۡنَا لَهُT<ۥ> جَهَN<نَّ>مَ یَصۡلَX<ى>T<ٰ>هَا مَذۡمُومX<ࣰ>ا N<مَّ>Q<دۡ>حُوR<ر>ࣰا A<۝>D<١٨>",
"وَمَنۡ أَR<رَ>ادَ ٱلۡءَاخِR<رَ>ةَ وَسَعَىٰ لَهَا سَعۡیَهَا وَهُوَ مُؤۡمِنN<ࣱ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ كَانَ سَعۡیُهُم N<مَّ>شۡكُوR<ر>ࣰا A<۝>D<١٩>",
"كُلّX<ࣰ>ا N<نُّ>مِدُّ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ وَهَW<ـٰۤ>ؤُلَW<اۤ>ءِ مِنۡ عَطَW<اۤ>ءِ R<رَ>بِّكَۚ وَمَا كَانَ عَطَW<اۤ>ءُ R<رَ>بِّكَ مَحۡظُوR<ر>ًا A<۝>D<٢٠>",
"ٱN<ن>ظُR<رۡ> كَیۡفَ فَضَّلۡنَا بَعۡضَهُمۡ عَلَىٰ بَعۡضX<ࣲ>ۚ N<وَ>لَلۡءَاخِR<رَ>ةُ أَكۡبَR<رُ> دَR<رَ>جَT<ـٰ>تX<ࣲ> N<وَ>أَكۡبَR<رُ> تَفۡضِیلࣰا A<۝>D<٢١>",
"لَّا تَQ<جۡ>عَلۡ مَعَ ٱللَّهِ إِلَT<ـٰ>هًا ءَاخَR<رَ> فَتَQ<قۡ>عُدَ مَذۡمُومX<ࣰ>ا N<مَّ>خۡذُولࣰا A<۝>D<٢٢>",
"۞ وَقَضَىٰ R<رَ>بُّكَ أَلَّا تَعۡبُدُW<وۤ>X<ا۟> إِلَّW<اۤ> إِیَّاهُ وَبِX<ٱ>لۡوَT< ٰ>لِدَیۡنِ إِحۡسَT<ـٰ>نًاۚ إِN<مَّ>ا یَQ<بۡ>لُغَN<نَّ> عِN<ن>دَكَ ٱلۡكِبَR<رَ> أَحَدُهُمَW<اۤ> أَوۡ كِلَاهُمَا فَلَا تَقُل لَّهُمَW<اۤ> أُفّX<ࣲ> N<وَ>لَا تَنۡهَR<رۡ>هُمَا وَقُل لَّهُمَا قَوۡلN<ࣰ>ا كَرِیمࣰا A<۝>D<٢٣>",
"وَX<ٱ>خۡفِضۡ لَهُمَا جَنَاحَ ٱX<ل>ذُّلِّ مِنَ ٱX<ل>R<رَّ>حۡمَةِ وَقُل R<رَّ>بِّ ٱR<رۡ>حَمۡهُمَا كَمَا R<رَ>بَّیَانِی صَغِیR<ر>ࣰا A<۝>D<٢٤>",
"R<رَّ>بُّكُمۡ أَعۡلَمُ بِمَا فِی نُفُوسِكُمۡۚ إِN<ن> تَكُونُوX<ا۟> صَT<ـٰ>لِحِینَ فَإِN<نَّ>هُT<ۥ> كَانَ لِلۡأَوَّT< ٰ>بِینَ غَفُوR<ر>ࣰا A<۝>D<٢٥>",
"وَءَاتِ ذَا ٱلۡقُR<رۡ>بَىٰ حَقَّهُT<ۥ> وَX<ٱ>لۡمِسۡكِینَ وَX<ٱ>Q<بۡ>نَ ٱX<ل>سَّبِیلِ وَلَا تُبَذِّرۡ تَQ<بۡ>ذِیR<ر>ًا A<۝>D<٢٦>",
"إِN<نَّ> ٱلۡمُبَذِّرِینَ كَانُW<وۤ>X<ا۟> إِخۡوَT< ٰ>نَ ٱX<ل>شَّیَT<ـٰ>طِینِۖ وَكَانَ ٱX<ل>شَّیۡطَT<ـٰ>نُ لِR<رَ>بِّهِT<ۦ> كَفُوR<ر>ࣰا A<۝>D<٢٧>",
"وَإِN<مَّ>ا تُعۡرِضَN<نَّ> عَنۡهُمُ ٱQ<بۡ>تِغَW<اۤ>ءَ R<رَ>حۡمَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّكَ تَR<رۡ>جُوهَا فَقُل لَّهُمۡ قَوۡلX<ࣰ>ا N<مَّ>یۡسُوR<ر>ࣰا A<۝>D<٢٨>",
"وَلَا تَQ<جۡ>عَلۡ یَدَكَ مَغۡلُولَةً إِلَىٰ عُنُقِكَ وَلَا تَQ<بۡ>سُQ<طۡ>هَا كُلَّ ٱلۡبَسۡطِ فَتَQ<قۡ>عُدَ مَلُومX<ࣰ>ا N<مَّ>حۡسُوR<ر>ًا A<۝>D<٢٩>",
"إِN<نَّ> R<رَ>بَّكَ یَQ<بۡ>سُطُ ٱX<ل>رِّزۡقَ لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیَQ<قۡ>دِR<رُ>ۚ إِN<نَّ>هُT<ۥ> كَانَ بِعِبَادِهِT<ۦ> خَبِیR<رَ>N<ۢ>ا بَصِیR<ر>ࣰا A<۝>D<٣٠>",
"وَلَا تَQ<قۡ>تُلُW<وۤ>X<ا۟> أَوۡلَT<ـٰ>دَكُمۡ خَشۡیَةَ إِمۡلَT<ـٰ>قX<ࣲ>ۖ N<نَّ>حۡنُ نَR<رۡ>زُقُهُمۡ وَإِیَّاكُمۡۚ إِN<نَّ> قَتۡلَهُمۡ كَانَ خِQ<طۡ>ءN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٣١>",
"وَلَا تَQ<قۡ>R<رَ>بُوX<ا۟> ٱX<ل>زِّنَىW<ٰۤ>ۖ إِN<نَّ>هُT<ۥ> كَانَ فَT<ـٰ>حِشَةX<ࣰ> N<وَ>سَW<اۤ>ءَ سَبِیلࣰا A<۝>D<٣٢>",
"وَلَا تَQ<قۡ>تُلُوX<ا۟> ٱX<ل>N<نَّ>فۡسَ ٱلَّتِی حَR<رَّ>مَ ٱللَّهُ إِلَّا بِX<ٱ>لۡحَقِّۗ وَمَN<ن> قُتِلَ مَظۡلُومN<ࣰ>ا فَقَQ<دۡ> جَعَلۡنَا لِوَلِیِّهِT<ۦ> سُلۡطَT<ـٰ>نN<ࣰ>ا فَلَا یُسۡرِف فِّی ٱلۡقَتۡلِۖ إِN<نَّ>هُT<ۥ> كَانَ مَN<ن>صُوR<ر>ࣰا A<۝>D<٣٣>",
"وَلَا تَQ<قۡ>R<رَ>بُوX<ا۟> مَالَ ٱلۡیَتِیمِ إِلَّا بِX<ٱ>لَّتِی هِیَ أَحۡسَنُ حَتَّىٰ یَQ<بۡ>لُغَ أَشُدَّهُT<ۥ>ۚ وَأَوۡفُوX<ا۟> بِX<ٱ>لۡعَهۡدِۖ إِN<نَّ> ٱلۡعَهۡدَ كَانَ مَسۡءُولࣰا A<۝>D<٣٤>",
"وَأَوۡفُوX<ا۟> ٱلۡكَیۡلَ إِذَا كِلۡتُمۡ وَزِنُوX<ا۟> بِX<ٱ>لۡقِسۡطَاسِ ٱلۡمُسۡتَقِیمِۚ ذَT< ٰ>لِكَ خَیۡR<ر>X<ࣱ> N<وَ>أَحۡسَنُ تَأۡوِیلࣰا A<۝>D<٣٥>",
"وَلَا تَQ<قۡ>فُ مَا لَیۡسَ لَكَ بِهِT<ۦ> عِلۡمٌۚ إِN<نَّ> ٱX<ل>سَّمۡعَ وَX<ٱ>لۡبَصَR<رَ> وَX<ٱ>لۡفُؤَادَ كُلُّ أُX<و۟>لَW<ـٰۤ>ئِكَ كَانَ عَنۡهُ مَسۡءُولࣰا A<۝>D<٣٦>",
"وَلَا تَمۡشِ فِی ٱلۡأَR<رۡ>ضِ مَR<رَ>حًاۖ إِN<نَّ>كَ لَN<ن> تَخۡرِقَ ٱلۡأَR<رۡ>ضَ وَلَN<ن> تَQ<بۡ>لُغَ ٱلۡجِبَالَ طُولࣰا A<۝>D<٣٧>",
"كُلُّ ذَT< ٰ>لِكَ كَانَ سَیِّئُهُT<ۥ> عِN<ن>دَ R<رَ>بِّكَ مَكۡR<رُ>وهࣰا A<۝>D<٣٨>",
"ذَT< ٰ>لِكَ مِN<مَّ>W<اۤ> أَوۡحَىW<ٰۤ> إِلَیۡكَ R<رَ>بُّكَ مِنَ ٱلۡحِكۡمَةِۗ وَلَا تَQ<جۡ>عَلۡ مَعَ ٱللَّهِ إِلَT<ـٰ>هًا ءَاخَR<رَ> فَتُلۡقَىٰ فِی جَهَN<نَّ>مَ مَلُومX<ࣰ>ا N<مَّ>Q<دۡ>حُوR<ر>ًا A<۝>D<٣٩>",
"أَفَأَصۡفَX<ى>T<ٰ>كُمۡ R<رَ>بُّكُN<م> بِX<ٱ>لۡبَنِینَ وَX<ٱ>تَّخَذَ مِنَ ٱلۡمَلَW<ـٰۤ>ئِكَةِ إِنَT<ـٰ>ثًاۚ إِN<نَّ>كُمۡ لَتَقُولُونَ قَوۡلًا عَظِیمࣰا A<۝>D<٤٠>",
"وَلَقَQ<دۡ> صَR<رَّ>فۡنَا فِی هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانِ لِیَذَّكَّR<رُ>وX<ا۟> وَمَا یَزِیدُهُمۡ إِلَّا نُفُوR<ر>ࣰا A<۝>D<٤١>",
"قُل لَّوۡ كَانَ مَعَهُW<ۥۤ> ءَالِهَةN<ࣱ> كَمَا یَقُولُونَ إِذX<ࣰ>ا لَّX<ٱ>Q<بۡ>تَغَوۡX<ا۟> إِلَىٰ ذِی ٱلۡعَR<رۡ>شِ سَبِیلࣰا A<۝>D<٤٢>",
"سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ> وَتَعَT<ـٰ>لَىٰ عَN<مَّ>ا یَقُولُونَ عُلُوّN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٤٣>",
"تُسَبِّحُ لَهُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تُ ٱX<ل>سَّQ<بۡ>عُ وَX<ٱ>لۡأَR<رۡ>ضُ وَمَN<ن> فِیهِN<نَّ>ۚ وَإِX<ن> N<مِّ>N<ن> شَیۡءٍ إِلَّا یُسَبِّحُ بِحَمۡدِهِT<ۦ> وَلَT<ـٰ>كِX<ن> لَّا تَفۡقَهُونَ تَسۡبِیحَهُمۡۚ إِN<نَّ>هُT<ۥ> كَانَ حَلِیمًا غَفُوR<ر>ࣰا A<۝>D<٤٤>",
"وَإِذَا قَR<رَ>أۡتَ ٱلۡقُR<رۡ>ءَانَ جَعَلۡنَا بَیۡنَكَ وَبَیۡنَ ٱلَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ حِجَابX<ࣰ>ا N<مَّ>سۡتُوR<ر>ࣰا A<۝>D<٤٥>",
"وَجَعَلۡنَا عَلَىٰ قُلُوبِهِمۡ أَكِN<نَّ>ةً أَX<ن> N<یَ>فۡقَهُوهُ وَفِW<یۤ> ءَاذَانِهِمۡ وَQ<قۡ>R<ر>X<ࣰ>اۚ N<وَ>إِذَا ذَكَR<رۡ>تَ R<رَ>بَّكَ فِی ٱلۡقُR<رۡ>ءَانِ وَحۡدَهُT<ۥ> وَلَّوۡX<ا۟> عَلَىW<ٰۤ> أَQ<دۡ>بَT<ـٰ>رِهِمۡ نُفُوR<ر>ࣰا A<۝>D<٤٦>",
"نَّحۡنُ أَعۡلَمُ بِمَا یَسۡتَمِعُونَ بِهِW<ۦۤ> إِذۡ یَسۡتَمِعُونَ إِلَیۡكَ وَإِذۡ هُمۡ نَQ<جۡ>وَىW<ٰۤ> إِذۡ یَقُولُ ٱX<ل>ظَّT<ـٰ>لِمُونَ إِN<ن> تَتَّبِعُونَ إِلَّا R<رَ>جُلX<ࣰ>ا N<مَّ>سۡحُوR<ر>ًا A<۝>D<٤٧>",
"ٱN<ن>ظُR<رۡ> كَیۡفَ ضَR<رَ>بُوX<ا۟> لَكَ ٱلۡأَمۡثَالَ فَضَلُّوX<ا۟> فَلَا یَسۡتَطِیعُونَ سَبِیلࣰا A<۝>D<٤٨>",
"وَقَالُW<وۤ>X<ا۟> أَءِذَا كُN<نَّ>ا عِظَT<ـٰ>مX<ࣰ>ا N<وَ>R<رُ>فَT<ـٰ>تًا أَءِN<نَّ>ا لَمَQ<بۡ>عُوثُونَ خَلۡقN<ࣰ>ا جَدِیدࣰا A<۝>D<٤٩>",
"۞ قُلۡ كُونُوX<ا۟> حِجَاR<رَ>ةً أَوۡ حَدِیدًا A<۝>D<٥٠>",
"أَوۡ خَلۡقX<ࣰ>ا N<مِّ>N<مَّ>ا یَكۡبُR<رُ> فِی صُدُورِكُمۡۚ فَسَیَقُولُونَ مَX<ن> N<یُ>عِیدُنَاۖ قُلِ ٱلَّذِی فَطَR<رَ>كُمۡ أَوَّلَ مَR<رَّ>ةN<ࣲ>ۚ فَسَیُنۡغِضُونَ إِلَیۡكَ R<رُ>ءُوسَهُمۡ وَیَقُولُونَ مَتَىٰ هُوَۖ قُلۡ عَسَىW<ٰۤ> أَX<ن> N<یَ>كُونَ قَرِیبࣰا A<۝>D<٥١>",
"یَوۡمَ یَQ<دۡ>عُوكُمۡ فَتَسۡتَجِیبُونَ بِحَمۡدِهِT<ۦ> وَتَظُN<نُّ>ونَ إِX<ن> لَّبِثۡتُمۡ إِلَّا قَلِیلࣰا A<۝>D<٥٢>",
"وَقُل لِّعِبَادِی یَقُولُوX<ا۟> ٱلَّتِی هِیَ أَحۡسَنُۚ إِN<نَّ> ٱX<ل>شَّیۡطَT<ـٰ>نَ یَN<ن>زَغُ بَیۡنَهُمۡۚ إِN<نَّ> ٱX<ل>شَّیۡطَT<ـٰ>نَ كَانَ لِلۡإِN<ن>سَT<ـٰ>نِ عَدُوّX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<٥٣>",
"R<رَّ>بُّكُمۡ أَعۡلَمُ بِكُمۡۖ إِX<ن> N<یَ>شَأۡ یَR<رۡ>حَمۡكُمۡ أَوۡ إِX<ن> N<یَ>شَأۡ یُعَذِّQ<بۡ>كُمۡۚ وَمَW<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ عَلَیۡهِمۡ وَكِیلࣰا A<۝>D<٥٤>",
"وَR<رَ>بُّكَ أَعۡلَمُ بِمَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۗ وَلَقَQ<دۡ> فَضَّلۡنَا بَعۡضَ ٱX<ل>N<نَّ>بِیِّT<ـۧ>نَ عَلَىٰ بَعۡضX<ࣲ>ۖ N<وَ>ءَاتَیۡنَا دَاوُT<ۥ>دَ زَبُوR<ر>ࣰا A<۝>D<٥٥>",
"قُلِ ٱQ<دۡ>عُوX<ا۟> ٱلَّذِینَ زَعَمۡتُم N<مِّ>N<ن> دُونِهِT<ۦ> فَلَا یَمۡلِكُونَ كَشۡفَ ٱX<ل>ضُّرِّ عَN<ن>كُمۡ وَلَا تَحۡوِیلًا A<۝>D<٥٦>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ یَQ<دۡ>عُونَ یَQ<بۡ>تَغُونَ إِلَىٰ R<رَ>بِّهِمُ ٱلۡوَسِیلَةَ أَیُّهُمۡ أَQ<قۡ>R<رَ>بُ وَیَR<رۡ>جُونَ R<رَ>حۡمَتَهُT<ۥ> وَیَخَافُونَ عَذَابَهُW<ۥۤ>ۚ إِN<نَّ> عَذَابَ R<رَ>بِّكَ كَانَ مَحۡذُوR<ر>ࣰا A<۝>D<٥٧>",
"وَإِX<ن> N<مِّ>N<ن> قَR<رۡ>یَةٍ إِلَّا نَحۡنُ مُهۡلِكُوهَا قَQ<بۡ>لَ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ أَوۡ مُعَذِّبُوهَا عَذَابN<ࣰ>ا شَدِیدN<ࣰ>اۚ كَانَ ذَT< ٰ>لِكَ فِی ٱلۡكِتَT<ـٰ>بِ مَسۡطُوR<ر>ࣰا A<۝>D<٥٨>",
"وَمَا مَنَعَنَW<اۤ> أَن N<نُّ>R<رۡ>سِلَ بِX<ٱ>لۡءَایَT<ـٰ>تِ إِلَّW<اۤ> أَN<ن> كَذَّبَ بِهَا ٱلۡأَوَّلُونَۚ وَءَاتَیۡنَا ثَمُودَ ٱX<ل>N<نَّ>اقَةَ مُQ<بۡ>صِR<رَ>ةN<ࣰ> فَظَلَمُوX<ا۟> بِهَاۚ وَمَا نُR<رۡ>سِلُ بِX<ٱ>لۡءَایَT<ـٰ>تِ إِلَّا تَخۡوِیفࣰا A<۝>D<٥٩>",
"وَإِذۡ قُلۡنَا لَكَ إِN<نَّ> R<رَ>بَّكَ أَحَاطَ بِX<ٱ>X<ل>N<نَّ>اسِۚ وَمَا جَعَلۡنَا ٱX<ل>R<رُّ>ءۡیَا ٱلَّتِW<یۤ> أَR<رَ>یۡنَT<ـٰ>كَ إِلَّا فِتۡنَةX<ࣰ> لِّX<ل>N<نَّ>اسِ وَX<ٱ>X<ل>شَّجَR<رَ>ةَ ٱلۡمَلۡعُونَةَ فِی ٱلۡقُR<رۡ>ءَانِۚ وَنُخَوِّفُهُمۡ فَمَا یَزِیدُهُمۡ إِلَّا طُغۡیَT<ـٰ>نN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٦٠>",
"وَإِذۡ قُلۡنَا لِلۡمَلَW<ـٰۤ>ئِكَةِ ٱسۡجُدُوX<ا۟> لِءَادَمَ فَسَجَدُW<وۤ>X<ا۟> إِلَّW<اۤ> إِQ<بۡ>لِیسَ قَالَ ءَأَسۡجُدُ لِمَنۡ خَلَQ<قۡ>تَ طِینࣰا A<۝>D<٦١>",
"قَالَ أَR<رَ>ءَیۡتَكَ هَT<ـٰ>ذَا ٱلَّذِی كَR<رَّ>مۡتَ عَلَیَّ لَئِنۡ أَخَّR<رۡ>تَنِ إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ لَأَحۡتَنِكَN<نَّ> ذُرِّیَّتَهُW<ۥۤ> إِلَّا قَلِیلࣰا A<۝>D<٦٢>",
"قَالَ ٱذۡهَQ<بۡ> فَمَN<ن> تَبِعَكَ مِنۡهُمۡ فَإِN<نَّ> جَهَN<نَّ>مَ جَزَW<اۤ>ؤُكُمۡ جَزَW<اۤ>ءX<ࣰ> N<مَّ>وۡفُوR<ر>ࣰا A<۝>D<٦٣>",
"وَX<ٱ>سۡتَفۡزِزۡ مَنِ ٱسۡتَطَعۡتَ مِنۡهُN<م> بِصَوۡتِكَ وَأَQ<جۡ>لِQ<بۡ> عَلَیۡهِN<م> بِخَیۡلِكَ وَR<رَ>جِلِكَ وَشَارِكۡهُمۡ فِی ٱلۡأَمۡوَT< ٰ>لِ وَX<ٱ>لۡأَوۡلَT<ـٰ>دِ وَعِQ<دۡ>هُمۡۚ وَمَا یَعِدُهُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ إِلَّا غُR<رُ>وR<ر>ًا A<۝>D<٦٤>",
"إِN<نَّ> عِبَادِی لَیۡسَ لَكَ عَلَیۡهِمۡ سُلۡطَT<ـٰ>نX<ࣱ>ۚ N<وَ>كَفَىٰ بِR<رَ>بِّكَ وَكِیلࣰا A<۝>D<٦٥>",
"R<رَّ>بُّكُمُ ٱلَّذِی یُزۡجِی لَكُمُ ٱلۡفُلۡكَ فِی ٱلۡبَحۡرِ لِتَQ<بۡ>تَغُوX<ا۟> مِN<ن> فَضۡلِهِW<ۦۤ>ۚ إِN<نَّ>هُT<ۥ> كَانَ بِكُمۡ R<رَ>حِیمࣰا A<۝>D<٦٦>",
"وَإِذَا مَسَّكُمُ ٱX<ل>ضُّR<رُّ> فِی ٱلۡبَحۡرِ ضَلَّ مَN<ن> تَQ<دۡ>عُونَ إِلَّW<اۤ> إِیَّاهُۖ فَلَN<مَّ>ا نَجَّX<ى>T<ٰ>كُمۡ إِلَى ٱلۡبَرِّ أَعۡR<رَ>ضۡتُمۡۚ وَكَانَ ٱلۡإِN<ن>سَT<ـٰ>نُ كَفُوR<ر>ًا A<۝>D<٦٧>",
"أَفَأَمِN<ن>تُمۡ أَX<ن> N<یَ>خۡسِفَ بِكُمۡ جَانِبَ ٱلۡبَرِّ أَوۡ یُR<رۡ>سِلَ عَلَیۡكُمۡ حَاصِبN<ࣰ>ا ثُN<مَّ> لَا تَجِدُوX<ا۟> لَكُمۡ وَكِیلًا A<۝>D<٦٨>",
"أَمۡ أَمِN<ن>تُمۡ أَX<ن> N<یُ>عِیدَكُمۡ فِیهِ تَاR<رَ>ةً أُخۡR<رَ>ىٰ فَیُR<رۡ>سِلَ عَلَیۡكُمۡ قَاصِفX<ࣰ>ا N<مِّ>نَ ٱX<ل>رِّیحِ فَیُغۡرِقَكُN<م> بِمَا كَفَR<رۡ>تُمۡ ثُN<مَّ> لَا تَجِدُوX<ا۟> لَكُمۡ عَلَیۡنَا بِهِT<ۦ> تَبِیعࣰا A<۝>D<٦٩>",
"۞ وَلَقَQ<دۡ> كَR<رَّ>مۡنَا بَنِW<یۤ> ءَادَمَ وَحَمَلۡنَT<ـٰ>هُمۡ فِی ٱلۡبَرِّ وَX<ٱ>لۡبَحۡرِ وَR<رَ>زَQ<قۡ>نَT<ـٰ>هُم N<مِّ>نَ ٱX<ل>طَّیِّبَT<ـٰ>تِ وَفَضَّلۡنَT<ـٰ>هُمۡ عَلَىٰ كَثِیرX<ࣲ> N<مِّ>N<مَّ>نۡ خَلَQ<قۡ>نَا تَفۡضِیلࣰا A<۝>D<٧٠>",
"یَوۡمَ نَQ<دۡ>عُوX<ا۟> كُلَّ أُنَاسِN<ۭ> بِإِمَT<ـٰ>مِهِمۡۖ فَمَنۡ أُوتِیَ كِتَT<ـٰ>بَهُT<ۥ> بِیَمِینِهِT<ۦ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ یَQ<قۡ>R<رَ>ءُونَ كِتَT<ـٰ>بَهُمۡ وَلَا یُظۡلَمُونَ فَتِیلࣰا A<۝>D<٧١>",
"وَمَN<ن> كَانَ فِی هَT<ـٰ>ذِهِW<ۦۤ> أَعۡمَىٰ فَهُوَ فِی ٱلۡءَاخِR<رَ>ةِ أَعۡمَىٰ وَأَضَلُّ سَبِیلࣰا A<۝>D<٧٢>",
"وَإِN<ن> كَادُوX<ا۟> لَیَفۡتِنُونَكَ عَنِ ٱلَّذِW<یۤ> أَوۡحَیۡنَW<اۤ> إِلَیۡكَ لِتَفۡتَرِیَ عَلَیۡنَا غَیۡR<رَ>هُT<ۥ>ۖ وَإِذX<ࣰ>ا لَّX<ٱ>تَّخَذُوكَ خَلِیلࣰا A<۝>D<٧٣>",
"وَلَوۡلَW<اۤ> أَN<ن> ثَبَّتۡنَT<ـٰ>كَ لَقَQ<دۡ> كِX<د>تَّ تَR<رۡ>كَنُ إِلَیۡهِمۡ شَیۡءN<ࣰ>ا قَلِیلًا A<۝>D<٧٤>",
"إِذX<ࣰ>ا لَّأَذَQ<قۡ>نَT<ـٰ>كَ ضِعۡفَ ٱلۡحَیَX<و>T<ٰ>ةِ وَضِعۡفَ ٱلۡمَمَاتِ ثُN<مَّ> لَا تَجِدُ لَكَ عَلَیۡنَا نَصِیR<ر>ࣰا A<۝>D<٧٥>",
"وَإِN<ن> كَادُوX<ا۟> لَیَسۡتَفِزُّونَكَ مِنَ ٱلۡأَR<رۡ>ضِ لِیُخۡرِجُوكَ مِنۡهَاۖ وَإِذX<ࣰ>ا لَّا یَلۡبَثُونَ خِلَT<ـٰ>فَكَ إِلَّا قَلِیلࣰا A<۝>D<٧٦>",
"سُN<نَّ>ةَ مَN<ن> قَQ<دۡ> أَR<رۡ>سَلۡنَا قَQ<بۡ>لَكَ مِX<ن> R<رُّ>سُلِنَاۖ وَلَا تَجِدُ لِسُN<نَّ>تِنَا تَحۡوِیلًا A<۝>D<٧٧>",
"أَقِمِ ٱX<ل>صَّلَX<و>T<ٰ>ةَ لِدُلُوكِ ٱX<ل>شَّمۡسِ إِلَىٰ غَسَقِ ٱلَّیۡلِ وَقُR<رۡ>ءَانَ ٱلۡفَQ<جۡ>رِۖ إِN<نَّ> قُR<رۡ>ءَانَ ٱلۡفَQ<جۡ>رِ كَانَ مَشۡهُودࣰا A<۝>D<٧٨>",
"وَمِنَ ٱلَّیۡلِ فَتَهَجَّQ<دۡ> بِهِT<ۦ> نَافِلَةX<ࣰ> لَّكَ عَسَىW<ٰۤ> أَX<ن> N<یَ>Q<بۡ>عَثَكَ R<رَ>بُّكَ مَقَامX<ࣰ>ا N<مَّ>حۡمُودࣰا A<۝>D<٧٩>",
"وَقُل R<رَّ>بِّ أَQ<دۡ>خِلۡنِی مُQ<دۡ>خَلَ صِQ<دۡ>قX<ࣲ> N<وَ>أَخۡرِQ<جۡ>نِی مُخۡR<رَ>جَ صِQ<دۡ>قX<ࣲ> N<وَ>X<ٱ>Q<جۡ>عَل لِّی مِX<ن> لَّدُN<ن>كَ سُلۡطَT<ـٰ>نX<ࣰ>ا N<نَّ>صِیR<ر>ࣰا A<۝>D<٨٠>",
"وَقُلۡ جَW<اۤ>ءَ ٱلۡحَقُّ وَزَهَقَ ٱلۡبَT<ـٰ>طِلُۚ إِN<نَّ> ٱلۡبَT<ـٰ>طِلَ كَانَ زَهُوقࣰا A<۝>D<٨١>",
"وَنُنَزِّلُ مِنَ ٱلۡقُR<رۡ>ءَانِ مَا هُوَ شِفَW<اۤ>ءX<ࣱ> N<وَ>R<رَ>حۡمَةX<ࣱ> لِّلۡمُؤۡمِنِینَ وَلَا یَزِیدُ ٱX<ل>ظَّT<ـٰ>لِمِینَ إِلَّا خَسَاR<ر>ࣰا A<۝>D<٨٢>",
"وَإِذَW<اۤ> أَنۡعَمۡنَا عَلَى ٱلۡإِN<ن>سَT<ـٰ>نِ أَعۡR<رَ>ضَ وَنَءَا بِجَانِبِهِT<ۦ> وَإِذَا مَسَّهُ ٱX<ل>شَّR<رُّ> كَانَ یَءُوسࣰا A<۝>D<٨٣>",
"قُلۡ كُلّX<ࣱ> N<یَ>عۡمَلُ عَلَىٰ شَاكِلَتِهِT<ۦ> فَR<رَ>بُّكُمۡ أَعۡلَمُ بِمَنۡ هُوَ أَهۡدَىٰ سَبِیلࣰا A<۝>D<٨٤>",
"وَیَسۡءَلُونَكَ عَنِ ٱX<ل>R<رُّ>وحِۖ قُلِ ٱX<ل>R<رُّ>وحُ مِنۡ أَمۡرِ R<رَ>بِّی وَمَW<اۤ> أُوتِیتُم N<مِّ>نَ ٱلۡعِلۡمِ إِلَّا قَلِیلࣰا A<۝>D<٨٥>",
"وَلَئِN<ن> شِئۡنَا لَنَذۡهَبَN<نَّ> بِX<ٱ>لَّذِW<یۤ> أَوۡحَیۡنَW<اۤ> إِلَیۡكَ ثُN<مَّ> لَا تَجِدُ لَكَ بِهِT<ۦ> عَلَیۡنَا وَكِیلًا A<۝>D<٨٦>",
"إِلَّا R<رَ>حۡمَةX<ࣰ> N<مِّ>X<ن> R<رَّ>بِّكَۚ إِN<نَّ> فَضۡلَهُT<ۥ> كَانَ عَلَیۡكَ كَبِیR<ر>ࣰا A<۝>D<٨٧>",
"قُل لَّئِنِ ٱQ<جۡ>تَمَعَتِ ٱلۡإِN<ن>سُ وَX<ٱ>لۡجِN<نُّ> عَلَىW<ٰۤ> أَX<ن> N<یَ>أۡتُوX<ا۟> بِمِثۡلِ هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانِ لَا یَأۡتُونَ بِمِثۡلِهِT<ۦ> وَلَوۡ كَانَ بَعۡضُهُمۡ لِبَعۡضN<ࣲ> ظَهِیR<ر>ࣰا A<۝>D<٨٨>",
"وَلَقَQ<دۡ> صَR<رَّ>فۡنَا لِX<ل>N<نَّ>اسِ فِی هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانِ مِN<ن> كُلِّ مَثَلN<ࣲ> فَأَبَىW<ٰۤ> أَكۡثَR<رُ> ٱX<ل>N<نَّ>اسِ إِلَّا كُفُوR<ر>ࣰا A<۝>D<٨٩>",
"وَقَالُوX<ا۟> لَن N<نُّ>ؤۡمِنَ لَكَ حَتَّىٰ تَفۡجُR<رَ> لَنَا مِنَ ٱلۡأَR<رۡ>ضِ یَX<ن>N<ۢ>بُوعًا A<۝>D<٩٠>",
"أَوۡ تَكُونَ لَكَ جَN<نَّ>ةX<ࣱ> N<مِّ>ن N<نَّ>خِیلX<ࣲ> N<وَ>عِنَبN<ࣲ> فَتُفَجِّR<رَ> ٱلۡأَنۡهَT<ـٰ>R<رَ> خِلَT<ـٰ>لَهَا تَفۡجِیR<ر>ًا A<۝>D<٩١>",
"أَوۡ تُسۡقِطَ ٱX<ل>سَّمَW<اۤ>ءَ كَمَا زَعَمۡتَ عَلَیۡنَا كِسَفًا أَوۡ تَأۡتِیَ بِٱللَّهِ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةِ قَبِیلًا A<۝>D<٩٢>",
"أَوۡ یَكُونَ لَكَ بَیۡتX<ࣱ> N<مِّ>N<ن> زُخۡR<رُ>فٍ أَوۡ تَR<رۡ>قَىٰ فِی ٱX<ل>سَّمَW<اۤ>ءِ وَلَن N<نُّ>ؤۡمِنَ لِR<رُ>قِیِّكَ حَتَّىٰ تُنَزِّلَ عَلَیۡنَا كِتَT<ـٰ>بX<ࣰ>ا N<نَّ>Q<قۡ>R<رَ>ؤُهُT<ۥ>ۗ قُلۡ سُQ<بۡ>حَانَ R<رَ>بِّی هَلۡ كُN<ن>تُ إِلَّا بَشَR<ر>X<ࣰ>ا R<رَّ>سُولࣰا A<۝>D<٩٣>",
"وَمَا مَنَعَ ٱX<ل>N<نَّ>اسَ أَX<ن> N<یُ>ؤۡمِنُW<وۤ>X<ا۟> إِذۡ جَW<اۤ>ءَهُمُ ٱلۡهُدَىW<ٰۤ> إِلَّW<اۤ> أَN<ن> قَالُW<وۤ>X<ا۟> أَبَعَثَ ٱللَّهُ بَشَR<ر>X<ࣰ>ا R<رَّ>سُولࣰا A<۝>D<٩٤>",
"قُل لَّوۡ كَانَ فِی ٱلۡأَR<رۡ>ضِ مَلَW<ـٰۤ>ئِكَةX<ࣱ> N<یَ>مۡشُونَ مُQ<طۡ>مَئِN<نِّ>ینَ لَنَزَّلۡنَا عَلَیۡهِم N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ مَلَكX<ࣰ>ا R<رَّ>سُولࣰا A<۝>D<٩٥>",
"قُلۡ كَفَىٰ بِٱللَّهِ شَهِیدَN<ۢ>ا بَیۡنِی وَبَیۡنَكُمۡۚ إِN<نَّ>هُT<ۥ> كَانَ بِعِبَادِهِT<ۦ> خَبِیR<رَ>N<ۢ>ا بَصِیR<ر>ࣰا A<۝>D<٩٦>",
"وَمَX<ن> N<یَ>هۡدِ ٱللَّهُ فَهُوَ ٱلۡمُهۡتَدِۖ وَمَX<ن> N<یُ>ضۡلِلۡ فَلَN<ن> تَجِدَ لَهُمۡ أَوۡلِیَW<اۤ>ءَ مِN<ن> دُونِهِT<ۦ>ۖ وَنَحۡشُR<رُ>هُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ عَلَىٰ وُجُوهِهِمۡ عُمۡیX<ࣰ>ا N<وَ>بُكۡمX<ࣰ>ا N<وَ>صُN<مّ>X<ࣰ>اۖ N<مَّ>أۡوَX<ى>T<ٰ>هُمۡ جَهَN<نَّ>مُۖ كُلَّمَا خَبَتۡ زِQ<دۡ>نَT<ـٰ>هُمۡ سَعِیR<ر>ࣰا A<۝>D<٩٧>",
"ذَT< ٰ>لِكَ جَزَW<اۤ>ؤُهُN<م> بِأَN<نَّ>هُمۡ كَفَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِنَا وَقَالُW<وۤ>X<ا۟> أَءِذَا كُN<نَّ>ا عِظَT<ـٰ>مX<ࣰ>ا N<وَ>R<رُ>فَT<ـٰ>تًا أَءِN<نَّ>ا لَمَQ<بۡ>عُوثُونَ خَلۡقN<ࣰ>ا جَدِیدًا A<۝>D<٩٨>",
"۞ أَوَ لَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ> ٱللَّهَ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ قَادِR<ر>ٌ عَلَىW<ٰۤ> أَX<ن> N<یَ>خۡلُقَ مِثۡلَهُمۡ وَجَعَلَ لَهُمۡ أَجَلX<ࣰ>ا لَّا R<رَ>یۡبَ فِیهِ فَأَبَى ٱX<ل>ظَّT<ـٰ>لِمُونَ إِلَّا كُفُوR<ر>ࣰا A<۝>D<٩٩>",
"قُل لَّوۡ أَN<ن>تُمۡ تَمۡلِكُونَ خَزَW<اۤ>ئِنَ R<رَ>حۡمَةِ R<رَ>بِّW<یۤ> إِذX<ࣰ>ا لَّأَمۡسَكۡتُمۡ خَشۡیَةَ ٱلۡإِN<ن>فَاقِۚ وَكَانَ ٱلۡإِN<ن>سَT<ـٰ>نُ قَتُوR<ر>ࣰا A<۝>D<١٠٠>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَىٰ تِسۡعَ ءَایَT<ـٰ>تِN<ۭ> بَیِّنَT<ـٰ>تN<ࣲ>ۖ فَسۡءَلۡ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ إِذۡ جَW<اۤ>ءَهُمۡ فَقَالَ لَهُT<ۥ> فِرۡعَوۡنُ إِN<نِّ>ی لَأَظُN<نُّ>كَ یَT<ـٰ>مُوسَىٰ مَسۡحُوR<ر>ࣰا A<۝>D<١٠١>",
"قَالَ لَقَQ<دۡ> عَلِمۡتَ مَW<اۤ> أَN<ن>زَلَ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ إِلَّا R<رَ>بُّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ بَصَW<اۤ>ئِR<رَ> وَإِN<نِّ>ی لَأَظُN<نُّ>كَ یَT<ـٰ>فِرۡعَوۡنُ مَثۡبُوR<ر>ࣰا A<۝>D<١٠٢>",
"فَأَR<رَ>ادَ أَX<ن> N<یَ>سۡتَفِزَّهُم N<مِّ>نَ ٱلۡأَR<رۡ>ضِ فَأَغۡR<رَ>Q<قۡ>نَT<ـٰ>هُ وَمَX<ن> N<مَّ>عَهُT<ۥ> جَمِیعࣰا A<۝>D<١٠٣>",
"وَقُلۡنَا مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> لِبَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ ٱسۡكُنُوX<ا۟> ٱلۡأَR<رۡ>ضَ فَإِذَا جَW<اۤ>ءَ وَعۡدُ ٱلۡءَاخِR<رَ>ةِ جِئۡنَا بِكُمۡ لَفِیفࣰا A<۝>D<١٠٤>",
"وَبِX<ٱ>لۡحَقِّ أَN<ن>زَلۡنَT<ـٰ>هُ وَبِX<ٱ>لۡحَقِّ نَزَلَۗ وَمَW<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ إِلَّا مُبَشِّR<ر>X<ࣰ>ا N<وَ>نَذِیR<ر>ࣰا A<۝>D<١٠٥>",
"وَقُR<رۡ>ءَانN<ࣰ>ا فَR<رَ>Q<قۡ>نَT<ـٰ>هُ لِتَQ<قۡ>R<رَ>أَهُT<ۥ> عَلَى ٱX<ل>N<نَّ>اسِ عَلَىٰ مُكۡثX<ࣲ> N<وَ>نَزَّلۡنَT<ـٰ>هُ تَN<ن>زِیلࣰا A<۝>D<١٠٦>",
"قُلۡ ءَامِنُوX<ا۟> بِهِW<ۦۤ> أَوۡ لَا تُؤۡمِنُW<وۤ>X<ا۟>ۚ إِN<نَّ> ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡعِلۡمَ مِN<ن> قَQ<بۡ>لِهِW<ۦۤ> إِذَا یُتۡلَىٰ عَلَیۡهِمۡ یَ̅خِ̅R<رُّ>̅و̅نَ̅ ̅لِ̅لۡ̅أَ̅ذۡ̅قَ̅ا̅نِ̅ ̅سُ̅جَّ̅دࣰ̅ا̅ A<۝>D<١٠٧>",
"وَیَقُولُونَ سُQ<بۡ>حَT<ـٰ>نَ R<رَ>بِّنَW<اۤ> إِN<ن> كَانَ وَعۡدُ R<رَ>بِّنَا لَمَفۡعُولࣰا A<۝>D<١٠٨>",
"وَیَخِR<رُّ>ونَ لِلۡأَذۡقَانِ یَQ<بۡ>كُونَ وَیَزِیدُهُمۡ خُشُوعࣰا A<۝>D<١٠٩> ۩",
"قُلِ ٱQ<دۡ>عُوX<ا۟> ٱللَّهَ أَوِ ٱQ<دۡ>عُوX<ا۟> ٱX<ل>R<رَّ>حۡمَT<ـٰ>نَۖ أَیّX<ࣰ>ا N<مَّ>ا تَQ<دۡ>عُوX<ا۟> فَلَهُ ٱلۡأَسۡمَW<اۤ>ءُ ٱلۡحُسۡنَىٰۚ وَلَا تَQ<جۡ>هَR<رۡ> بِصَلَاتِكَ وَلَا تُخَافِتۡ بِهَا وَX<ٱ>Q<بۡ>تَغِ بَیۡنَ ذَT< ٰ>لِكَ سَبِیلࣰا A<۝>D<١١٠>",
"وَقُلِ ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِی لَمۡ یَتَّخِذۡ وَلَدX<ࣰ>ا N<وَ>لَمۡ یَكُX<ن> لَّهُT<ۥ> شَرِیكN<ࣱ> فِی ٱلۡمُلۡكِ وَلَمۡ یَكُX<ن> لَّهُT<ۥ> وَلِیّX<ࣱ> N<مِّ>نَ ٱX<ل>ذُّلِّۖ وَكَبِّرۡهُ تَكۡبِیR<رَ>ۢا A<۝>D<١١١>",
"#ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِW<یۤ> أَN<ن>زَلَ عَلَىٰ عَQ<بۡ>دِهِ ٱلۡكِتَT<ـٰ>بَ وَلَمۡ یَQ<جۡ>عَل لَّهُT<ۥ> عِوَجَاۜ A<۝>D<١>",
"قَیِّمX<ࣰ>ا لِّیُN<ن>ذِR<رَ> بَأۡسN<ࣰ>ا شَدِیدX<ࣰ>ا N<مِّ>X<ن> لَّدُنۡهُ وَیُبَشِّR<رَ> ٱلۡمُؤۡمِنِینَ ٱلَّذِینَ یَعۡمَلُونَ ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ أَN<نَّ> لَهُمۡ أَQ<جۡ>R<ر>ًا حَسَنࣰا A<۝>D<٢>",
"مَّT<ـٰ>كِثِینَ فِیهِ أَبَدࣰا A<۝>D<٣>",
"وَیُN<ن>ذِR<رَ> ٱلَّذِینَ قَالُوX<ا۟> ٱتَّخَذَ ٱللَّهُ وَلَدࣰا A<۝>D<٤>",
"مَّا لَهُN<م> بِهِT<ۦ> مِنۡ عِلۡمX<ࣲ> N<وَ>لَا لِءَابَW<اۤ>ئِهِمۡۚ كَبُR<رَ>تۡ كَلِمَةN<ࣰ> تَخۡR<رُ>جُ مِنۡ أَفۡوَT< ٰ>هِهِمۡۚ إِX<ن> N<یَ>قُولُونَ إِلَّا كَذِبࣰا A<۝>D<٥>",
"فَلَعَلَّكَ بَT<ـٰ>خِعX<ࣱ> N<نَّ>فۡسَكَ عَلَىW<ٰۤ> ءَاثَT<ـٰ>رِهِمۡ إِX<ن> لَّمۡ یُؤۡمِنُوX<ا۟> بِهَT<ـٰ>ذَا ٱلۡحَدِیثِ أَسَفًا A<۝>D<٦>",
"إِN<نَّ>ا جَعَلۡنَا مَا عَلَى ٱلۡأَR<رۡ>ضِ زِینَةX<ࣰ> لَّهَا لِنَQ<بۡ>لُوَهُمۡ أَیُّهُمۡ أَحۡسَنُ عَمَلࣰا A<۝>D<٧>",
"وَإِN<نَّ>ا لَجَT<ـٰ>عِلُونَ مَا عَلَیۡهَا صَعِیدN<ࣰ>ا جُR<رُ>زًا A<۝>D<٨>",
"أَمۡ حَسِQ<بۡ>تَ أَN<نَّ> أَصۡحَT<ـٰ>بَ ٱلۡكَهۡفِ وَX<ٱ>X<ل>R<رَّ>قِیمِ كَانُوX<ا۟> مِنۡ ءَایَT<ـٰ>تِنَا عَجَبًا A<۝>D<٩>",
"إِذۡ أَوَى ٱلۡفِتۡیَةُ إِلَى ٱلۡكَهۡفِ فَقَالُوX<ا۟> R<رَ>بَّنَW<اۤ> ءَاتِنَا مِX<ن> لَّدُN<ن>كَ R<رَ>حۡمَةX<ࣰ> N<وَ>هَیِّئۡ لَنَا مِنۡ أَمۡرِنَا R<رَ>شَدࣰا A<۝>D<١٠>",
"فَضَR<رَ>Q<بۡ>نَا عَلَىW<ٰۤ> ءَاذَانِهِمۡ فِی ٱلۡكَهۡفِ سِنِینَ عَدَدࣰا A<۝>D<١١>",
"ثُN<مَّ> بَعَثۡنَT<ـٰ>هُمۡ لِنَعۡلَمَ أَیُّ ٱلۡحِزۡبَیۡنِ أَحۡصَىٰ لِمَا لَبِثُW<وۤ>X<ا۟> أَمَدࣰا A<۝>D<١٢>",
"نَّحۡنُ نَقُصُّ عَلَیۡكَ نَبَأَهُN<م> بِX<ٱ>لۡحَقِّۚ إِN<نَّ>هُمۡ فِتۡیَةٌ ءَامَنُوX<ا۟> بِR<رَ>بِّهِمۡ وَزِQ<دۡ>نَT<ـٰ>هُمۡ هُدࣰى A<۝>D<١٣>",
"وَR<رَ>بَQ<طۡ>نَا عَلَىٰ قُلُوبِهِمۡ إِذۡ قَامُوX<ا۟> فَقَالُوX<ا۟> R<رَ>بُّنَا R<رَ>بُّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ لَن N<نَّ>Q<دۡ>عُوَX<ا۟> مِN<ن> دُونِهِW<ۦۤ> إِلَT<ـٰ>هX<ࣰ>اۖ لَّقَQ<دۡ> قُلۡنَW<اۤ> إِذN<ࣰ>ا شَطَطًا A<۝>D<١٤>",
"هَW<ـٰۤ>ؤُلَW<اۤ>ءِ قَوۡمُنَا ٱتَّخَذُوX<ا۟> مِN<ن> دُونِهِW<ۦۤ> ءَالِهَةX<ࣰ>ۖ لَّوۡلَا یَأۡتُونَ عَلَیۡهِN<م> بِسُلۡطَT<ـٰ>نِN<ۭ> بَیِّنN<ࣲ>ۖ فَمَنۡ أَظۡلَمُ مِN<مَّ>نِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبࣰا A<۝>D<١٥>",
"وَإِذِ ٱعۡتَزَلۡتُمُوهُمۡ وَمَا یَعۡبُدُونَ إِلَّا ٱللَّهَ فَأۡوُW<ۥۤ>X<ا۟> إِلَى ٱلۡكَهۡفِ یَN<ن>شُR<رۡ> لَكُمۡ R<رَ>بُّكُم N<مِّ>X<ن> R<رَّ>حۡمَتِهِT<ۦ> وَیُهَیِّئۡ لَكُم N<مِّ>نۡ أَمۡرِكُم N<مِّ>رۡفَقࣰا A<۝>D<١٦>",
"۞ وَتَR<رَ>ى ٱX<ل>شَّمۡسَ إِذَا طَلَعَت تَّزَT< ٰ>وَR<رُ> عَN<ن> كَهۡفِهِمۡ ذَاتَ ٱلۡیَمِینِ وَإِذَا غَR<رَ>بَت تَّQ<قۡ>رِضُهُمۡ ذَاتَ ٱX<ل>شِّمَالِ وَهُمۡ فِی فَQ<جۡ>وَةX<ࣲ> N<مِّ>نۡهُۚ ذَT< ٰ>لِكَ مِنۡ ءَایَT<ـٰ>تِ ٱللَّهِۗ مَX<ن> N<یَ>هۡدِ ٱللَّهُ فَهُوَ ٱلۡمُهۡتَدِۖ وَمَX<ن> N<یُ>ضۡلِلۡ فَلَN<ن> تَجِدَ لَهُT<ۥ> وَلِیّX<ࣰ>ا N<مُّ>R<رۡ>شِدࣰا A<۝>D<١٧>",
"وَتَحۡسَبُهُمۡ أَیۡقَاظX<ࣰ>ا N<وَ>هُمۡ R<رُ>قُودX<ࣱ>ۚ N<وَ>نُقَلِّبُهُمۡ ذَاتَ ٱلۡیَمِینِ وَذَاتَ ٱX<ل>شِّمَالِۖ وَكَلۡبُهُN<م> بَT<ـٰ>سِطN<ࣱ> ذِR<رَ>اعَیۡهِ بِX<ٱ>لۡوَصِیدِۚ لَوِ ٱطَّلَعۡتَ عَلَیۡهِمۡ لَوَلَّیۡتَ مِنۡهُمۡ فِR<رَ>اR<ر>X<ࣰ>ا N<وَ>لَمُلِئۡتَ مِنۡهُمۡ R<رُ>عۡبࣰا A<۝>D<١٨>",
"وَكَذَT< ٰ>لِكَ بَعَثۡنَT<ـٰ>هُمۡ لِیَتَسَW<اۤ>ءَلُوX<ا۟> بَیۡنَهُمۡۚ قَالَ قَW<اۤ>ئِلX<ࣱ> N<مِّ>نۡهُمۡ كَمۡ لَبِثۡتُمۡۖ قَالُوX<ا۟> لَبِثۡنَا یَوۡمًا أَوۡ بَعۡضَ یَوۡمN<ࣲ>ۚ قَالُوX<ا۟> R<رَ>بُّكُمۡ أَعۡلَمُ بِمَا لَبِثۡتُمۡ فَX<ٱ>Q<بۡ>عَثُW<وۤ>X<ا۟> أَحَدَكُN<م> بِوَرِقِكُمۡ هَT<ـٰ>ذِهِW<ۦۤ> إِلَى ٱلۡمَدِینَةِ فَلۡیَN<ن>ظُR<رۡ> أَیُّهَW<اۤ> أَزۡكَىٰ طَعَامN<ࣰ>ا فَلۡیَأۡتِكُN<م> بِرِزۡقX<ࣲ> N<مِّ>نۡهُ وَلۡیَتَلَطَّفۡ وَلَا یُشۡعِR<رَ>N<نَّ> بِكُمۡ أَحَدًا A<۝>D<١٩>",
"إِN<نَّ>هُمۡ إِX<ن> N<یَ>ظۡهَR<رُ>وX<ا۟> عَلَیۡكُمۡ یَR<رۡ>جُمُوكُمۡ أَوۡ یُعِیدُوكُمۡ فِی مِلَّتِهِمۡ وَلَN<ن> تُفۡلِحُW<وۤ>X<ا۟> إِذًا أَبَدࣰا A<۝>D<٢٠>",
"وَكَذَT< ٰ>لِكَ أَعۡثَR<رۡ>نَا عَلَیۡهِمۡ لِیَعۡلَمُW<وۤ>X<ا۟> أَN<نَّ> وَعۡدَ ٱللَّهِ حَقّX<ࣱ> N<وَ>أَN<نَّ> ٱX<ل>سَّاعَةَ لَا R<رَ>یۡبَ فِیهَW<اۤ> إِذۡ یَتَنَT<ـٰ>زَعُونَ بَیۡنَهُمۡ أَمۡR<رَ>هُمۡۖ فَقَالُوX<ا۟> ٱQ<بۡ>نُوX<ا۟> عَلَیۡهِN<م> بُنۡیَT<ـٰ>نX<ࣰ>اۖ R<رَّ>بُّهُمۡ أَعۡلَمُ بِهِمۡۚ قَالَ ٱلَّذِینَ غَلَبُوX<ا۟> عَلَىW<ٰۤ> أَمۡرِهِمۡ لَنَتَّخِذَN<نَّ> عَلَیۡهِم N<مَّ>سۡجِدࣰا A<۝>D<٢١>",
"سَیَقُولُونَ ثَلَT<ـٰ>ثَةX<ࣱ> R<رَّ>ابِعُهُمۡ كَلۡبُهُمۡ وَیَقُولُونَ خَمۡسَةN<ࣱ> سَادِسُهُمۡ كَلۡبُهُمۡ R<رَ>Q<جۡ>مَN<ۢ>ا بِX<ٱ>لۡغَیۡبِۖ وَیَقُولُونَ سَQ<بۡ>عَةX<ࣱ> N<وَ>ثَامِنُهُمۡ كَلۡبُهُمۡۚ قُل R<رَّ>بِّW<یۤ> أَعۡلَمُ بِعِدَّتِهِم N<مَّ>ا یَعۡلَمُهُمۡ إِلَّا قَلِیلN<ࣱ>ۗ فَلَا تُمَارِ فِیهِمۡ إِلَّا مِR<رَ>W<اۤ>ءN<ࣰ> ظَT<ـٰ>هِR<ر>X<ࣰ>ا N<وَ>لَا تَسۡتَفۡتِ فِیهِم N<مِّ>نۡهُمۡ أَحَدࣰا A<۝>D<٢٢>",
"وَلَا تَقُولَN<نَّ> لِشَX<ا۟>یۡءٍ إِN<نِّ>ی فَاعِلN<ࣱ> ذَT< ٰ>لِكَ غَدًا A<۝>D<٢٣>",
"إِلَّW<اۤ> أَX<ن> N<یَ>شَW<اۤ>ءَ ٱللَّهُۚ وَX<ٱ>ذۡكُر R<رَّ>بَّكَ إِذَا نَسِیتَ وَقُلۡ عَسَىW<ٰۤ> أَX<ن> N<یَ>هۡدِیَنِ R<رَ>بِّی لِأَQ<قۡ>R<رَ>بَ مِنۡ هَT<ـٰ>ذَا R<رَ>شَدࣰا A<۝>D<٢٤>",
"وَلَبِثُوX<ا۟> فِی كَهۡفِهِمۡ ثَلَT<ـٰ>ثَ مِX<ا۟>ئَةN<ࣲ> سِنِینَ وَX<ٱ>زۡدَادُوX<ا۟> تِسۡعࣰا A<۝>D<٢٥>",
"قُلِ ٱللَّهُ أَعۡلَمُ بِمَا لَبِثُوX<ا۟>ۖ لَهُT<ۥ> غَیۡبُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ أَQ<بۡ>صِرۡ بِهِT<ۦ> وَأَسۡمِعۡۚ مَا لَهُم N<مِّ>N<ن> دُونِهِT<ۦ> مِX<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا یُشۡرِكُ فِی حُكۡمِهِW<ۦۤ> أَحَدࣰا A<۝>D<٢٦>",
"وَX<ٱ>تۡلُ مَW<اۤ> أُوحِیَ إِلَیۡكَ مِN<ن> كِتَابِ R<رَ>بِّكَۖ لَا مُبَدِّلَ لِكَلِمَT<ـٰ>تِهِT<ۦ> وَلَN<ن> تَجِدَ مِN<ن> دُونِهِT<ۦ> مُلۡتَحَدࣰا A<۝>D<٢٧>",
"وَX<ٱ>صۡبِرۡ نَفۡسَكَ مَعَ ٱلَّذِینَ یَQ<دۡ>عُونَ R<رَ>بَّهُN<م> بِX<ٱ>لۡغَدَX<و>T<ٰ>ةِ وَX<ٱ>لۡعَشِیِّ یُرِیدُونَ وَQ<جۡ>هَهُT<ۥ>ۖ وَلَا تَعۡدُ عَیۡنَاكَ عَنۡهُمۡ تُرِیدُ زِینَةَ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۖ وَلَا تُطِعۡ مَنۡ أَغۡفَلۡنَا قَلۡبَهُT<ۥ> عَN<ن> ذِكۡرِنَا وَX<ٱ>تَّبَعَ هَوَX<ى>T<ٰ>هُ وَكَانَ أَمۡR<رُ>هُT<ۥ> فُR<رُ>طࣰا A<۝>D<٢٨>",
"وَقُلِ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّكُمۡۖ فَمَN<ن> شَW<اۤ>ءَ فَلۡیُؤۡمِX<ن> N<وَ>مَN<ن> شَW<اۤ>ءَ فَلۡیَكۡفُR<رۡ>ۚ إِN<نَّ>W<اۤ> أَعۡتَQ<دۡ>نَا لِX<ل>ظَّT<ـٰ>لِمِینَ نَاR<ر>ًا أَحَاطَ بِهِمۡ سُR<رَ>ادِقُهَاۚ وَإِX<ن> N<یَ>سۡتَغِیثُوX<ا۟> یُغَاثُوX<ا۟> بِمَW<اۤ>ءN<ࣲ> كَX<ٱ>لۡمُهۡلِ یَشۡوِی ٱلۡوُجُوهَۚ بِئۡسَ ٱX<ل>شَّR<رَ>ابُ وَسَW<اۤ>ءَتۡ مُR<رۡ>تَفَقًا A<۝>D<٢٩>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ إِN<نَّ>ا لَا نُضِیعُ أَQ<جۡ>R<رَ> مَنۡ أَحۡسَنَ عَمَلًا A<۝>D<٣٠>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ جَN<نَّ>T<ـٰ>تُ عَQ<دۡ>نN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهِمُ ٱلۡأَنۡهَT<ـٰ>R<رُ> یُحَلَّوۡنَ فِیهَا مِنۡ أَسَاوِR<رَ> مِN<ن> ذَهَبX<ࣲ> N<وَ>یَلۡبَسُونَ ثِیَابًا خُضۡR<ر>X<ࣰ>ا N<مِّ>N<ن> سُN<ن>دُسX<ࣲ> N<وَ>إِسۡتَQ<بۡ>R<رَ>قX<ࣲ> N<مُّ>تَّكِءِینَ فِیهَا عَلَى ٱلۡأَR<رَ>W<اۤ>ئِكِۚ نِعۡمَ ٱX<ل>ثَّوَابُ وحَسُنَتۡ مُR<رۡ>تَفَقࣰا A<۝>D<٣١>",
"۞ وَX<ٱ>ضۡرِQ<بۡ> لَهُم N<مَّ>ثَلX<ࣰ>ا R<رَّ>جُلَیۡنِ جَعَلۡنَا لِأَحَدِهِمَا جَN<نَّ>تَیۡنِ مِنۡ أَعۡنَT<ـٰ>بX<ࣲ> N<وَ>حَفَفۡنَT<ـٰ>هُمَا بِنَخۡلX<ࣲ> N<وَ>جَعَلۡنَا بَیۡنَهُمَا زَR<رۡ>عࣰا A<۝>D<٣٢>",
"كِلۡتَا ٱلۡجَN<نَّ>تَیۡنِ ءَاتَتۡ أُكُلَهَا وَلَمۡ تَظۡلِم N<مِّ>نۡهُ شَیۡءX<ࣰ>اۚ N<وَ>فَجَّR<رۡ>نَا خِلَT<ـٰ>لَهُمَا نَهَR<ر>ࣰا A<۝>D<٣٣>",
"وَكَانَ لَهُT<ۥ> ثَمَR<ر>N<ࣱ> فَقَالَ لِصَT<ـٰ>حِبِهِT<ۦ> وَهُوَ یُحَاوِR<رُ>هُW<ۥۤ> أَنَا۠ أَكۡثَR<رُ> مِN<ن>كَ مَالX<ࣰ>ا N<وَ>أَعَزُّ نَفَR<ر>ࣰا A<۝>D<٣٤>",
"وَدَخَلَ جَN<نَّ>تَهُT<ۥ> وَهُوَ ظَالِمX<ࣱ> لِّنَفۡسِهِT<ۦ> قَالَ مَW<اۤ> أَظُN<نُّ> أَN<ن> تَبِیدَ هَT<ـٰ>ذِهِW<ۦۤ> أَبَدࣰا A<۝>D<٣٥>",
"وَمَW<اۤ> أَظُN<نُّ> ٱX<ل>سَّاعَةَ قَW<اۤ>ئِمَةX<ࣰ> N<وَ>لَئِX<ن> R<رُّ>دِX<د>تُّ إِلَىٰ R<رَ>بِّی لَأَجِدَN<نَّ> خَیۡR<ر>X<ࣰ>ا N<مِّ>نۡهَا مُN<ن>قَلَبࣰا A<۝>D<٣٦>",
"قَالَ لَهُT<ۥ> صَاحِبُهُT<ۥ> وَهُوَ یُحَاوِR<رُ>هُW<ۥۤ> أَكَفَR<رۡ>تَ بِX<ٱ>لَّذِی خَلَقَكَ مِN<ن> تُR<رَ>ابN<ࣲ> ثُN<مَّ> مِن N<نُّ>Q<طۡ>فَةN<ࣲ> ثُN<مَّ> سَوَّX<ى>T<ٰ>كَ R<رَ>جُلࣰا A<۝>D<٣٧>",
"لَّT<ـٰ>كِN<نَّ>ا۠ هُوَ ٱللَّهُ R<رَ>بِّی وَلَW<اۤ> أُشۡرِكُ بِR<رَ>بِّW<یۤ> أَحَدࣰا A<۝>D<٣٨>",
"وَلَوۡلَW<اۤ> إِذۡ دَخَلۡتَ جَN<نَّ>تَكَ قُلۡتَ مَا شَW<اۤ>ءَ ٱللَّهُ لَا قُوَّةَ إِلَّا بِٱللَّهِۚ إِN<ن> تَR<رَ>نِ أَنَا۠ أَقَلَّ مِN<ن>كَ مَالX<ࣰ>ا N<وَ>وَلَدࣰا A<۝>D<٣٩>",
"فَعَسَىٰ R<رَ>بِّW<یۤ> أَX<ن> N<یُ>ؤۡتِیَنِ خَیۡR<ر>X<ࣰ>ا N<مِّ>N<ن> جَN<نَّ>تِكَ وَیُR<رۡ>سِلَ عَلَیۡهَا حُسۡبَانX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ فَتُصۡبِحَ صَعِیدN<ࣰ>ا زَلَقًا A<۝>D<٤٠>",
"أَوۡ یُصۡبِحَ مَW<اۤ>ؤُهَا غَوۡR<ر>N<ࣰ>ا فَلَN<ن> تَسۡتَطِیعَ لَهُT<ۥ> طَلَبࣰا A<۝>D<٤١>",
"وَأُحِیطَ بِثَمَرِهِT<ۦ> فَأَصۡبَحَ یُقَلِّبُ كَفَّیۡهِ عَلَىٰ مَW<اۤ> أَN<ن>فَقَ فِیهَا وَهِیَ خَاوِیَةٌ عَلَىٰ عُR<رُ>وشِهَا وَیَقُولُ یَT<ـٰ>لَیۡتَنِی لَمۡ أُشۡرِكۡ بِR<رَ>بِّW<یۤ> أَحَدࣰا A<۝>D<٤٢>",
"وَلَمۡ تَكُX<ن> لَّهُT<ۥ> فِئَةX<ࣱ> N<یَ>N<ن>صُR<رُ>ونَهُT<ۥ> مِN<ن> دُونِ ٱللَّهِ وَمَا كَانَ مُN<ن>تَصِR<ر>ًا A<۝>D<٤٣>",
"هُنَالِكَ ٱلۡوَلَT<ـٰ>یَةُ لِلَّهِ ٱلۡحَقِّۚ هُوَ خَیۡR<ر>N<ࣱ> ثَوَابX<ࣰ>ا N<وَ>خَیۡR<ر>ٌ عُQ<قۡ>بࣰا A<۝>D<٤٤>",
"وَX<ٱ>ضۡرِQ<بۡ> لَهُم N<مَّ>ثَلَ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا كَمَW<اۤ>ءٍ أَN<ن>زَلۡنَT<ـٰ>هُ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ فَX<ٱ>خۡتَلَطَ بِهِT<ۦ> نَبَاتُ ٱلۡأَR<رۡ>ضِ فَأَصۡبَحَ هَشِیمN<ࣰ>ا تَذۡR<رُ>وهُ ٱX<ل>رِّیَT<ـٰ>حُۗ وَكَانَ ٱللَّهُ عَلَىٰ كُلِّ شَیۡءX<ࣲ> N<مُّ>Q<قۡ>تَدِR<ر>ًا A<۝>D<٤٥>",
"ٱلۡمَالُ وَX<ٱ>لۡبَنُونَ زِینَةُ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۖ وَX<ٱ>لۡبَT<ـٰ>قِیَT<ـٰ>تُ ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تُ خَیۡR<ر>ٌ عِN<ن>دَ R<رَ>بِّكَ ثَوَابX<ࣰ>ا N<وَ>خَیۡR<ر>ٌ أَمَلࣰا A<۝>D<٤٦>",
"وَیَوۡمَ نُسَیِّR<رُ> ٱلۡجِبَالَ وَتَR<رَ>ى ٱلۡأَR<رۡ>ضَ بَارِزَةX<ࣰ> N<وَ>حَشَR<رۡ>نَT<ـٰ>هُمۡ فَلَمۡ نُغَادِرۡ مِنۡهُمۡ أَحَدࣰا A<۝>D<٤٧>",
"وَعُرِضُوX<ا۟> عَلَىٰ R<رَ>بِّكَ صَفّX<ࣰ>ا لَّقَQ<دۡ> جِئۡتُمُونَا كَمَا خَلَQ<قۡ>نَT<ـٰ>كُمۡ أَوَّلَ مَR<رَّ>ةِN<ۭ>ۚ بَلۡ زَعَمۡتُمۡ أَلَّن N<نَّ>Q<جۡ>عَلَ لَكُم N<مَّ>وۡعِدࣰا A<۝>D<٤٨>",
"وَوُضِعَ ٱلۡكِتَT<ـٰ>بُ فَتَR<رَ>ى ٱلۡمُQ<جۡ>رِمِینَ مُشۡفِقِینَ مِN<مَّ>ا فِیهِ وَیَقُولُونَ یَT<ـٰ>وَیۡلَتَنَا مَالِ هَT<ـٰ>ذَا ٱلۡكِتَT<ـٰ>بِ لَا یُغَادِR<رُ> صَغِیR<رَ>ةX<ࣰ> N<وَ>لَا كَبِیR<رَ>ةً إِلَّW<اۤ> أَحۡصَX<ى>T<ٰ>هَاۚ وَوَجَدُوX<ا۟> مَا عَمِلُوX<ا۟> حَاضِR<ر>X<ࣰ>اۗ N<وَ>لَا یَظۡلِمُ R<رَ>بُّكَ أَحَدࣰا A<۝>D<٤٩>",
"وَإِذۡ قُلۡنَا لِلۡمَلَW<ـٰۤ>ئِكَةِ ٱسۡجُدُوX<ا۟> لِءَادَمَ فَسَجَدُW<وۤ>X<ا۟> إِلَّW<اۤ> إِQ<بۡ>لِیسَ كَانَ مِنَ ٱلۡجِN<نِّ> فَفَسَقَ عَنۡ أَمۡرِ R<رَ>بِّهِW<ۦۤ>ۗ أَفَتَتَّخِذُونَهُT<ۥ> وَذُرِّیَّتَهُW<ۥۤ> أَوۡلِیَW<اۤ>ءَ مِN<ن> دُونِی وَهُمۡ لَكُمۡ عَدُوُّN<ۢ>ۚ بِئۡسَ لِX<ل>ظَّT<ـٰ>لِمِینَ بَدَلࣰا A<۝>D<٥٠>",
"۞ N<مَّ>W<اۤ> أَشۡهَX<د>تُّهُمۡ خَلۡقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَلَا خَلۡقَ أَN<ن>فُسِهِمۡ وَمَا كُN<ن>تُ مُتَّخِذَ ٱلۡمُضِلِّینَ عَضُدࣰا A<۝>D<٥١>",
"وَیَوۡمَ یَقُولُ نَادُوX<ا۟> شُR<رَ>كَW<اۤ>ءِیَ ٱلَّذِینَ زَعَمۡتُمۡ فَدَعَوۡهُمۡ فَلَمۡ یَسۡتَجِیبُوX<ا۟> لَهُمۡ وَجَعَلۡنَا بَیۡنَهُم N<مَّ>وۡبِقࣰا A<۝>D<٥٢>",
"وَR<رَ>ءَا ٱلۡمُQ<جۡ>رِمُونَ ٱX<ل>N<نَّ>اR<رَ> فَظَN<نُّ>W<وۤ>X<ا۟> أَN<نَّ>هُم N<مُّ>وَاقِعُوهَا وَلَمۡ یَجِدُوX<ا۟> عَنۡهَا مَصۡرِفࣰا A<۝>D<٥٣>",
"وَلَقَQ<دۡ> صَR<رَّ>فۡنَا فِی هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانِ لِX<ل>N<نَّ>اسِ مِN<ن> كُلِّ مَثَلX<ࣲ>ۚ N<وَ>كَانَ ٱلۡإِN<ن>سَT<ـٰ>نُ أَكۡثَR<رَ> شَیۡءN<ࣲ> جَدَلࣰا A<۝>D<٥٤>",
"وَمَا مَنَعَ ٱX<ل>N<نَّ>اسَ أَX<ن> N<یُ>ؤۡمِنُW<وۤ>X<ا۟> إِذۡ جَW<اۤ>ءَهُمُ ٱلۡهُدَىٰ وَیَسۡتَغۡفِR<رُ>وX<ا۟> R<رَ>بَّهُمۡ إِلَّW<اۤ> أَN<ن> تَأۡتِیَهُمۡ سُN<نَّ>ةُ ٱلۡأَوَّلِینَ أَوۡ یَأۡتِیَهُمُ ٱلۡعَذَابُ قُبُلࣰا A<۝>D<٥٥>",
"وَمَا نُR<رۡ>سِلُ ٱلۡمُR<رۡ>سَلِینَ إِلَّا مُبَشِّرِینَ وَمُN<ن>ذِرِینَۚ وَیُجَT<ـٰ>دِلُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِX<ٱ>لۡبَT<ـٰ>طِلِ لِیُQ<دۡ>حِضُوX<ا۟> بِهِ ٱلۡحَقَّۖ وَX<ٱ>تَّخَذُW<وۤ>X<ا۟> ءَایَT<ـٰ>تِی وَمَW<اۤ> أُN<ن>ذِR<رُ>وX<ا۟> هُزُوࣰا A<۝>D<٥٦>",
"وَمَنۡ أَظۡلَمُ مِN<مَّ>N<ن> ذُكِّR<رَ> بِءَایَT<ـٰ>تِ R<رَ>بِّهِT<ۦ> فَأَعۡR<رَ>ضَ عَنۡهَا وَنَسِیَ مَا قَدَّمَتۡ یَدَاهُۚ إِN<نَّ>ا جَعَلۡنَا عَلَىٰ قُلُوبِهِمۡ أَكِN<نَّ>ةً أَX<ن> N<یَ>فۡقَهُوهُ وَفِW<یۤ> ءَاذَانِهِمۡ وَQ<قۡ>R<ر>X<ࣰ>اۖ N<وَ>إِN<ن> تَQ<دۡ>عُهُمۡ إِلَى ٱلۡهُدَىٰ فَلَX<ن> N<یَ>هۡتَدُW<وۤ>X<ا۟> إِذًا أَبَدࣰا A<۝>D<٥٧>",
"وَR<رَ>بُّكَ ٱلۡغَفُوR<رُ> ذُو ٱX<ل>R<رَّ>حۡمَةِۖ لَوۡ یُؤَاخِذُهُN<م> بِمَا كَسَبُوX<ا۟> لَعَجَّلَ لَهُمُ ٱلۡعَذَابَۚ بَل لَّهُم N<مَّ>وۡعِدX<ࣱ> لَّX<ن> N<یَ>جِدُوX<ا۟> مِN<ن> دُونِهِT<ۦ> مَوۡئِلࣰا A<۝>D<٥٨>",
"وَتِلۡكَ ٱلۡقُR<رَ>ىW<ٰۤ> أَهۡلَكۡنَT<ـٰ>هُمۡ لَN<مَّ>ا ظَلَمُوX<ا۟> وَجَعَلۡنَا لِمَهۡلِكِهِم N<مَّ>وۡعِدࣰا A<۝>D<٥٩>",
"وَإِذۡ قَالَ مُوسَىٰ لِفَتَX<ى>T<ٰ>هُ لَW<اۤ> أَQ<بۡ>R<رَ>حُ حَتَّىW<ٰۤ> أَQ<بۡ>لُغَ مَQ<جۡ>مَعَ ٱلۡبَحۡR<رَ>یۡنِ أَوۡ أَمۡضِیَ حُقُبࣰا A<۝>D<٦٠>",
"فَلَN<مَّ>ا بَلَغَا مَQ<جۡ>مَعَ بَیۡنِهِمَا نَسِیَا حُوتَهُمَا فَX<ٱ>تَّخَذَ سَبِیلَهُT<ۥ> فِی ٱلۡبَحۡرِ سَR<رَ>بࣰا A<۝>D<٦١>",
"فَلَN<مَّ>ا جَاوَزَا قَالَ لِفَتَX<ى>T<ٰ>هُ ءَاتِنَا غَدَW<اۤ>ءَنَا لَقَQ<دۡ> لَقِینَا مِN<ن> سَفَرِنَا هَT<ـٰ>ذَا نَصَبࣰا A<۝>D<٦٢>",
"قَالَ أَR<رَ>ءَیۡتَ إِذۡ أَوَیۡنَW<اۤ> إِلَى ٱX<ل>صَّخۡR<رَ>ةِ فَإِN<نِّ>ی نَسِیتُ ٱلۡحُوتَ وَمَW<اۤ> أَN<ن>سَX<ى>T<ٰ>نِیهُ إِلَّا ٱX<ل>شَّیۡطَT<ـٰ>نُ أَنۡ أَذۡكُR<رَ>هُT<ۥ>ۚ وَX<ٱ>تَّخَذَ سَبِیلَهُT<ۥ> فِی ٱلۡبَحۡرِ عَجَبࣰا A<۝>D<٦٣>",
"قَالَ ذَT< ٰ>لِكَ مَا كُN<نَّ>ا نَQ<بۡ>غِۚ فَX<ٱ>R<رۡ>تَدَّا عَلَىW<ٰۤ> ءَاثَارِهِمَا قَصَصࣰا A<۝>D<٦٤>",
"فَوَجَدَا عَQ<بۡ>دX<ࣰ>ا N<مِّ>نۡ عِبَادِنَW<اۤ> ءَاتَیۡنَT<ـٰ>هُ R<رَ>حۡمَةX<ࣰ> N<مِّ>نۡ عِN<ن>دِنَا وَعَلَّمۡنَT<ـٰ>هُ مِX<ن> لَّدُN<نَّ>ا عِلۡمࣰا A<۝>D<٦٥>",
"قَالَ لَهُT<ۥ> مُوسَىٰ هَلۡ أَتَّبِعُكَ عَلَىW<ٰۤ> أَN<ن> تُعَلِّمَنِ مِN<مَّ>ا عُلِّمۡتَ R<رُ>شۡدࣰا A<۝>D<٦٦>",
"قَالَ إِN<نَّ>كَ لَN<ن> تَسۡتَطِیعَ مَعِیَ صَQ<بۡ>R<ر>ࣰا A<۝>D<٦٧>",
"وَكَیۡفَ تَصۡبِR<رُ> عَلَىٰ مَا لَمۡ تُحِQ<طۡ> بِهِT<ۦ> خُQ<بۡ>R<ر>ࣰا A<۝>D<٦٨>",
"قَالَ سَتَجِدُنِW<یۤ> إِN<ن> شَW<اۤ>ءَ ٱللَّهُ صَابِR<ر>X<ࣰ>ا N<وَ>لَW<اۤ> أَعۡصِی لَكَ أَمۡR<ر>ࣰا A<۝>D<٦٩>",
"قَالَ فَإِنِ ٱتَّبَعۡتَنِی فَلَا تَسۡءَلۡنِی عَN<ن> شَیۡءٍ حَتَّىW<ٰۤ> أُحۡدِثَ لَكَ مِنۡهُ ذِكۡR<ر>ࣰا A<۝>D<٧٠>",
"فَX<ٱ>N<ن>طَلَقَا حَتَّىW<ٰۤ> إِذَا R<رَ>كِبَا فِی ٱX<ل>سَّفِینَةِ خَR<رَ>قَهَاۖ قَالَ أَخَR<رَ>Q<قۡ>تَهَا لِتُغۡرِقَ أَهۡلَهَا لَقَQ<دۡ> جِئۡتَ شَیۡءًا إِمۡR<ر>ࣰا A<۝>D<٧١>",
"قَالَ أَلَمۡ أَقُلۡ إِN<نَّ>كَ لَN<ن> تَسۡتَطِیعَ مَعِیَ صَQ<بۡ>R<ر>ࣰا A<۝>D<٧٢>",
"قَالَ لَا تُؤَاخِذۡنِی بِمَا نَسِیتُ وَلَا تُR<رۡ>هِQ<قۡ>نِی مِنۡ أَمۡرِی عُسۡR<ر>ࣰا A<۝>D<٧٣>",
"فَX<ٱ>N<ن>طَلَقَا حَتَّىW<ٰۤ> إِذَا لَقِیَا غُلَT<ـٰ>مN<ࣰ>ا فَقَتَلَهُT<ۥ> قَالَ أَقَتَلۡتَ نَفۡسN<ࣰ>ا زَكِیَّةَN<ۢ> بِغَیۡرِ نَفۡسX<ࣲ> لَّقَQ<دۡ> جِئۡتَ شَیۡءX<ࣰ>ا N<نُّ>كۡR<ر>ࣰا A<۝>D<٧٤>",
"۞ قَالَ أَلَمۡ أَقُل لَّكَ إِN<نَّ>كَ لَN<ن> تَسۡتَطِیعَ مَعِیَ صَQ<بۡ>R<ر>ࣰا A<۝>D<٧٥>",
"قَالَ إِN<ن> سَأَلۡتُكَ عَN<ن> شَیۡءِN<ۭ> بَعۡدَهَا فَلَا تُصَT<ـٰ>حِQ<بۡ>نِیۖ قَQ<دۡ> بَلَغۡتَ مِX<ن> لَّدُN<نِّ>ی عُذۡR<ر>ࣰا A<۝>D<٧٦>",
"فَX<ٱ>N<ن>طَلَقَا حَتَّىW<ٰۤ> إِذَW<اۤ> أَتَیَW<اۤ> أَهۡلَ قَR<رۡ>یَةٍ ٱسۡتَQ<طۡ>عَمَW<اۤ> أَهۡلَهَا فَأَبَوۡX<ا۟> أَX<ن> N<یُ>ضَیِّفُوهُمَا فَوَجَدَا فِیهَا جِدَاR<ر>X<ࣰ>ا N<یُ>رِیدُ أَX<ن> N<یَ>N<ن>قَضَّ فَأَقَامَهُT<ۥ>ۖ قَالَ لَوۡ شِئۡتَ لَتَّخَذۡتَ عَلَیۡهِ أَQ<جۡ>R<ر>ࣰا A<۝>D<٧٧>",
"قَالَ هَT<ـٰ>ذَا فِR<رَ>اقُ بَیۡنِی وَبَیۡنِكَۚ سَأُنَبِّئُكَ بِتَأۡوِیلِ مَا لَمۡ تَسۡتَطِع عَّلَیۡهِ صَQ<بۡ>R<ر>ًا A<۝>D<٧٨>",
"أَN<مَّ>ا ٱX<ل>سَّفِینَةُ فَكَانَتۡ لِمَسَT<ـٰ>كِینَ یَعۡمَلُونَ فِی ٱلۡبَحۡرِ فَأَR<رَ>X<د>تُّ أَنۡ أَعِیبَهَا وَكَانَ وَR<رَ>W<اۤ>ءَهُم N<مَّ>لِكX<ࣱ> N<یَ>أۡخُذُ كُلَّ سَفِینَةٍ غَصۡبࣰا A<۝>D<٧٩>",
"وَأَN<مَّ>ا ٱلۡغُلَT<ـٰ>مُ فَكَانَ أَبَوَاهُ مُؤۡمِنَیۡنِ فَخَشِینَW<اۤ> أَX<ن> N<یُ>R<رۡ>هِقَهُمَا طُغۡیَT<ـٰ>نX<ࣰ>ا N<وَ>كُفۡR<ر>ࣰا A<۝>D<٨٠>",
"فَأَR<رَ>Q<دۡ>نَW<اۤ> أَX<ن> N<یُ>Q<بۡ>دِلَهُمَا R<رَ>بُّهُمَا خَیۡR<ر>X<ࣰ>ا N<مِّ>نۡهُ زَكَX<و>T<ٰ>ةX<ࣰ> N<وَ>أَQ<قۡ>R<رَ>بَ R<رُ>حۡمࣰا A<۝>D<٨١>",
"وَأَN<مَّ>ا ٱلۡجِدَاR<رُ> فَكَانَ لِغُلَT<ـٰ>مَیۡنِ یَتِیمَیۡنِ فِی ٱلۡمَدِینَةِ وَكَانَ تَحۡتَهُT<ۥ> كَN<ن>زX<ࣱ> لَّهُمَا وَكَانَ أَبُوهُمَا صَT<ـٰ>لِحN<ࣰ>ا فَأَR<رَ>ادَ R<رَ>بُّكَ أَX<ن> N<یَ>Q<بۡ>لُغَW<اۤ> أَشُدَّهُمَا وَیَسۡتَخۡرِجَا كَN<ن>زَهُمَا R<رَ>حۡمَةX<ࣰ> N<مِّ>X<ن> R<رَّ>بِّكَۚ وَمَا فَعَلۡتُهُT<ۥ> عَنۡ أَمۡرِیۚ ذَT< ٰ>لِكَ تَأۡوِیلُ مَا لَمۡ تَسۡطِع عَّلَیۡهِ صَQ<بۡ>R<ر>ࣰا A<۝>D<٨٢>",
"وَیَسۡءَلُونَكَ عَN<ن> ذِی ٱلۡقَR<رۡ>نَیۡنِۖ قُلۡ سَأَتۡلُوX<ا۟> عَلَیۡكُم N<مِّ>نۡهُ ذِكۡR<ر>ًا A<۝>D<٨٣>",
"إِN<نَّ>ا مَكَّN<نَّ>ا لَهُT<ۥ> فِی ٱلۡأَR<رۡ>ضِ وَءَاتَیۡنَT<ـٰ>هُ مِN<ن> كُلِّ شَیۡءN<ࣲ> سَبَبࣰا A<۝>D<٨٤>",
"فَأَتۡبَعَ سَبَبًا A<۝>D<٨٥>",
"حَتَّىW<ٰۤ> إِذَا بَلَغَ مَغۡرِبَ ٱX<ل>شَّمۡسِ وَجَدَهَا تَغۡR<رُ>بُ فِی عَیۡنٍ حَمِئَةX<ࣲ> N<وَ>وَجَدَ عِN<ن>دَهَا قَوۡمN<ࣰ>اۖ قُلۡنَا یَT<ـٰ>ذَا ٱلۡقَR<رۡ>نَیۡنِ إِN<مَّ>W<اۤ> أَN<ن> تُعَذِّبَ وَإِN<مَّ>W<اۤ> أَN<ن> تَتَّخِذَ فِیهِمۡ حُسۡنࣰا A<۝>D<٨٦>",
"قَالَ أَN<مَّ>ا مَN<ن> ظَلَمَ فَسَوۡفَ نُعَذِّبُهُT<ۥ> ثُN<مَّ> یُR<رَ>دُّ إِلَىٰ R<رَ>بِّهِT<ۦ> فَیُعَذِّبُهُT<ۥ> عَذَابX<ࣰ>ا N<نُّ>كۡR<ر>ࣰا A<۝>D<٨٧>",
"وَأَN<مَّ>ا مَنۡ ءَامَنَ وَعَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَلَهُT<ۥ> جَزَW<اۤ>ءً ٱلۡحُسۡنَىٰۖ وَسَنَقُولُ لَهُT<ۥ> مِنۡ أَمۡرِنَا یُسۡR<ر>ࣰا A<۝>D<٨٨>",
"ثُN<مَّ> أَتۡبَعَ سَبَبًا A<۝>D<٨٩>",
"حَتَّىW<ٰۤ> إِذَا بَلَغَ مَQ<طۡ>لِعَ ٱX<ل>شَّمۡسِ وَجَدَهَا تَQ<طۡ>لُعُ عَلَىٰ قَوۡمX<ࣲ> لَّمۡ نَQ<جۡ>عَل لَّهُم N<مِّ>N<ن> دُونِهَا سِتۡR<ر>ࣰا A<۝>D<٩٠>",
"كَذَT< ٰ>لِكَۖ وَقَQ<دۡ> أَحَQ<طۡ>نَا بِمَا لَدَیۡهِ خُQ<بۡ>R<ر>ࣰا A<۝>D<٩١>",
"ثُN<مَّ> أَتۡبَعَ سَبَبًا A<۝>D<٩٢>",
"حَتَّىW<ٰۤ> إِذَا بَلَغَ بَیۡنَ ٱX<ل>سَّدَّیۡنِ وَجَدَ مِN<ن> دُونِهِمَا قَوۡمX<ࣰ>ا لَّا یَكَادُونَ یَفۡقَهُونَ قَوۡلࣰا A<۝>D<٩٣>",
"قَالُوX<ا۟> یَT<ـٰ>ذَا ٱلۡقَR<رۡ>نَیۡنِ إِN<نَّ> یَأۡجُوجَ وَمَأۡجُوجَ مُفۡسِدُونَ فِی ٱلۡأَR<رۡ>ضِ فَهَلۡ نَQ<جۡ>عَلُ لَكَ خَR<رۡ>جًا عَلَىW<ٰۤ> أَN<ن> تَQ<جۡ>عَلَ بَیۡنَنَا وَبَیۡنَهُمۡ سَدࣰّا A<۝>D<٩٤>",
"قَالَ مَا مَكَّN<نِّ>ی فِیهِ R<رَ>بِّی خَیۡR<ر>N<ࣱ> فَأَعِینُونِی بِقُوَّةٍ أَQ<جۡ>عَلۡ بَیۡنَكُمۡ وَبَیۡنَهُمۡ R<رَ>Q<دۡ>مًا A<۝>D<٩٥>",
"ءَاتُونِی زُبَR<رَ> ٱلۡحَدِیدِۖ حَتَّىW<ٰۤ> إِذَا سَاوَىٰ بَیۡنَ ٱX<ل>صَّدَفَیۡنِ قَالَ ٱN<ن>فُخُوX<ا۟>ۖ حَتَّىW<ٰۤ> إِذَا جَعَلَهُT<ۥ> نَاR<ر>N<ࣰ>ا قَالَ ءَاتُونِW<یۤ> أُفۡرِغۡ عَلَیۡهِ قِQ<طۡ>R<ر>ࣰا A<۝>D<٩٦>",
"فَمَا ٱسۡطَT<ـٰ>عُW<وۤ>X<ا۟> أَX<ن> N<یَ>ظۡهَR<رُ>وهُ وَمَا ٱسۡتَطَT<ـٰ>عُوX<ا۟> لَهُT<ۥ> نَQ<قۡ>بࣰا A<۝>D<٩٧>",
"قَالَ هَT<ـٰ>ذَا R<رَ>حۡمَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّیۖ فَإِذَا جَW<اۤ>ءَ وَعۡدُ R<رَ>بِّی جَعَلَهُT<ۥ> دَكَّW<اۤ>ءَۖ وَكَانَ وَعۡدُ R<رَ>بِّی حَقࣰّا A<۝>D<٩٨>",
"۞ وَتَR<رَ>كۡنَا بَعۡضَهُمۡ یَوۡمَئِذX<ࣲ> N<یَ>مُوجُ فِی بَعۡضX<ࣲ>ۖ N<وَ>نُفِخَ فِی ٱX<ل>صُّورِ فَجَمَعۡنَT<ـٰ>هُمۡ جَمۡعࣰا A<۝>D<٩٩>",
"وَعَR<رَ>ضۡنَا جَهَN<نَّ>مَ یَوۡمَئِذX<ࣲ> لِّلۡكَT<ـٰ>فِرِینَ عَR<رۡ>ضًا A<۝>D<١٠٠>",
"ٱلَّذِینَ كَانَتۡ أَعۡیُنُهُمۡ فِی غِطَW<اۤ>ءٍ عَN<ن> ذِكۡرِی وَكَانُوX<ا۟> لَا یَسۡتَطِیعُونَ سَمۡعًا A<۝>D<١٠١>",
"أَفَحَسِبَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَX<ن> N<یَ>تَّخِذُوX<ا۟> عِبَادِی مِN<ن> دُونِW<یۤ> أَوۡلِیَW<اۤ>ءَۚ إِN<نَّ>W<اۤ> أَعۡتَQ<دۡ>نَا جَهَN<نَّ>مَ لِلۡكَT<ـٰ>فِرِینَ نُزُلࣰا A<۝>D<١٠٢>",
"قُلۡ هَلۡ نُنَبِّئُكُN<م> بِX<ٱ>لۡأَخۡسَرِینَ أَعۡمَT<ـٰ>لًا A<۝>D<١٠٣>",
"ٱلَّذِینَ ضَلَّ سَعۡیُهُمۡ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَهُمۡ یَحۡسَبُونَ أَN<نَّ>هُمۡ یُحۡسِنُونَ صُنۡعًا A<۝>D<١٠٤>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِ R<رَ>بِّهِمۡ وَلِقَW<اۤ>ئِهِT<ۦ> فَحَبِطَتۡ أَعۡمَT<ـٰ>لُهُمۡ فَلَا نُقِیمُ لَهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ وَزۡنࣰا A<۝>D<١٠٥>",
"ذَT< ٰ>لِكَ جَزَW<اۤ>ؤُهُمۡ جَهَN<نَّ>مُ بِمَا كَفَR<رُ>وX<ا۟> وَX<ٱ>تَّخَذُW<وۤ>X<ا۟> ءَایَT<ـٰ>تِی وَR<رُ>سُلِی هُزُوًا A<۝>D<١٠٦>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ كَانَتۡ لَهُمۡ جَN<نَّ>T<ـٰ>تُ ٱلۡفِرۡدَوۡسِ نُزُلًا A<۝>D<١٠٧>",
"خَT<ـٰ>لِدِینَ فِیهَا لَا یَQ<بۡ>غُونَ عَنۡهَا حِوَلࣰا A<۝>D<١٠٨>",
"قُل لَّوۡ كَانَ ٱلۡبَحۡR<رُ> مِدَادX<ࣰ>ا لِّكَلِمَT<ـٰ>تِ R<رَ>بِّی لَنَفِدَ ٱلۡبَحۡR<رُ> قَQ<بۡ>لَ أَN<ن> تَN<ن>فَدَ كَلِمَT<ـٰ>تُ R<رَ>بِّی وَلَوۡ جِئۡنَا بِمِثۡلِهِT<ۦ> مَدَدࣰا A<۝>D<١٠٩>",
"قُلۡ إِN<نَّ>مَW<اۤ> أَنَا۠ بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُكُمۡ یُوحَىW<ٰۤ> إِلَیَّ أَN<نَّ>مَW<اۤ> إِلَT<ـٰ>هُكُمۡ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدN<ࣱ>ۖ فَمَN<ن> كَانَ یَR<رۡ>جُوX<ا۟> لِقَW<اۤ>ءَ R<رَ>بِّهِT<ۦ> فَلۡیَعۡمَلۡ عَمَلN<ࣰ>ا صَT<ـٰ>لِحX<ࣰ>ا N<وَ>لَا یُشۡرِكۡ بِعِبَادَةِ R<رَ>بِّهِW<ۦۤ> أَحَدَۢا A<۝>D<١١٠>",
"#L<كۤ>هیL<عۤ>L<صۤ> A<۝>D<١>",
"ذِكۡR<رُ> R<رَ>حۡمَتِ R<رَ>بِّكَ عَQ<بۡ>دَهُT<ۥ> زَكَرِیَّاۤ A<۝>D<٢>",
"إِذۡ نَادَىٰ R<رَ>بَّهُT<ۥ> نِدَW<اۤ>ءً خَفِیࣰّا A<۝>D<٣>",
"قَالَ R<رَ>بِّ إِN<نِّ>ی وَهَنَ ٱلۡعَظۡمُ مِN<نِّ>ی وَX<ٱ>شۡتَعَلَ ٱX<ل>R<رَّ>أۡسُ شَیۡبX<ࣰ>ا N<وَ>لَمۡ أَكُX<ن>N<ۢ> بِدُعَW<اۤ>ئِكَ R<رَ>بِّ شَقِیࣰّا A<۝>D<٤>",
"وَإِN<نِّ>ی خِفۡتُ ٱلۡمَوَT< ٰ>لِیَ مِX<ن> N<وَ>R<رَ>W<اۤ>ءِی وَكَانَتِ ٱمۡR<رَ>أَتِی عَاقِR<ر>N<ࣰ>ا فَهَQ<بۡ> لِی مِX<ن> لَّدُN<ن>كَ وَلِیࣰّا A<۝>D<٥>",
"یَرِثُنِی وَیَرِثُ مِنۡ ءَالِ یَعۡقُوبَۖ وَX<ٱ>Q<جۡ>عَلۡهُ R<رَ>بِّ R<رَ>ضِیࣰّا A<۝>D<٦>",
"یَT<ـٰ>زَكَرِیَّW<اۤ> إِN<نَّ>ا نُبَشِّR<رُ>كَ بِغُلَT<ـٰ>مٍ ٱسۡمُهُT<ۥ> یَحۡیَىٰ لَمۡ نَQ<جۡ>عَل لَّهُT<ۥ> مِN<ن> قَQ<بۡ>لُ سَمِیࣰّا A<۝>D<٧>",
"قَالَ R<رَ>بِّ أَN<نَّ>ىٰ یَكُونُ لِی غُلَT<ـٰ>مX<ࣱ> N<وَ>كَانَتِ ٱمۡR<رَ>أَتِی عَاقِR<ر>X<ࣰ>ا N<وَ>قَQ<دۡ> بَلَغۡتُ مِنَ ٱلۡكِبَرِ عِتِیࣰّا A<۝>D<٨>",
"قَالَ كَذَT< ٰ>لِكَ قَالَ R<رَ>بُّكَ هُوَ عَلَیَّ هَیِّنX<ࣱ> N<وَ>قَQ<دۡ> خَلَQ<قۡ>تُكَ مِN<ن> قَQ<بۡ>لُ وَلَمۡ تَكُ شَیۡءࣰا A<۝>D<٩>",
"قَالَ R<رَ>بِّ ٱQ<جۡ>عَل لِّW<یۤ> ءَایَةN<ࣰ>ۖ قَالَ ءَایَتُكَ أَلَّا تُكَلِّمَ ٱX<ل>N<نَّ>اسَ ثَلَT<ـٰ>ثَ لَیَالN<ࣲ> سَوِیࣰّا A<۝>D<١٠>",
"فَخَR<رَ>جَ عَلَىٰ قَوۡمِهِT<ۦ> مِنَ ٱلۡمِحۡR<رَ>ابِ فَأَوۡحَىW<ٰۤ> إِلَیۡهِمۡ أَN<ن> سَبِّحُوX<ا۟> بُكۡR<رَ>ةX<ࣰ> N<وَ>عَشِیࣰّا A<۝>D<١١>",
"یَT<ـٰ>یَحۡیَىٰ خُذِ ٱلۡكِتَT<ـٰ>بَ بِقُوَّةX<ࣲ>ۖ N<وَ>ءَاتَیۡنَT<ـٰ>هُ ٱلۡحُكۡمَ صَبِیࣰّا A<۝>D<١٢>",
"وَحَنَانX<ࣰ>ا N<مِّ>X<ن> لَّدُN<نَّ>ا وَزَكَX<و>T<ٰ>ةX<ࣰ>ۖ N<وَ>كَانَ تَقِیࣰّا A<۝>D<١٣>",
"وَبَR<رَّ>N<ۢ>ا بِوَT< ٰ>لِدَیۡهِ وَلَمۡ یَكُN<ن> جَبَّاR<ر>ًا عَصِیࣰّا A<۝>D<١٤>",
"وَسَلَT<ـٰ>مٌ عَلَیۡهِ یَوۡمَ وُلِدَ وَیَوۡمَ یَمُوتُ وَیَوۡمَ یُQ<بۡ>عَثُ حَیࣰّا A<۝>D<١٥>",
"وَX<ٱ>ذۡكُR<رۡ> فِی ٱلۡكِتَT<ـٰ>بِ مَR<رۡ>یَمَ إِذِ ٱN<ن>تَبَذَتۡ مِنۡ أَهۡلِهَا مَكَانN<ࣰ>ا شَR<رۡ>قِیࣰّا A<۝>D<١٦>",
"فَX<ٱ>تَّخَذَتۡ مِN<ن> دُونِهِمۡ حِجَابN<ࣰ>ا فَأَR<رۡ>سَلۡنَW<اۤ> إِلَیۡهَا R<رُ>وحَنَا فَتَمَثَّلَ لَهَا بَشَR<ر>N<ࣰ>ا سَوِیࣰّا A<۝>D<١٧>",
"قَالَتۡ إِN<نِّ>W<یۤ> أَعُوذُ بِX<ٱ>X<ل>R<رَّ>حۡمَT<ـٰ>نِ مِN<ن>كَ إِN<ن> كُN<ن>تَ تَقِیࣰّا A<۝>D<١٨>",
"قَالَ إِN<نَّ>مَW<اۤ> أَنَا۠ R<رَ>سُولُ R<رَ>بِّكِ لِأَهَبَ لَكِ غُلَT<ـٰ>مN<ࣰ>ا زَكِیࣰّا A<۝>D<١٩>",
"قَالَتۡ أَN<نَّ>ىٰ یَكُونُ لِی غُلَT<ـٰ>مX<ࣱ> N<وَ>لَمۡ یَمۡسَسۡنِی بَشَR<ر>X<ࣱ> N<وَ>لَمۡ أَكُ بَغِیࣰّا A<۝>D<٢٠>",
"قَالَ كَذَT< ٰ>لِكِ قَالَ R<رَ>بُّكِ هُوَ عَلَیَّ هَیِّنX<ࣱ>ۖ N<وَ>لِنَQ<جۡ>عَلَهُW<ۥۤ> ءَایَةX<ࣰ> لِّX<ل>N<نَّ>اسِ وَR<رَ>حۡمَةX<ࣰ> N<مِّ>N<نَّ>اۚ وَكَانَ أَمۡR<ر>X<ࣰ>ا N<مَّ>Q<قۡ>ضِیࣰّا A<۝>D<٢١>",
"۞ فَحَمَلَتۡهُ فَX<ٱ>N<ن>تَبَذَتۡ بِهِT<ۦ> مَكَانN<ࣰ>ا قَصِیࣰّا A<۝>D<٢٢>",
"فَأَجَW<اۤ>ءَهَا ٱلۡمَخَاضُ إِلَىٰ جِذۡعِ ٱX<ل>N<نَّ>خۡلَةِ قَالَتۡ یَT<ـٰ>لَیۡتَنِی مِتُّ قَQ<بۡ>لَ هَT<ـٰ>ذَا وَكُN<ن>تُ نَسۡیX<ࣰ>ا N<مَّ>N<ن>سِیࣰّا A<۝>D<٢٣>",
"فَنَادَX<ى>T<ٰ>هَا مِN<ن> تَحۡتِهَW<اۤ> أَلَّا تَحۡزَنِی قَQ<دۡ> جَعَلَ R<رَ>بُّكِ تَحۡتَكِ سَرِیࣰّا A<۝>D<٢٤>",
"وَهُزِّW<یۤ> إِلَیۡكِ بِجِذۡعِ ٱX<ل>N<نَّ>خۡلَةِ تُسَT<ـٰ>قِQ<طۡ> عَلَیۡكِ R<رُ>طَبN<ࣰ>ا جَنِیࣰّا A<۝>D<٢٥>",
"فَكُلِی وَX<ٱ>شۡR<رَ>بِی وَقَرِّی عَیۡنN<ࣰ>اۖ فَإِN<مَّ>ا تَR<رَ>یِN<نَّ> مِنَ ٱلۡبَشَرِ أَحَدN<ࣰ>ا فَقُولِW<یۤ> إِN<نِّ>ی نَذَR<رۡ>تُ لِX<ل>R<رَّ>حۡمَT<ـٰ>نِ صَوۡمN<ࣰ>ا فَلَنۡ أُكَلِّمَ ٱلۡیَوۡمَ إِN<ن>سِیࣰّا A<۝>D<٢٦>",
"فَأَتَتۡ بِهِT<ۦ> قَوۡمَهَا تَحۡمِلُهُT<ۥ>ۖ قَالُوX<ا۟> یَT<ـٰ>مَR<رۡ>یَمُ لَقَQ<دۡ> جِئۡتِ شَیۡءN<ࣰ>ا فَرِیࣰّا A<۝>D<٢٧>",
"یَW<ـٰۤ>أُخۡتَ هَT<ـٰ>R<رُ>ونَ مَا كَانَ أَبُوكِ ٱمۡR<رَ>أَ سَوۡءX<ࣲ> N<وَ>مَا كَانَتۡ أُN<مُّ>كِ بَغِیࣰّا A<۝>D<٢٨>",
"فَأَشَاR<رَ>تۡ إِلَیۡهِۖ قَالُوX<ا۟> كَیۡفَ نُكَلِّمُ مَN<ن> كَانَ فِی ٱلۡمَهۡدِ صَبِیࣰّا A<۝>D<٢٩>",
"قَالَ إِN<نِّ>ی عَQ<بۡ>دُ ٱللَّهِ ءَاتَX<ى>T<ٰ>نِیَ ٱلۡكِتَT<ـٰ>بَ وَجَعَلَنِی نَبِیࣰّا A<۝>D<٣٠>",
"وَجَعَلَنِی مُبَاR<رَ>كًا أَیۡنَ مَا كُN<ن>تُ وَأَوۡصَT<ـٰ>نِی بِX<ٱ>X<ل>صَّلَX<و>T<ٰ>ةِ وَX<ٱ>X<ل>زَّكَX<و>T<ٰ>ةِ مَا دُمۡتُ حَیࣰّا A<۝>D<٣١>",
"وَبَR<رَّ>N<ۢ>ا بِوَT< ٰ>لِدَتِی وَلَمۡ یَQ<جۡ>عَلۡنِی جَبَّاR<ر>N<ࣰ>ا شَقِیࣰّا A<۝>D<٣٢>",
"وَX<ٱ>X<ل>سَّلَT<ـٰ>مُ عَلَیَّ یَوۡمَ وُلِX<د>تُّ وَیَوۡمَ أَمُوتُ وَیَوۡمَ أُQ<بۡ>عَثُ حَیࣰّا A<۝>D<٣٣>",
"ذَT< ٰ>لِكَ عِیسَى ٱQ<بۡ>نُ مَR<رۡ>یَمَۖ قَوۡلَ ٱلۡحَقِّ ٱلَّذِی فِیهِ یَمۡتَR<رُ>J<و>نَ A<۝>D<٣٤>",
"مَا كَانَ لِلَّهِ أَX<ن> N<یَ>تَّخِذَ مِX<ن> N<وَ>لَدN<ࣲ>ۖ سُQ<بۡ>حَT<ـٰ>نَهُW<ۥۤ>ۚ إِذَا قَضَىW<ٰۤ> أَمۡR<ر>N<ࣰ>ا فَإِN<نَّ>مَا یَقُولُ لَهُT<ۥ> كُN<ن> فَیَكُJ<و>نُ A<۝>D<٣٥>",
"وَإِN<نَّ> ٱللَّهَ R<رَ>بِّی وَR<رَ>بُّكُمۡ فَX<ٱ>عۡبُدُوهُۚ هَT<ـٰ>ذَا صِR<رَ>T< ٰ>طX<ࣱ> N<مُّ>سۡتَقِJ<ی>مࣱ A<۝>D<٣٦>",
"فَX<ٱ>خۡتَلَفَ ٱلۡأَحۡزَابُ مِX<ن>N<ۢ> بَیۡنِهِمۡۖ فَوَیۡلX<ࣱ> لِّلَّذِینَ كَفَR<رُ>وX<ا۟> مِX<ن> N<مَّ>شۡهَدِ یَوۡمٍ عَظِJ<ی>مٍ A<۝>D<٣٧>",
"أَسۡمِعۡ بِهِمۡ وَأَQ<بۡ>صِرۡ یَوۡمَ یَأۡتُونَنَاۖ لَT<ـٰ>كِنِ ٱX<ل>ظَّT<ـٰ>لِمُونَ ٱلۡیَوۡمَ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٣٨>",
"وَأَN<ن>ذِرۡهُمۡ یَوۡمَ ٱلۡحَسۡR<رَ>ةِ إِذۡ قُضِیَ ٱلۡأَمۡR<رُ> وَهُمۡ فِی غَفۡلَةX<ࣲ> N<وَ>هُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٣٩>",
"إِN<نَّ>ا نَحۡنُ نَرِثُ ٱلۡأَR<رۡ>ضَ وَمَنۡ عَلَیۡهَا وَإِلَیۡنَا یُR<رۡ>جَعُJ<و>نَ A<۝>D<٤٠>",
"وَX<ٱ>ذۡكُR<رۡ> فِی ٱلۡكِتَT<ـٰ>بِ إِQ<بۡ>R<رَ>T< ٰ>هِیمَۚ إِN<نَّ>هُT<ۥ> كَانَ صِدِّیقX<ࣰ>ا N<نَّ>بِیًّا A<۝>D<٤١>",
"إِذۡ قَالَ لِأَبِیهِ یَW<ـٰۤ>أَبَتِ لِمَ تَعۡبُدُ مَا لَا یَسۡمَعُ وَلَا یُQ<بۡ>صِR<رُ> وَلَا یُغۡنِی عَN<ن>كَ شَیۡءࣰا A<۝>D<٤٢>",
"یَW<ـٰۤ>أَبَتِ إِN<نِّ>ی قَQ<دۡ> جَW<اۤ>ءَنِی مِنَ ٱلۡعِلۡمِ مَا لَمۡ یَأۡتِكَ فَX<ٱ>تَّبِعۡنِW<یۤ> أَهۡدِكَ صِR<رَ>T< ٰ>طN<ࣰ>ا سَوِیࣰّا A<۝>D<٤٣>",
"یَW<ـٰۤ>أَبَتِ لَا تَعۡبُدِ ٱX<ل>شَّیۡطَT<ـٰ>نَۖ إِN<نَّ> ٱX<ل>شَّیۡطَT<ـٰ>نَ كَانَ لِX<ل>R<رَّ>حۡمَT<ـٰ>نِ عَصِیࣰّا A<۝>D<٤٤>",
"یَW<ـٰۤ>أَبَتِ إِN<نِّ>W<یۤ> أَخَافُ أَX<ن> N<یَ>مَسَّكَ عَذَابX<ࣱ> N<مِّ>نَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ فَتَكُونَ لِX<ل>شَّیۡطَT<ـٰ>نِ وَلِیࣰّا A<۝>D<٤٥>",
"قَالَ أَR<رَ>اغِبٌ أَN<ن>تَ عَنۡ ءَالِهَتِی یَW<ـٰۤ>إِQ<بۡ>R<رَ>T< ٰ>هِیمُۖ لَئِX<ن> لَّمۡ تَN<ن>تَهِ لَأَR<رۡ>جُمَN<نَّ>كَۖ وَX<ٱ>هۡجُR<رۡ>نِی مَلِیࣰّا A<۝>D<٤٦>",
"قَالَ سَلَT<ـٰ>مٌ عَلَیۡكَۖ سَأَسۡتَغۡفِR<رُ> لَكَ R<رَ>بِّW<یۤ>ۖ إِN<نَّ>هُT<ۥ> كَانَ بِی حَفِیࣰّا A<۝>D<٤٧>",
"وَأَعۡتَزِلُكُمۡ وَمَا تَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ وَأَQ<دۡ>عُوX<ا۟> R<رَ>بِّی عَسَىW<ٰۤ> أَلَّW<اۤ> أَكُونَ بِدُعَW<اۤ>ءِ R<رَ>بِّی شَقِیࣰّا A<۝>D<٤٨>",
"فَلَN<مَّ>ا ٱعۡتَزَلَهُمۡ وَمَا یَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ وَهَQ<بۡ>نَا لَهُW<ۥۤ> إِسۡحَT<ـٰ>قَ وَیَعۡقُوبَۖ وَكُلّN<ࣰ>ا جَعَلۡنَا نَبِیࣰّا A<۝>D<٤٩>",
"وَوَهَQ<بۡ>نَا لَهُم N<مِّ>X<ن> R<رَّ>حۡمَتِنَا وَجَعَلۡنَا لَهُمۡ لِسَانَ صِQ<دۡ>قٍ عَلِیࣰّا A<۝>D<٥٠>",
"وَX<ٱ>ذۡكُR<رۡ> فِی ٱلۡكِتَT<ـٰ>بِ مُوسَىW<ٰۤ>ۚ إِN<نَّ>هُT<ۥ> كَانَ مُخۡلَصX<ࣰ>ا N<وَ>كَانَ R<رَ>سُولX<ࣰ>ا N<نَّ>بِیࣰّا A<۝>D<٥١>",
"وَنَT<ـٰ>دَیۡنَT<ـٰ>هُ مِN<ن> جَانِبِ ٱX<ل>طُّورِ ٱلۡأَیۡمَنِ وَقَR<رَّ>Q<بۡ>نَT<ـٰ>هُ نَجِیࣰّا A<۝>D<٥٢>",
"وَوَهَQ<بۡ>نَا لَهُT<ۥ> مِX<ن> R<رَّ>حۡمَتِنَW<اۤ> أَخَاهُ هَT<ـٰ>R<رُ>ونَ نَبِیࣰّا A<۝>D<٥٣>",
"وَX<ٱ>ذۡكُR<رۡ> فِی ٱلۡكِتَT<ـٰ>بِ إِسۡمَT<ـٰ>عِیلَۚ إِN<نَّ>هُT<ۥ> كَانَ صَادِقَ ٱلۡوَعۡدِ وَكَانَ R<رَ>سُولX<ࣰ>ا N<نَّ>بِیࣰّا A<۝>D<٥٤>",
"وَكَانَ یَأۡمُR<رُ> أَهۡلَهُT<ۥ> بِX<ٱ>X<ل>صَّلَX<و>T<ٰ>ةِ وَX<ٱ>X<ل>زَّكَX<و>T<ٰ>ةِ وَكَانَ عِN<ن>دَ R<رَ>بِّهِT<ۦ> مَR<رۡ>ضِیࣰّا A<۝>D<٥٥>",
"وَX<ٱ>ذۡكُR<رۡ> فِی ٱلۡكِتَT<ـٰ>بِ إِQ<دۡ>رِیسَۚ إِN<نَّ>هُT<ۥ> كَانَ صِدِّیقX<ࣰ>ا N<نَّ>بِیࣰّا A<۝>D<٥٦>",
"وَR<رَ>فَعۡنَT<ـٰ>هُ مَكَانًا عَلِیًّا A<۝>D<٥٧>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ أَنۡعَمَ ٱللَّهُ عَلَیۡهِم N<مِّ>نَ ٱX<ل>N<نَّ>بِیِّT<ـۧ>نَ مِN<ن> ذُرِّیَّةِ ءَادَمَ وَمِN<مَّ>نۡ حَمَلۡنَا مَعَ نُوحX<ࣲ> N<وَ>مِN<ن> ذُرِّیَّةِ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَإِسۡR<رَ>W< ٰۤ>ءِیلَ وَمِN<مَّ>نۡ هَدَیۡنَا وَX<ٱ>Q<جۡ>تَبَیۡنَW<اۤ>ۚ إِذَا تُتۡلَىٰ عَلَیۡهِمۡ ءَایَT<ـٰ>تُ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ خَ̅R<رُّ>̅و̅X<ا۟>̅ ̅سُ̅جَّ̅دX<ࣰ>̅ا̅ N<وَ>بُكِیࣰّا A<۝>D<٥٨> ۩",
"۞ فَخَلَفَ مِX<ن>N<ۢ> بَعۡدِهِمۡ خَلۡفٌ أَضَاعُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَX<ٱ>تَّبَعُوX<ا۟> ٱX<ل>شَّهَوَT< ٰ>تِۖ فَسَوۡفَ یَلۡقَوۡنَ غَیًّا A<۝>D<٥٩>",
"إِلَّا مَN<ن> تَابَ وَءَامَنَ وَعَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَأُX<و۟>لَW<ـٰۤ>ئِكَ یَQ<دۡ>خُلُونَ ٱلۡجَN<نَّ>ةَ وَلَا یُظۡلَمُونَ شَیۡءࣰا A<۝>D<٦٠>",
"جَN<نَّ>T<ـٰ>تِ عَQ<دۡ>نٍ ٱلَّتِی وَعَدَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ عِبَادَهُT<ۥ> بِX<ٱ>لۡغَیۡبِۚ إِN<نَّ>هُT<ۥ> كَانَ وَعۡدُهُT<ۥ> مَأۡتِیࣰّا A<۝>D<٦١>",
"لَّا یَسۡمَعُونَ فِیهَا لَغۡوًا إِلَّا سَلَT<ـٰ>مX<ࣰ>اۖ N<وَ>لَهُمۡ رِزۡقُهُمۡ فِیهَا بُكۡR<رَ>ةX<ࣰ> N<وَ>عَشِیࣰّا A<۝>D<٦٢>",
"تِلۡكَ ٱلۡجَN<نَّ>ةُ ٱلَّتِی نُورِثُ مِنۡ عِبَادِنَا مَN<ن> كَانَ تَقِیࣰّا A<۝>D<٦٣>",
"وَمَا نَتَنَزَّلُ إِلَّا بِأَمۡرِ R<رَ>بِّكَۖ لَهُT<ۥ> مَا بَیۡنَ أَیۡدِینَا وَمَا خَلۡفَنَا وَمَا بَیۡنَ ذَT< ٰ>لِكَۚ وَمَا كَانَ R<رَ>بُّكَ نَسِیࣰّا A<۝>D<٦٤>",
"R<رَّ>بُّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَا فَX<ٱ>عۡبُQ<دۡ>هُ وَX<ٱ>صۡطَبِرۡ لِعِبَT<ـٰ>دَتِهِT<ۦ>ۚ هَلۡ تَعۡلَمُ لَهُT<ۥ> سَمِیࣰّا A<۝>D<٦٥>",
"وَیَقُولُ ٱلۡإِN<ن>سَT<ـٰ>نُ أَءِذَا مَا مِتُّ لَسَوۡفَ أُخۡR<رَ>جُ حَیًّا A<۝>D<٦٦>",
"أَوَ لَا یَذۡكُR<رُ> ٱلۡإِN<ن>سَT<ـٰ>نُ أَN<نَّ>ا خَلَQ<قۡ>نَT<ـٰ>هُ مِN<ن> قَQ<بۡ>لُ وَلَمۡ یَكُ شَیۡءࣰا A<۝>D<٦٧>",
"فَوَR<رَ>بِّكَ لَنَحۡشُR<رَ>N<نَّ>هُمۡ وَX<ٱ>X<ل>شَّیَT<ـٰ>طِینَ ثُN<مَّ> لَنُحۡضِR<رَ>N<نَّ>هُمۡ حَوۡلَ جَهَN<نَّ>مَ جِثِیࣰّا A<۝>D<٦٨>",
"ثُN<مَّ> لَنَN<ن>زِعَN<نَّ> مِN<ن> كُلِّ شِیعَةٍ أَیُّهُمۡ أَشَدُّ عَلَى ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ عِتِیࣰّا A<۝>D<٦٩>",
"ثُN<مَّ> لَنَحۡنُ أَعۡلَمُ بِX<ٱ>لَّذِینَ هُمۡ أَوۡلَىٰ بِهَا صِلِیࣰّا A<۝>D<٧٠>",
"وَإِX<ن> N<مِّ>N<ن>كُمۡ إِلَّا وَارِدُهَاۚ كَانَ عَلَىٰ R<رَ>بِّكَ حَتۡمX<ࣰ>ا N<مَّ>Q<قۡ>ضِیࣰّا A<۝>D<٧١>",
"ثُN<مَّ> نُنَجِّی ٱلَّذِینَ ٱتَّقَوX<ا۟> وَّنَذَR<رُ> ٱX<ل>ظَّT<ـٰ>لِمِینَ فِیهَا جِثِیࣰّا A<۝>D<٧٢>",
"وَإِذَا تُتۡلَىٰ عَلَیۡهِمۡ ءَایَT<ـٰ>تُنَا بَیِّنَT<ـٰ>تN<ࣲ> قَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لِلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَیُّ ٱلۡفَرِیقَیۡنِ خَیۡR<ر>X<ࣱ> N<مَّ>قَامX<ࣰ>ا N<وَ>أَحۡسَنُ نَدِیࣰّا A<۝>D<٧٣>",
"وَكَمۡ أَهۡلَكۡنَا قَQ<بۡ>لَهُم N<مِّ>N<ن> قَR<رۡ>نٍ هُمۡ أَحۡسَنُ أَثَT<ـٰ>ثX<ࣰ>ا N<وَ>رِءۡیࣰا A<۝>D<٧٤>",
"قُلۡ مَN<ن> كَانَ فِی ٱX<ل>ضَّلَT<ـٰ>لَةِ فَلۡیَمۡدُQ<دۡ> لَهُ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ مَدًّاۚ حَتَّىW<ٰۤ> إِذَا R<رَ>أَوۡX<ا۟> مَا یُوعَدُونَ إِN<مَّ>ا ٱلۡعَذَابَ وَإِN<مَّ>ا ٱX<ل>سَّاعَةَ فَسَیَعۡلَمُونَ مَنۡ هُوَ شَR<رّ>X<ࣱ> N<مَّ>كَانX<ࣰ>ا N<وَ>أَضۡعَفُ جُN<ن>دࣰا A<۝>D<٧٥>",
"وَیَزِیدُ ٱللَّهُ ٱلَّذِینَ ٱهۡتَدَوۡX<ا۟> هُدX<ࣰ>ىۗ N<وَ>X<ٱ>لۡبَT<ـٰ>قِیَT<ـٰ>تُ ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تُ خَیۡR<ر>ٌ عِN<ن>دَ R<رَ>بِّكَ ثَوَابX<ࣰ>ا N<وَ>خَیۡR<ر>X<ࣱ> N<مَّ>R<رَ>دًّا A<۝>D<٧٦>",
"أَفَR<رَ>ءَیۡتَ ٱلَّذِی كَفَR<رَ> بِءَایَT<ـٰ>تِنَا وَقَالَ لَأُوتَیَN<نَّ> مَالX<ࣰ>ا N<وَ>وَلَدًا A<۝>D<٧٧>",
"أَطَّلَعَ ٱلۡغَیۡبَ أَمِ ٱتَّخَذَ عِN<ن>دَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ عَهۡدࣰا A<۝>D<٧٨>",
"كَلَّاۚ سَنَكۡتُبُ مَا یَقُولُ وَنَمُدُّ لَهُT<ۥ> مِنَ ٱلۡعَذَابِ مَدࣰّا A<۝>D<٧٩>",
"وَنَرِثُهُT<ۥ> مَا یَقُولُ وَیَأۡتِینَا فَR<رۡ>دࣰا A<۝>D<٨٠>",
"وَX<ٱ>تَّخَذُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ ءَالِهَةX<ࣰ> لِّیَكُونُوX<ا۟> لَهُمۡ عِزࣰّا A<۝>D<٨١>",
"كَلَّاۚ سَیَكۡفُR<رُ>ونَ بِعِبَادَتِهِمۡ وَیَكُونُونَ عَلَیۡهِمۡ ضِدًّا A<۝>D<٨٢>",
"أَلَمۡ تَR<رَ> أَN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَا ٱX<ل>شَّیَT<ـٰ>طِینَ عَلَى ٱلۡكَT<ـٰ>فِرِینَ تَؤُزُّهُمۡ أَزࣰّا A<۝>D<٨٣>",
"فَلَا تَعۡجَلۡ عَلَیۡهِمۡۖ إِN<نَّ>مَا نَعُدُّ لَهُمۡ عَدࣰّا A<۝>D<٨٤>",
"یَوۡمَ نَحۡشُR<رُ> ٱلۡمُتَّقِینَ إِلَى ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ وَفۡدࣰا A<۝>D<٨٥>",
"وَنَسُوقُ ٱلۡمُQ<جۡ>رِمِینَ إِلَىٰ جَهَN<نَّ>مَ وِرۡدࣰا A<۝>D<٨٦>",
"لَّا یَمۡلِكُونَ ٱX<ل>شَّفَT<ـٰ>عَةَ إِلَّا مَنِ ٱتَّخَذَ عِN<ن>دَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ عَهۡدࣰا A<۝>D<٨٧>",
"وَقَالُوX<ا۟> ٱتَّخَذَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ وَلَدࣰا A<۝>D<٨٨>",
"لَّقَQ<دۡ> جِئۡتُمۡ شَیۡءًا إِدࣰّا A<۝>D<٨٩>",
"تَكَادُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تُ یَتَفَطَّR<رۡ>نَ مِنۡهُ وَتَN<ن>شَقُّ ٱلۡأَR<رۡ>ضُ وَتَخِR<رُّ> ٱلۡجِبَالُ هَدًّا A<۝>D<٩٠>",
"أَN<ن> دَعَوۡX<ا۟> لِX<ل>R<رَّ>حۡمَT<ـٰ>نِ وَلَدࣰا A<۝>D<٩١>",
"وَمَا یَX<ن>N<ۢ>بَغِی لِX<ل>R<رَّ>حۡمَT<ـٰ>نِ أَX<ن> N<یَ>تَّخِذَ وَلَدًا A<۝>D<٩٢>",
"إِN<ن> كُلُّ مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ إِلَّW<اۤ> ءَاتِی ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ عَQ<بۡ>دࣰا A<۝>D<٩٣>",
"لَّقَQ<دۡ> أَحۡصَX<ى>T<ٰ>هُمۡ وَعَدَّهُمۡ عَدࣰّا A<۝>D<٩٤>",
"وَكُلُّهُمۡ ءَاتِیهِ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ فَR<رۡ>دًا A<۝>D<٩٥>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ سَیَQ<جۡ>عَلُ لَهُمُ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ وُدࣰّا A<۝>D<٩٦>",
"فَإِN<نَّ>مَا یَسَّR<رۡ>نَT<ـٰ>هُ بِلِسَانِكَ لِتُبَشِّR<رَ> بِهِ ٱلۡمُتَّقِینَ وَتُN<ن>ذِR<رَ> بِهِT<ۦ> قَوۡمX<ࣰ>ا لُّدࣰّا A<۝>D<٩٧>",
"وَكَمۡ أَهۡلَكۡنَا قَQ<بۡ>لَهُم N<مِّ>N<ن> قَR<رۡ>نٍ هَلۡ تُحِسُّ مِنۡهُم N<مِّ>نۡ أَحَدٍ أَوۡ تَسۡمَعُ لَهُمۡ رِكۡزَۢا A<۝>D<٩٨>",
"#۞ طه A<۝>D<١>",
"مَW<اۤ> أَN<ن>زَلۡنَا عَلَیۡكَ ٱلۡقُR<رۡ>ءَانَ لِتَشۡقَىٰۤ A<۝>D<٢>",
"إِلَّا تَذۡكِR<رَ>ةX<ࣰ> لِّمَX<ن> N<یَ>خۡشَىٰ A<۝>D<٣>",
"تَN<ن>زِیلX<ࣰ>ا N<مِّ>N<مَّ>نۡ خَلَقَ ٱلۡأَR<رۡ>ضَ وَX<ٱ>X<ل>سَّمَT<ـٰ>وَT< ٰ>تِ ٱلۡعُلَى A<۝>D<٤>",
"ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ عَلَى ٱلۡعَR<رۡ>شِ ٱسۡتَوَىٰ A<۝>D<٥>",
"لَهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَا وَمَا تَحۡتَ ٱX<ل>ثَّR<رَ>ىٰ A<۝>D<٦>",
"وَإِN<ن> تَQ<جۡ>هَR<رۡ> بِX<ٱ>لۡقَوۡلِ فَإِN<نَّ>هُT<ۥ> یَعۡلَمُ ٱX<ل>سِّR<رَّ> وَأَخۡفَى A<۝>D<٧>",
"ٱللَّهُ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ لَهُ ٱلۡأَسۡمَW<اۤ>ءُ ٱلۡحُسۡنَىٰ A<۝>D<٨>",
"وَهَلۡ أَتَX<ى>T<ٰ>كَ حَدِیثُ مُوسَىٰۤ A<۝>D<٩>",
"إِذۡ R<رَ>ءَا نَاR<ر>N<ࣰ>ا فَقَالَ لِأَهۡلِهِ ٱمۡكُثُW<وۤ>X<ا۟> إِN<نِّ>W<یۤ> ءَانَسۡتُ نَاR<ر>X<ࣰ>ا لَّعَلِّW<یۤ> ءَاتِیكُم N<مِّ>نۡهَا بِقَبَسٍ أَوۡ أَجِدُ عَلَى ٱX<ل>N<نَّ>ارِ هُدࣰى A<۝>D<١٠>",
"فَلَN<مَّ>W<اۤ> أَتَX<ى>T<ٰ>هَا نُودِیَ یَT<ـٰ>مُوسَىٰۤ A<۝>D<١١>",
"إِN<نِّ>W<یۤ> أَنَا۠ R<رَ>بُّكَ فَX<ٱ>خۡلَعۡ نَعۡلَیۡكَ إِN<نَّ>كَ بِX<ٱ>لۡوَادِ ٱلۡمُقَدَّسِ طُوࣰى A<۝>D<١٢>",
"وَأَنَا ٱخۡتَR<رۡ>تُكَ فَX<ٱ>سۡتَمِعۡ لِمَا یُوحَىٰۤ A<۝>D<١٣>",
"إِN<نَّ>نِW<یۤ> أَنَا ٱللَّهُ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّW<اۤ> أَنَا۠ فَX<ٱ>عۡبُQ<دۡ>نِی وَأَقِمِ ٱX<ل>صَّلَX<و>T<ٰ>ةَ لِذِكۡرِیۤ A<۝>D<١٤>",
"إِN<نَّ> ٱX<ل>سَّاعَةَ ءَاتِیَةٌ أَكَادُ أُخۡفِیهَا لِتُQ<جۡ>زَىٰ كُلُّ نَفۡسِN<ۭ> بِمَا تَسۡعَىٰ A<۝>D<١٥>",
"فَلَا یَصُدَّN<نَّ>كَ عَنۡهَا مَX<ن> لَّا یُؤۡمِنُ بِهَا وَX<ٱ>تَّبَعَ هَوَX<ى>T<ٰ>هُ فَتَR<رۡ>دَىٰ A<۝>D<١٦>",
"وَمَا تِلۡكَ بِیَمِینِكَ یَT<ـٰ>مُوسَىٰ A<۝>D<١٧>",
"قَالَ هِیَ عَصَایَ أَتَوَكَّؤُX<ا۟> عَلَیۡهَا وَأَهُشُّ بِهَا عَلَىٰ غَنَمِی وَلِیَ فِیهَا مَءَارِبُ أُخۡR<رَ>ىٰ A<۝>D<١٨>",
"قَالَ أَلۡقِهَا یَT<ـٰ>مُوسَىٰ A<۝>D<١٩>",
"فَأَلۡقَX<ى>T<ٰ>هَا فَإِذَا هِیَ حَیَّةN<ࣱ> تَسۡعَىٰ A<۝>D<٢٠>",
"قَالَ خُذۡهَا وَلَا تَخَفۡۖ سَنُعِیدُهَا سِیR<رَ>تَهَا ٱلۡأُولَىٰ A<۝>D<٢١>",
"وَX<ٱ>ضۡمُمۡ یَدَكَ إِلَىٰ جَنَاحِكَ تَخۡR<رُ>Q<جۡ> بَیۡضَW<اۤ>ءَ مِنۡ غَیۡرِ سُW<وۤ>ءٍ ءَایَةً أُخۡR<رَ>ىٰ A<۝>D<٢٢>",
"لِنُرِیَكَ مِنۡ ءَایَT<ـٰ>تِنَا ٱلۡكُQ<بۡ>R<رَ>ى A<۝>D<٢٣>",
"ٱذۡهَQ<بۡ> إِلَىٰ فِرۡعَوۡنَ إِN<نَّ>هُT<ۥ> طَغَىٰ A<۝>D<٢٤>",
"قَالَ R<رَ>بِّ ٱشۡR<رَ>حۡ لِی صَQ<دۡ>رِی A<۝>D<٢٥>",
"وَیَسِّرۡ لِW<یۤ> أَمۡرِی A<۝>D<٢٦>",
"وَX<ٱ>حۡلُلۡ عُQ<قۡ>دَةX<ࣰ> N<مِّ>X<ن> لِّسَانِی A<۝>D<٢٧>",
"یَفۡقَهُوX<ا۟> قَوۡلِی A<۝>D<٢٨>",
"وَX<ٱ>Q<جۡ>عَل لِّی وَزِیR<ر>X<ࣰ>ا N<مِّ>نۡ أَهۡلِی A<۝>D<٢٩>",
"هَT<ـٰ>R<رُ>ونَ أَخِی A<۝>D<٣٠>",
"ٱشۡدُQ<دۡ> بِهِW<ۦۤ> أَزۡرِی A<۝>D<٣١>",
"وَأَشۡرِكۡهُ فِW<یۤ> أَمۡرِی A<۝>D<٣٢>",
"كَیۡ نُسَبِّحَكَ كَثِیR<ر>ࣰا A<۝>D<٣٣>",
"وَنَذۡكُR<رَ>كَ كَثِیR<ر>ًا A<۝>D<٣٤>",
"إِN<نَّ>كَ كُN<ن>تَ بِنَا بَصِیR<ر>ࣰا A<۝>D<٣٥>",
"قَالَ قَQ<دۡ> أُوتِیتَ سُؤۡلَكَ یَT<ـٰ>مُوسَىٰ A<۝>D<٣٦>",
"وَلَقَQ<دۡ> مَنَN<نَّ>ا عَلَیۡكَ مَR<رَّ>ةً أُخۡR<رَ>ىٰۤ A<۝>D<٣٧>",
"إِذۡ أَوۡحَیۡنَW<اۤ> إِلَىW<ٰۤ> أُN<مِّ>كَ مَا یُوحَىٰۤ A<۝>D<٣٨>",
"أَنِ ٱQ<قۡ>ذِفِیهِ فِی ٱX<ل>تَّابُوتِ فَX<ٱ>Q<قۡ>ذِفِیهِ فِی ٱلۡیَN<مِّ> فَلۡیُلۡقِهِ ٱلۡیَN<مُّ> بِX<ٱ>X<ل>سَّاحِلِ یَأۡخُذۡهُ عَدُوّX<ࣱ> لِّی وَعَدُوّX<ࣱ> لَّهُT<ۥ>ۚ وَأَلۡقَیۡتُ عَلَیۡكَ مَحَبَّةX<ࣰ> N<مِّ>N<نِّ>ی وَلِتُصۡنَعَ عَلَىٰ عَیۡنِیۤ A<۝>D<٣٩>",
"إِذۡ تَمۡشِW<یۤ> أُخۡتُكَ فَتَقُولُ هَلۡ أَدُلُّكُمۡ عَلَىٰ مَX<ن> N<یَ>كۡفُلُهُT<ۥ>ۖ فَR<رَ>جَعۡنَT<ـٰ>كَ إِلَىW<ٰۤ> أُN<مِّ>كَ كَیۡ تَقَR<رَّ> عَیۡنُهَا وَلَا تَحۡزَنَۚ وَقَتَلۡتَ نَفۡسN<ࣰ>ا فَنَجَّیۡنَT<ـٰ>كَ مِنَ ٱلۡغَN<مِّ> وَفَتَN<نَّ>T<ـٰ>كَ فُتُونN<ࣰ>اۚ فَلَبِثۡتَ سِنِینَ فِW<یۤ> أَهۡلِ مَQ<دۡ>یَنَ ثُN<مَّ> جِئۡتَ عَلَىٰ قَدَرX<ࣲ> N<یَ>T<ـٰ>مُوسَىٰ A<۝>D<٤٠>",
"وَX<ٱ>صۡطَنَعۡتُكَ لِنَفۡسِی A<۝>D<٤١>",
"ٱذۡهَQ<بۡ> أَN<ن>تَ وَأَخُوكَ بِءَایَT<ـٰ>تِی وَلَا تَنِیَا فِی ذِكۡرِی A<۝>D<٤٢>",
"ٱذۡهَبَW<اۤ> إِلَىٰ فِرۡعَوۡنَ إِN<نَّ>هُT<ۥ> طَغَىٰ A<۝>D<٤٣>",
"فَقُولَا لَهُT<ۥ> قَوۡلX<ࣰ>ا لَّیِّنX<ࣰ>ا لَّعَلَّهُT<ۥ> یَتَذَكَّR<رُ> أَوۡ یَخۡشَىٰ A<۝>D<٤٤>",
"قَالَا R<رَ>بَّنَW<اۤ> إِN<نَّ>نَا نَخَافُ أَX<ن> N<یَ>فۡR<رُ>طَ عَلَیۡنَW<اۤ> أَوۡ أَX<ن> N<یَ>Q<طۡ>غَىٰ A<۝>D<٤٥>",
"قَالَ لَا تَخَافَW<اۤ>ۖ إِN<نَّ>نِی مَعَكُمَW<اۤ> أَسۡمَعُ وَأَR<رَ>ىٰ A<۝>D<٤٦>",
"فَأۡتِیَاهُ فَقُولَW<اۤ> إِN<نَّ>ا R<رَ>سُولَا R<رَ>بِّكَ فَأَR<رۡ>سِلۡ مَعَنَا بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ وَلَا تُعَذِّQ<بۡ>هُمۡۖ قَQ<دۡ> جِئۡنَT<ـٰ>كَ بِءَایَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّكَۖ وَX<ٱ>X<ل>سَّلَT<ـٰ>مُ عَلَىٰ مَنِ ٱتَّبَعَ ٱلۡهُدَىٰۤ A<۝>D<٤٧>",
"إِN<نَّ>ا قَQ<دۡ> أُوحِیَ إِلَیۡنَW<اۤ> أَN<نَّ> ٱلۡعَذَابَ عَلَىٰ مَN<ن> كَذَّبَ وَتَوَلَّىٰ A<۝>D<٤٨>",
"قَالَ فَمَX<ن> R<رَّ>بُّكُمَا یَT<ـٰ>مُوسَىٰ A<۝>D<٤٩>",
"قَالَ R<رَ>بُّنَا ٱلَّذِW<یۤ> أَعۡطَىٰ كُلَّ شَیۡءٍ خَلۡقَهُT<ۥ> ثُN<مَّ> هَدَىٰ A<۝>D<٥٠>",
"قَالَ فَمَا بَالُ ٱلۡقُR<رُ>ونِ ٱلۡأُولَىٰ A<۝>D<٥١>",
"قَالَ عِلۡمُهَا عِN<ن>دَ R<رَ>بِّی فِی كِتَT<ـٰ>بX<ࣲ>ۖ لَّا یَضِلُّ R<رَ>بِّی وَلَا یَN<ن>سَى A<۝>D<٥٢>",
"ٱلَّذِی جَعَلَ لَكُمُ ٱلۡأَR<رۡ>ضَ مَهۡدX<ࣰ>ا N<وَ>سَلَكَ لَكُمۡ فِیهَا سُبُلX<ࣰ>ا N<وَ>أَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَخۡR<رَ>Q<جۡ>نَا بِهِW<ۦۤ> أَزۡوَT< ٰ>جX<ࣰ>ا N<مِّ>ن N<نَّ>بَاتN<ࣲ> شَتَّىٰ A<۝>D<٥٣>",
"كُلُوX<ا۟> وَX<ٱ>R<رۡ>عَوۡX<ا۟> أَنۡعَT<ـٰ>مَكُمۡۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّأُX<و۟>لِی ٱX<ل>N<نُّ>هَىٰ A<۝>D<٥٤>",
"۞ مِنۡهَا خَلَQ<قۡ>نَT<ـٰ>كُمۡ وَفِیهَا نُعِیدُكُمۡ وَمِنۡهَا نُخۡرِجُكُمۡ تَاR<رَ>ةً أُخۡR<رَ>ىٰ A<۝>D<٥٥>",
"وَلَقَQ<دۡ> أَR<رَ>یۡنَT<ـٰ>هُ ءَایَT<ـٰ>تِنَا كُلَّهَا فَكَذَّبَ وَأَبَىٰ A<۝>D<٥٦>",
"قَالَ أَجِئۡتَنَا لِتُخۡرِجَنَا مِنۡ أَR<رۡ>ضِنَا بِسِحۡرِكَ یَT<ـٰ>مُوسَىٰ A<۝>D<٥٧>",
"فَلَنَأۡتِیَN<نَّ>كَ بِسِحۡرX<ࣲ> N<مِّ>ثۡلِهِT<ۦ> فَX<ٱ>Q<جۡ>عَلۡ بَیۡنَنَا وَبَیۡنَكَ مَوۡعِدX<ࣰ>ا لَّا نُخۡلِفُهُT<ۥ> نَحۡنُ وَلَW<اۤ> أَN<ن>تَ مَكَانN<ࣰ>ا سُوࣰى A<۝>D<٥٨>",
"قَالَ مَوۡعِدُكُمۡ یَوۡمُ ٱX<ل>زِّینَةِ وَأَX<ن> N<یُ>حۡشَR<رَ> ٱX<ل>N<نَّ>اسُ ضُحࣰى A<۝>D<٥٩>",
"فَتَوَلَّىٰ فِرۡعَوۡنُ فَجَمَعَ كَیۡدَهُT<ۥ> ثُN<مَّ> أَتَىٰ A<۝>D<٦٠>",
"قَالَ لَهُم N<مُّ>وسَىٰ وَیۡلَكُمۡ لَا تَفۡتَR<رُ>وX<ا۟> عَلَى ٱللَّهِ كَذِبN<ࣰ>ا فَیُسۡحِتَكُN<م> بِعَذَابX<ࣲ>ۖ N<وَ>قَQ<دۡ> خَابَ مَنِ ٱفۡتَR<رَ>ىٰ A<۝>D<٦١>",
"فَتَنَT<ـٰ>زَعُW<وۤ>X<ا۟> أَمۡR<رَ>هُN<م> بَیۡنَهُمۡ وَأَسَR<رُّ>وX<ا۟> ٱX<ل>N<نَّ>Q<جۡ>وَىٰ A<۝>D<٦٢>",
"قَالُW<وۤ>X<ا۟> إِنۡ هَT<ـٰ>ذَT< ٰ>نِ لَسَT<ـٰ>حِR<رَ>T< ٰ>نِ یُرِیدَانِ أَX<ن> N<یُ>خۡرِجَاكُم N<مِّ>نۡ أَR<رۡ>ضِكُN<م> بِسِحۡرِهِمَا وَیَذۡهَبَا بِطَرِیقَتِكُمُ ٱلۡمُثۡلَىٰ A<۝>D<٦٣>",
"فَأَQ<جۡ>مِعُوX<ا۟> كَیۡدَكُمۡ ثُN<مَّ> ٱئۡتُوX<ا۟> صَفّX<ࣰ>اۚ N<وَ>قَQ<دۡ> أَفۡلَحَ ٱلۡیَوۡمَ مَنِ ٱسۡتَعۡلَىٰ A<۝>D<٦٤>",
"قَالُوX<ا۟> یَT<ـٰ>مُوسَىW<ٰۤ> إِN<مَّ>W<اۤ> أَN<ن> تُلۡقِیَ وَإِN<مَّ>W<اۤ> أَن N<نَّ>كُونَ أَوَّلَ مَنۡ أَلۡقَىٰ A<۝>D<٦٥>",
"قَالَ بَلۡ أَلۡقُوX<ا۟>ۖ فَإِذَا حِبَالُهُمۡ وَعِصِیُّهُمۡ یُخَیَّلُ إِلَیۡهِ مِN<ن> سِحۡرِهِمۡ أَN<نَّ>هَا تَسۡعَىٰ A<۝>D<٦٦>",
"فَأَوۡجَسَ فِی نَفۡسِهِT<ۦ> خِیفَةX<ࣰ> N<مُّ>وسَىٰ A<۝>D<٦٧>",
"قُلۡنَا لَا تَخَفۡ إِN<نَّ>كَ أَN<ن>تَ ٱلۡأَعۡلَىٰ A<۝>D<٦٨>",
"وَأَلۡقِ مَا فِی یَمِینِكَ تَلۡقَفۡ مَا صَنَعُW<وۤ>X<ا۟>ۖ إِN<نَّ>مَا صَنَعُوX<ا۟> كَیۡدُ سَT<ـٰ>حِرX<ࣲ>ۖ N<وَ>لَا یُفۡلِحُ ٱX<ل>سَّاحِR<رُ> حَیۡثُ أَتَىٰ A<۝>D<٦٩>",
"فَأُلۡقِیَ ٱX<ل>سَّحَR<رَ>ةُ سُجَّدN<ࣰ>ا قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا بِR<رَ>بِّ هَT<ـٰ>R<رُ>ونَ وَمُوسَىٰ A<۝>D<٧٠>",
"قَالَ ءَامَN<ن>تُمۡ لَهُT<ۥ> قَQ<بۡ>لَ أَنۡ ءَاذَنَ لَكُمۡۖ إِN<نَّ>هُT<ۥ> لَكَبِیR<رُ>كُمُ ٱلَّذِی عَلَّمَكُمُ ٱX<ل>سِّحۡR<رَ>ۖ فَلَأُقَطِّعَN<نَّ> أَیۡدِیَكُمۡ وَأَR<رۡ>جُلَكُم N<مِّ>نۡ خِلَT<ـٰ>فX<ࣲ> N<وَ>لَأُصَلِّبَN<نَّ>كُمۡ فِی جُذُوعِ ٱX<ل>N<نَّ>خۡلِ وَلَتَعۡلَمُN<نَّ> أَیُّنَW<اۤ> أَشَدُّ عَذَابX<ࣰ>ا N<وَ>أَQ<بۡ>قَىٰ A<۝>D<٧١>",
"قَالُوX<ا۟> لَن N<نُّ>ؤۡثِR<رَ>كَ عَلَىٰ مَا جَW<اۤ>ءَنَا مِنَ ٱلۡبَیِّنَT<ـٰ>تِ وَX<ٱ>لَّذِی فَطَR<رَ>نَاۖ فَX<ٱ>Q<قۡ>ضِ مَW<اۤ> أَN<ن>تَ قَاضٍۖ إِN<نَّ>مَا تَQ<قۡ>ضِی هَT<ـٰ>ذِهِ ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَاۤ A<۝>D<٧٢>",
"إِN<نَّ>W<اۤ> ءَامَN<نَّ>ا بِR<رَ>بِّنَا لِیَغۡفِR<رَ> لَنَا خَطَT<ـٰ>یَT<ـٰ>نَا وَمَW<اۤ> أَكۡR<رَ>هۡتَنَا عَلَیۡهِ مِنَ ٱX<ل>سِّحۡرِۗ وَٱللَّهُ خَیۡR<ر>X<ࣱ> N<وَ>أَQ<بۡ>قَىٰۤ A<۝>D<٧٣>",
"إِN<نَّ>هُT<ۥ> مَX<ن> N<یَ>أۡتِ R<رَ>بَّهُT<ۥ> مُQ<جۡ>رِمN<ࣰ>ا فَإِN<نَّ> لَهُT<ۥ> جَهَN<نَّ>مَ لَا یَمُوتُ فِیهَا وَلَا یَحۡیَىٰ A<۝>D<٧٤>",
"وَمَX<ن> N<یَ>أۡتِهِT<ۦ> مُؤۡمِنN<ࣰ>ا قَQ<دۡ> عَمِلَ ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ فَأُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمُ ٱX<ل>دَّR<رَ>جَT<ـٰ>تُ ٱلۡعُلَىٰ A<۝>D<٧٥>",
"جَN<نَّ>T<ـٰ>تُ عَQ<دۡ>نN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَاۚ وَذَT< ٰ>لِكَ جَزَW<اۤ>ءُ مَN<ن> تَزَكَّىٰ A<۝>D<٧٦>",
"وَلَقَQ<دۡ> أَوۡحَیۡنَW<اۤ> إِلَىٰ مُوسَىW<ٰۤ> أَنۡ أَسۡرِ بِعِبَادِی فَX<ٱ>ضۡرِQ<بۡ> لَهُمۡ طَرِیقN<ࣰ>ا فِی ٱلۡبَحۡرِ یَبَسX<ࣰ>ا لَّا تَخَT<ـٰ>فُ دَR<رَ>كX<ࣰ>ا N<وَ>لَا تَخۡشَىٰ A<۝>D<٧٧>",
"فَأَتۡبَعَهُمۡ فِرۡعَوۡنُ بِجُنُودِهِT<ۦ> فَغَشِیَهُم N<مِّ>نَ ٱلۡیَN<مِّ> مَا غَشِیَهُمۡ A<۝>D<٧٨>",
"وَأَضَلَّ فِرۡعَوۡنُ قَوۡمَهُT<ۥ> وَمَا هَدَىٰ A<۝>D<٧٩>",
"یَT<ـٰ>بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ قَQ<دۡ> أَN<ن>جَیۡنَT<ـٰ>كُم N<مِّ>نۡ عَدُوِّكُمۡ وَوَT< ٰ>عَQ<دۡ>نَT<ـٰ>كُمۡ جَانِبَ ٱX<ل>طُّورِ ٱلۡأَیۡمَنَ وَنَزَّلۡنَا عَلَیۡكُمُ ٱلۡمَN<نَّ> وَX<ٱ>X<ل>سَّلۡوَىٰ A<۝>D<٨٠>",
"كُلُوX<ا۟> مِN<ن> طَیِّبَT<ـٰ>تِ مَا R<رَ>زَQ<قۡ>نَT<ـٰ>كُمۡ وَلَا تَQ<طۡ>غَوۡX<ا۟> فِیهِ فَیَحِلَّ عَلَیۡكُمۡ غَضَبِیۖ وَمَX<ن> N<یَ>حۡلِلۡ عَلَیۡهِ غَضَبِی فَقَQ<دۡ> هَوَىٰ A<۝>D<٨١>",
"وَإِN<نِّ>ی لَغَفَّاR<ر>X<ࣱ> لِّمَN<ن> تَابَ وَءَامَنَ وَعَمِلَ صَT<ـٰ>لِحN<ࣰ>ا ثُN<مَّ> ٱهۡتَدَىٰ A<۝>D<٨٢>",
"۞ وَمَW<اۤ> أَعۡجَلَكَ عَN<ن> قَوۡمِكَ یَT<ـٰ>مُوسَىٰ A<۝>D<٨٣>",
"قَالَ هُمۡ أُX<و۟>لَW<اۤ>ءِ عَلَىW<ٰۤ> أَثَرِی وَعَجِلۡتُ إِلَیۡكَ R<رَ>بِّ لِتَR<رۡ>ضَىٰ A<۝>D<٨٤>",
"قَالَ فَإِN<نَّ>ا قَQ<دۡ> فَتَN<نَّ>ا قَوۡمَكَ مِX<ن>N<ۢ> بَعۡدِكَ وَأَضَلَّهُمُ ٱX<ل>سَّامِرِیُّ A<۝>D<٨٥>",
"فَR<رَ>جَعَ مُوسَىW<ٰۤ> إِلَىٰ قَوۡمِهِT<ۦ> غَضۡبَT<ـٰ>نَ أَسِفN<ࣰ>اۚ قَالَ یَT<ـٰ>قَوۡمِ أَلَمۡ یَعِQ<دۡ>كُمۡ R<رَ>بُّكُمۡ وَعۡدًا حَسَنًاۚ أَفَطَالَ عَلَیۡكُمُ ٱلۡعَهۡدُ أَمۡ أَR<رَ>X<د>تُّمۡ أَX<ن> N<یَ>حِلَّ عَلَیۡكُمۡ غَضَبX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ فَأَخۡلَفۡتُم N<مَّ>وۡعِدِی A<۝>D<٨٦>",
"قَالُوX<ا۟> مَW<اۤ> أَخۡلَفۡنَا مَوۡعِدَكَ بِمَلۡكِنَا وَلَT<ـٰ>كِN<نَّ>ا حُN<مِّ>لۡنَW<اۤ> أَوۡزَاR<ر>X<ࣰ>ا N<مِّ>N<ن> زِینَةِ ٱلۡقَوۡمِ فَقَذَفۡنَT<ـٰ>هَا فَكَذَT< ٰ>لِكَ أَلۡقَى ٱX<ل>سَّامِرِیُّ A<۝>D<٨٧>",
"فَأَخۡR<رَ>جَ لَهُمۡ عِQ<جۡ>لN<ࣰ>ا جَسَدX<ࣰ>ا لَّهُT<ۥ> خُوَاR<ر>N<ࣱ> فَقَالُوX<ا۟> هَT<ـٰ>ذَW<اۤ> إِلَT<ـٰ>هُكُمۡ وَإِلَT<ـٰ>هُ مُوسَىٰ فَنَسِیَ A<۝>D<٨٨>",
"أَفَلَا یَR<رَ>وۡنَ أَلَّا یَR<رۡ>جِعُ إِلَیۡهِمۡ قَوۡلX<ࣰ>ا N<وَ>لَا یَمۡلِكُ لَهُمۡ ضَR<رّ>X<ࣰ>ا N<وَ>لَا نَفۡعࣰا A<۝>D<٨٩>",
"وَلَقَQ<دۡ> قَالَ لَهُمۡ هَT<ـٰ>R<رُ>ونُ مِN<ن> قَQ<بۡ>لُ یَT<ـٰ>قَوۡمِ إِN<نَّ>مَا فُتِN<ن>تُN<م> بِهِT<ۦ>ۖ وَإِN<نَّ> R<رَ>بَّكُمُ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ فَX<ٱ>تَّبِعُونِی وَأَطِیعُW<وۤ>X<ا۟> أَمۡرِی A<۝>D<٩٠>",
"قَالُوX<ا۟> لَن N<نَّ>Q<بۡ>R<رَ>حَ عَلَیۡهِ عَT<ـٰ>كِفِینَ حَتَّىٰ یَR<رۡ>جِعَ إِلَیۡنَا مُوسَىٰ A<۝>D<٩١>",
"قَالَ یَT<ـٰ>هَT<ـٰ>R<رُ>ونُ مَا مَنَعَكَ إِذۡ R<رَ>أَیۡتَهُمۡ ضَلُّوۤX<ا۟> A<۝>D<٩٢>",
"أَلَّا تَتَّبِعَنِۖ أَفَعَصَیۡتَ أَمۡرِی A<۝>D<٩٣>",
"قَالَ یَQ<بۡ>نَؤُN<مَّ> لَا تَأۡخُذۡ بِلِحۡیَتِی وَلَا بِR<رَ>أۡسِW<یۤ>ۖ إِN<نِّ>ی خَشِیتُ أَN<ن> تَقُولَ فَR<رَّ>Q<قۡ>تَ بَیۡنَ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ وَلَمۡ تَR<رۡ>قُQ<بۡ> قَوۡلِی A<۝>D<٩٤>",
"قَالَ فَمَا خَQ<طۡ>بُكَ یَT<ـٰ>سَT<ـٰ>مِرِیُّ A<۝>D<٩٥>",
"قَالَ بَصُR<رۡ>تُ بِمَا لَمۡ یَQ<بۡ>صُR<رُ>وX<ا۟> بِهِT<ۦ> فَقَبَضۡتُ قَQ<بۡ>ضَةX<ࣰ> N<مِّ>نۡ أَثَرِ ٱX<ل>R<رَّ>سُولِ فَنَبَذۡتُهَا وَكَذَT< ٰ>لِكَ سَوَّلَتۡ لِی نَفۡسِی A<۝>D<٩٦>",
"قَالَ فَX<ٱ>ذۡهَQ<بۡ> فَإِN<نَّ> لَكَ فِی ٱلۡحَیَX<و>T<ٰ>ةِ أَN<ن> تَقُولَ لَا مِسَاسَۖ وَإِN<نَّ> لَكَ مَوۡعِدX<ࣰ>ا لَّN<ن> تُخۡلَفَهُT<ۥ>ۖ وَX<ٱ>N<ن>ظُR<رۡ> إِلَىW<ٰۤ> إِلَT<ـٰ>هِكَ ٱلَّذِی ظَلۡتَ عَلَیۡهِ عَاكِفX<ࣰ>اۖ لَّنُحَرِّقَN<نَّ>هُT<ۥ> ثُN<مَّ> لَنَN<ن>سِفَN<نَّ>هُT<ۥ> فِی ٱلۡیَN<مِّ> نَسۡفًا A<۝>D<٩٧>",
"إِN<نَّ>مَW<اۤ> إِلَT<ـٰ>هُكُمُ ٱللَّهُ ٱلَّذِی لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۚ وَسِعَ كُلَّ شَیۡءٍ عِلۡمࣰا A<۝>D<٩٨>",
"كَذَT< ٰ>لِكَ نَقُصُّ عَلَیۡكَ مِنۡ أَX<ن>N<ۢ>بَW<اۤ>ءِ مَا قَQ<دۡ> سَبَقَۚ وَقَQ<دۡ> ءَاتَیۡنَT<ـٰ>كَ مِX<ن> لَّدُN<نَّ>ا ذِكۡR<ر>ࣰا A<۝>D<٩٩>",
"مَّنۡ أَعۡR<رَ>ضَ عَنۡهُ فَإِN<نَّ>هُT<ۥ> یَحۡمِلُ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ وِزۡR<ر>ًا A<۝>D<١٠٠>",
"خَT<ـٰ>لِدِینَ فِیهِۖ وَسَW<اۤ>ءَ لَهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ حِمۡلࣰا A<۝>D<١٠١>",
"یَوۡمَ یُN<ن>فَخُ فِی ٱX<ل>صُّورِۚ وَنَحۡشُR<رُ> ٱلۡمُQ<جۡ>رِمِینَ یَوۡمَئِذN<ࣲ> زُR<رۡ>قࣰا A<۝>D<١٠٢>",
"یَتَخَT<ـٰ>فَتُونَ بَیۡنَهُمۡ إِX<ن> لَّبِثۡتُمۡ إِلَّا عَشۡR<ر>ࣰا A<۝>D<١٠٣>",
"نَّحۡنُ أَعۡلَمُ بِمَا یَقُولُونَ إِذۡ یَقُولُ أَمۡثَلُهُمۡ طَرِیقَةً إِX<ن> لَّبِثۡتُمۡ إِلَّا یَوۡمࣰا A<۝>D<١٠٤>",
"وَیَسۡءَلُونَكَ عَنِ ٱلۡجِبَالِ فَقُلۡ یَN<ن>سِفُهَا R<رَ>بِّی نَسۡفࣰا A<۝>D<١٠٥>",
"فَیَذَR<رُ>هَا قَاعN<ࣰ>ا صَفۡصَفࣰا A<۝>D<١٠٦>",
"لَّا تَR<رَ>ىٰ فِیهَا عِوَجX<ࣰ>ا N<وَ>لَW<اۤ> أَمۡتࣰا A<۝>D<١٠٧>",
"یَوۡمَئِذX<ࣲ> N<یَ>تَّبِعُونَ ٱX<ل>دَّاعِیَ لَا عِوَجَ لَهُT<ۥ>ۖ وَخَشَعَتِ ٱلۡأَصۡوَاتُ لِX<ل>R<رَّ>حۡمَT<ـٰ>نِ فَلَا تَسۡمَعُ إِلَّا هَمۡسࣰا A<۝>D<١٠٨>",
"یَوۡمَئِذX<ࣲ> لَّا تَN<ن>فَعُ ٱX<ل>شَّفَT<ـٰ>عَةُ إِلَّا مَنۡ أَذِنَ لَهُ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ وَR<رَ>ضِیَ لَهُT<ۥ> قَوۡلࣰا A<۝>D<١٠٩>",
"یَعۡلَمُ مَا بَیۡنَ أَیۡدِیهِمۡ وَمَا خَلۡفَهُمۡ وَلَا یُحِیطُونَ بِهِT<ۦ> عِلۡمࣰا A<۝>D<١١٠>",
"۞ وَعَنَتِ ٱلۡوُجُوهُ لِلۡحَیِّ ٱلۡقَیُّومِۖ وَقَQ<دۡ> خَابَ مَنۡ حَمَلَ ظُلۡمࣰا A<۝>D<١١١>",
"وَمَX<ن> N<یَ>عۡمَلۡ مِنَ ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَهُوَ مُؤۡمِنN<ࣱ> فَلَا یَخَافُ ظُلۡمX<ࣰ>ا N<وَ>لَا هَضۡمࣰا A<۝>D<١١٢>",
"وَكَذَT< ٰ>لِكَ أَN<ن>زَلۡنَT<ـٰ>هُ قُR<رۡ>ءَانًا عَR<رَ>بِیّX<ࣰ>ا N<وَ>صَR<رَّ>فۡنَا فِیهِ مِنَ ٱلۡوَعِیدِ لَعَلَّهُمۡ یَتَّقُونَ أَوۡ یُحۡدِثُ لَهُمۡ ذِكۡR<ر>ࣰا A<۝>D<١١٣>",
"فَتَعَT<ـٰ>لَى ٱللَّهُ ٱلۡمَلِكُ ٱلۡحَقُّۗ وَلَا تَعۡجَلۡ بِX<ٱ>لۡقُR<رۡ>ءَانِ مِN<ن> قَQ<بۡ>لِ أَX<ن> N<یُ>Q<قۡ>ضَىW<ٰۤ> إِلَیۡكَ وَحۡیُهُT<ۥ>ۖ وَقُل R<رَّ>بِّ زِQ<دۡ>نِی عِلۡمࣰا A<۝>D<١١٤>",
"وَلَقَQ<دۡ> عَهِQ<دۡ>نَW<اۤ> إِلَىW<ٰۤ> ءَادَمَ مِN<ن> قَQ<بۡ>لُ فَنَسِیَ وَلَمۡ نَجِQ<دۡ> لَهُT<ۥ> عَزۡمࣰا A<۝>D<١١٥>",
"وَإِذۡ قُلۡنَا لِلۡمَلَW<ـٰۤ>ئِكَةِ ٱسۡجُدُوX<ا۟> لِءَادَمَ فَسَجَدُW<وۤ>X<ا۟> إِلَّW<اۤ> إِQ<بۡ>لِیسَ أَبَىٰ A<۝>D<١١٦>",
"فَقُلۡنَا یَW<ـٰۤ>ءَادَمُ إِN<نَّ> هَT<ـٰ>ذَا عَدُوّX<ࣱ> لَّكَ وَلِزَوۡجِكَ فَلَا یُخۡرِجَN<نَّ>كُمَا مِنَ ٱلۡجَN<نَّ>ةِ فَتَشۡقَىٰۤ A<۝>D<١١٧>",
"إِN<نَّ> لَكَ أَلَّا تَجُوعَ فِیهَا وَلَا تَعۡR<رَ>ىٰ A<۝>D<١١٨>",
"وَأَN<نَّ>كَ لَا تَظۡمَؤُX<ا۟> فِیهَا وَلَا تَضۡحَىٰ A<۝>D<١١٩>",
"فَوَسۡوَسَ إِلَیۡهِ ٱX<ل>شَّیۡطَT<ـٰ>نُ قَالَ یَW<ـٰۤ>ءَادَمُ هَلۡ أَدُلُّكَ عَلَىٰ شَجَR<رَ>ةِ ٱلۡخُلۡدِ وَمُلۡكX<ࣲ> لَّا یَQ<بۡ>لَىٰ A<۝>D<١٢٠>",
"فَأَكَلَا مِنۡهَا فَبَدَتۡ لَهُمَا سَوۡءَT< ٰ>تُهُمَا وَطَفِقَا یَخۡصِفَانِ عَلَیۡهِمَا مِX<ن> N<وَ>R<رَ>قِ ٱلۡجَN<نَّ>ةِۚ وَعَصَىW<ٰۤ> ءَادَمُ R<رَ>بَّهُT<ۥ> فَغَوَىٰ A<۝>D<١٢١>",
"ثُN<مَّ> ٱQ<جۡ>تَبَT<ـٰ>هُ R<رَ>بُّهُT<ۥ> فَتَابَ عَلَیۡهِ وَهَدَىٰ A<۝>D<١٢٢>",
"قَالَ ٱهۡبِطَا مِنۡهَا جَمِیعَN<ۢ>اۖ بَعۡضُكُمۡ لِبَعۡضٍ عَدُوّN<ࣱ>ۖ فَإِN<مَّ>ا یَأۡتِیَN<نَّ>كُم N<مِّ>N<نِّ>ی هُدN<ࣰ>ى فَمَنِ ٱتَّبَعَ هُدَایَ فَلَا یَضِلُّ وَلَا یَشۡقَىٰ A<۝>D<١٢٣>",
"وَمَنۡ أَعۡR<رَ>ضَ عَN<ن> ذِكۡرِی فَإِN<نَّ> لَهُT<ۥ> مَعِیشَةN<ࣰ> ضَN<ن>كX<ࣰ>ا N<وَ>نَحۡشُR<رُ>هُT<ۥ> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ أَعۡمَىٰ A<۝>D<١٢٤>",
"قَالَ R<رَ>بِّ لِمَ حَشَR<رۡ>تَنِW<یۤ> أَعۡمَىٰ وَقَQ<دۡ> كُN<ن>تُ بَصِیR<ر>ࣰا A<۝>D<١٢٥>",
"قَالَ كَذَT< ٰ>لِكَ أَتَتۡكَ ءَایَT<ـٰ>تُنَا فَنَسِیتَهَاۖ وَكَذَT< ٰ>لِكَ ٱلۡیَوۡمَ تُN<ن>سَىٰ A<۝>D<١٢٦>",
"وَكَذَT< ٰ>لِكَ نَQ<جۡ>زِی مَنۡ أَسۡR<رَ>فَ وَلَمۡ یُؤۡمِX<ن>N<ۢ> بِءَایَT<ـٰ>تِ R<رَ>بِّهِT<ۦ>ۚ وَلَعَذَابُ ٱلۡءَاخِR<رَ>ةِ أَشَدُّ وَأَQ<بۡ>قَىٰۤ A<۝>D<١٢٧>",
"أَفَلَمۡ یَهۡدِ لَهُمۡ كَمۡ أَهۡلَكۡنَا قَQ<بۡ>لَهُم N<مِّ>نَ ٱلۡقُR<رُ>ونِ یَمۡشُونَ فِی مَسَT<ـٰ>كِنِهِمۡۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّأُX<و۟>لِی ٱX<ل>N<نُّ>هَىٰ A<۝>D<١٢٨>",
"وَلَوۡلَا كَلِمَةN<ࣱ> سَبَقَتۡ مِX<ن> R<رَّ>بِّكَ لَكَانَ لِزَامX<ࣰ>ا N<وَ>أَجَلX<ࣱ> N<مُّ>سَN<مّ>ࣰى A<۝>D<١٢٩>",
"فَX<ٱ>صۡبِرۡ عَلَىٰ مَا یَقُولُونَ وَسَبِّحۡ بِحَمۡدِ R<رَ>بِّكَ قَQ<بۡ>لَ طُلُوعِ ٱX<ل>شَّمۡسِ وَقَQ<بۡ>لَ غُR<رُ>وبِهَاۖ وَمِنۡ ءَانَW<اۤ>ئِ ٱلَّیۡلِ فَسَبِّحۡ وَأَQ<طۡ>R<رَ>افَ ٱX<ل>N<نَّ>هَارِ لَعَلَّكَ تَR<رۡ>ضَىٰ A<۝>D<١٣٠>",
"وَلَا تَمُدَّN<نَّ> عَیۡنَیۡكَ إِلَىٰ مَا مَتَّعۡنَا بِهِW<ۦۤ> أَزۡوَT< ٰ>جX<ࣰ>ا N<مِّ>نۡهُمۡ زَهۡR<رَ>ةَ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا لِنَفۡتِنَهُمۡ فِیهِۚ وَرِزۡقُ R<رَ>بِّكَ خَیۡR<ر>X<ࣱ> N<وَ>أَQ<بۡ>قَىٰ A<۝>D<١٣١>",
"وَأۡمُR<رۡ> أَهۡلَكَ بِX<ٱ>X<ل>صَّلَX<و>T<ٰ>ةِ وَX<ٱ>صۡطَبِرۡ عَلَیۡهَاۖ لَا نَسۡءَلُكَ رِزۡقX<ࣰ>اۖ N<نَّ>حۡنُ نَR<رۡ>زُقُكَۗ وَX<ٱ>لۡعَT<ـٰ>قِبَةُ لِX<ل>تَّQ<قۡ>وَىٰ A<۝>D<١٣٢>",
"وَقَالُوX<ا۟> لَوۡلَا یَأۡتِینَا بِءَایَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّهِW<ۦۤ>ۚ أَوَ لَمۡ تَأۡتِهِN<م> بَیِّنَةُ مَا فِی ٱX<ل>صُّحُفِ ٱلۡأُولَىٰ A<۝>D<١٣٣>",
"وَلَوۡ أَN<نَّ>W<اۤ> أَهۡلَكۡنَT<ـٰ>هُN<م> بِعَذَابX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِهِT<ۦ> لَقَالُوX<ا۟> R<رَ>بَّنَا لَوۡلَW<اۤ> أَR<رۡ>سَلۡتَ إِلَیۡنَا R<رَ>سُولN<ࣰ>ا فَنَتَّبِعَ ءَایَT<ـٰ>تِكَ مِN<ن> قَQ<بۡ>لِ أَن N<نَّ>ذِلَّ وَنَخۡزَىٰ A<۝>D<١٣٤>",
"قُلۡ كُلّX<ࣱ> N<مُّ>تَR<رَ>بِّصN<ࣱ> فَتَR<رَ>بَّصُوX<ا۟>ۖ فَسَتَعۡلَمُونَ مَنۡ أَصۡحَT<ـٰ>بُ ٱX<ل>صِّR<رَ>T< ٰ>طِ ٱX<ل>سَّوِیِّ وَمَنِ ٱهۡتَدَىٰ A<۝>D<١٣٥>",
"#۞ ٱQ<قۡ>تَR<رَ>بَ لِX<ل>N<نَّ>اسِ حِسَابُهُمۡ وَهُمۡ فِی غَفۡلَةX<ࣲ> N<مُّ>عۡرِضُJ<و>نَ A<۝>D<١>",
"مَا یَأۡتِیهِم N<مِّ>N<ن> ذِكۡرX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّهِم N<مُّ>حۡدَثٍ إِلَّا ٱسۡتَمَعُوهُ وَهُمۡ یَلۡعَبُJ<و>نَ A<۝>D<٢>",
"لَاهِیَةN<ࣰ> قُلُوبُهُمۡۗ وَأَسَR<رُّ>وX<ا۟> ٱX<ل>N<نَّ>Q<جۡ>وَى ٱلَّذِینَ ظَلَمُوX<ا۟> هَلۡ هَT<ـٰ>ذَW<اۤ> إِلَّا بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُكُمۡۖ أَفَتَأۡتُونَ ٱX<ل>سِّحۡR<رَ> وَأَN<ن>تُمۡ تُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٣>",
"قَالَ R<رَ>بِّی یَعۡلَمُ ٱلۡقَوۡلَ فِی ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِۖ وَهُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٤>",
"بَلۡ قَالُW<وۤ>X<ا۟> أَضۡغَT<ـٰ>ثُ أَحۡلَT<ـٰ>مِN<ۭ> بَلِ ٱفۡتَR<رَ>X<ى>T<ٰ>هُ بَلۡ هُوَ شَاعِR<ر>N<ࣱ> فَلۡیَأۡتِنَا بِءَایَةN<ࣲ> كَمَW<اۤ> أُR<رۡ>سِلَ ٱلۡأَوَّلُJ<و>نَ A<۝>D<٥>",
"مَW<اۤ> ءَامَنَتۡ قَQ<بۡ>لَهُم N<مِّ>N<ن> قَR<رۡ>یَةٍ أَهۡلَكۡنَT<ـٰ>هَW<اۤ>ۖ أَفَهُمۡ یُؤۡمِنُJ<و>نَ A<۝>D<٦>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا قَQ<بۡ>لَكَ إِلَّا رِجَالX<ࣰ>ا N<نُّ>وحِW<یۤ> إِلَیۡهِمۡۖ فَسۡءَلُW<وۤ>X<ا۟> أَهۡلَ ٱX<ل>ذِّكۡرِ إِN<ن> كُN<ن>تُمۡ لَا تَعۡلَمُJ<و>نَ A<۝>D<٧>",
"وَمَا جَعَلۡنَT<ـٰ>هُمۡ جَسَدX<ࣰ>ا لَّا یَأۡكُلُونَ ٱX<ل>طَّعَامَ وَمَا كَانُوX<ا۟> خَT<ـٰ>لِدِJ<ی>نَ A<۝>D<٨>",
"ثُN<مَّ> صَدَQ<قۡ>نَT<ـٰ>هُمُ ٱلۡوَعۡدَ فَأَN<ن>جَیۡنَT<ـٰ>هُمۡ وَمَن N<نَّ>شَW<اۤ>ءُ وَأَهۡلَكۡنَا ٱلۡمُسۡرِفِJ<ی>نَ A<۝>D<٩>",
"لَقَQ<دۡ> أَN<ن>زَلۡنَW<اۤ> إِلَیۡكُمۡ كِتَT<ـٰ>بN<ࣰ>ا فِیهِ ذِكۡR<رُ>كُمۡۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<١٠>",
"وَكَمۡ قَصَمۡنَا مِN<ن> قَR<رۡ>یَةN<ࣲ> كَانَتۡ ظَالِمَةX<ࣰ> N<وَ>أَN<ن>شَأۡنَا بَعۡدَهَا قَوۡمًا ءَاخَرِJ<ی>نَ A<۝>D<١١>",
"فَلَN<مَّ>W<اۤ> أَحَسُّوX<ا۟> بَأۡسَنَW<اۤ> إِذَا هُم N<مِّ>نۡهَا یَR<رۡ>كُضُJ<و>نَ A<۝>D<١٢>",
"لَا تَR<رۡ>كُضُوX<ا۟> وَX<ٱ>R<رۡ>جِعُW<وۤ>X<ا۟> إِلَىٰ مَW<اۤ> أُتۡرِفۡتُمۡ فِیهِ وَمَسَT<ـٰ>كِنِكُمۡ لَعَلَّكُمۡ تُسۡءَلُJ<و>نَ A<۝>D<١٣>",
"قَالُوX<ا۟> یَT<ـٰ>وَیۡلَنَW<اۤ> إِN<نَّ>ا كُN<نَّ>ا ظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٤>",
"فَمَا زَالَت تِّلۡكَ دَعۡوَX<ى>T<ٰ>هُمۡ حَتَّىٰ جَعَلۡنَT<ـٰ>هُمۡ حَصِیدًا خَT<ـٰ>مِدِJ<ی>نَ A<۝>D<١٥>",
"وَمَا خَلَQ<قۡ>نَا ٱX<ل>سَّمَW<اۤ>ءَ وَX<ٱ>لۡأَR<رۡ>ضَ وَمَا بَیۡنَهُمَا لَT<ـٰ>عِبِJ<ی>نَ A<۝>D<١٦>",
"لَوۡ أَR<رَ>Q<دۡ>نَW<اۤ> أَن N<نَّ>تَّخِذَ لَهۡوX<ࣰ>ا لَّX<ٱ>تَّخَذۡنَT<ـٰ>هُ مِX<ن> لَّدُN<نَّ>W<اۤ> إِN<ن> كُN<نَّ>ا فَT<ـٰ>عِلِJ<ی>نَ A<۝>D<١٧>",
"بَلۡ نَQ<قۡ>ذِفُ بِX<ٱ>لۡحَقِّ عَلَى ٱلۡبَT<ـٰ>طِلِ فَیَQ<دۡ>مَغُهُT<ۥ> فَإِذَا هُوَ زَاهِقX<ࣱ>ۚ N<وَ>لَكُمُ ٱلۡوَیۡلُ مِN<مَّ>ا تَصِفُJ<و>نَ A<۝>D<١٨>",
"وَلَهُT<ۥ> مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَمَنۡ عِN<ن>دَهُT<ۥ> لَا یَسۡتَكۡبِR<رُ>ونَ عَنۡ عِبَادَتِهِT<ۦ> وَلَا یَسۡتَحۡسِR<رُ>J<و>نَ A<۝>D<١٩>",
"یُسَبِّحُونَ ٱلَّیۡلَ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ> لَا یَفۡتُR<رُ>J<و>نَ A<۝>D<٢٠>",
"أَمِ ٱتَّخَذُW<وۤ>X<ا۟> ءَالِهَةX<ࣰ> N<مِّ>نَ ٱلۡأَR<رۡ>ضِ هُمۡ یُN<ن>شِR<رُ>J<و>نَ A<۝>D<٢١>",
"لَوۡ كَانَ فِیهِمَW<اۤ> ءَالِهَةٌ إِلَّا ٱللَّهُ لَفَسَدَتَاۚ فَسُQ<بۡ>حَT<ـٰ>نَ ٱللَّهِ R<رَ>بِّ ٱلۡعَR<رۡ>شِ عَN<مَّ>ا یَصِفُJ<و>نَ A<۝>D<٢٢>",
"لَا یُسۡءَلُ عَN<مَّ>ا یَفۡعَلُ وَهُمۡ یُسۡءَلُJ<و>نَ A<۝>D<٢٣>",
"أَمِ ٱتَّخَذُوX<ا۟> مِN<ن> دُونِهِW<ۦۤ> ءَالِهَةN<ࣰ>ۖ قُلۡ هَاتُوX<ا۟> بُR<رۡ>هَT<ـٰ>نَكُمۡۖ هَT<ـٰ>ذَا ذِكۡR<رُ> مَX<ن> N<مَّ>عِیَ وَذِكۡR<رُ> مَN<ن> قَQ<بۡ>لِیۚ بَلۡ أَكۡثَR<رُ>هُمۡ لَا یَعۡلَمُونَ ٱلۡحَقَّۖ فَهُم N<مُّ>عۡرِضُJ<و>نَ A<۝>D<٢٤>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا مِN<ن> قَQ<بۡ>لِكَ مِX<ن> R<رَّ>سُولٍ إِلَّا نُوحِW<یۤ> إِلَیۡهِ أَN<نَّ>هُT<ۥ> لَW<اۤ> إِلَT<ـٰ>هَ إِلَّW<اۤ> أَنَا۠ فَX<ٱ>عۡبُدُJ<و>نِ A<۝>D<٢٥>",
"وَقَالُوX<ا۟> ٱتَّخَذَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ وَلَدN<ࣰ>اۗ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ>ۚ بَلۡ عِبَادX<ࣱ> N<مُّ>كۡR<رَ>مُJ<و>نَ A<۝>D<٢٦>",
"لَا یَسۡبِقُونَهُT<ۥ> بِX<ٱ>لۡقَوۡلِ وَهُN<م> بِأَمۡرِهِT<ۦ> یَعۡمَلُJ<و>نَ A<۝>D<٢٧>",
"یَعۡلَمُ مَا بَیۡنَ أَیۡدِیهِمۡ وَمَا خَلۡفَهُمۡ وَلَا یَشۡفَعُونَ إِلَّا لِمَنِ ٱR<رۡ>تَضَىٰ وَهُم N<مِّ>نۡ خَشۡیَتِهِT<ۦ> مُشۡفِقُJ<و>نَ A<۝>D<٢٨>",
"۞ وَمَX<ن> N<یَ>قُلۡ مِنۡهُمۡ إِN<نِّ>W<یۤ> إِلَT<ـٰ>هX<ࣱ> N<مِّ>N<ن> دُونِهِT<ۦ> فَذَT< ٰ>لِكَ نَQ<جۡ>زِیهِ جَهَN<نَّ>مَۚ كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢٩>",
"أَوَ لَمۡ یَR<رَ> ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَN<نَّ> ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ كَانَتَا R<رَ>تۡقN<ࣰ>ا فَفَتَQ<قۡ>نَT<ـٰ>هُمَاۖ وَجَعَلۡنَا مِنَ ٱلۡمَW<اۤ>ءِ كُلَّ شَیۡءٍ حَیٍّۚ أَفَلَا یُؤۡمِنُJ<و>نَ A<۝>D<٣٠>",
"وَجَعَلۡنَا فِی ٱلۡأَR<رۡ>ضِ R<رَ>وَT< ٰ>سِیَ أَN<ن> تَمِیدَ بِهِمۡ وَجَعَلۡنَا فِیهَا فِجَاجN<ࣰ>ا سُبُلX<ࣰ>ا لَّعَلَّهُمۡ یَهۡتَدُJ<و>نَ A<۝>D<٣١>",
"وَجَعَلۡنَا ٱX<ل>سَّمَW<اۤ>ءَ سَQ<قۡ>فX<ࣰ>ا N<مَّ>حۡفُوظX<ࣰ>اۖ N<وَ>هُمۡ عَنۡ ءَایَT<ـٰ>تِهَا مُعۡرِضُJ<و>نَ A<۝>D<٣٢>",
"وَهُوَ ٱلَّذِی خَلَقَ ٱلَّیۡلَ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ> وَX<ٱ>X<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ>ۖ كُلّN<ࣱ> فِی فَلَكX<ࣲ> N<یَ>سۡبَحُJ<و>نَ A<۝>D<٣٣>",
"وَمَا جَعَلۡنَا لِبَشَرX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِكَ ٱلۡخُلۡدَۖ أَفَإِX<ی۟>X<ن> N<مِّ>تَّ فَهُمُ ٱلۡخَT<ـٰ>لِدُJ<و>نَ A<۝>D<٣٤>",
"كُلُّ نَفۡسN<ࣲ> ذَW<اۤ>ئِقَةُ ٱلۡمَوۡتِۗ وَنَQ<بۡ>لُوكُN<م> بِX<ٱ>X<ل>شَّرِّ وَX<ٱ>لۡخَیۡرِ فِتۡنَةX<ࣰ>ۖ N<وَ>إِلَیۡنَا تُR<رۡ>جَعُJ<و>نَ A<۝>D<٣٥>",
"وَإِذَا R<رَ>ءَاكَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِX<ن> N<یَ>تَّخِذُونَكَ إِلَّا هُزُوًا أَهَT<ـٰ>ذَا ٱلَّذِی یَذۡكُR<رُ> ءَالِهَتَكُمۡ وَهُN<م> بِذِكۡرِ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ هُمۡ كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٣٦>",
"خُلِقَ ٱلۡإِN<ن>سَT<ـٰ>نُ مِنۡ عَجَلN<ࣲ>ۚ سَأُX<و۟>رِیكُمۡ ءَایَT<ـٰ>تِی فَلَا تَسۡتَعۡجِلُJ<و>نِ A<۝>D<٣٧>",
"وَیَقُولُونَ مَتَىٰ هَT<ـٰ>ذَا ٱلۡوَعۡدُ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٣٨>",
"لَوۡ یَعۡلَمُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> حِینَ لَا یَكُفُّونَ عَX<ن> N<وُ>جُوهِهِمُ ٱX<ل>N<نَّ>اR<رَ> وَلَا عَN<ن> ظُهُورِهِمۡ وَلَا هُمۡ یُN<ن>صَR<رُ>J<و>نَ A<۝>D<٣٩>",
"بَلۡ تَأۡتِیهِN<م> بَغۡتَةN<ࣰ> فَتَQ<بۡ>هَتُهُمۡ فَلَا یَسۡتَطِیعُونَ R<رَ>دَّهَا وَلَا هُمۡ یُN<ن>ظَR<رُ>J<و>نَ A<۝>D<٤٠>",
"وَلَقَدِ ٱسۡتُهۡزِئَ بِR<رُ>سُلX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِكَ فَحَاقَ بِX<ٱ>لَّذِینَ سَخِR<رُ>وX<ا۟> مِنۡهُم N<مَّ>ا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٤١>",
"قُلۡ مَX<ن> N<یَ>كۡلَؤُكُN<م> بِX<ٱ>لَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِ مِنَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِۚ بَلۡ هُمۡ عَN<ن> ذِكۡرِ R<رَ>بِّهِم N<مُّ>عۡرِضُJ<و>نَ A<۝>D<٤٢>",
"أَمۡ لَهُمۡ ءَالِهَةN<ࣱ> تَمۡنَعُهُم N<مِّ>N<ن> دُونِنَاۚ لَا یَسۡتَطِیعُونَ نَصۡR<رَ> أَN<ن>فُسِهِمۡ وَلَا هُم N<مِّ>N<نَّ>ا یُصۡحَبُJ<و>نَ A<۝>D<٤٣>",
"بَلۡ مَتَّعۡنَا هَW<ـٰۤ>ؤُلَW<اۤ>ءِ وَءَابَW<اۤ>ءَهُمۡ حَتَّىٰ طَالَ عَلَیۡهِمُ ٱلۡعُمُR<رُ>ۗ أَفَلَا یَR<رَ>وۡنَ أَN<نَّ>ا نَأۡتِی ٱلۡأَR<رۡ>ضَ نَN<ن>قُصُهَا مِنۡ أَQ<طۡ>R<رَ>افِهَW<اۤ>ۚ أَفَهُمُ ٱلۡغَT<ـٰ>لِبُJ<و>نَ A<۝>D<٤٤>",
"قُلۡ إِN<نَّ>مَW<اۤ> أُN<ن>ذِR<رُ>كُN<م> بِX<ٱ>لۡوَحۡیِۚ وَلَا یَسۡمَعُ ٱX<ل>صُّN<مُّ> ٱX<ل>دُّعَW<اۤ>ءَ إِذَا مَا یُN<ن>ذَR<رُ>J<و>نَ A<۝>D<٤٥>",
"وَلَئِX<ن> N<مَّ>سَّتۡهُمۡ نَفۡحَةX<ࣱ> N<مِّ>نۡ عَذَابِ R<رَ>بِّكَ لَیَقُولُN<نَّ> یَT<ـٰ>وَیۡلَنَW<اۤ> إِN<نَّ>ا كُN<نَّ>ا ظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤٦>",
"وَنَضَعُ ٱلۡمَوَT< ٰ>زِینَ ٱلۡقِسۡطَ لِیَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ فَلَا تُظۡلَمُ نَفۡسN<ࣱ> شَیۡءX<ࣰ>اۖ N<وَ>إِN<ن> كَانَ مِثۡقَالَ حَبَّةX<ࣲ> N<مِّ>نۡ خَR<رۡ>دَلٍ أَتَیۡنَا بِهَاۗ وَكَفَىٰ بِنَا حَT<ـٰ>سِبِJ<ی>نَ A<۝>D<٤٧>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَىٰ وَهَT<ـٰ>R<رُ>ونَ ٱلۡفُR<رۡ>قَانَ وَضِیَW<اۤ>ءX<ࣰ> N<وَ>ذِكۡR<ر>X<ࣰ>ا لِّلۡمُتَّقِJ<ی>نَ A<۝>D<٤٨>",
"ٱلَّذِینَ یَخۡشَوۡنَ R<رَ>بَّهُN<م> بِX<ٱ>لۡغَیۡبِ وَهُم N<مِّ>نَ ٱX<ل>سَّاعَةِ مُشۡفِقُJ<و>نَ A<۝>D<٤٩>",
"وَهَT<ـٰ>ذَا ذِكۡR<ر>X<ࣱ> N<مُّ>بَاR<رَ>كٌ أَN<ن>زَلۡنَT<ـٰ>هُۚ أَفَأَN<ن>تُمۡ لَهُT<ۥ> مُN<ن>كِR<رُ>J<و>نَ A<۝>D<٥٠>",
"۞ وَلَقَQ<دۡ> ءَاتَیۡنَW<اۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ R<رُ>شۡدَهُT<ۥ> مِN<ن> قَQ<بۡ>لُ وَكُN<نَّ>ا بِهِT<ۦ> عَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥١>",
"إِذۡ قَالَ لِأَبِیهِ وَقَوۡمِهِT<ۦ> مَا هَT<ـٰ>ذِهِ ٱX<ل>تَّمَاثِیلُ ٱلَّتِW<یۤ> أَN<ن>تُمۡ لَهَا عَT<ـٰ>كِفُJ<و>نَ A<۝>D<٥٢>",
"قَالُوX<ا۟> وَجَQ<دۡ>نَW<اۤ> ءَابَW<اۤ>ءَنَا لَهَا عَT<ـٰ>بِدِJ<ی>نَ A<۝>D<٥٣>",
"قَالَ لَقَQ<دۡ> كُN<ن>تُمۡ أَN<ن>تُمۡ وَءَابَW<اۤ>ؤُكُمۡ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٥٤>",
"قَالُW<وۤ>X<ا۟> أَجِئۡتَنَا بِX<ٱ>لۡحَقِّ أَمۡ أَN<ن>تَ مِنَ ٱX<ل>لَّT<ـٰ>عِبِJ<ی>نَ A<۝>D<٥٥>",
"قَالَ بَل R<رَّ>بُّكُمۡ R<رَ>بُّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ ٱلَّذِی فَطَR<رَ>هُN<نَّ> وَأَنَا۠ عَلَىٰ ذَT< ٰ>لِكُم N<مِّ>نَ ٱX<ل>شَّT<ـٰ>هِدِJ<ی>نَ A<۝>D<٥٦>",
"وَتَٱللَّهِ لَأَكِیدَN<نَّ> أَصۡنَT<ـٰ>مَكُN<م> بَعۡدَ أَN<ن> تُوَلُّوX<ا۟> مُQ<دۡ>بِرِJ<ی>نَ A<۝>D<٥٧>",
"فَجَعَلَهُمۡ جُذَT< ٰ>ذًا إِلَّا كَبِیR<ر>X<ࣰ>ا لَّهُمۡ لَعَلَّهُمۡ إِلَیۡهِ یَR<رۡ>جِعُJ<و>نَ A<۝>D<٥٨>",
"قَالُوX<ا۟> مَN<ن> فَعَلَ هَT<ـٰ>ذَا بِءَالِهَتِنَW<اۤ> إِN<نَّ>هُT<ۥ> لَمِنَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥٩>",
"قَالُوX<ا۟> سَمِعۡنَا فَتX<ࣰ>ى N<یَ>ذۡكُR<رُ>هُمۡ یُقَالُ لَهُW<ۥۤ> إِQ<بۡ>R<رَ>T< ٰ>هِJ<ی>مُ A<۝>D<٦٠>",
"قَالُوX<ا۟> فَأۡتُوX<ا۟> بِهِT<ۦ> عَلَىW<ٰۤ> أَعۡیُنِ ٱX<ل>N<نَّ>اسِ لَعَلَّهُمۡ یَشۡهَدُJ<و>نَ A<۝>D<٦١>",
"قَالُW<وۤ>X<ا۟> ءَأَN<ن>تَ فَعَلۡتَ هَT<ـٰ>ذَا بِءَالِهَتِنَا یَW<ـٰۤ>إِQ<بۡ>R<رَ>T< ٰ>هِJ<ی>مُ A<۝>D<٦٢>",
"قَالَ بَلۡ فَعَلَهُT<ۥ> كَبِیR<رُ>هُمۡ هَT<ـٰ>ذَا فَسۡءَلُوهُمۡ إِN<ن> كَانُوX<ا۟> یَN<ن>طِقُJ<و>نَ A<۝>D<٦٣>",
"فَR<رَ>جَعُW<وۤ>X<ا۟> إِلَىW<ٰۤ> أَN<ن>فُسِهِمۡ فَقَالُW<وۤ>X<ا۟> إِN<نَّ>كُمۡ أَN<ن>تُمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٦٤>",
"ثُN<مَّ> نُكِسُوX<ا۟> عَلَىٰ R<رُ>ءُوسِهِمۡ لَقَQ<دۡ> عَلِمۡتَ مَا هَW<ـٰۤ>ؤُلَW<اۤ>ءِ یَN<ن>طِقُJ<و>نَ A<۝>D<٦٥>",
"قَالَ أَفَتَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ مَا لَا یَN<ن>فَعُكُمۡ شَیۡءX<ࣰ>ا N<وَ>لَا یَضُR<رُّ>كُمۡ A<۝>D<٦٦>",
"أُفّX<ࣲ> لَّكُمۡ وَلِمَا تَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<٦٧>",
"قَالُوX<ا۟> حَرِّقُوهُ وَX<ٱ>N<ن>صُR<رُ>W<وۤ>X<ا۟> ءَالِهَتَكُمۡ إِN<ن> كُN<ن>تُمۡ فَT<ـٰ>عِلِJ<ی>نَ A<۝>D<٦٨>",
"قُلۡنَا یَT<ـٰ>نَاR<رُ> كُونِی بَR<رۡ>دX<ࣰ>ا N<وَ>سَلَT<ـٰ>مًا عَلَىW<ٰۤ> إِQ<بۡ>R<رَ>T< ٰ>هِJ<ی>مَ A<۝>D<٦٩>",
"وَأَR<رَ>ادُوX<ا۟> بِهِT<ۦ> كَیۡدN<ࣰ>ا فَجَعَلۡنَT<ـٰ>هُمُ ٱلۡأَخۡسَرِJ<ی>نَ A<۝>D<٧٠>",
"وَنَجَّیۡنَT<ـٰ>هُ وَلُوطًا إِلَى ٱلۡأَR<رۡ>ضِ ٱلَّتِی بَT<ـٰ>R<رَ>كۡنَا فِیهَا لِلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٧١>",
"وَوَهَQ<بۡ>نَا لَهُW<ۥۤ> إِسۡحَT<ـٰ>قَ وَیَعۡقُوبَ نَافِلَةX<ࣰ>ۖ N<وَ>كُلّN<ࣰ>ا جَعَلۡنَا صَT<ـٰ>لِحِJ<ی>نَ A<۝>D<٧٢>",
"وَجَعَلۡنَT<ـٰ>هُمۡ أَئِN<مَّ>ةX<ࣰ> N<یَ>هۡدُونَ بِأَمۡرِنَا وَأَوۡحَیۡنَW<اۤ> إِلَیۡهِمۡ فِعۡلَ ٱلۡخَیۡR<رَ>T< ٰ>تِ وَإِقَامَ ٱX<ل>صَّلَX<و>T<ٰ>ةِ وَإِیتَW<اۤ>ءَ ٱX<ل>زَّكَX<و>T<ٰ>ةِۖ وَكَانُوX<ا۟> لَنَا عَT<ـٰ>بِدِJ<ی>نَ A<۝>D<٧٣>",
"وَلُوطًا ءَاتَیۡنَT<ـٰ>هُ حُكۡمX<ࣰ>ا N<وَ>عِلۡمX<ࣰ>ا N<وَ>نَجَّیۡنَT<ـٰ>هُ مِنَ ٱلۡقَR<رۡ>یَةِ ٱلَّتِی كَانَت تَّعۡمَلُ ٱلۡخَبَW<ـٰۤ>ئِثَۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> قَوۡمَ سَوۡءN<ࣲ> فَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٧٤>",
"وَأَQ<دۡ>خَلۡنَT<ـٰ>هُ فِی R<رَ>حۡمَتِنَW<اۤ>ۖ إِN<نَّ>هُT<ۥ> مِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٧٥>",
"وَنُوحًا إِذۡ نَادَىٰ مِN<ن> قَQ<بۡ>لُ فَX<ٱ>سۡتَجَQ<بۡ>نَا لَهُT<ۥ> فَنَجَّیۡنَT<ـٰ>هُ وَأَهۡلَهُT<ۥ> مِنَ ٱلۡكَR<رۡ>بِ ٱلۡعَظِJ<ی>مِ A<۝>D<٧٦>",
"وَنَصَR<رۡ>نَT<ـٰ>هُ مِنَ ٱلۡقَوۡمِ ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَW<اۤ>ۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> قَوۡمَ سَوۡءN<ࣲ> فَأَغۡR<رَ>Q<قۡ>نَT<ـٰ>هُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٧٧>",
"وَدَاوُT<ۥ>دَ وَسُلَیۡمَT<ـٰ>نَ إِذۡ یَحۡكُمَانِ فِی ٱلۡحَR<رۡ>ثِ إِذۡ نَفَشَتۡ فِیهِ غَنَمُ ٱلۡقَوۡمِ وَكُN<نَّ>ا لِحُكۡمِهِمۡ شَT<ـٰ>هِدِJ<ی>نَ A<۝>D<٧٨>",
"فَفَهَّمۡنَT<ـٰ>هَا سُلَیۡمَT<ـٰ>نَۚ وَكُلًّا ءَاتَیۡنَا حُكۡمX<ࣰ>ا N<وَ>عِلۡمX<ࣰ>اۚ N<وَ>سَخَّR<رۡ>نَا مَعَ دَاوُT<ۥ>دَ ٱلۡجِبَالَ یُسَبِّحۡنَ وَX<ٱ>X<ل>طَّیۡR<رَ>ۚ وَكُN<نَّ>ا فَT<ـٰ>عِلِJ<ی>نَ A<۝>D<٧٩>",
"وَعَلَّمۡنَT<ـٰ>هُ صَنۡعَةَ لَبُوسX<ࣲ> لَّكُمۡ لِتُحۡصِنَكُم N<مِّ>X<ن>N<ۢ> بَأۡسِكُمۡۖ فَهَلۡ أَN<ن>تُمۡ شَT<ـٰ>كِR<رُ>J<و>نَ A<۝>D<٨٠>",
"وَلِسُلَیۡمَT<ـٰ>نَ ٱX<ل>رِّیحَ عَاصِفَةN<ࣰ> تَQ<جۡ>رِی بِأَمۡرِهِW<ۦۤ> إِلَى ٱلۡأَR<رۡ>ضِ ٱلَّتِی بَT<ـٰ>R<رَ>كۡنَا فِیهَاۚ وَكُN<نَّ>ا بِكُلِّ شَیۡءٍ عَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٨١>",
"وَمِنَ ٱX<ل>شَّیَT<ـٰ>طِینِ مَX<ن> N<یَ>غُوصُونَ لَهُT<ۥ> وَیَعۡمَلُونَ عَمَلN<ࣰ>ا دُونَ ذَT< ٰ>لِكَۖ وَكُN<نَّ>ا لَهُمۡ حَT<ـٰ>فِظِJ<ی>نَ A<۝>D<٨٢>",
"۞ وَأَیُّوبَ إِذۡ نَادَىٰ R<رَ>بَّهُW<ۥۤ> أَN<نِّ>ی مَسَّنِیَ ٱX<ل>ضُّR<رُّ> وَأَN<ن>تَ أَR<رۡ>حَمُ ٱX<ل>R<رَّ>T< ٰ>حِمِJ<ی>نَ A<۝>D<٨٣>",
"فَX<ٱ>سۡتَجَQ<بۡ>نَا لَهُT<ۥ> فَكَشَفۡنَا مَا بِهِT<ۦ> مِN<ن> ضُرّX<ࣲ>ۖ N<وَ>ءَاتَیۡنَT<ـٰ>هُ أَهۡلَهُT<ۥ> وَمِثۡلَهُم N<مَّ>عَهُمۡ R<رَ>حۡمَةX<ࣰ> N<مِّ>نۡ عِN<ن>دِنَا وَذِكۡR<رَ>ىٰ لِلۡعَT<ـٰ>بِدِJ<ی>نَ A<۝>D<٨٤>",
"وَإِسۡمَT<ـٰ>عِیلَ وَإِQ<دۡ>رِیسَ وَذَا ٱلۡكِفۡلِۖ كُلّX<ࣱ> N<مِّ>نَ ٱX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<٨٥>",
"وَأَQ<دۡ>خَلۡنَT<ـٰ>هُمۡ فِی R<رَ>حۡمَتِنَW<اۤ>ۖ إِN<نَّ>هُم N<مِّ>نَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٨٦>",
"وَذَا ٱX<ل>N<نُّ>ونِ إِذ ذَّهَبَ مُغَT<ـٰ>ضِبN<ࣰ>ا فَظَN<نَّ> أَX<ن> لَّن N<نَّ>Q<قۡ>دِR<رَ> عَلَیۡهِ فَنَادَىٰ فِی ٱX<ل>ظُّلُمَT<ـٰ>تِ أَX<ن> لَّW<اۤ> إِلَT<ـٰ>هَ إِلَّW<اۤ> أَN<ن>تَ سُQ<بۡ>حَT<ـٰ>نَكَ إِN<نِّ>ی كُN<ن>تُ مِنَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٨٧>",
"فَX<ٱ>سۡتَجَQ<بۡ>نَا لَهُT<ۥ> وَنَجَّیۡنَT<ـٰ>هُ مِنَ ٱلۡغَN<مِّ>ۚ وَكَذَT< ٰ>لِكَ نُۨجِی ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٨٨>",
"وَزَكَرِیَّW<اۤ> إِذۡ نَادَىٰ R<رَ>بَّهُT<ۥ> R<رَ>بِّ لَا تَذَR<رۡ>نِی فَR<رۡ>دX<ࣰ>ا N<وَ>أَN<ن>تَ خَیۡR<رُ> ٱلۡوَT< ٰ>رِثِJ<ی>نَ A<۝>D<٨٩>",
"فَX<ٱ>سۡتَجَQ<بۡ>نَا لَهُT<ۥ> وَوَهَQ<بۡ>نَا لَهُT<ۥ> یَحۡیَىٰ وَأَصۡلَحۡنَا لَهُT<ۥ> زَوۡجَهُW<ۥۤ>ۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> یُسَT<ـٰ>رِعُونَ فِی ٱلۡخَیۡR<رَ>T< ٰ>تِ وَیَQ<دۡ>عُونَنَا R<رَ>غَبX<ࣰ>ا N<وَ>R<رَ>هَبX<ࣰ>اۖ N<وَ>كَانُوX<ا۟> لَنَا خَT<ـٰ>شِعِJ<ی>نَ A<۝>D<٩٠>",
"وَX<ٱ>لَّتِW<یۤ> أَحۡصَنَتۡ فَR<رۡ>جَهَا فَنَفَخۡنَا فِیهَا مِX<ن> R<رُّ>وحِنَا وَجَعَلۡنَT<ـٰ>هَا وَX<ٱ>Q<بۡ>نَهَW<اۤ> ءَایَةX<ࣰ> لِّلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٩١>",
"إِN<نَّ> هَT<ـٰ>ذِهِW<ۦۤ> أُN<مَّ>تُكُمۡ أُN<مَّ>ةX<ࣰ> N<وَ>T< ٰ>حِدَةX<ࣰ> N<وَ>أَنَا۠ R<رَ>بُّكُمۡ فَX<ٱ>عۡبُدُJ<و>نِ A<۝>D<٩٢>",
"وَتَقَطَّعُW<وۤ>X<ا۟> أَمۡR<رَ>هُN<م> بَیۡنَهُمۡۖ كُلٌّ إِلَیۡنَا R<رَ>T< ٰ>جِعُJ<و>نَ A<۝>D<٩٣>",
"فَمَX<ن> N<یَ>عۡمَلۡ مِنَ ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَهُوَ مُؤۡمِنN<ࣱ> فَلَا كُفۡR<رَ>انَ لِسَعۡیِهِT<ۦ> وَإِN<نَّ>ا لَهُT<ۥ> كَT<ـٰ>تِبُJ<و>نَ A<۝>D<٩٤>",
"وَحَR<رَ>T< ٰ>مٌ عَلَىٰ قَR<رۡ>یَةٍ أَهۡلَكۡنَT<ـٰ>هَW<اۤ> أَN<نَّ>هُمۡ لَا یَR<رۡ>جِعُJ<و>نَ A<۝>D<٩٥>",
"حَتَّىW<ٰۤ> إِذَا فُتِحَتۡ یَأۡجُوجُ وَمَأۡجُوجُ وَهُم N<مِّ>N<ن> كُلِّ حَدَبX<ࣲ> N<یَ>N<ن>سِلُJ<و>نَ A<۝>D<٩٦>",
"وَX<ٱ>Q<قۡ>تَR<رَ>بَ ٱلۡوَعۡدُ ٱلۡحَقُّ فَإِذَا هِیَ شَT<ـٰ>خِصَةٌ أَQ<بۡ>صَT<ـٰ>R<رُ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> یَT<ـٰ>وَیۡلَنَا قَQ<دۡ> كُN<نَّ>ا فِی غَفۡلَةX<ࣲ> N<مِّ>نۡ هَT<ـٰ>ذَا بَلۡ كُN<نَّ>ا ظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٩٧>",
"إِN<نَّ>كُمۡ وَمَا تَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ حَصَبُ جَهَN<نَّ>مَ أَN<ن>تُمۡ لَهَا وَT< ٰ>رِدُJ<و>نَ A<۝>D<٩٨>",
"لَوۡ كَانَ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ ءَالِهَةX<ࣰ> N<مَّ>ا وَR<رَ>دُوهَاۖ وَكُلّN<ࣱ> فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٩٩>",
"لَهُمۡ فِیهَا زَفِیR<ر>X<ࣱ> N<وَ>هُمۡ فِیهَا لَا یَسۡمَعُJ<و>نَ A<۝>D<١٠٠>",
"إِN<نَّ> ٱلَّذِینَ سَبَقَتۡ لَهُم N<مِّ>N<نَّ>ا ٱلۡحُسۡنَىW<ٰۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ عَنۡهَا مُQ<بۡ>عَدُJ<و>نَ A<۝>D<١٠١>",
"لَا یَسۡمَعُونَ حَسِیسَهَاۖ وَهُمۡ فِی مَا ٱشۡتَهَتۡ أَN<ن>فُسُهُمۡ خَT<ـٰ>لِدُJ<و>نَ A<۝>D<١٠٢>",
"لَا یَحۡزُنُهُمُ ٱلۡفَزَعُ ٱلۡأَكۡبَR<رُ> وَتَتَلَقَّX<ى>T<ٰ>هُمُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ هَT<ـٰ>ذَا یَوۡمُكُمُ ٱلَّذِی كُN<ن>تُمۡ تُوعَدُJ<و>نَ A<۝>D<١٠٣>",
"یَوۡمَ نَQ<طۡ>وِی ٱX<ل>سَّمَW<اۤ>ءَ كَطَیِّ ٱX<ل>سِّجِلِّ لِلۡكُتُبِۚ كَمَا بَدَأۡنَW<اۤ> أَوَّلَ خَلۡقX<ࣲ> N<نُّ>عِیدُهُT<ۥ>ۚ وَعۡدًا عَلَیۡنَW<اۤ>ۚ إِN<نَّ>ا كُN<نَّ>ا فَT<ـٰ>عِلِJ<ی>نَ A<۝>D<١٠٤>",
"وَلَقَQ<دۡ> كَتَQ<بۡ>نَا فِی ٱX<ل>زَّبُورِ مِX<ن>N<ۢ> بَعۡدِ ٱX<ل>ذِّكۡرِ أَN<نَّ> ٱلۡأَR<رۡ>ضَ یَرِثُهَا عِبَادِیَ ٱX<ل>صَّT<ـٰ>لِحُJ<و>نَ A<۝>D<١٠٥>",
"إِN<نَّ> فِی هَT<ـٰ>ذَا لَبَلَT<ـٰ>غX<ࣰ>ا لِّقَوۡمٍ عَT<ـٰ>بِدِJ<ی>نَ A<۝>D<١٠٦>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ إِلَّا R<رَ>حۡمَةX<ࣰ> لِّلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٠٧>",
"قُلۡ إِN<نَّ>مَا یُوحَىW<ٰۤ> إِلَیَّ أَN<نَّ>مَW<اۤ> إِلَT<ـٰ>هُكُمۡ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدN<ࣱ>ۖ فَهَلۡ أَN<ن>تُم N<مُّ>سۡلِمُJ<و>نَ A<۝>D<١٠٨>",
"فَإِN<ن> تَوَلَّوۡX<ا۟> فَقُلۡ ءَاذَN<ن>تُكُمۡ عَلَىٰ سَوَW<اۤ>ءX<ࣲ>ۖ N<وَ>إِنۡ أَQ<دۡ>رِW<یۤ> أَقَرِیبٌ أَN<م> بَعِیدX<ࣱ> N<مَّ>ا تُوعَدُJ<و>نَ A<۝>D<١٠٩>",
"إِN<نَّ>هُT<ۥ> یَعۡلَمُ ٱلۡجَهۡR<رَ> مِنَ ٱلۡقَوۡلِ وَیَعۡلَمُ مَا تَكۡتُمُJ<و>نَ A<۝>D<١١٠>",
"وَإِنۡ أَQ<دۡ>رِی لَعَلَّهُT<ۥ> فِتۡنَةX<ࣱ> لَّكُمۡ وَمَتَT<ـٰ>عٌ إِلَىٰ حِJ<ی>نࣲ A<۝>D<١١١>",
"قَT<ـٰ>لَ R<رَ>بِّ ٱحۡكُN<م> بِX<ٱ>لۡحَقِّۗ وَR<رَ>بُّنَا ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ ٱلۡمُسۡتَعَانُ عَلَىٰ مَا تَصِفُJ<و>نَ A<۝>D<١١٢>",
"#۞ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ ٱتَّقُوX<ا۟> R<رَ>بَّكُمۡۚ إِN<نَّ> زَلۡزَلَةَ ٱX<ل>سَّاعَةِ شَیۡءٌ عَظِJ<ی>مࣱ A<۝>D<١>",
"یَوۡمَ تَR<رَ>وۡنَهَا تَذۡهَلُ كُلُّ مُR<رۡ>ضِعَةٍ عَN<مَّ>W<اۤ> أَR<رۡ>ضَعَتۡ وَتَضَعُ كُلُّ ذَاتِ حَمۡلٍ حَمۡلَهَا وَتَR<رَ>ى ٱX<ل>N<نَّ>اسَ سُكَT<ـٰ>R<رَ>ىٰ وَمَا هُN<م> بِسُكَT<ـٰ>R<رَ>ىٰ وَلَT<ـٰ>كِN<نَّ> عَذَابَ ٱللَّهِ شَدِJ<ی>Q<د>ࣱ A<۝>D<٢>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یُ>جَT<ـٰ>دِلُ فِی ٱللَّهِ بِغَیۡرِ عِلۡمX<ࣲ> N<وَ>یَتَّبِعُ كُلَّ شَیۡطَT<ـٰ>نX<ࣲ> N<مَّ>رِJ<ی>Q<د>ࣲ A<۝>D<٣>",
"كُتِبَ عَلَیۡهِ أَN<نَّ>هُT<ۥ> مَN<ن> تَوَلَّاهُ فَأَN<نَّ>هُT<ۥ> یُضِلُّهُT<ۥ> وَیَهۡدِیهِ إِلَىٰ عَذَابِ ٱX<ل>سَّعِJ<ی>رِ A<۝>D<٤>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ إِN<ن> كُN<ن>تُمۡ فِی R<رَ>یۡبX<ࣲ> N<مِّ>نَ ٱلۡبَعۡثِ فَإِN<نَّ>ا خَلَQ<قۡ>نَT<ـٰ>كُم N<مِّ>N<ن> تُR<رَ>ابN<ࣲ> ثُN<مَّ> مِن N<نُّ>Q<طۡ>فَةN<ࣲ> ثُN<مَّ> مِنۡ عَلَقَةN<ࣲ> ثُN<مَّ> مِX<ن> N<مُّ>ضۡغَةX<ࣲ> N<مُّ>خَلَّقَةX<ࣲ> N<وَ>غَیۡرِ مُخَلَّقَةX<ࣲ> لِّنُبَیِّنَ لَكُمۡۚ وَنُقِR<رُّ> فِی ٱلۡأَR<رۡ>حَامِ مَا نَشَW<اۤ>ءُ إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>N<ࣰ>ى ثُN<مَّ> نُخۡرِجُكُمۡ طِفۡلN<ࣰ>ا ثُN<مَّ> لِتَQ<بۡ>لُغُW<وۤ>X<ا۟> أَشُدَّكُمۡۖ وَمِN<ن>كُم N<مَّ>X<ن> N<یُ>تَوَفَّىٰ وَمِN<ن>كُم N<مَّ>X<ن> N<یُ>R<رَ>دُّ إِلَىW<ٰۤ> أَR<رۡ>ذَلِ ٱلۡعُمُرِ لِكَیۡلَا یَعۡلَمَ مِX<ن>N<ۢ> بَعۡدِ عِلۡمN<ࣲ> شَیۡءX<ࣰ>اۚ N<وَ>تَR<رَ>ى ٱلۡأَR<رۡ>ضَ هَامِدَةN<ࣰ> فَإِذَW<اۤ> أَN<ن>زَلۡنَا عَلَیۡهَا ٱلۡمَW<اۤ>ءَ ٱهۡتَزَّتۡ وَR<رَ>بَتۡ وَأَX<ن>N<ۢ>بَتَتۡ مِN<ن> كُلِّ زَوۡجِN<ۭ> بَهِJ<ی>Q<ج>ࣲ A<۝>D<٥>",
"ذَT< ٰ>لِكَ بِأَN<نَّ> ٱللَّهَ هُوَ ٱلۡحَقُّ وَأَN<نَّ>هُT<ۥ> یُحۡیِ ٱلۡمَوۡتَىٰ وَأَN<نَّ>هُT<ۥ> عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٦>",
"وَأَN<نَّ> ٱX<ل>سَّاعَةَ ءَاتِیَةX<ࣱ> لَّا R<رَ>یۡبَ فِیهَا وَأَN<نَّ> ٱللَّهَ یَQ<بۡ>عَثُ مَN<ن> فِی ٱلۡقُبُJ<و>R<ر>ِ A<۝>D<٧>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یُ>جَT<ـٰ>دِلُ فِی ٱللَّهِ بِغَیۡرِ عِلۡمX<ࣲ> N<وَ>لَا هُدX<ࣰ>ى N<وَ>لَا كِتَT<ـٰ>بX<ࣲ> N<مُّ>نِJ<ی>رࣲ A<۝>D<٨>",
"ثَانِیَ عِQ<طۡ>فِهِT<ۦ> لِیُضِلَّ عَN<ن> سَبِیلِ ٱللَّهِۖ لَهُT<ۥ> فِی ٱX<ل>دُّنۡیَا خِزۡیX<ࣱ>ۖ N<وَ>نُذِیقُهُT<ۥ> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ عَذَابَ ٱلۡحَرِJ<ی>Q<ق>ِ A<۝>D<٩>",
"ذَT< ٰ>لِكَ بِمَا قَدَّمَتۡ یَدَاكَ وَأَN<نَّ> ٱللَّهَ لَیۡسَ بِظَلَّT<ـٰ>مX<ࣲ> لِّلۡعَبِJ<ی>Q<د>ِ A<۝>D<١٠>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یَ>عۡبُدُ ٱللَّهَ عَلَىٰ حَR<رۡ>فN<ࣲ>ۖ فَإِنۡ أَصَابَهُT<ۥ> خَیۡR<ر>ٌ ٱQ<طۡ>مَأَN<نَّ> بِهِT<ۦ>ۖ وَإِنۡ أَصَابَتۡهُ فِتۡنَةٌ ٱN<ن>قَلَبَ عَلَىٰ وَQ<جۡ>هِهِT<ۦ> خَسِR<رَ> ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةَۚ ذَT< ٰ>لِكَ هُوَ ٱلۡخُسۡR<رَ>انُ ٱلۡمُبِJ<ی>نُ A<۝>D<١١>",
"یَQ<دۡ>عُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ مَا لَا یَضُR<رُّ>هُT<ۥ> وَمَا لَا یَN<ن>فَعُهُT<ۥ>ۚ ذَT< ٰ>لِكَ هُوَ ٱX<ل>ضَّلَT<ـٰ>لُ ٱلۡبَعِJ<ی>Q<د>ُ A<۝>D<١٢>",
"یَQ<دۡ>عُوX<ا۟> لَمَN<ن> ضَR<رُّ>هُW<ۥۤ> أَQ<قۡ>R<رَ>بُ مِن N<نَّ>فۡعِهِT<ۦ>ۚ لَبِئۡسَ ٱلۡمَوۡلَىٰ وَلَبِئۡسَ ٱلۡعَشِJ<ی>رُ A<۝>D<١٣>",
"إِN<نَّ> ٱللَّهَ یُQ<دۡ>خِلُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ>ۚ إِN<نَّ> ٱللَّهَ یَفۡعَلُ مَا یُرِJ<ی>Q<د>ُ A<۝>D<١٤>",
"مَN<ن> كَانَ یَظُN<نُّ> أَX<ن> لَّX<ن> N<یَ>N<ن>صُR<رَ>هُ ٱللَّهُ فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِ فَلۡیَمۡدُQ<دۡ> بِسَبَبٍ إِلَى ٱX<ل>سَّمَW<اۤ>ءِ ثُN<مَّ> لۡیَQ<قۡ>طَعۡ فَلۡیَN<ن>ظُR<رۡ> هَلۡ یُذۡهِبَN<نَّ> كَیۡدُهُT<ۥ> مَا یَغِJ<ی>ظُ A<۝>D<١٥>",
"وَكَذَT< ٰ>لِكَ أَN<ن>زَلۡنَT<ـٰ>هُ ءَایَT<ـٰ>تِN<ۭ> بَیِّنَT<ـٰ>تX<ࣲ> N<وَ>أَN<نَّ> ٱللَّهَ یَهۡدِی مَX<ن> N<یُ>رِJ<ی>Q<د>ُ A<۝>D<١٦>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَX<ٱ>لَّذِینَ هَادُوX<ا۟> وَX<ٱ>X<ل>صَّT<ـٰ>بِءِینَ وَX<ٱ>X<ل>N<نَّ>صَT<ـٰ>R<رَ>ىٰ وَX<ٱ>لۡمَجُوسَ وَX<ٱ>لَّذِینَ أَشۡR<رَ>كُW<وۤ>X<ا۟> إِN<نَّ> ٱللَّهَ یَفۡصِلُ بَیۡنَهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۚ إِN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> شَهِJ<ی>Q<د>ٌ A<۝>D<١٧>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ یَ̅سۡ̅جُ̅دُ̅ ̅لَ̅هُ̅T<ۥ>̅ مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَN<ن> فِی ٱلۡأَR<رۡ>ضِ وَX<ٱ>X<ل>شَّمۡسُ وَX<ٱ>لۡقَمَR<رُ> وَX<ٱ>X<ل>N<نُّ>جُومُ وَX<ٱ>لۡجِبَالُ وَX<ٱ>X<ل>شَّجَR<رُ> وَX<ٱ>X<ل>دَّوَL<اۤ>بُّ وَكَثِیR<ر>X<ࣱ> N<مِّ>نَ ٱX<ل>N<نَّ>اسِۖ وَكَثِیR<ر>ٌ حَقَّ عَلَیۡهِ ٱلۡعَذَابُۗ وَمَX<ن> N<یُ>هِنِ ٱللَّهُ فَمَا لَهُT<ۥ> مِX<ن> N<مُّ>كۡرِمٍۚ إِN<نَّ> ٱللَّهَ یَفۡعَلُ مَا یَشَW<اۤ>ءُ A<۝>D<١٨> ۩",
"۞ هَT<ـٰ>ذَانِ خَصۡمَانِ ٱخۡتَصَمُوX<ا۟> فِی R<رَ>بِّهِمۡۖ فَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> قُطِّعَتۡ لَهُمۡ ثِیَابX<ࣱ> N<مِّ>ن N<نَّ>ارX<ࣲ> N<یُ>صَبُّ مِN<ن> فَوۡقِ R<رُ>ءُوسِهِمُ ٱلۡحَمِJ<ی>مُ A<۝>D<١٩>",
"یُصۡهَR<رُ> بِهِT<ۦ> مَا فِی بُطُونِهِمۡ وَX<ٱ>لۡجُلُJ<و>Q<د>ُ A<۝>D<٢٠>",
"وَلَهُم N<مَّ>قَT<ـٰ>مِعُ مِنۡ حَدِJ<ی>Q<د>ࣲ A<۝>D<٢١>",
"كُلَّمَW<اۤ> أَR<رَ>ادُW<وۤ>X<ا۟> أَX<ن> N<یَ>خۡR<رُ>جُوX<ا۟> مِنۡهَا مِنۡ غَN<مّ>ٍ أُعِیدُوX<ا۟> فِیهَا وَذُوقُوX<ا۟> عَذَابَ ٱلۡحَرِJ<ی>Q<ق>ِ A<۝>D<٢٢>",
"إِN<نَّ> ٱللَّهَ یُQ<دۡ>خِلُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> یُحَلَّوۡنَ فِیهَا مِنۡ أَسَاوِR<رَ> مِN<ن> ذَهَبX<ࣲ> N<وَ>لُؤۡلُؤX<ࣰ>اۖ N<وَ>لِبَاسُهُمۡ فِیهَا حَرِJ<ی>رࣱ A<۝>D<٢٣>",
"وَهُدُW<وۤ>X<ا۟> إِلَى ٱX<ل>طَّیِّبِ مِنَ ٱلۡقَوۡلِ وَهُدُW<وۤ>X<ا۟> إِلَىٰ صِR<رَ>T< ٰ>طِ ٱلۡحَمِJ<ی>Q<د>ِ A<۝>D<٢٤>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَیَصُدُّونَ عَN<ن> سَبِیلِ ٱللَّهِ وَX<ٱ>لۡمَسۡجِدِ ٱلۡحَR<رَ>امِ ٱلَّذِی جَعَلۡنَT<ـٰ>هُ لِX<ل>N<نَّ>اسِ سَوَW<اۤ>ءً ٱلۡعَT<ـٰ>كِفُ فِیهِ وَX<ٱ>لۡبَادِۚ وَمَX<ن> N<یُ>رِQ<دۡ> فِیهِ بِإِلۡحَادِN<ۭ> بِظُلۡمX<ࣲ> N<نُّ>ذِQ<قۡ>هُ مِنۡ عَذَابٍ أَلِJ<ی>مࣲ A<۝>D<٢٥>",
"وَإِذۡ بَوَّأۡنَا لِإِQ<بۡ>R<رَ>T< ٰ>هِیمَ مَكَانَ ٱلۡبَیۡتِ أَX<ن> لَّا تُشۡرِكۡ بِی شَیۡءX<ࣰ>ا N<وَ>طَهِّرۡ بَیۡتِیَ لِX<ل>طَّW<اۤ>ئِفِینَ وَX<ٱ>لۡقَW<اۤ>ئِمِینَ وَX<ٱ>X<ل>R<رُّ>كَّعِ ٱX<ل>سُّجُJ<و>Q<د>ِ A<۝>D<٢٦>",
"وَأَذِّN<ن> فِی ٱX<ل>N<نَّ>اسِ بِX<ٱ>لۡحَجِّ یَأۡتُوكَ رِجَالX<ࣰ>ا N<وَ>عَلَىٰ كُلِّ ضَامِرX<ࣲ> N<یَ>أۡتِینَ مِN<ن> كُلِّ فَجٍّ عَمِJ<ی>Q<ق>ࣲ A<۝>D<٢٧>",
"لِّیَشۡهَدُوX<ا۟> مَنَT<ـٰ>فِعَ لَهُمۡ وَیَذۡكُR<رُ>وX<ا۟> ٱسۡمَ ٱللَّهِ فِW<یۤ> أَیَّامX<ࣲ> N<مَّ>عۡلُومَT<ـٰ>تٍ عَلَىٰ مَا R<رَ>زَقَهُم N<مِّ>X<ن>N<ۢ> بَهِیمَةِ ٱلۡأَنۡعَT<ـٰ>مِۖ فَكُلُوX<ا۟> مِنۡهَا وَأَQ<طۡ>عِمُوX<ا۟> ٱلۡبَW<اۤ>ئِسَ ٱلۡفَقِJ<ی>رَ A<۝>D<٢٨>",
"ثُN<مَّ> لۡیَQ<قۡ>ضُوX<ا۟> تَفَثَهُمۡ وَلۡیُوفُوX<ا۟> نُذُوR<رَ>هُمۡ وَلۡیَطَّوَّفُوX<ا۟> بِX<ٱ>لۡبَیۡتِ ٱلۡعَتِJ<ی>Q<ق>ِ A<۝>D<٢٩>",
"ذَT< ٰ>لِكَۖ وَمَX<ن> N<یُ>عَظِّمۡ حُR<رُ>مَT<ـٰ>تِ ٱللَّهِ فَهُوَ خَیۡR<ر>X<ࣱ> لَّهُT<ۥ> عِN<ن>دَ R<رَ>بِّهِT<ۦ>ۗ وَأُحِلَّتۡ لَكُمُ ٱلۡأَنۡعَT<ـٰ>مُ إِلَّا مَا یُتۡلَىٰ عَلَیۡكُمۡۖ فَX<ٱ>Q<جۡ>تَنِبُوX<ا۟> ٱX<ل>رِّQ<جۡ>سَ مِنَ ٱلۡأَوۡثَT<ـٰ>نِ وَX<ٱ>Q<جۡ>تَنِبُوX<ا۟> قَوۡلَ ٱX<ل>زُّJ<و>R<ر>ِ A<۝>D<٣٠>",
"حُنَفَW<اۤ>ءَ لِلَّهِ غَیۡR<رَ> مُشۡرِكِینَ بِهِT<ۦ>ۚ وَمَX<ن> N<یُ>شۡرِكۡ بِٱللَّهِ فَكَأَN<نَّ>مَا خَR<رَّ> مِنَ ٱX<ل>سَّمَW<اۤ>ءِ فَتَخۡطَفُهُ ٱX<ل>طَّیۡR<رُ> أَوۡ تَهۡوِی بِهِ ٱX<ل>رِّیحُ فِی مَكَانN<ࣲ> سَحِJ<ی>Q<ق>ࣲ A<۝>D<٣١>",
"ذَT< ٰ>لِكَۖ وَمَX<ن> N<یُ>عَظِّمۡ شَعَW<ـٰۤ>ئِR<رَ> ٱللَّهِ فَإِN<نَّ>هَا مِN<ن> تَQ<قۡ>وَى ٱلۡقُلُJ<و>Q<ب>ِ A<۝>D<٣٢>",
"لَكُمۡ فِیهَا مَنَT<ـٰ>فِعُ إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>N<ࣰ>ى ثُN<مَّ> مَحِلُّهَW<اۤ> إِلَى ٱلۡبَیۡتِ ٱلۡعَتِJ<ی>Q<ق>ِ A<۝>D<٣٣>",
"وَلِكُلِّ أُN<مَّ>ةN<ࣲ> جَعَلۡنَا مَN<ن>سَكX<ࣰ>ا لِّیَذۡكُR<رُ>وX<ا۟> ٱسۡمَ ٱللَّهِ عَلَىٰ مَا R<رَ>زَقَهُم N<مِّ>X<ن>N<ۢ> بَهِیمَةِ ٱلۡأَنۡعَT<ـٰ>مِۗ فَإِلَT<ـٰ>هُكُمۡ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدN<ࣱ> فَلَهُW<ۥۤ> أَسۡلِمُوX<ا۟>ۗ وَبَشِّرِ ٱلۡمُخۡبِتِJ<ی>نَ A<۝>D<٣٤>",
"ٱلَّذِینَ إِذَا ذُكِR<رَ> ٱللَّهُ وَجِلَتۡ قُلُوبُهُمۡ وَX<ٱ>X<ل>صَّT<ـٰ>بِرِینَ عَلَىٰ مَW<اۤ> أَصَابَهُمۡ وَX<ٱ>لۡمُقِیمِی ٱX<ل>صَّلَX<و>T<ٰ>ةِ وَمِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡ یُN<ن>فِقُJ<و>نَ A<۝>D<٣٥>",
"وَX<ٱ>لۡبُQ<دۡ>نَ جَعَلۡنَT<ـٰ>هَا لَكُم N<مِّ>N<ن> شَعَW<ـٰۤ>ئِرِ ٱللَّهِ لَكُمۡ فِیهَا خَیۡR<ر>N<ࣱ>ۖ فَX<ٱ>ذۡكُR<رُ>وX<ا۟> ٱسۡمَ ٱللَّهِ عَلَیۡهَا صَوَL<اۤ>فَّۖ فَإِذَا وَجَبَتۡ جُنُوبُهَا فَكُلُوX<ا۟> مِنۡهَا وَأَQ<طۡ>عِمُوX<ا۟> ٱلۡقَانِعَ وَX<ٱ>لۡمُعۡتَR<رَّ>ۚ كَذَT< ٰ>لِكَ سَخَّR<رۡ>نَT<ـٰ>هَا لَكُمۡ لَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<٣٦>",
"لَX<ن> N<یَ>نَالَ ٱللَّهَ لُحُومُهَا وَلَا دِمَW<اۤ>ؤُهَا وَلَT<ـٰ>كِX<ن> N<یَ>نَالُهُ ٱX<ل>تَّQ<قۡ>وَىٰ مِN<ن>كُمۡۚ كَذَT< ٰ>لِكَ سَخَّR<رَ>هَا لَكُمۡ لِتُكَبِّR<رُ>وX<ا۟> ٱللَّهَ عَلَىٰ مَا هَدَX<ى>T<ٰ>كُمۡۗ وَبَشِّرِ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٣٧>",
"۞ إِN<نَّ> ٱللَّهَ یُدَT< ٰ>فِعُ عَنِ ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟>ۗ إِN<نَّ> ٱللَّهَ لَا یُحِبُّ كُلَّ خَوَّانN<ࣲ> كَفُJ<و>R<ر>ٍ A<۝>D<٣٨>",
"أُذِنَ لِلَّذِینَ یُقَT<ـٰ>تَلُونَ بِأَN<نَّ>هُمۡ ظُلِمُوX<ا۟>ۚ وَإِN<نَّ> ٱللَّهَ عَلَىٰ نَصۡرِهِمۡ لَقَدِJ<ی>رٌ A<۝>D<٣٩>",
"ٱلَّذِینَ أُخۡرِجُوX<ا۟> مِN<ن> دِیَT<ـٰ>رِهِN<م> بِغَیۡرِ حَقٍّ إِلَّW<اۤ> أَX<ن> N<یَ>قُولُوX<ا۟> R<رَ>بُّنَا ٱللَّهُۗ وَلَوۡلَا دَفۡعُ ٱللَّهِ ٱX<ل>N<نَّ>اسَ بَعۡضَهُN<م> بِبَعۡضX<ࣲ> لَّهُدِّمَتۡ صَوَT< ٰ>مِعُ وَبِیَعX<ࣱ> N<وَ>صَلَوَT< ٰ>تX<ࣱ> N<وَ>مَسَT<ـٰ>جِدُ یُذۡكَR<رُ> فِیهَا ٱسۡمُ ٱللَّهِ كَثِیR<ر>X<ࣰ>اۗ N<وَ>لَیَN<ن>صُR<رَ>N<نَّ> ٱللَّهُ مَX<ن> N<یَ>N<ن>صُR<رُ>هُW<ۥۤ>ۚ إِN<نَّ> ٱللَّهَ لَقَوِیٌّ عَزِJ<ی>زٌ A<۝>D<٤٠>",
"ٱلَّذِینَ إِX<ن> N<مَّ>كَّN<نَّ>T<ـٰ>هُمۡ فِی ٱلۡأَR<رۡ>ضِ أَقَامُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتَوُX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَأَمَR<رُ>وX<ا۟> بِX<ٱ>لۡمَعۡR<رُ>وفِ وَنَهَوۡX<ا۟> عَنِ ٱلۡمُN<ن>كَرِۗ وَلِلَّهِ عَT<ـٰ>قِبَةُ ٱلۡأُمُJ<و>R<ر>ِ A<۝>D<٤١>",
"وَإِX<ن> N<یُ>كَذِّبُوكَ فَقَQ<دۡ> كَذَّبَتۡ قَQ<بۡ>لَهُمۡ قَوۡمُ نُوحX<ࣲ> N<وَ>عَادX<ࣱ> N<وَ>ثَمُJ<و>Q<د>ُ A<۝>D<٤٢>",
"وَقَوۡمُ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَقَوۡمُ لُJ<و>Q<ط>ࣲ A<۝>D<٤٣>",
"وَأَصۡحَT<ـٰ>بُ مَQ<دۡ>یَنَۖ وَكُذِّبَ مُوسَىٰۖ فَأَمۡلَیۡتُ لِلۡكَT<ـٰ>فِرِینَ ثُN<مَّ> أَخَذۡتُهُمۡۖ فَكَیۡفَ كَانَ نَكِJ<ی>رِ A<۝>D<٤٤>",
"فَكَأَیِّX<ن> N<مِّ>N<ن> قَR<رۡ>یَةٍ أَهۡلَكۡنَT<ـٰ>هَا وَهِیَ ظَالِمَةN<ࣱ> فَهِیَ خَاوِیَةٌ عَلَىٰ عُR<رُ>وشِهَا وَبِئۡرX<ࣲ> N<مُّ>عَطَّلَةX<ࣲ> N<وَ>قَصۡرX<ࣲ> N<مَّ>شِJ<ی>Q<د>ٍ A<۝>D<٤٥>",
"أَفَلَمۡ یَسِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَتَكُونَ لَهُمۡ قُلُوبX<ࣱ> N<یَ>عۡقِلُونَ بِهَW<اۤ> أَوۡ ءَاذَانX<ࣱ> N<یَ>سۡمَعُونَ بِهَاۖ فَإِN<نَّ>هَا لَا تَعۡمَى ٱلۡأَQ<بۡ>صَT<ـٰ>R<رُ> وَلَT<ـٰ>كِN<ن> تَعۡمَى ٱلۡقُلُوبُ ٱلَّتِی فِی ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٤٦>",
"وَیَسۡتَعۡجِلُونَكَ بِX<ٱ>لۡعَذَابِ وَلَX<ن> N<یُ>خۡلِفَ ٱللَّهُ وَعۡدَهُT<ۥ>ۚ وَإِN<نَّ> یَوۡمًا عِN<ن>دَ R<رَ>بِّكَ كَأَلۡفِ سَنَةX<ࣲ> N<مِّ>N<مَّ>ا تَعُدُّJ<و>نَ A<۝>D<٤٧>",
"وَكَأَیِّX<ن> N<مِّ>N<ن> قَR<رۡ>یَةٍ أَمۡلَیۡتُ لَهَا وَهِیَ ظَالِمَةN<ࣱ> ثُN<مَّ> أَخَذۡتُهَا وَإِلَیَّ ٱلۡمَصِJ<ی>رُ A<۝>D<٤٨>",
"قُلۡ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ إِN<نَّ>مَW<اۤ> أَنَا۠ لَكُمۡ نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٤٩>",
"فَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<وَ>رِزۡقN<ࣱ> كَرِJ<ی>مࣱ A<۝>D<٥٠>",
"وَX<ٱ>لَّذِینَ سَعَوۡX<ا۟> فِW<یۤ> ءَایَT<ـٰ>تِنَا مُعَT<ـٰ>جِزِینَ أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡجَحِJ<ی>مِ A<۝>D<٥١>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا مِN<ن> قَQ<بۡ>لِكَ مِX<ن> R<رَّ>سُولX<ࣲ> N<وَ>لَا نَبِیٍّ إِلَّW<اۤ> إِذَا تَمَN<نَّ>ىW<ٰۤ> أَلۡقَى ٱX<ل>شَّیۡطَT<ـٰ>نُ فِW<یۤ> أُمۡنِیَّتِهِT<ۦ> فَیَN<ن>سَخُ ٱللَّهُ مَا یُلۡقِی ٱX<ل>شَّیۡطَT<ـٰ>نُ ثُN<مَّ> یُحۡكِمُ ٱللَّهُ ءَایَT<ـٰ>تِهِT<ۦ>ۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<٥٢>",
"لِّیَQ<جۡ>عَلَ مَا یُلۡقِی ٱX<ل>شَّیۡطَT<ـٰ>نُ فِتۡنَةX<ࣰ> لِّلَّذِینَ فِی قُلُوبِهِم N<مَّ>R<رَ>ضX<ࣱ> N<وَ>X<ٱ>لۡقَاسِیَةِ قُلُوبُهُمۡۗ وَإِN<نَّ> ٱX<ل>ظَّT<ـٰ>لِمِینَ لَفِی شِقَاقِN<ۭ> بَعِJ<ی>Q<د>ࣲ A<۝>D<٥٣>",
"وَلِیَعۡلَمَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡعِلۡمَ أَN<نَّ>هُ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّكَ فَیُؤۡمِنُوX<ا۟> بِهِT<ۦ> فَتُخۡبِتَ لَهُT<ۥ> قُلُوبُهُمۡۗ وَإِN<نَّ> ٱللَّهَ لَهَادِ ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٥٤>",
"وَلَا یَزَالُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فِی مِرۡیَةX<ࣲ> N<مِّ>نۡهُ حَتَّىٰ تَأۡتِیَهُمُ ٱX<ل>سَّاعَةُ بَغۡتَةً أَوۡ یَأۡتِیَهُمۡ عَذَابُ یَوۡمٍ عَقِJ<ی>مٍ A<۝>D<٥٥>",
"ٱلۡمُلۡكُ یَوۡمَئِذX<ࣲ> لِّلَّهِ یَحۡكُمُ بَیۡنَهُمۡۚ فَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ فِی جَN<نَّ>T<ـٰ>تِ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<٥٦>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> وَكَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا فَأُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ عَذَابX<ࣱ> N<مُّ>هِJ<ی>نࣱ A<۝>D<٥٧>",
"وَX<ٱ>لَّذِینَ هَاجَR<رُ>وX<ا۟> فِی سَبِیلِ ٱللَّهِ ثُN<مَّ> قُتِلُW<وۤ>X<ا۟> أَوۡ مَاتُوX<ا۟> لَیَR<رۡ>زُقَN<نَّ>هُمُ ٱللَّهُ رِزۡقًا حَسَنX<ࣰ>اۚ N<وَ>إِN<نَّ> ٱللَّهَ لَهُوَ خَیۡR<رُ> ٱX<ل>R<رَّ>T< ٰ>زِقِJ<ی>نَ A<۝>D<٥٨>",
"لَیُQ<دۡ>خِلَN<نَّ>هُم N<مُّ>Q<دۡ>خَلX<ࣰ>ا N<یَ>R<رۡ>ضَوۡنَهُT<ۥ>ۚ وَإِN<نَّ> ٱللَّهَ لَعَلِیمٌ حَلِJ<ی>مࣱ A<۝>D<٥٩>",
"۞ ذَT< ٰ>لِكَۖ وَمَنۡ عَاقَبَ بِمِثۡلِ مَا عُوقِبَ بِهِT<ۦ> ثُN<مَّ> بُغِیَ عَلَیۡهِ لَیَN<ن>صُR<رَ>N<نَّ>هُ ٱللَّهُۚ إِN<نَّ> ٱللَّهَ لَعَفُوٌّ غَفُJ<و>R<ر>ࣱ A<۝>D<٦٠>",
"ذَT< ٰ>لِكَ بِأَN<نَّ> ٱللَّهَ یُولِجُ ٱلَّیۡلَ فِی ٱX<ل>N<نَّ>هَارِ وَیُولِجُ ٱX<ل>N<نَّ>هَاR<رَ> فِی ٱلَّیۡلِ وَأَN<نَّ> ٱللَّهَ سَمِیعُN<ۢ> بَصِJ<ی>رࣱ A<۝>D<٦١>",
"ذَT< ٰ>لِكَ بِأَN<نَّ> ٱللَّهَ هُوَ ٱلۡحَقُّ وَأَN<نَّ> مَا یَQ<دۡ>عُونَ مِN<ن> دُونِهِT<ۦ> هُوَ ٱلۡبَT<ـٰ>طِلُ وَأَN<نَّ> ٱللَّهَ هُوَ ٱلۡعَلِیُّ ٱلۡكَبِJ<ی>رُ A<۝>D<٦٢>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ أَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَتُصۡبِحُ ٱلۡأَR<رۡ>ضُ مُخۡضَR<رَّ>ةًۚ إِN<نَّ> ٱللَّهَ لَطِیفٌ خَبِJ<ی>رࣱ A<۝>D<٦٣>",
"لَّهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۚ وَإِN<نَّ> ٱللَّهَ لَهُوَ ٱلۡغَنِیُّ ٱلۡحَمِJ<ی>Q<د>ُ A<۝>D<٦٤>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ سَخَّR<رَ> لَكُم N<مَّ>ا فِی ٱلۡأَR<رۡ>ضِ وَX<ٱ>لۡفُلۡكَ تَQ<جۡ>رِی فِی ٱلۡبَحۡرِ بِأَمۡرِهِT<ۦ> وَیُمۡسِكُ ٱX<ل>سَّمَW<اۤ>ءَ أَN<ن> تَقَعَ عَلَى ٱلۡأَR<رۡ>ضِ إِلَّا بِإِذۡنِهِW<ۦۤ>ۚ إِN<نَّ> ٱللَّهَ بِX<ٱ>X<ل>N<نَّ>اسِ لَR<رَ>ءُوفX<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٦٥>",
"وَهُوَ ٱلَّذِW<یۤ> أَحۡیَاكُمۡ ثُN<مَّ> یُمِیتُكُمۡ ثُN<مَّ> یُحۡیِیكُمۡۗ إِN<نَّ> ٱلۡإِN<ن>سَT<ـٰ>نَ لَكَفُJ<و>R<ر>ࣱ A<۝>D<٦٦>",
"لِّكُلِّ أُN<مَّ>ةN<ࣲ> جَعَلۡنَا مَN<ن>سَكًا هُمۡ نَاسِكُوهُۖ فَلَا یُنَT<ـٰ>زِعُN<نَّ>كَ فِی ٱلۡأَمۡرِۚ وَX<ٱ>Q<دۡ>عُ إِلَىٰ R<رَ>بِّكَۖ إِN<نَّ>كَ لَعَلَىٰ هُدX<ࣰ>ى N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٦٧>",
"وَإِN<ن> جَT<ـٰ>دَلُوكَ فَقُلِ ٱللَّهُ أَعۡلَمُ بِمَا تَعۡمَلُJ<و>نَ A<۝>D<٦٨>",
"ٱللَّهُ یَحۡكُمُ بَیۡنَكُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ فِیمَا كُN<ن>تُمۡ فِیهِ تَخۡتَلِفُJ<و>نَ A<۝>D<٦٩>",
"أَلَمۡ تَعۡلَمۡ أَN<نَّ> ٱللَّهَ یَعۡلَمُ مَا فِی ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِۚ إِN<نَّ> ذَT< ٰ>لِكَ فِی كِتَT<ـٰ>بٍۚ إِN<نَّ> ذَT< ٰ>لِكَ عَلَى ٱللَّهِ یَسِJ<ی>رࣱ A<۝>D<٧٠>",
"وَیَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ مَا لَمۡ یُنَزِّلۡ بِهِT<ۦ> سُلۡطَT<ـٰ>نX<ࣰ>ا N<وَ>مَا لَیۡسَ لَهُN<م> بِهِT<ۦ> عِلۡمX<ࣱ>ۗ N<وَ>مَا لِX<ل>ظَّT<ـٰ>لِمِینَ مِن N<نَّ>صِJ<ی>رࣲ A<۝>D<٧١>",
"وَإِذَا تُتۡلَىٰ عَلَیۡهِمۡ ءَایَT<ـٰ>تُنَا بَیِّنَT<ـٰ>تN<ࣲ> تَعۡرِفُ فِی وُجُوهِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> ٱلۡمُN<ن>كَR<رَ>ۖ یَكَادُونَ یَسۡطُونَ بِX<ٱ>لَّذِینَ یَتۡلُونَ عَلَیۡهِمۡ ءَایَT<ـٰ>تِنَاۗ قُلۡ أَفَأُنَبِّئُكُN<م> بِشَرّX<ࣲ> N<مِّ>N<ن> ذَT< ٰ>لِكُمُۚ ٱX<ل>N<نَّ>اR<رُ> وَعَدَهَا ٱللَّهُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟>ۖ وَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<٧٢>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ ضُرِبَ مَثَلN<ࣱ> فَX<ٱ>سۡتَمِعُوX<ا۟> لَهُW<ۥۤ>ۚ إِN<نَّ> ٱلَّذِینَ تَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ لَX<ن> N<یَ>خۡلُقُوX<ا۟> ذُبَابX<ࣰ>ا N<وَ>لَوِ ٱQ<جۡ>تَمَعُوX<ا۟> لَهُT<ۥ>ۖ وَإِX<ن> N<یَ>سۡلُQ<بۡ>هُمُ ٱX<ل>ذُّبَابُ شَیۡءX<ࣰ>ا لَّا یَسۡتَN<ن>قِذُوهُ مِنۡهُۚ ضَعُفَ ٱX<ل>طَّالِبُ وَX<ٱ>لۡمَQ<طۡ>لُJ<و>Q<ب>ُ A<۝>D<٧٣>",
"مَا قَدَR<رُ>وX<ا۟> ٱللَّهَ حَقَّ قَQ<دۡ>رِهِW<ۦۤ>ۚ إِN<نَّ> ٱللَّهَ لَقَوِیٌّ عَزِJ<ی>زٌ A<۝>D<٧٤>",
"ٱللَّهُ یَصۡطَفِی مِنَ ٱلۡمَلَW<ـٰۤ>ئِكَةِ R<رُ>سُلX<ࣰ>ا N<وَ>مِنَ ٱX<ل>N<نَّ>اسِۚ إِN<نَّ> ٱللَّهَ سَمِیعُN<ۢ> بَصِJ<ی>رࣱ A<۝>D<٧٥>",
"یَعۡلَمُ مَا بَیۡنَ أَیۡدِیهِمۡ وَمَا خَلۡفَهُمۡۚ وَإِلَى ٱللَّهِ تُR<رۡ>جَعُ ٱلۡأُمُJ<و>R<ر>ُ A<۝>D<٧٦>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱR<رۡ>كَعُوX<ا۟> وَ̅X<ٱ>̅سۡ̅جُ̅دُ̅و̅X<ا۟>̅ وَX<ٱ>عۡبُدُوX<ا۟> R<رَ>بَّكُمۡ وَX<ٱ>فۡعَلُوX<ا۟> ٱلۡخَیۡR<رَ> لَعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<٧٧> ۩",
"وَجَT<ـٰ>هِدُوX<ا۟> فِی ٱللَّهِ حَقَّ جِهَادِهِT<ۦ>ۚ هُوَ ٱQ<جۡ>تَبَX<ى>T<ٰ>كُمۡ وَمَا جَعَلَ عَلَیۡكُمۡ فِی ٱX<ل>دِّینِ مِنۡ حَR<رَ>جX<ࣲ>ۚ N<مِّ>لَّةَ أَبِیكُمۡ إِQ<بۡ>R<رَ>T< ٰ>هِیمَۚ هُوَ سَN<مَّ>X<ى>T<ٰ>كُمُ ٱلۡمُسۡلِمِینَ مِN<ن> قَQ<بۡ>لُ وَفِی هَT<ـٰ>ذَا لِیَكُونَ ٱX<ل>R<رَّ>سُولُ شَهِیدًا عَلَیۡكُمۡ وَتَكُونُوX<ا۟> شُهَدَW<اۤ>ءَ عَلَى ٱX<ل>N<نَّ>اسِۚ فَأَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتُوX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَX<ٱ>عۡتَصِمُوX<ا۟> بِٱللَّهِ هُوَ مَوۡلَX<ى>T<ٰ>كُمۡۖ فَنِعۡمَ ٱلۡمَوۡلَىٰ وَنِعۡمَ ٱX<ل>N<نَّ>صِJ<ی>رُ A<۝>D<٧٨>",
"#۞ قَQ<دۡ> أَفۡلَحَ ٱلۡمُؤۡمِنُJ<و>نَ A<۝>D<١>",
"ٱلَّذِینَ هُمۡ فِی صَلَاتِهِمۡ خَT<ـٰ>شِعُJ<و>نَ A<۝>D<٢>",
"وَX<ٱ>لَّذِینَ هُمۡ عَنِ ٱX<ل>لَّغۡوِ مُعۡرِضُJ<و>نَ A<۝>D<٣>",
"وَX<ٱ>لَّذِینَ هُمۡ لِX<ل>زَّكَX<و>T<ٰ>ةِ فَT<ـٰ>عِلُJ<و>نَ A<۝>D<٤>",
"وَX<ٱ>لَّذِینَ هُمۡ لِفُR<رُ>وجِهِمۡ حَT<ـٰ>فِظُJ<و>نَ A<۝>D<٥>",
"إِلَّا عَلَىW<ٰۤ> أَزۡوَT< ٰ>جِهِمۡ أَوۡ مَا مَلَكَتۡ أَیۡمَT<ـٰ>نُهُمۡ فَإِN<نَّ>هُمۡ غَیۡR<رُ> مَلُومِJ<ی>نَ A<۝>D<٦>",
"فَمَنِ ٱQ<بۡ>تَغَىٰ وَR<رَ>W<اۤ>ءَ ذَT< ٰ>لِكَ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡعَادُJ<و>نَ A<۝>D<٧>",
"وَX<ٱ>لَّذِینَ هُمۡ لِأَمَT<ـٰ>نَT<ـٰ>تِهِمۡ وَعَهۡدِهِمۡ R<رَ>T< ٰ>عُJ<و>نَ A<۝>D<٨>",
"وَX<ٱ>لَّذِینَ هُمۡ عَلَىٰ صَلَوَT< ٰ>تِهِمۡ یُحَافِظُJ<و>نَ A<۝>D<٩>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡوَT< ٰ>رِثُJ<و>نَ A<۝>D<١٠>",
"ٱلَّذِینَ یَرِثُونَ ٱلۡفِرۡدَوۡسَ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<١١>",
"وَلَقَQ<دۡ> خَلَQ<قۡ>نَا ٱلۡإِN<ن>سَT<ـٰ>نَ مِN<ن> سُلَT<ـٰ>لَةX<ࣲ> N<مِّ>N<ن> طِJ<ی>نࣲ A<۝>D<١٢>",
"ثُN<مَّ> جَعَلۡنَT<ـٰ>هُ نُQ<طۡ>فَةN<ࣰ> فِی قَR<رَ>ارX<ࣲ> N<مَّ>كِJ<ی>نࣲ A<۝>D<١٣>",
"ثُN<مَّ> خَلَQ<قۡ>نَا ٱX<ل>N<نُّ>Q<طۡ>فَةَ عَلَقَةN<ࣰ> فَخَلَQ<قۡ>نَا ٱلۡعَلَقَةَ مُضۡغَةN<ࣰ> فَخَلَQ<قۡ>نَا ٱلۡمُضۡغَةَ عِظَT<ـٰ>مN<ࣰ>ا فَكَسَوۡنَا ٱلۡعِظَT<ـٰ>مَ لَحۡمN<ࣰ>ا ثُN<مَّ> أَN<ن>شَأۡنَT<ـٰ>هُ خَلۡقًا ءَاخَR<رَ>ۚ فَتَبَاR<رَ>كَ ٱللَّهُ أَحۡسَنُ ٱلۡخَT<ـٰ>لِقِJ<ی>نَ A<۝>D<١٤>",
"ثُN<مَّ> إِN<نَّ>كُN<م> بَعۡدَ ذَT< ٰ>لِكَ لَمَیِّتُJ<و>نَ A<۝>D<١٥>",
"ثُN<مَّ> إِN<نَّ>كُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ تُQ<بۡ>عَثُJ<و>نَ A<۝>D<١٦>",
"وَلَقَQ<دۡ> خَلَQ<قۡ>نَا فَوۡقَكُمۡ سَQ<بۡ>عَ طَR<رَ>W<اۤ>ئِقَ وَمَا كُN<نَّ>ا عَنِ ٱلۡخَلۡقِ غَT<ـٰ>فِلِJ<ی>نَ A<۝>D<١٧>",
"وَأَN<ن>زَلۡنَا مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءَN<ۢ> بِقَدَرN<ࣲ> فَأَسۡكَN<نَّ>T<ـٰ>هُ فِی ٱلۡأَR<رۡ>ضِۖ وَإِN<نَّ>ا عَلَىٰ ذَهَابِN<ۭ> بِهِT<ۦ> لَقَT<ـٰ>دِR<رُ>J<و>نَ A<۝>D<١٨>",
"فَأَN<ن>شَأۡنَا لَكُN<م> بِهِT<ۦ> جَN<نَّ>T<ـٰ>تX<ࣲ> N<مِّ>ن N<نَّ>خِیلX<ࣲ> N<وَ>أَعۡنَT<ـٰ>بX<ࣲ> لَّكُمۡ فِیهَا فَوَT< ٰ>كِهُ كَثِیR<رَ>ةX<ࣱ> N<وَ>مِنۡهَا تَأۡكُلُJ<و>نَ A<۝>D<١٩>",
"وَشَجَR<رَ>ةN<ࣰ> تَخۡR<رُ>جُ مِN<ن> طُورِ سَیۡنَW<اۤ>ءَ تَX<ن>N<ۢ>بُتُ بِX<ٱ>X<ل>دُّهۡنِ وَصِQ<بۡ>غX<ࣲ> لِّلۡءَاكِلِJ<ی>نَ A<۝>D<٢٠>",
"وَإِN<نَّ> لَكُمۡ فِی ٱلۡأَنۡعَT<ـٰ>مِ لَعِQ<بۡ>R<رَ>ةX<ࣰ>ۖ N<نُّ>سۡقِیكُم N<مِّ>N<مَّ>ا فِی بُطُونِهَا وَلَكُمۡ فِیهَا مَنَT<ـٰ>فِعُ كَثِیR<رَ>ةX<ࣱ> N<وَ>مِنۡهَا تَأۡكُلُJ<و>نَ A<۝>D<٢١>",
"وَعَلَیۡهَا وَعَلَى ٱلۡفُلۡكِ تُحۡمَلُJ<و>نَ A<۝>D<٢٢>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا نُوحًا إِلَىٰ قَوۡمِهِT<ۦ> فَقَالَ یَT<ـٰ>قَوۡمِ ٱعۡبُدُوX<ا۟> ٱللَّهَ مَا لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡR<رُ>هُW<ۥۤ>ۚ أَفَلَا تَتَّقُJ<و>نَ A<۝>D<٢٣>",
"فَقَالَ ٱلۡمَلَؤُX<ا۟> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِN<ن> قَوۡمِهِT<ۦ> مَا هَT<ـٰ>ذَW<اۤ> إِلَّا بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُكُمۡ یُرِیدُ أَX<ن> N<یَ>تَفَضَّلَ عَلَیۡكُمۡ وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ لَأَN<ن>زَلَ مَلَW<ـٰۤ>ئِكَةX<ࣰ> N<مَّ>ا سَمِعۡنَا بِهَT<ـٰ>ذَا فِW<یۤ> ءَابَW<اۤ>ئِنَا ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٢٤>",
"إِنۡ هُوَ إِلَّا R<رَ>جُلُN<ۢ> بِهِT<ۦ> جِN<نَّ>ةN<ࣱ> فَتَR<رَ>بَّصُوX<ا۟> بِهِT<ۦ> حَتَّىٰ حِJ<ی>نࣲ A<۝>D<٢٥>",
"قَالَ R<رَ>بِّ ٱN<ن>صُR<رۡ>نِی بِمَا كَذَّبُJ<و>نِ A<۝>D<٢٦>",
"فَأَوۡحَیۡنَW<اۤ> إِلَیۡهِ أَنِ ٱصۡنَعِ ٱلۡفُلۡكَ بِأَعۡیُنِنَا وَوَحۡیِنَا فَإِذَا جَW<اۤ>ءَ أَمۡR<رُ>نَا وَفَاR<رَ> ٱX<ل>تَّN<نُّ>وR<رُ> فَX<ٱ>سۡلُكۡ فِیهَا مِN<ن> كُلّN<ࣲ> زَوۡجَیۡنِ ٱثۡنَیۡنِ وَأَهۡلَكَ إِلَّا مَN<ن> سَبَقَ عَلَیۡهِ ٱلۡقَوۡلُ مِنۡهُمۡۖ وَلَا تُخَT<ـٰ>طِQ<بۡ>نِی فِی ٱلَّذِینَ ظَلَمُW<وۤ>X<ا۟> إِN<نَّ>هُم N<مُّ>غۡR<رَ>قُJ<و>نَ A<۝>D<٢٧>",
"فَإِذَا ٱسۡتَوَیۡتَ أَN<ن>تَ وَمَX<ن> N<مَّ>عَكَ عَلَى ٱلۡفُلۡكِ فَقُلِ ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِی نَجَّX<ى>T<ٰ>نَا مِنَ ٱلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢٨>",
"وَقُل R<رَّ>بِّ أَN<ن>زِلۡنِی مُN<ن>زَلX<ࣰ>ا N<مُّ>بَاR<رَ>كX<ࣰ>ا N<وَ>أَN<ن>تَ خَیۡR<رُ> ٱلۡمُN<ن>زِلِJ<ی>نَ A<۝>D<٢٩>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> N<وَ>إِN<ن> كُN<نَّ>ا لَمُQ<بۡ>تَلِJ<ی>نَ A<۝>D<٣٠>",
"ثُN<مَّ> أَN<ن>شَأۡنَا مِX<ن>N<ۢ> بَعۡدِهِمۡ قَR<رۡ>نًا ءَاخَرِJ<ی>نَ A<۝>D<٣١>",
"فَأَR<رۡ>سَلۡنَا فِیهِمۡ R<رَ>سُولX<ࣰ>ا N<مِّ>نۡهُمۡ أَنِ ٱعۡبُدُوX<ا۟> ٱللَّهَ مَا لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡR<رُ>هُW<ۥۤ>ۚ أَفَلَا تَتَّقُJ<و>نَ A<۝>D<٣٢>",
"وَقَالَ ٱلۡمَلَأُ مِN<ن> قَوۡمِهِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَكَذَّبُوX<ا۟> بِلِقَW<اۤ>ءِ ٱلۡءَاخِR<رَ>ةِ وَأَتۡR<رَ>فۡنَT<ـٰ>هُمۡ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا مَا هَT<ـٰ>ذَW<اۤ> إِلَّا بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُكُمۡ یَأۡكُلُ مِN<مَّ>ا تَأۡكُلُونَ مِنۡهُ وَیَشۡR<رَ>بُ مِN<مَّ>ا تَشۡR<رَ>بُJ<و>نَ A<۝>D<٣٣>",
"وَلَئِنۡ أَطَعۡتُN<م> بَشَR<ر>X<ࣰ>ا N<مِّ>ثۡلَكُمۡ إِN<نَّ>كُمۡ إِذX<ࣰ>ا لَّخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<٣٤>",
"أَیَعِدُكُمۡ أَN<نَّ>كُمۡ إِذَا مِتُّمۡ وَكُN<ن>تُمۡ تُR<رَ>ابX<ࣰ>ا N<وَ>عِظَT<ـٰ>مًا أَN<نَّ>كُم N<مُّ>خۡR<رَ>جُJ<و>نَ A<۝>D<٣٥>",
"۞ هَیۡهَاتَ هَیۡهَاتَ لِمَا تُوعَدُJ<و>نَ A<۝>D<٣٦>",
"إِنۡ هِیَ إِلَّا حَیَاتُنَا ٱX<ل>دُّنۡیَا نَمُوتُ وَنَحۡیَا وَمَا نَحۡنُ بِمَQ<بۡ>عُوثِJ<ی>نَ A<۝>D<٣٧>",
"إِنۡ هُوَ إِلَّا R<رَ>جُلٌ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبX<ࣰ>ا N<وَ>مَا نَحۡنُ لَهُT<ۥ> بِمُؤۡمِنِJ<ی>نَ A<۝>D<٣٨>",
"قَالَ R<رَ>بِّ ٱN<ن>صُR<رۡ>نِی بِمَا كَذَّبُJ<و>نِ A<۝>D<٣٩>",
"قَالَ عَN<مَّ>ا قَلِیلX<ࣲ> لَّیُصۡبِحُN<نَّ> نَT<ـٰ>دِمِJ<ی>نَ A<۝>D<٤٠>",
"فَأَخَذَتۡهُمُ ٱX<ل>صَّیۡحَةُ بِX<ٱ>لۡحَقِّ فَجَعَلۡنَT<ـٰ>هُمۡ غُثَW<اۤ>ءN<ࣰ>ۚ فَبُعۡدX<ࣰ>ا لِّلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤١>",
"ثُN<مَّ> أَN<ن>شَأۡنَا مِX<ن>N<ۢ> بَعۡدِهِمۡ قُR<رُ>ونًا ءَاخَرِJ<ی>نَ A<۝>D<٤٢>",
"مَا تَسۡبِقُ مِنۡ أُN<مَّ>ةٍ أَجَلَهَا وَمَا یَسۡتَءۡخِR<رُ>J<و>نَ A<۝>D<٤٣>",
"ثُN<مَّ> أَR<رۡ>سَلۡنَا R<رُ>سُلَنَا تَتۡR<رَ>اۖ كُلَّ مَا جَW<اۤ>ءَ أُN<مَّ>ةX<ࣰ> R<رَّ>سُولُهَا كَذَّبُوهُۖ فَأَتۡبَعۡنَا بَعۡضَهُN<م> بَعۡضX<ࣰ>ا N<وَ>جَعَلۡنَT<ـٰ>هُمۡ أَحَادِیثَۚ فَبُعۡدX<ࣰ>ا لِّقَوۡمX<ࣲ> لَّا یُؤۡمِنُJ<و>نَ A<۝>D<٤٤>",
"ثُN<مَّ> أَR<رۡ>سَلۡنَا مُوسَىٰ وَأَخَاهُ هَT<ـٰ>R<رُ>ونَ بِءَایَT<ـٰ>تِنَا وَسُلۡطَT<ـٰ>نX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٤٥>",
"إِلَىٰ فِرۡعَوۡنَ وَمَلَإِX<ی۟>هِT<ۦ> فَX<ٱ>سۡتَكۡبَR<رُ>وX<ا۟> وَكَانُوX<ا۟> قَوۡمًا عَالِJ<ی>نَ A<۝>D<٤٦>",
"فَقَالُW<وۤ>X<ا۟> أَنُؤۡمِنُ لِبَشَR<رَ>یۡنِ مِثۡلِنَا وَقَوۡمُهُمَا لَنَا عَT<ـٰ>بِدُJ<و>نَ A<۝>D<٤٧>",
"فَكَذَّبُوهُمَا فَكَانُوX<ا۟> مِنَ ٱلۡمُهۡلَكِJ<ی>نَ A<۝>D<٤٨>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ لَعَلَّهُمۡ یَهۡتَدُJ<و>نَ A<۝>D<٤٩>",
"وَجَعَلۡنَا ٱQ<بۡ>نَ مَR<رۡ>یَمَ وَأُN<مَّ>هُW<ۥۤ> ءَایَةX<ࣰ> N<وَ>ءَاوَیۡنَT<ـٰ>هُمَW<اۤ> إِلَىٰ R<رَ>Q<بۡ>وَةN<ࣲ> ذَاتِ قَR<رَ>ارX<ࣲ> N<وَ>مَعِJ<ی>نࣲ A<۝>D<٥٠>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>R<رُّ>سُلُ كُلُوX<ا۟> مِنَ ٱX<ل>طَّیِّبَT<ـٰ>تِ وَX<ٱ>عۡمَلُوX<ا۟> صَT<ـٰ>لِحًاۖ إِN<نِّ>ی بِمَا تَعۡمَلُونَ عَلِJ<ی>مࣱ A<۝>D<٥١>",
"وَإِN<نَّ> هَT<ـٰ>ذِهِW<ۦۤ> أُN<مَّ>تُكُمۡ أُN<مَّ>ةX<ࣰ> N<وَ>T< ٰ>حِدَةX<ࣰ> N<وَ>أَنَا۠ R<رَ>بُّكُمۡ فَX<ٱ>تَّقُJ<و>نِ A<۝>D<٥٢>",
"فَتَقَطَّعُW<وۤ>X<ا۟> أَمۡR<رَ>هُN<م> بَیۡنَهُمۡ زُبُR<ر>N<ࣰ>اۖ كُلُّ حِزۡبِN<ۭ> بِمَا لَدَیۡهِمۡ فَرِحُJ<و>نَ A<۝>D<٥٣>",
"فَذَR<رۡ>هُمۡ فِی غَمۡR<رَ>تِهِمۡ حَتَّىٰ حِJ<ی>نٍ A<۝>D<٥٤>",
"أَیَحۡسَبُونَ أَN<نَّ>مَا نُمِدُّهُN<م> بِهِT<ۦ> مِX<ن> N<مَّ>الX<ࣲ> N<وَ>بَنِJ<ی>نَ A<۝>D<٥٥>",
"نُسَارِعُ لَهُمۡ فِی ٱلۡخَیۡR<رَ>T< ٰ>تِۚ بَل لَّا یَشۡعُR<رُ>J<و>نَ A<۝>D<٥٦>",
"إِN<نَّ> ٱلَّذِینَ هُم N<مِّ>نۡ خَشۡیَةِ R<رَ>بِّهِم N<مُّ>شۡفِقُJ<و>نَ A<۝>D<٥٧>",
"وَX<ٱ>لَّذِینَ هُN<م> بِءَایَT<ـٰ>تِ R<رَ>بِّهِمۡ یُؤۡمِنُJ<و>نَ A<۝>D<٥٨>",
"وَX<ٱ>لَّذِینَ هُN<م> بِR<رَ>بِّهِمۡ لَا یُشۡرِكُJ<و>نَ A<۝>D<٥٩>",
"وَX<ٱ>لَّذِینَ یُؤۡتُونَ مَW<اۤ> ءَاتَوX<ا۟> وَّقُلُوبُهُمۡ وَجِلَةٌ أَN<نَّ>هُمۡ إِلَىٰ R<رَ>بِّهِمۡ R<رَ>T< ٰ>جِعُJ<و>نَ A<۝>D<٦٠>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ یُسَT<ـٰ>رِعُونَ فِی ٱلۡخَیۡR<رَ>T< ٰ>تِ وَهُمۡ لَهَا سَT<ـٰ>بِقُJ<و>نَ A<۝>D<٦١>",
"وَلَا نُكَلِّفُ نَفۡسًا إِلَّا وُسۡعَهَاۚ وَلَدَیۡنَا كِتَT<ـٰ>بX<ࣱ> N<یَ>N<ن>طِقُ بِX<ٱ>لۡحَقِّ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<٦٢>",
"بَلۡ قُلُوبُهُمۡ فِی غَمۡR<رَ>ةX<ࣲ> N<مِّ>نۡ هَT<ـٰ>ذَا وَلَهُمۡ أَعۡمَT<ـٰ>لX<ࣱ> N<مِّ>N<ن> دُونِ ذَT< ٰ>لِكَ هُمۡ لَهَا عَT<ـٰ>مِلُJ<و>نَ A<۝>D<٦٣>",
"حَتَّىW<ٰۤ> إِذَW<اۤ> أَخَذۡنَا مُتۡR<رَ>فِیهِN<م> بِX<ٱ>لۡعَذَابِ إِذَا هُمۡ یَQ<جۡ>ءَR<رُ>J<و>نَ A<۝>D<٦٤>",
"لَا تَQ<جۡ>ءَR<رُ>وX<ا۟> ٱلۡیَوۡمَۖ إِN<نَّ>كُم N<مِّ>N<نَّ>ا لَا تُN<ن>صَR<رُ>J<و>نَ A<۝>D<٦٥>",
"قَQ<دۡ> كَانَتۡ ءَایَT<ـٰ>تِی تُتۡلَىٰ عَلَیۡكُمۡ فَكُN<ن>تُمۡ عَلَىW<ٰۤ> أَعۡقَT<ـٰ>بِكُمۡ تَN<ن>كِصُJ<و>نَ A<۝>D<٦٦>",
"مُسۡتَكۡبِرِینَ بِهِT<ۦ> سَT<ـٰ>مِR<ر>N<ࣰ>ا تَهۡجُR<رُ>J<و>نَ A<۝>D<٦٧>",
"أَفَلَمۡ یَدَّبَّR<رُ>وX<ا۟> ٱلۡقَوۡلَ أَمۡ جَW<اۤ>ءَهُم N<مَّ>ا لَمۡ یَأۡتِ ءَابَW<اۤ>ءَهُمُ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٦٨>",
"أَمۡ لَمۡ یَعۡرِفُوX<ا۟> R<رَ>سُولَهُمۡ فَهُمۡ لَهُT<ۥ> مُN<ن>كِR<رُ>J<و>نَ A<۝>D<٦٩>",
"أَمۡ یَقُولُونَ بِهِT<ۦ> جِN<نَّ>ةُN<ۢ>ۚ بَلۡ جَW<اۤ>ءَهُN<م> بِX<ٱ>لۡحَقِّ وَأَكۡثَR<رُ>هُمۡ لِلۡحَقِّ كَT<ـٰ>رِهُJ<و>نَ A<۝>D<٧٠>",
"وَلَوِ ٱتَّبَعَ ٱلۡحَقُّ أَهۡوَW<اۤ>ءَهُمۡ لَفَسَدَتِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تُ وَX<ٱ>لۡأَR<رۡ>ضُ وَمَN<ن> فِیهِN<نَّ>ۚ بَلۡ أَتَیۡنَT<ـٰ>هُN<م> بِذِكۡرِهِمۡ فَهُمۡ عَN<ن> ذِكۡرِهِم N<مُّ>عۡرِضُJ<و>نَ A<۝>D<٧١>",
"أَمۡ تَسۡءَلُهُمۡ خَR<رۡ>جN<ࣰ>ا فَخَR<رَ>اجُ R<رَ>بِّكَ خَیۡR<ر>X<ࣱ>ۖ N<وَ>هُوَ خَیۡR<رُ> ٱX<ل>R<رَّ>T< ٰ>زِقِJ<ی>نَ A<۝>D<٧٢>",
"وَإِN<نَّ>كَ لَتَQ<دۡ>عُوهُمۡ إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٧٣>",
"وَإِN<نَّ> ٱلَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ عَنِ ٱX<ل>صِّR<رَ>T< ٰ>طِ لَنَT<ـٰ>كِبُJ<و>نَ A<۝>D<٧٤>",
"۞ وَلَوۡ R<رَ>حِمۡنَT<ـٰ>هُمۡ وَكَشَفۡنَا مَا بِهِم N<مِّ>N<ن> ضُرّX<ࣲ> لَّلَجُّوX<ا۟> فِی طُغۡیَT<ـٰ>نِهِمۡ یَعۡمَهُJ<و>نَ A<۝>D<٧٥>",
"وَلَقَQ<دۡ> أَخَذۡنَT<ـٰ>هُN<م> بِX<ٱ>لۡعَذَابِ فَمَا ٱسۡتَكَانُوX<ا۟> لِR<رَ>بِّهِمۡ وَمَا یَتَضَR<رَّ>عُJ<و>نَ A<۝>D<٧٦>",
"حَتَّىW<ٰۤ> إِذَا فَتَحۡنَا عَلَیۡهِN<م> بَابN<ࣰ>ا ذَا عَذَابN<ࣲ> شَدِیدٍ إِذَا هُمۡ فِیهِ مُQ<بۡ>لِسُJ<و>نَ A<۝>D<٧٧>",
"وَهُوَ ٱلَّذِW<یۤ> أَN<ن>شَأَ لَكُمُ ٱX<ل>سَّمۡعَ وَX<ٱ>لۡأَQ<بۡ>صَT<ـٰ>R<رَ> وَX<ٱ>لۡأَفۡءِدَةَۚ قَلِیلX<ࣰ>ا N<مَّ>ا تَشۡكُR<رُ>J<و>نَ A<۝>D<٧٨>",
"وَهُوَ ٱلَّذِی ذَR<رَ>أَكُمۡ فِی ٱلۡأَR<رۡ>ضِ وَإِلَیۡهِ تُحۡشَR<رُ>J<و>نَ A<۝>D<٧٩>",
"وَهُوَ ٱلَّذِی یُحۡیِT<ۦ> وَیُمِیتُ وَلَهُ ٱخۡتِلَT<ـٰ>فُ ٱلَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<٨٠>",
"بَلۡ قَالُوX<ا۟> مِثۡلَ مَا قَالَ ٱلۡأَوَّلُJ<و>نَ A<۝>D<٨١>",
"قَالُW<وۤ>X<ا۟> أَءِذَا مِتۡنَا وَكُN<نَّ>ا تُR<رَ>ابX<ࣰ>ا N<وَ>عِظَT<ـٰ>مًا أَءِN<نَّ>ا لَمَQ<بۡ>عُوثُJ<و>نَ A<۝>D<٨٢>",
"لَقَQ<دۡ> وُعِQ<دۡ>نَا نَحۡنُ وَءَابَW<اۤ>ؤُنَا هَT<ـٰ>ذَا مِN<ن> قَQ<بۡ>لُ إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّW<اۤ> أَسَT<ـٰ>طِیR<رُ> ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٨٣>",
"قُل لِّمَنِ ٱلۡأَR<رۡ>ضُ وَمَN<ن> فِیهَW<اۤ> إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٨٤>",
"سَیَقُولُونَ لِلَّهِۚ قُلۡ أَفَلَا تَذَكَّR<رُ>J<و>نَ A<۝>D<٨٥>",
"قُلۡ مَX<ن> R<رَّ>بُّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ ٱX<ل>سَّQ<بۡ>عِ وَR<رَ>بُّ ٱلۡعَR<رۡ>شِ ٱلۡعَظِJ<ی>مِ A<۝>D<٨٦>",
"سَیَقُولُونَ لِلَّهِۚ قُلۡ أَفَلَا تَتَّقُJ<و>نَ A<۝>D<٨٧>",
"قُلۡ مَX<ن>N<ۢ> بِیَدِهِT<ۦ> مَلَكُوتُ كُلِّ شَیۡءX<ࣲ> N<وَ>هُوَ یُجِیR<رُ> وَلَا یُجَاR<رُ> عَلَیۡهِ إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٨٨>",
"سَیَقُولُونَ لِلَّهِۚ قُلۡ فَأَN<نَّ>ىٰ تُسۡحَR<رُ>J<و>نَ A<۝>D<٨٩>",
"بَلۡ أَتَیۡنَT<ـٰ>هُN<م> بِX<ٱ>لۡحَقِّ وَإِN<نَّ>هُمۡ لَكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<٩٠>",
"مَا ٱتَّخَذَ ٱللَّهُ مِX<ن> N<وَ>لَدX<ࣲ> N<وَ>مَا كَانَ مَعَهُT<ۥ> مِنۡ إِلَT<ـٰ>هٍۚ إِذX<ࣰ>ا لَّذَهَبَ كُلُّ إِلَT<ـٰ>هِN<ۭ> بِمَا خَلَقَ وَلَعَلَا بَعۡضُهُمۡ عَلَىٰ بَعۡضN<ࣲ>ۚ سُQ<بۡ>حَT<ـٰ>نَ ٱللَّهِ عَN<مَّ>ا یَصِفُJ<و>نَ A<۝>D<٩١>",
"عَT<ـٰ>لِمِ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِ فَتَعَT<ـٰ>لَىٰ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٩٢>",
"قُل R<رَّ>بِّ إِN<مَّ>ا تُرِیَN<نِّ>ی مَا یُوعَدُJ<و>نَ A<۝>D<٩٣>",
"R<رَ>بِّ فَلَا تَQ<جۡ>عَلۡنِی فِی ٱلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٩٤>",
"وَإِN<نَّ>ا عَلَىW<ٰۤ> أَن N<نُّ>رِیَكَ مَا نَعِدُهُمۡ لَقَT<ـٰ>دِR<رُ>J<و>نَ A<۝>D<٩٥>",
"ٱQ<دۡ>فَعۡ بِX<ٱ>لَّتِی هِیَ أَحۡسَنُ ٱX<ل>سَّیِّئَةَۚ نَحۡنُ أَعۡلَمُ بِمَا یَصِفُJ<و>نَ A<۝>D<٩٦>",
"وَقُل R<رَّ>بِّ أَعُوذُ بِكَ مِنۡ هَمَزَT< ٰ>تِ ٱX<ل>شَّیَT<ـٰ>طِJ<ی>نِ A<۝>D<٩٧>",
"وَأَعُوذُ بِكَ R<رَ>بِّ أَX<ن> N<یَ>حۡضُR<رُ>J<و>نِ A<۝>D<٩٨>",
"حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءَ أَحَدَهُمُ ٱلۡمَوۡتُ قَالَ R<رَ>بِّ ٱR<رۡ>جِعُJ<و>نِ A<۝>D<٩٩>",
"لَعَلِّW<یۤ> أَعۡمَلُ صَT<ـٰ>لِحN<ࣰ>ا فِیمَا تَR<رَ>كۡتُۚ كَلَّW<اۤ>ۚ إِN<نَّ>هَا كَلِمَةٌ هُوَ قَW<اۤ>ئِلُهَاۖ وَمِX<ن> N<وَ>R<رَ>W<اۤ>ئِهِN<م> بَR<رۡ>زَخٌ إِلَىٰ یَوۡمِ یُQ<بۡ>عَثُJ<و>نَ A<۝>D<١٠٠>",
"فَإِذَا نُفِخَ فِی ٱX<ل>صُّورِ فَلَW<اۤ> أَN<ن>سَابَ بَیۡنَهُمۡ یَوۡمَئِذX<ࣲ> N<وَ>لَا یَتَسَW<اۤ>ءَلُJ<و>نَ A<۝>D<١٠١>",
"فَمَN<ن> ثَقُلَتۡ مَوَT< ٰ>زِینُهُT<ۥ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<١٠٢>",
"وَمَنۡ خَفَّتۡ مَوَT< ٰ>زِینُهُT<ۥ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ خَسِR<رُ>W<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ فِی جَهَN<نَّ>مَ خَT<ـٰ>لِدُJ<و>نَ A<۝>D<١٠٣>",
"تَلۡفَحُ وُجُوهَهُمُ ٱX<ل>N<نَّ>اR<رُ> وَهُمۡ فِیهَا كَT<ـٰ>لِحُJ<و>نَ A<۝>D<١٠٤>",
"أَلَمۡ تَكُنۡ ءَایَT<ـٰ>تِی تُتۡلَىٰ عَلَیۡكُمۡ فَكُN<ن>تُN<م> بِهَا تُكَذِّبُJ<و>نَ A<۝>D<١٠٥>",
"قَالُوX<ا۟> R<رَ>بَّنَا غَلَبَتۡ عَلَیۡنَا شِQ<قۡ>وَتُنَا وَكُN<نَّ>ا قَوۡمN<ࣰ>ا ضَL<اۤ>لِّJ<ی>نَ A<۝>D<١٠٦>",
"R<رَ>بَّنَW<اۤ> أَخۡرِQ<جۡ>نَا مِنۡهَا فَإِنۡ عُQ<دۡ>نَا فَإِN<نَّ>ا ظَT<ـٰ>لِمُJ<و>نَ A<۝>D<١٠٧>",
"قَالَ ٱخۡسَءُوX<ا۟> فِیهَا وَلَا تُكَلِّمُJ<و>نِ A<۝>D<١٠٨>",
"إِN<نَّ>هُT<ۥ> كَانَ فَرِیقX<ࣱ> N<مِّ>نۡ عِبَادِی یَقُولُونَ R<رَ>بَّنَW<اۤ> ءَامَN<نَّ>ا فَX<ٱ>غۡفِرۡ لَنَا وَX<ٱ>R<رۡ>حَمۡنَا وَأَN<ن>تَ خَیۡR<رُ> ٱX<ل>R<رَّ>T< ٰ>حِمِJ<ی>نَ A<۝>D<١٠٩>",
"فَX<ٱ>تَّخَذۡتُمُوهُمۡ سِخۡرِیًّا حَتَّىW<ٰۤ> أَN<ن>سَوۡكُمۡ ذِكۡرِی وَكُN<ن>تُم N<مِّ>نۡهُمۡ تَضۡحَكُJ<و>نَ A<۝>D<١١٠>",
"إِN<نِّ>ی جَزَیۡتُهُمُ ٱلۡیَوۡمَ بِمَا صَبَR<رُ>W<وۤ>X<ا۟> أَN<نَّ>هُمۡ هُمُ ٱلۡفَW<اۤ>ئِزُJ<و>نَ A<۝>D<١١١>",
"قَT<ـٰ>لَ كَمۡ لَبِثۡتُمۡ فِی ٱلۡأَR<رۡ>ضِ عَدَدَ سِنِJ<ی>نَ A<۝>D<١١٢>",
"قَالُوX<ا۟> لَبِثۡنَا یَوۡمًا أَوۡ بَعۡضَ یَوۡمN<ࣲ> فَسۡءَلِ ٱلۡعَL<اۤ>دِّJ<ی>نَ A<۝>D<١١٣>",
"قَT<ـٰ>لَ إِX<ن> لَّبِثۡتُمۡ إِلَّا قَلِیلX<ࣰ>اۖ لَّوۡ أَN<نَّ>كُمۡ كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<١١٤>",
"أَفَحَسِQ<بۡ>تُمۡ أَN<نَّ>مَا خَلَQ<قۡ>نَT<ـٰ>كُمۡ عَبَثX<ࣰ>ا N<وَ>أَN<نَّ>كُمۡ إِلَیۡنَا لَا تُR<رۡ>جَعُJ<و>نَ A<۝>D<١١٥>",
"فَتَعَT<ـٰ>لَى ٱللَّهُ ٱلۡمَلِكُ ٱلۡحَقُّۖ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ R<رَ>بُّ ٱلۡعَR<رۡ>شِ ٱلۡكَرِJ<ی>مِ A<۝>D<١١٦>",
"وَمَX<ن> N<یَ>Q<دۡ>عُ مَعَ ٱللَّهِ إِلَT<ـٰ>هًا ءَاخَR<رَ> لَا بُR<رۡ>هَT<ـٰ>نَ لَهُT<ۥ> بِهِT<ۦ> فَإِN<نَّ>مَا حِسَابُهُT<ۥ> عِN<ن>دَ R<رَ>بِّهِW<ۦۤ>ۚ إِN<نَّ>هُT<ۥ> لَا یُفۡلِحُ ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<١١٧>",
"وَقُل R<رَّ>بِّ ٱغۡفِرۡ وَX<ٱ>R<رۡ>حَمۡ وَأَN<ن>تَ خَیۡR<رُ> ٱX<ل>R<رَّ>T< ٰ>حِمِJ<ی>نَ A<۝>D<١١٨>",
"#۞ سُوR<رَ>ةٌ أَN<ن>زَلۡنَT<ـٰ>هَا وَفَR<رَ>ضۡنَT<ـٰ>هَا وَأَN<ن>زَلۡنَا فِیهَW<اۤ> ءَایَT<ـٰ>تِN<ۭ> بَیِّنَT<ـٰ>تX<ࣲ> لَّعَلَّكُمۡ تَذَكَّR<رُ>J<و>نَ A<۝>D<١>",
"ٱX<ل>زَّانِیَةُ وَX<ٱ>X<ل>زَّانِی فَX<ٱ>Q<جۡ>لِدُوX<ا۟> كُلَّ وَT< ٰ>حِدX<ࣲ> N<مِّ>نۡهُمَا مِX<ا۟>ئَةَ جَلۡدَةX<ࣲ>ۖ N<وَ>لَا تَأۡخُذۡكُN<م> بِهِمَا R<رَ>أۡفَةN<ࣱ> فِی دِینِ ٱللَّهِ إِN<ن> كُN<ن>تُمۡ تُؤۡمِنُونَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِۖ وَلۡیَشۡهَQ<دۡ> عَذَابَهُمَا طَW<اۤ>ئِفَةX<ࣱ> N<مِّ>نَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٢>",
"ٱX<ل>زَّانِی لَا یَN<ن>كِحُ إِلَّا زَانِیَةً أَوۡ مُشۡرِكَةX<ࣰ> N<وَ>X<ٱ>X<ل>زَّانِیَةُ لَا یَN<ن>كِحُهَW<اۤ> إِلَّا زَانٍ أَوۡ مُشۡرِكX<ࣱ>ۚ N<وَ>حُرِّمَ ذَT< ٰ>لِكَ عَلَى ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٣>",
"وَX<ٱ>لَّذِینَ یَR<رۡ>مُونَ ٱلۡمُحۡصَنَT<ـٰ>تِ ثُN<مَّ> لَمۡ یَأۡتُوX<ا۟> بِأَR<رۡ>بَعَةِ شُهَدَW<اۤ>ءَ فَX<ٱ>Q<جۡ>لِدُوهُمۡ ثَمَT<ـٰ>نِینَ جَلۡدَةX<ࣰ> N<وَ>لَا تَQ<قۡ>بَلُوX<ا۟> لَهُمۡ شَهَT<ـٰ>دَةً أَبَدX<ࣰ>اۚ N<وَ>أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡفَT<ـٰ>سِقُJ<و>نَ A<۝>D<٤>",
"إِلَّا ٱلَّذِینَ تَابُوX<ا۟> مِX<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَ وَأَصۡلَحُوX<ا۟> فَإِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٥>",
"وَX<ٱ>لَّذِینَ یَR<رۡ>مُونَ أَزۡوَT< ٰ>جَهُمۡ وَلَمۡ یَكُX<ن> لَّهُمۡ شُهَدَW<اۤ>ءُ إِلَّW<اۤ> أَN<ن>فُسُهُمۡ فَشَهَT<ـٰ>دَةُ أَحَدِهِمۡ أَR<رۡ>بَعُ شَهَT<ـٰ>دَT< ٰ>تِN<ۭ> بِٱللَّهِ إِN<نَّ>هُT<ۥ> لَمِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٦>",
"وَX<ٱ>لۡخَT<ـٰ>مِسَةُ أَN<نَّ> لَعۡنَتَ ٱللَّهِ عَلَیۡهِ إِN<ن> كَانَ مِنَ ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٧>",
"وَیَQ<دۡ>R<رَ>ؤُX<ا۟> عَنۡهَا ٱلۡعَذَابَ أَN<ن> تَشۡهَدَ أَR<رۡ>بَعَ شَهَT<ـٰ>دَT< ٰ>تِN<ۭ> بِٱللَّهِ إِN<نَّ>هُT<ۥ> لَمِنَ ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٨>",
"وَX<ٱ>لۡخَT<ـٰ>مِسَةَ أَN<نَّ> غَضَبَ ٱللَّهِ عَلَیۡهَW<اۤ> إِN<ن> كَانَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٩>",
"وَلَوۡلَا فَضۡلُ ٱللَّهِ عَلَیۡكُمۡ وَR<رَ>حۡمَتُهُT<ۥ> وَأَN<نَّ> ٱللَّهَ تَوَّابٌ حَكِJ<ی>مٌ A<۝>D<١٠>",
"إِN<نَّ> ٱلَّذِینَ جَW<اۤ>ءُو بِX<ٱ>لۡإِفۡكِ عُصۡبَةX<ࣱ> N<مِّ>N<ن>كُمۡۚ لَا تَحۡسَبُوهُ شَR<رّ>X<ࣰ>ا لَّكُN<م>ۖ بَلۡ هُوَ خَیۡR<ر>X<ࣱ> لَّكُمۡۚ لِكُلِّ ٱمۡرِئX<ࣲ> N<مِّ>نۡهُم N<مَّ>ا ٱكۡتَسَبَ مِنَ ٱلۡإِثۡمِۚ وَX<ٱ>لَّذِی تَوَلَّىٰ كِQ<بۡ>R<رَ>هُT<ۥ> مِنۡهُمۡ لَهُT<ۥ> عَذَابٌ عَظِJ<ی>مࣱ A<۝>D<١١>",
"لَّوۡلَW<اۤ> إِذۡ سَمِعۡتُمُوهُ ظَN<نَّ> ٱلۡمُؤۡمِنُونَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تُ بِأَN<ن>فُسِهِمۡ خَیۡR<ر>X<ࣰ>ا N<وَ>قَالُوX<ا۟> هَT<ـٰ>ذَW<اۤ> إِفۡكX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٢>",
"لَّوۡلَا جَW<اۤ>ءُو عَلَیۡهِ بِأَR<رۡ>بَعَةِ شُهَدَW<اۤ>ءَۚ فَإِذۡ لَمۡ یَأۡتُوX<ا۟> بِX<ٱ>X<ل>شُّهَدَW<اۤ>ءِ فَأُX<و۟>لَW<ـٰۤ>ئِكَ عِN<ن>دَ ٱللَّهِ هُمُ ٱلۡكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<١٣>",
"وَلَوۡلَا فَضۡلُ ٱللَّهِ عَلَیۡكُمۡ وَR<رَ>حۡمَتُهُT<ۥ> فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِ لَمَسَّكُمۡ فِی مَW<اۤ> أَفَضۡتُمۡ فِیهِ عَذَابٌ عَظِJ<ی>مٌ A<۝>D<١٤>",
"إِذۡ تَلَقَّوۡنَهُT<ۥ> بِأَلۡسِنَتِكُمۡ وَتَقُولُونَ بِأَفۡوَاهِكُم N<مَّ>ا لَیۡسَ لَكُN<م> بِهِT<ۦ> عِلۡمX<ࣱ> N<وَ>تَحۡسَبُونَهُT<ۥ> هَیِّنX<ࣰ>ا N<وَ>هُوَ عِN<ن>دَ ٱللَّهِ عَظِJ<ی>مࣱ A<۝>D<١٥>",
"وَلَوۡلَW<اۤ> إِذۡ سَمِعۡتُمُوهُ قُلۡتُم N<مَّ>ا یَكُونُ لَنَW<اۤ> أَن N<نَّ>تَكَلَّمَ بِهَT<ـٰ>ذَا سُQ<بۡ>حَT<ـٰ>نَكَ هَT<ـٰ>ذَا بُهۡتَT<ـٰ>نٌ عَظِJ<ی>مࣱ A<۝>D<١٦>",
"یَعِظُكُمُ ٱللَّهُ أَN<ن> تَعُودُوX<ا۟> لِمِثۡلِهِW<ۦۤ> أَبَدًا إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٧>",
"وَیُبَیِّنُ ٱللَّهُ لَكُمُ ٱلۡءَایَT<ـٰ>تِۚ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مٌ A<۝>D<١٨>",
"إِN<نَّ> ٱلَّذِینَ یُحِبُّونَ أَN<ن> تَشِیعَ ٱلۡفَT<ـٰ>حِشَةُ فِی ٱلَّذِینَ ءَامَنُوX<ا۟> لَهُمۡ عَذَابٌ أَلِیمN<ࣱ> فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِۚ وَٱللَّهُ یَعۡلَمُ وَأَN<ن>تُمۡ لَا تَعۡلَمُJ<و>نَ A<۝>D<١٩>",
"وَلَوۡلَا فَضۡلُ ٱللَّهِ عَلَیۡكُمۡ وَR<رَ>حۡمَتُهُT<ۥ> وَأَN<نَّ> ٱللَّهَ R<رَ>ءُوفX<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٢٠>",
"۞ یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَتَّبِعُوX<ا۟> خُطُوَT< ٰ>تِ ٱX<ل>شَّیۡطَT<ـٰ>نِۚ وَمَX<ن> N<یَ>تَّبِعۡ خُطُوَT< ٰ>تِ ٱX<ل>شَّیۡطَT<ـٰ>نِ فَإِN<نَّ>هُT<ۥ> یَأۡمُR<رُ> بِX<ٱ>لۡفَحۡشَW<اۤ>ءِ وَX<ٱ>لۡمُN<ن>كَرِۚ وَلَوۡلَا فَضۡلُ ٱللَّهِ عَلَیۡكُمۡ وَR<رَ>حۡمَتُهُT<ۥ> مَا زَكَىٰ مِN<ن>كُم N<مِّ>نۡ أَحَدٍ أَبَدX<ࣰ>ا N<وَ>لَT<ـٰ>كِN<نَّ> ٱللَّهَ یُزَكِّی مَX<ن> N<یَ>شَW<اۤ>ءُۗ وَٱللَّهُ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<٢١>",
"وَلَا یَأۡتَلِ أُX<و۟>لُوX<ا۟> ٱلۡفَضۡلِ مِN<ن>كُمۡ وَX<ٱ>X<ل>سَّعَةِ أَX<ن> N<یُ>ؤۡتُW<وۤ>X<ا۟> أُX<و۟>لِی ٱلۡقُR<رۡ>بَىٰ وَX<ٱ>لۡمَسَT<ـٰ>كِینَ وَX<ٱ>لۡمُهَT<ـٰ>جِرِینَ فِی سَبِیلِ ٱللَّهِۖ وَلۡیَعۡفُوX<ا۟> وَلۡیَصۡفَحُW<وۤ>X<ا۟>ۗ أَلَا تُحِبُّونَ أَX<ن> N<یَ>غۡفِR<رَ> ٱللَّهُ لَكُمۡۚ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<٢٢>",
"إِN<نَّ> ٱلَّذِینَ یَR<رۡ>مُونَ ٱلۡمُحۡصَنَT<ـٰ>تِ ٱلۡغَT<ـٰ>فِلَT<ـٰ>تِ ٱلۡمُؤۡمِنَT<ـٰ>تِ لُعِنُوX<ا۟> فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِ وَلَهُمۡ عَذَابٌ عَظِJ<ی>مࣱ A<۝>D<٢٣>",
"یَوۡمَ تَشۡهَدُ عَلَیۡهِمۡ أَلۡسِنَتُهُمۡ وَأَیۡدِیهِمۡ وَأَR<رۡ>جُلُهُN<م> بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٢٤>",
"یَوۡمَئِذX<ࣲ> N<یُ>وَفِّیهِمُ ٱللَّهُ دِینَهُمُ ٱلۡحَقَّ وَیَعۡلَمُونَ أَN<نَّ> ٱللَّهَ هُوَ ٱلۡحَقُّ ٱلۡمُبِJ<ی>نُ A<۝>D<٢٥>",
"ٱلۡخَبِیثَT<ـٰ>تُ لِلۡخَبِیثِینَ وَX<ٱ>لۡخَبِیثُونَ لِلۡخَبِیثَT<ـٰ>تِۖ وَX<ٱ>X<ل>طَّیِّبَT<ـٰ>تُ لِX<ل>طَّیِّبِینَ وَX<ٱ>X<ل>طَّیِّبُونَ لِX<ل>طَّیِّبَT<ـٰ>تِۚ أُX<و۟>لَW<ـٰۤ>ئِكَ مُبَR<رَّ>ءُونَ مِN<مَّ>ا یَقُولُونَۖ لَهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<وَ>رِزۡقN<ࣱ> كَرِJ<ی>مࣱ A<۝>D<٢٦>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَQ<دۡ>خُلُوX<ا۟> بُیُوتًا غَیۡR<رَ> بُیُوتِكُمۡ حَتَّىٰ تَسۡتَأۡنِسُوX<ا۟> وَتُسَلِّمُوX<ا۟> عَلَىW<ٰۤ> أَهۡلِهَاۚ ذَT< ٰ>لِكُمۡ خَیۡR<ر>X<ࣱ> لَّكُمۡ لَعَلَّكُمۡ تَذَكَّR<رُ>J<و>نَ A<۝>D<٢٧>",
"فَإِX<ن> لَّمۡ تَجِدُوX<ا۟> فِیهَW<اۤ> أَحَدN<ࣰ>ا فَلَا تَQ<دۡ>خُلُوهَا حَتَّىٰ یُؤۡذَنَ لَكُمۡۖ وَإِN<ن> قِیلَ لَكُمُ ٱR<رۡ>جِعُوX<ا۟> فَX<ٱ>R<رۡ>جِعُوX<ا۟>ۖ هُوَ أَزۡكَىٰ لَكُمۡۚ وَٱللَّهُ بِمَا تَعۡمَلُونَ عَلِJ<ی>مࣱ A<۝>D<٢٨>",
"لَّیۡسَ عَلَیۡكُمۡ جُنَاحٌ أَN<ن> تَQ<دۡ>خُلُوX<ا۟> بُیُوتًا غَیۡR<رَ> مَسۡكُونَةN<ࣲ> فِیهَا مَتَT<ـٰ>عX<ࣱ> لَّكُمۡۚ وَٱللَّهُ یَعۡلَمُ مَا تُQ<بۡ>دُونَ وَمَا تَكۡتُمُJ<و>نَ A<۝>D<٢٩>",
"قُل لِّلۡمُؤۡمِنِینَ یَغُضُّوX<ا۟> مِنۡ أَQ<بۡ>صَT<ـٰ>رِهِمۡ وَیَحۡفَظُوX<ا۟> فُR<رُ>وجَهُمۡۚ ذَT< ٰ>لِكَ أَزۡكَىٰ لَهُمۡۚ إِN<نَّ> ٱللَّهَ خَبِیR<رُ>N<ۢ> بِمَا یَصۡنَعُJ<و>نَ A<۝>D<٣٠>",
"وَقُل لِّلۡمُؤۡمِنَT<ـٰ>تِ یَغۡضُضۡنَ مِنۡ أَQ<بۡ>صَT<ـٰ>رِهِN<نَّ> وَیَحۡفَظۡنَ فُR<رُ>وجَهُN<نَّ> وَلَا یُQ<بۡ>دِینَ زِینَتَهُN<نَّ> إِلَّا مَا ظَهَR<رَ> مِنۡهَاۖ وَلۡیَضۡرِQ<بۡ>نَ بِخُمُرِهِN<نَّ> عَلَىٰ جُیُوبِهِN<نَّ>ۖ وَلَا یُQ<بۡ>دِینَ زِینَتَهُN<نَّ> إِلَّا لِبُعُولَتِهِN<نَّ> أَوۡ ءَابَW<اۤ>ئِهِN<نَّ> أَوۡ ءَابَW<اۤ>ءِ بُعُولَتِهِN<نَّ> أَوۡ أَQ<بۡ>نَW<اۤ>ئِهِN<نَّ> أَوۡ أَQ<بۡ>نَW<اۤ>ءِ بُعُولَتِهِN<نَّ> أَوۡ إِخۡوَT< ٰ>نِهِN<نَّ> أَوۡ بَنِW<یۤ> إِخۡوَT< ٰ>نِهِN<نَّ> أَوۡ بَنِW<یۤ> أَخَوَT< ٰ>تِهِN<نَّ> أَوۡ نِسَW<اۤ>ئِهِN<نَّ> أَوۡ مَا مَلَكَتۡ أَیۡمَT<ـٰ>نُهُN<نَّ> أَوِ ٱX<ل>تَّT<ـٰ>بِعِینَ غَیۡرِ أُX<و۟>لِی ٱلۡإِرۡبَةِ مِنَ ٱX<ل>رِّجَالِ أَوِ ٱX<ل>طِّفۡلِ ٱلَّذِینَ لَمۡ یَظۡهَR<رُ>وX<ا۟> عَلَىٰ عَوۡR<رَ>T< ٰ>تِ ٱX<ل>N<نِّ>سَW<اۤ>ءِۖ وَلَا یَضۡرِQ<بۡ>نَ بِأَR<رۡ>جُلِهِN<نَّ> لِیُعۡلَمَ مَا یُخۡفِینَ مِN<ن> زِینَتِهِN<نَّ>ۚ وَتُوبُW<وۤ>X<ا۟> إِلَى ٱللَّهِ جَمِیعًا أَیُّهَ ٱلۡمُؤۡمِنُونَ لَعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<٣١>",
"وَأَN<ن>كِحُوX<ا۟> ٱلۡأَیَT<ـٰ>مَىٰ مِN<ن>كُمۡ وَX<ٱ>X<ل>صَّT<ـٰ>لِحِینَ مِنۡ عِبَادِكُمۡ وَإِمَW<اۤ>ئِكُمۡۚ إِX<ن> N<یَ>كُونُوX<ا۟> فُقَR<رَ>W<اۤ>ءَ یُغۡنِهِمُ ٱللَّهُ مِN<ن> فَضۡلِهِT<ۦ>ۗ وَٱللَّهُ وَT< ٰ>سِعٌ عَلِJ<ی>مࣱ A<۝>D<٣٢>",
"وَلۡیَسۡتَعۡفِفِ ٱلَّذِینَ لَا یَجِدُونَ نِكَاحًا حَتَّىٰ یُغۡنِیَهُمُ ٱللَّهُ مِN<ن> فَضۡلِهِT<ۦ>ۗ وَX<ٱ>لَّذِینَ یَQ<بۡ>تَغُونَ ٱلۡكِتَT<ـٰ>بَ مِN<مَّ>ا مَلَكَتۡ أَیۡمَT<ـٰ>نُكُمۡ فَكَاتِبُوهُمۡ إِنۡ عَلِمۡتُمۡ فِیهِمۡ خَیۡR<ر>X<ࣰ>اۖ N<وَ>ءَاتُوهُم N<مِّ>X<ن> N<مَّ>الِ ٱللَّهِ ٱلَّذِW<یۤ> ءَاتَX<ى>T<ٰ>كُمۡۚ وَلَا تُكۡرِهُوX<ا۟> فَتَیَT<ـٰ>تِكُمۡ عَلَى ٱلۡبِغَW<اۤ>ءِ إِنۡ أَR<رَ>Q<دۡ>نَ تَحَصُّنX<ࣰ>ا لِّتَQ<بۡ>تَغُوX<ا۟> عَR<رَ>ضَ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۚ وَمَX<ن> N<یُ>كۡرِX<ه>هُّN<نَّ> فَإِN<نَّ> ٱللَّهَ مِX<ن>N<ۢ> بَعۡدِ إِكۡR<رَ>T< ٰ>هِهِN<نَّ> غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٣٣>",
"وَلَقَQ<دۡ> أَN<ن>زَلۡنَW<اۤ> إِلَیۡكُمۡ ءَایَT<ـٰ>تX<ࣲ> N<مُّ>بَیِّنَT<ـٰ>تX<ࣲ> N<وَ>مَثَلX<ࣰ>ا N<مِّ>نَ ٱلَّذِینَ خَلَوۡX<ا۟> مِN<ن> قَQ<بۡ>لِكُمۡ وَمَوۡعِظَةX<ࣰ> لِّلۡمُتَّقِJ<ی>نَ A<۝>D<٣٤>",
"۞ ٱللَّهُ نُوR<رُ> ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ مَثَلُ نُورِهِT<ۦ> كَمِشۡكَX<و>T<ٰ>ةN<ࣲ> فِیهَا مِصۡبَاحٌۖ ٱلۡمِصۡبَاحُ فِی زُجَاجَةٍۖ ٱX<ل>زُّجَاجَةُ كَأَN<نَّ>هَا كَوۡكَبN<ࣱ> دُرِّیّX<ࣱ> N<یُ>وقَدُ مِN<ن> شَجَR<رَ>ةX<ࣲ> N<مُّ>بَT<ـٰ>R<رَ>كَةN<ࣲ> زَیۡتُونَةX<ࣲ> لَّا شَR<رۡ>قِیَّةX<ࣲ> N<وَ>لَا غَR<رۡ>بِیَّةX<ࣲ> N<یَ>كَادُ زَیۡتُهَا یُضِW<یۤ>ءُ وَلَوۡ لَمۡ تَمۡسَسۡهُ نَاR<ر>X<ࣱ>ۚ N<نُّ>وR<ر>ٌ عَلَىٰ نُورX<ࣲ>ۚ N<یَ>هۡدِی ٱللَّهُ لِنُورِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَیَضۡرِبُ ٱللَّهُ ٱلۡأَمۡثَT<ـٰ>لَ لِX<ل>N<نَّ>اسِۗ وَٱللَّهُ بِكُلِّ شَیۡءٍ عَلِJ<ی>مࣱ A<۝>D<٣٥>",
"فِی بُیُوتٍ أَذِنَ ٱللَّهُ أَN<ن> تُR<رۡ>فَعَ وَیُذۡكَR<رَ> فِیهَا ٱسۡمُهُT<ۥ> یُسَبِّحُ لَهُT<ۥ> فِیهَا بِX<ٱ>لۡغُدُوِّ وَX<ٱ>لۡءَاصَJ<ا>لِ A<۝>D<٣٦>",
"رِجَالX<ࣱ> لَّا تُلۡهِیهِمۡ تِجَT<ـٰ>R<رَ>ةX<ࣱ> N<وَ>لَا بَیۡعٌ عَN<ن> ذِكۡرِ ٱللَّهِ وَإِقَامِ ٱX<ل>صَّلَX<و>T<ٰ>ةِ وَإِیتَW<اۤ>ءِ ٱX<ل>زَّكَX<و>T<ٰ>ةِ یَخَافُونَ یَوۡمN<ࣰ>ا تَتَقَلَّبُ فِیهِ ٱلۡقُلُوبُ وَX<ٱ>لۡأَQ<بۡ>صَT<ـٰ>R<ر>ُ A<۝>D<٣٧>",
"لِیَQ<جۡ>زِیَهُمُ ٱللَّهُ أَحۡسَنَ مَا عَمِلُوX<ا۟> وَیَزِیدَهُم N<مِّ>N<ن> فَضۡلِهِT<ۦ>ۗ وَٱللَّهُ یَR<رۡ>زُقُ مَX<ن> N<یَ>شَW<اۤ>ءُ بِغَیۡرِ حِسَJ<ا>Q<ب>ࣲ A<۝>D<٣٨>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَعۡمَT<ـٰ>لُهُمۡ كَسَR<رَ>ابِN<ۭ> بِقِیعَةX<ࣲ> N<یَ>حۡسَبُهُ ٱX<ل>ظَّمۡءَانُ مَW<اۤ>ءً حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءَهُT<ۥ> لَمۡ یَجِQ<دۡ>هُ شَیۡءX<ࣰ>ا N<وَ>وَجَدَ ٱللَّهَ عِN<ن>دَهُT<ۥ> فَوَفَّX<ى>T<ٰ>هُ حِسَابَهُT<ۥ>ۗ وَٱللَّهُ سَرِیعُ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<٣٩>",
"أَوۡ كَظُلُمَT<ـٰ>تN<ࣲ> فِی بَحۡرX<ࣲ> لُّجِّیّX<ࣲ> N<یَ>غۡشَX<ى>T<ٰ>هُ مَوۡجX<ࣱ> N<مِّ>N<ن> فَوۡقِهِT<ۦ> مَوۡجX<ࣱ> N<مِّ>N<ن> فَوۡقِهِT<ۦ> سَحَابN<ࣱ>ۚ ظُلُمَT<ـٰ>تُN<ۢ> بَعۡضُهَا فَوۡقَ بَعۡضٍ إِذَW<اۤ> أَخۡR<رَ>جَ یَدَهُT<ۥ> لَمۡ یَكَQ<دۡ> یَR<رَ>X<ى>T<ٰ>هَاۗ وَمَX<ن> لَّمۡ یَQ<جۡ>عَلِ ٱللَّهُ لَهُT<ۥ> نُوR<ر>N<ࣰ>ا فَمَا لَهُT<ۥ> مِن N<نُّ>J<و>R<ر>ٍ A<۝>D<٤٠>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ یُسَبِّحُ لَهُT<ۥ> مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَX<ٱ>X<ل>طَّیۡR<رُ> صَL<ـٰۤ>فَّT<ـٰ>تN<ࣲ>ۖ كُلّN<ࣱ> قَQ<دۡ> عَلِمَ صَلَاتَهُT<ۥ> وَتَسۡبِیحَهُT<ۥ>ۗ وَٱللَّهُ عَلِیمُN<ۢ> بِمَا یَفۡعَلُJ<و>نَ A<۝>D<٤١>",
"وَلِلَّهِ مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ وَإِلَى ٱللَّهِ ٱلۡمَصِJ<ی>رُ A<۝>D<٤٢>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ یُزۡجِی سَحَابN<ࣰ>ا ثُN<مَّ> یُؤَلِّفُ بَیۡنَهُT<ۥ> ثُN<مَّ> یَQ<جۡ>عَلُهُT<ۥ> R<رُ>كَامN<ࣰ>ا فَتَR<رَ>ى ٱلۡوَQ<دۡ>قَ یَخۡR<رُ>جُ مِنۡ خِلَT<ـٰ>لِهِT<ۦ> وَیُنَزِّلُ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مِN<ن> جِبَالN<ࣲ> فِیهَا مِX<ن>N<ۢ> بَR<رَ>دN<ࣲ> فَیُصِیبُ بِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُ وَیَصۡرِفُهُT<ۥ> عَX<ن> N<مَّ>X<ن> N<یَ>شَW<اۤ>ءُۖ یَكَادُ سَنَا بَR<رۡ>قِهِT<ۦ> یَذۡهَبُ بِX<ٱ>لۡأَQ<بۡ>صَT<ـٰ>R<ر>ِ A<۝>D<٤٣>",
"یُقَلِّبُ ٱللَّهُ ٱلَّیۡلَ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَعِQ<بۡ>R<رَ>ةX<ࣰ> لِّأُX<و۟>لِی ٱلۡأَQ<بۡ>صَT<ـٰ>R<ر>ِ A<۝>D<٤٤>",
"وَٱللَّهُ خَلَقَ كُلَّ دَL<اۤ>بَّةX<ࣲ> N<مِّ>X<ن> N<مَّ>W<اۤ>ءN<ࣲ>ۖ فَمِنۡهُم N<مَّ>X<ن> N<یَ>مۡشِی عَلَىٰ بَQ<طۡ>نِهِT<ۦ> وَمِنۡهُم N<مَّ>X<ن> N<یَ>مۡشِی عَلَىٰ رِQ<جۡ>لَیۡنِ وَمِنۡهُم N<مَّ>X<ن> N<یَ>مۡشِی عَلَىW<ٰۤ> أَR<رۡ>بَعX<ࣲ>ۚ N<یَ>خۡلُقُ ٱللَّهُ مَا یَشَW<اۤ>ءُۚ إِN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٤٥>",
"لَّقَQ<دۡ> أَN<ن>زَلۡنَW<اۤ> ءَایَT<ـٰ>تX<ࣲ> N<مُّ>بَیِّنَT<ـٰ>تX<ࣲ>ۚ N<وَ>ٱللَّهُ یَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُ إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٤٦>",
"وَیَقُولُونَ ءَامَN<نَّ>ا بِٱللَّهِ وَبِX<ٱ>X<ل>R<رَّ>سُولِ وَأَطَعۡنَا ثُN<مَّ> یَتَوَلَّىٰ فَرِیقX<ࣱ> N<مِّ>نۡهُم N<مِّ>X<ن>N<ۢ> بَعۡدِ ذَT< ٰ>لِكَۚ وَمَW<اۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ بِX<ٱ>لۡمُؤۡمِنِJ<ی>نَ A<۝>D<٤٧>",
"وَإِذَا دُعُW<وۤ>X<ا۟> إِلَى ٱللَّهِ وَR<رَ>سُولِهِT<ۦ> لِیَحۡكُمَ بَیۡنَهُمۡ إِذَا فَرِیقX<ࣱ> N<مِّ>نۡهُم N<مُّ>عۡرِضُJ<و>نَ A<۝>D<٤٨>",
"وَإِX<ن> N<یَ>كُX<ن> لَّهُمُ ٱلۡحَقُّ یَأۡتُW<وۤ>X<ا۟> إِلَیۡهِ مُذۡعِنِJ<ی>نَ A<۝>D<٤٩>",
"أَفِی قُلُوبِهِم N<مَّ>R<رَ>ضٌ أَمِ ٱR<رۡ>تَابُW<وۤ>X<ا۟> أَمۡ یَخَافُونَ أَX<ن> N<یَ>حِیفَ ٱللَّهُ عَلَیۡهِمۡ وَR<رَ>سُولُهُT<ۥ>ۚ بَلۡ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٥٠>",
"إِN<نَّ>مَا كَانَ قَوۡلَ ٱلۡمُؤۡمِنِینَ إِذَا دُعُW<وۤ>X<ا۟> إِلَى ٱللَّهِ وَR<رَ>سُولِهِT<ۦ> لِیَحۡكُمَ بَیۡنَهُمۡ أَX<ن> N<یَ>قُولُوX<ا۟> سَمِعۡنَا وَأَطَعۡنَاۚ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<٥١>",
"وَمَX<ن> N<یُ>طِعِ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> وَیَخۡشَ ٱللَّهَ وَیَتَّQ<قۡ>هِ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡفَW<اۤ>ئِزُJ<و>نَ A<۝>D<٥٢>",
"۞ وَأَQ<قۡ>سَمُوX<ا۟> بِٱللَّهِ جَهۡدَ أَیۡمَT<ـٰ>نِهِمۡ لَئِنۡ أَمَR<رۡ>تَهُمۡ لَیَخۡR<رُ>جُN<نَّ>ۖ قُل لَّا تُQ<قۡ>سِمُوX<ا۟>ۖ طَاعَةX<ࣱ> N<مَّ>عۡR<رُ>وفَةٌۚ إِN<نَّ> ٱللَّهَ خَبِیR<رُ>N<ۢ> بِمَا تَعۡمَلُJ<و>نَ A<۝>D<٥٣>",
"قُلۡ أَطِیعُوX<ا۟> ٱللَّهَ وَأَطِیعُوX<ا۟> ٱX<ل>R<رَّ>سُولَۖ فَإِN<ن> تَوَلَّوۡX<ا۟> فَإِN<نَّ>مَا عَلَیۡهِ مَا حُN<مِّ>لَ وَعَلَیۡكُم N<مَّ>ا حُN<مِّ>لۡتُمۡۖ وَإِN<ن> تُطِیعُوهُ تَهۡتَدُوX<ا۟>ۚ وَمَا عَلَى ٱX<ل>R<رَّ>سُولِ إِلَّا ٱلۡبَلَT<ـٰ>غُ ٱلۡمُبِJ<ی>نُ A<۝>D<٥٤>",
"وَعَدَ ٱللَّهُ ٱلَّذِینَ ءَامَنُوX<ا۟> مِN<ن>كُمۡ وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَیَسۡتَخۡلِفَN<نَّ>هُمۡ فِی ٱلۡأَR<رۡ>ضِ كَمَا ٱسۡتَخۡلَفَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ وَلَیُمَكِّنَN<نَّ> لَهُمۡ دِینَهُمُ ٱلَّذِی ٱR<رۡ>تَضَىٰ لَهُمۡ وَلَیُبَدِّلَN<نَّ>هُم N<مِّ>X<ن>N<ۢ> بَعۡدِ خَوۡفِهِمۡ أَمۡنX<ࣰ>اۚ N<یَ>عۡبُدُونَنِی لَا یُشۡرِكُونَ بِی شَیۡءX<ࣰ>اۚ N<وَ>مَN<ن> كَفَR<رَ> بَعۡدَ ذَT< ٰ>لِكَ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡفَT<ـٰ>سِقُJ<و>نَ A<۝>D<٥٥>",
"وَأَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتُوX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَأَطِیعُوX<ا۟> ٱX<ل>R<رَّ>سُولَ لَعَلَّكُمۡ تُR<رۡ>حَمُJ<و>نَ A<۝>D<٥٦>",
"لَا تَحۡسَبَN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مُعۡجِزِینَ فِی ٱلۡأَR<رۡ>ضِۚ وَمَأۡوَX<ى>T<ٰ>هُمُ ٱX<ل>N<نَّ>اR<رُ>ۖ وَلَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<٥٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لِیَسۡتَءۡذِN<ن>كُمُ ٱلَّذِینَ مَلَكَتۡ أَیۡمَT<ـٰ>نُكُمۡ وَX<ٱ>لَّذِینَ لَمۡ یَQ<بۡ>لُغُوX<ا۟> ٱلۡحُلُمَ مِN<ن>كُمۡ ثَلَT<ـٰ>ثَ مَR<رَّ>T< ٰ>تX<ࣲ>ۚ N<مِّ>N<ن> قَQ<بۡ>لِ صَلَX<و>T<ٰ>ةِ ٱلۡفَQ<جۡ>رِ وَحِینَ تَضَعُونَ ثِیَابَكُم N<مِّ>نَ ٱX<ل>ظَّهِیR<رَ>ةِ وَمِX<ن>N<ۢ> بَعۡدِ صَلَX<و>T<ٰ>ةِ ٱلۡعِشَW<اۤ>ءِۚ ثَلَT<ـٰ>ثُ عَوۡR<رَ>T< ٰ>تX<ࣲ> لَّكُمۡۚ لَیۡسَ عَلَیۡكُمۡ وَلَا عَلَیۡهِمۡ جُنَاحُN<ۢ> بَعۡدَهُN<نَّ>ۚ طَوَّT< ٰ>فُونَ عَلَیۡكُN<م> بَعۡضُكُمۡ عَلَىٰ بَعۡضN<ࣲ>ۚ كَذَT< ٰ>لِكَ یُبَیِّنُ ٱللَّهُ لَكُمُ ٱلۡءَایَT<ـٰ>تِۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<٥٨>",
"وَإِذَا بَلَغَ ٱلۡأَQ<طۡ>فَT<ـٰ>لُ مِN<ن>كُمُ ٱلۡحُلُمَ فَلۡیَسۡتَءۡذِنُوX<ا۟> كَمَا ٱسۡتَءۡذَنَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۚ كَذَT< ٰ>لِكَ یُبَیِّنُ ٱللَّهُ لَكُمۡ ءَایَT<ـٰ>تِهِT<ۦ>ۗ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<٥٩>",
"وَX<ٱ>لۡقَوَT< ٰ>عِدُ مِنَ ٱX<ل>N<نِّ>سَW<اۤ>ءِ ٱلَّT<ـٰ>تِی لَا یَR<رۡ>جُونَ نِكَاحN<ࣰ>ا فَلَیۡسَ عَلَیۡهِN<نَّ> جُنَاحٌ أَX<ن> N<یَ>ضَعۡنَ ثِیَابَهُN<نَّ> غَیۡR<رَ> مُتَبَرِّجَT<ـٰ>تِN<ۭ> بِزِینَةX<ࣲ>ۖ N<وَ>أَX<ن> N<یَ>سۡتَعۡفِفۡنَ خَیۡR<ر>X<ࣱ> لَّهُN<نَّ>ۗ وَٱللَّهُ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<٦٠>",
"لَّیۡسَ عَلَى ٱلۡأَعۡمَىٰ حَR<رَ>جX<ࣱ> N<وَ>لَا عَلَى ٱلۡأَعۡR<رَ>جِ حَR<رَ>جX<ࣱ> N<وَ>لَا عَلَى ٱلۡمَرِیضِ حَR<رَ>جX<ࣱ> N<وَ>لَا عَلَىW<ٰۤ> أَN<ن>فُسِكُمۡ أَN<ن> تَأۡكُلُوX<ا۟> مِX<ن>N<ۢ> بُیُوتِكُمۡ أَوۡ بُیُوتِ ءَابَW<اۤ>ئِكُمۡ أَوۡ بُیُوتِ أُN<مَّ>هَT<ـٰ>تِكُمۡ أَوۡ بُیُوتِ إِخۡوَT< ٰ>نِكُمۡ أَوۡ بُیُوتِ أَخَوَT< ٰ>تِكُمۡ أَوۡ بُیُوتِ أَعۡمَT<ـٰ>مِكُمۡ أَوۡ بُیُوتِ عَN<مَّ>T<ـٰ>تِكُمۡ أَوۡ بُیُوتِ أَخۡوَT< ٰ>لِكُمۡ أَوۡ بُیُوتِ خَT<ـٰ>لَT<ـٰ>تِكُمۡ أَوۡ مَا مَلَكۡتُم N<مَّ>فَاتِحَهُW<ۥۤ> أَوۡ صَدِیقِكُمۡۚ لَیۡسَ عَلَیۡكُمۡ جُنَاحٌ أَN<ن> تَأۡكُلُوX<ا۟> جَمِیعًا أَوۡ أَشۡتَاتN<ࣰ>اۚ فَإِذَا دَخَلۡتُN<م> بُیُوتN<ࣰ>ا فَسَلِّمُوX<ا۟> عَلَىW<ٰۤ> أَN<ن>فُسِكُمۡ تَحِیَّةX<ࣰ> N<مِّ>نۡ عِN<ن>دِ ٱللَّهِ مُبَT<ـٰ>R<رَ>كَةN<ࣰ> طَیِّبَةN<ࣰ>ۚ كَذَT< ٰ>لِكَ یُبَیِّنُ ٱللَّهُ لَكُمُ ٱلۡءَایَT<ـٰ>تِ لَعَلَّكُمۡ تَعۡقِلُJ<و>نَ A<۝>D<٦١>",
"إِN<نَّ>مَا ٱلۡمُؤۡمِنُونَ ٱلَّذِینَ ءَامَنُوX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ> وَإِذَا كَانُوX<ا۟> مَعَهُT<ۥ> عَلَىW<ٰۤ> أَمۡرN<ࣲ> جَامِعX<ࣲ> لَّمۡ یَذۡهَبُوX<ا۟> حَتَّىٰ یَسۡتَءۡذِنُوهُۚ إِN<نَّ> ٱلَّذِینَ یَسۡتَءۡذِنُونَكَ أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ یُؤۡمِنُونَ بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ>ۚ فَإِذَا ٱسۡتَءۡذَنُوكَ لِبَعۡضِ شَأۡنِهِمۡ فَأۡذَX<ن> لِّمَN<ن> شِئۡتَ مِنۡهُمۡ وَX<ٱ>سۡتَغۡفِرۡ لَهُمُ ٱللَّهَۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٦٢>",
"لَّا تَQ<جۡ>عَلُوX<ا۟> دُعَW<اۤ>ءَ ٱX<ل>R<رَّ>سُولِ بَیۡنَكُمۡ كَدُعَW<اۤ>ءِ بَعۡضِكُN<م> بَعۡضN<ࣰ>اۚ قَQ<دۡ> یَعۡلَمُ ٱللَّهُ ٱلَّذِینَ یَتَسَلَّلُونَ مِN<ن>كُمۡ لِوَاذN<ࣰ>اۚ فَلۡیَحۡذَرِ ٱلَّذِینَ یُخَالِفُونَ عَنۡ أَمۡرِهِW<ۦۤ> أَN<ن> تُصِیبَهُمۡ فِتۡنَةٌ أَوۡ یُصِیبَهُمۡ عَذَابٌ أَلِJ<ی>مٌ A<۝>D<٦٣>",
"أَلَW<اۤ> إِN<نَّ> لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ قَQ<دۡ> یَعۡلَمُ مَW<اۤ> أَN<ن>تُمۡ عَلَیۡهِ وَیَوۡمَ یُR<رۡ>جَعُونَ إِلَیۡهِ فَیُنَبِّئُهُN<م> بِمَا عَمِلُوX<ا۟>ۗ وَٱللَّهُ بِكُلِّ شَیۡءٍ عَلِJ<ی>مُۢ A<۝>D<٦٤>",
"#۞ تَبَاR<رَ>كَ ٱلَّذِی نَزَّلَ ٱلۡفُR<رۡ>قَانَ عَلَىٰ عَQ<بۡ>دِهِT<ۦ> لِیَكُونَ لِلۡعَT<ـٰ>لَمِینَ نَذِیR<ر>ًا A<۝>D<١>",
"ٱلَّذِی لَهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَلَمۡ یَتَّخِذۡ وَلَدX<ࣰ>ا N<وَ>لَمۡ یَكُX<ن> لَّهُT<ۥ> شَرِیكN<ࣱ> فِی ٱلۡمُلۡكِ وَخَلَقَ كُلَّ شَیۡءN<ࣲ> فَقَدَّR<رَ>هُT<ۥ> تَQ<قۡ>دِیR<ر>ࣰا A<۝>D<٢>",
"وَX<ٱ>تَّخَذُوX<ا۟> مِN<ن> دُونِهِW<ۦۤ> ءَالِهَةX<ࣰ> لَّا یَخۡلُقُونَ شَیۡءX<ࣰ>ا N<وَ>هُمۡ یُخۡلَقُونَ وَلَا یَمۡلِكُونَ لِأَN<ن>فُسِهِمۡ ضَR<رّ>X<ࣰ>ا N<وَ>لَا نَفۡعX<ࣰ>ا N<وَ>لَا یَمۡلِكُونَ مَوۡتX<ࣰ>ا N<وَ>لَا حَیَX<و>T<ٰ>ةX<ࣰ> N<وَ>لَا نُشُوR<ر>ࣰا A<۝>D<٣>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّW<اۤ> إِفۡكٌ ٱفۡتَR<رَ>X<ى>T<ٰ>هُ وَأَعَانَهُT<ۥ> عَلَیۡهِ قَوۡمٌ ءَاخَR<رُ>ونَۖ فَقَQ<دۡ> جَW<اۤ>ءُو ظُلۡمX<ࣰ>ا N<وَ>زُوR<ر>ࣰا A<۝>D<٤>",
"وَقَالُW<وۤ>X<ا۟> أَسَT<ـٰ>طِیR<رُ> ٱلۡأَوَّلِینَ ٱكۡتَتَبَهَا فَهِیَ تُمۡلَىٰ عَلَیۡهِ بُكۡR<رَ>ةX<ࣰ> N<وَ>أَصِیلࣰا A<۝>D<٥>",
"قُلۡ أَN<ن>زَلَهُ ٱلَّذِی یَعۡلَمُ ٱX<ل>سِّR<رَّ> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ إِN<نَّ>هُT<ۥ> كَانَ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<٦>",
"وَقَالُوX<ا۟> مَالِ هَT<ـٰ>ذَا ٱX<ل>R<رَّ>سُولِ یَأۡكُلُ ٱX<ل>طَّعَامَ وَیَمۡشِی فِی ٱلۡأَسۡوَاقِ لَوۡلَW<اۤ> أُN<ن>زِلَ إِلَیۡهِ مَلَكN<ࣱ> فَیَكُونَ مَعَهُT<ۥ> نَذِیR<ر>ًا A<۝>D<٧>",
"أَوۡ یُلۡقَىW<ٰۤ> إِلَیۡهِ كَN<ن>زٌ أَوۡ تَكُونُ لَهُT<ۥ> جَN<نَّ>ةX<ࣱ> N<یَ>أۡكُلُ مِنۡهَاۚ وَقَالَ ٱX<ل>ظَّT<ـٰ>لِمُونَ إِN<ن> تَتَّبِعُونَ إِلَّا R<رَ>جُلX<ࣰ>ا N<مَّ>سۡحُوR<ر>ًا A<۝>D<٨>",
"ٱN<ن>ظُR<رۡ> كَیۡفَ ضَR<رَ>بُوX<ا۟> لَكَ ٱلۡأَمۡثَT<ـٰ>لَ فَضَلُّوX<ا۟> فَلَا یَسۡتَطِیعُونَ سَبِیلࣰا A<۝>D<٩>",
"تَبَاR<رَ>كَ ٱلَّذِW<یۤ> إِN<ن> شَW<اۤ>ءَ جَعَلَ لَكَ خَیۡR<ر>X<ࣰ>ا N<مِّ>N<ن> ذَT< ٰ>لِكَ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> وَیَQ<جۡ>عَل لَّكَ قُصُوR<رَ>ۢا A<۝>D<١٠>",
"بَلۡ كَذَّبُوX<ا۟> بِX<ٱ>X<ل>سَّاعَةِۖ وَأَعۡتَQ<دۡ>نَا لِمَN<ن> كَذَّبَ بِX<ٱ>X<ل>سَّاعَةِ سَعِیR<ر>ًا A<۝>D<١١>",
"إِذَا R<رَ>أَتۡهُم N<مِّ>X<ن> N<مَّ>كَانِN<ۭ> بَعِیدN<ࣲ> سَمِعُوX<ا۟> لَهَا تَغَیُّظX<ࣰ>ا N<وَ>زَفِیR<ر>ࣰا A<۝>D<١٢>",
"وَإِذَW<اۤ> أُلۡقُوX<ا۟> مِنۡهَا مَكَانN<ࣰ>ا ضَیِّقX<ࣰ>ا N<مُّ>قَR<رَّ>نِینَ دَعَوۡX<ا۟> هُنَالِكَ ثُبُوR<ر>ࣰا A<۝>D<١٣>",
"لَّا تَQ<دۡ>عُوX<ا۟> ٱلۡیَوۡمَ ثُبُوR<ر>X<ࣰ>ا N<وَ>T< ٰ>حِدX<ࣰ>ا N<وَ>X<ٱ>Q<دۡ>عُوX<ا۟> ثُبُوR<ر>N<ࣰ>ا كَثِیR<ر>ࣰا A<۝>D<١٤>",
"قُلۡ أَذَT< ٰ>لِكَ خَیۡR<ر>ٌ أَمۡ جَN<نَّ>ةُ ٱلۡخُلۡدِ ٱلَّتِی وُعِدَ ٱلۡمُتَّقُونَۚ كَانَتۡ لَهُمۡ جَزَW<اۤ>ءX<ࣰ> N<وَ>مَصِیR<ر>ࣰا A<۝>D<١٥>",
"لَّهُمۡ فِیهَا مَا یَشَW<اۤ>ءُونَ خَT<ـٰ>لِدِینَۚ كَانَ عَلَىٰ R<رَ>بِّكَ وَعۡدX<ࣰ>ا N<مَّ>سۡءُولࣰا A<۝>D<١٦>",
"وَیَوۡمَ یَحۡشُR<رُ>هُمۡ وَمَا یَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ فَیَقُولُ ءَأَN<ن>تُمۡ أَضۡلَلۡتُمۡ عِبَادِی هَW<ـٰۤ>ؤُلَW<اۤ>ءِ أَمۡ هُمۡ ضَلُّوX<ا۟> ٱX<ل>سَّبِJ<ی>لَ A<۝>D<١٧>",
"قَالُوX<ا۟> سُQ<بۡ>حَT<ـٰ>نَكَ مَا كَانَ یَX<ن>N<ۢ>بَغِی لَنَW<اۤ> أَن N<نَّ>تَّخِذَ مِN<ن> دُونِكَ مِنۡ أَوۡلِیَW<اۤ>ءَ وَلَT<ـٰ>كِX<ن> N<مَّ>تَّعۡتَهُمۡ وَءَابَW<اۤ>ءَهُمۡ حَتَّىٰ نَسُوX<ا۟> ٱX<ل>ذِّكۡR<رَ> وَكَانُوX<ا۟> قَوۡمَN<ۢ>ا بُوR<ر>ࣰا A<۝>D<١٨>",
"فَقَQ<دۡ> كَذَّبُوكُN<م> بِمَا تَقُولُونَ فَمَا تَسۡتَطِیعُونَ صَR<رۡ>فX<ࣰ>ا N<وَ>لَا نَصۡR<ر>X<ࣰ>اۚ N<وَ>مَX<ن> N<یَ>ظۡلِم N<مِّ>N<ن>كُمۡ نُذِQ<قۡ>هُ عَذَابN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<١٩>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا قَQ<بۡ>لَكَ مِنَ ٱلۡمُR<رۡ>سَلِینَ إِلَّW<اۤ> إِN<نَّ>هُمۡ لَیَأۡكُلُونَ ٱX<ل>طَّعَامَ وَیَمۡشُونَ فِی ٱلۡأَسۡوَاقِۗ وَجَعَلۡنَا بَعۡضَكُمۡ لِبَعۡضN<ࣲ> فِتۡنَةً أَتَصۡبِR<رُ>ونَۗ وَكَانَ R<رَ>بُّكَ بَصِیR<ر>ࣰا A<۝>D<٢٠>",
"۞ وَقَالَ ٱلَّذِینَ لَا یَR<رۡ>جُونَ لِقَW<اۤ>ءَنَا لَوۡلَW<اۤ> أُN<ن>زِلَ عَلَیۡنَا ٱلۡمَلَW<ـٰۤ>ئِكَةُ أَوۡ نَR<رَ>ىٰ R<رَ>بَّنَاۗ لَقَدِ ٱسۡتَكۡبَR<رُ>وX<ا۟> فِW<یۤ> أَN<ن>فُسِهِمۡ وَعَتَوۡ عُتُوّN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٢١>",
"یَوۡمَ یَR<رَ>وۡنَ ٱلۡمَلَW<ـٰۤ>ئِكَةَ لَا بُشۡR<رَ>ىٰ یَوۡمَئِذX<ࣲ> لِّلۡمُQ<جۡ>رِمِینَ وَیَقُولُونَ حِQ<جۡ>R<ر>X<ࣰ>ا N<مَّ>حۡجُوR<ر>ࣰا A<۝>D<٢٢>",
"وَقَدِمۡنَW<اۤ> إِلَىٰ مَا عَمِلُوX<ا۟> مِنۡ عَمَلN<ࣲ> فَجَعَلۡنَT<ـٰ>هُ هَبَW<اۤ>ءX<ࣰ> N<مَّ>N<ن>ثُوR<ر>ًا A<۝>D<٢٣>",
"أَصۡحَT<ـٰ>بُ ٱلۡجَN<نَّ>ةِ یَوۡمَئِذٍ خَیۡR<ر>X<ࣱ> N<مُّ>سۡتَقَR<رّ>X<ࣰ>ا N<وَ>أَحۡسَنُ مَقِیلࣰا A<۝>D<٢٤>",
"وَیَوۡمَ تَشَقَّقُ ٱX<ل>سَّمَW<اۤ>ءُ بِX<ٱ>لۡغَمَT<ـٰ>مِ وَنُزِّلَ ٱلۡمَلَW<ـٰۤ>ئِكَةُ تَN<ن>زِیلًا A<۝>D<٢٥>",
"ٱلۡمُلۡكُ یَوۡمَئِذٍ ٱلۡحَقُّ لِX<ل>R<رَّ>حۡمَT<ـٰ>نِۚ وَكَانَ یَوۡمًا عَلَى ٱلۡكَT<ـٰ>فِرِینَ عَسِیR<ر>ࣰا A<۝>D<٢٦>",
"وَیَوۡمَ یَعَضُّ ٱX<ل>ظَّالِمُ عَلَىٰ یَدَیۡهِ یَقُولُ یَT<ـٰ>لَیۡتَنِی ٱتَّخَذۡتُ مَعَ ٱX<ل>R<رَّ>سُولِ سَبِیلࣰا A<۝>D<٢٧>",
"یَT<ـٰ>وَیۡلَتَىٰ لَیۡتَنِی لَمۡ أَتَّخِذۡ فُلَانًا خَلِیلࣰا A<۝>D<٢٨>",
"لَّقَQ<دۡ> أَضَلَّنِی عَنِ ٱX<ل>ذِّكۡرِ بَعۡدَ إِذۡ جَW<اۤ>ءَنِیۗ وَكَانَ ٱX<ل>شَّیۡطَT<ـٰ>نُ لِلۡإِN<ن>سَT<ـٰ>نِ خَذُولࣰا A<۝>D<٢٩>",
"وَقَالَ ٱX<ل>R<رَّ>سُولُ یَT<ـٰ>R<رَ>بِّ إِN<نَّ> قَوۡمِی ٱتَّخَذُوX<ا۟> هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانَ مَهۡجُوR<ر>ࣰا A<۝>D<٣٠>",
"وَكَذَT< ٰ>لِكَ جَعَلۡنَا لِكُلِّ نَبِیٍّ عَدُوّX<ࣰ>ا N<مِّ>نَ ٱلۡمُQ<جۡ>رِمِینَۗ وَكَفَىٰ بِR<رَ>بِّكَ هَادِیX<ࣰ>ا N<وَ>نَصِیR<ر>ࣰا A<۝>D<٣١>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَوۡلَا نُزِّلَ عَلَیۡهِ ٱلۡقُR<رۡ>ءَانُ جُمۡلَةX<ࣰ> N<وَ>T< ٰ>حِدَةN<ࣰ>ۚ كَذَT< ٰ>لِكَ لِنُثَبِّتَ بِهِT<ۦ> فُؤَادَكَۖ وَR<رَ>تَّلۡنَT<ـٰ>هُ تَR<رۡ>تِیلࣰا A<۝>D<٣٢>",
"وَلَا یَأۡتُونَكَ بِمَثَلٍ إِلَّا جِئۡنَT<ـٰ>كَ بِX<ٱ>لۡحَقِّ وَأَحۡسَنَ تَفۡسِیR<ر>ًا A<۝>D<٣٣>",
"ٱلَّذِینَ یُحۡشَR<رُ>ونَ عَلَىٰ وُجُوهِهِمۡ إِلَىٰ جَهَN<نَّ>مَ أُX<و۟>لَW<ـٰۤ>ئِكَ شَR<رّ>X<ࣱ> N<مَّ>كَانX<ࣰ>ا N<وَ>أَضَلُّ سَبِیلࣰا A<۝>D<٣٤>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ وَجَعَلۡنَا مَعَهُW<ۥۤ> أَخَاهُ هَT<ـٰ>R<رُ>ونَ وَزِیR<ر>ࣰا A<۝>D<٣٥>",
"فَقُلۡنَا ٱذۡهَبَW<اۤ> إِلَى ٱلۡقَوۡمِ ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا فَدَN<مَّ>R<رۡ>نَT<ـٰ>هُمۡ تَQ<دۡ>مِیR<ر>ࣰا A<۝>D<٣٦>",
"وَقَوۡمَ نُوحX<ࣲ> لَّN<مَّ>ا كَذَّبُوX<ا۟> ٱX<ل>R<رُّ>سُلَ أَغۡR<رَ>Q<قۡ>نَT<ـٰ>هُمۡ وَجَعَلۡنَT<ـٰ>هُمۡ لِX<ل>N<نَّ>اسِ ءَایَةX<ࣰ>ۖ N<وَ>أَعۡتَQ<دۡ>نَا لِX<ل>ظَّT<ـٰ>لِمِینَ عَذَابًا أَلِیمࣰا A<۝>D<٣٧>",
"وَعَادX<ࣰ>ا N<وَ>ثَمُودَX<ا۟> وَأَصۡحَT<ـٰ>بَ ٱX<ل>R<رَّ>سِّ وَقُR<رُ>ونَN<ۢ>ا بَیۡنَ ذَT< ٰ>لِكَ كَثِیR<ر>ࣰا A<۝>D<٣٨>",
"وَكُلّN<ࣰ>ا ضَR<رَ>Q<بۡ>نَا لَهُ ٱلۡأَمۡثَT<ـٰ>لَۖ وَكُلّN<ࣰ>ا تَبَّR<رۡ>نَا تَتۡبِیR<ر>ࣰا A<۝>D<٣٩>",
"وَلَقَQ<دۡ> أَتَوۡX<ا۟> عَلَى ٱلۡقَR<رۡ>یَةِ ٱلَّتِW<یۤ> أُمۡطِR<رَ>تۡ مَطَR<رَ> ٱX<ل>سَّوۡءِۚ أَفَلَمۡ یَكُونُوX<ا۟> یَR<رَ>وۡنَهَاۚ بَلۡ كَانُوX<ا۟> لَا یَR<رۡ>جُونَ نُشُوR<ر>ࣰا A<۝>D<٤٠>",
"وَإِذَا R<رَ>أَوۡكَ إِX<ن> N<یَ>تَّخِذُونَكَ إِلَّا هُزُوًا أَهَT<ـٰ>ذَا ٱلَّذِی بَعَثَ ٱللَّهُ R<رَ>سُولًا A<۝>D<٤١>",
"إِN<ن> كَادَ لَیُضِلُّنَا عَنۡ ءَالِهَتِنَا لَوۡلَW<اۤ> أَN<ن> صَبَR<رۡ>نَا عَلَیۡهَاۚ وَسَوۡفَ یَعۡلَمُونَ حِینَ یَR<رَ>وۡنَ ٱلۡعَذَابَ مَنۡ أَضَلُّ سَبِیلًا A<۝>D<٤٢>",
"أَR<رَ>ءَیۡتَ مَنِ ٱتَّخَذَ إِلَT<ـٰ>هَهُT<ۥ> هَوَX<ى>T<ٰ>هُ أَفَأَN<ن>تَ تَكُونُ عَلَیۡهِ وَكِیلًا A<۝>D<٤٣>",
"أَمۡ تَحۡسَبُ أَN<نَّ> أَكۡثَR<رَ>هُمۡ یَسۡمَعُونَ أَوۡ یَعۡقِلُونَۚ إِنۡ هُمۡ إِلَّا كَX<ٱ>لۡأَنۡعَT<ـٰ>مِ بَلۡ هُمۡ أَضَلُّ سَبِیلًا A<۝>D<٤٤>",
"أَلَمۡ تَR<رَ> إِلَىٰ R<رَ>بِّكَ كَیۡفَ مَدَّ ٱX<ل>ظِّلَّ وَلَوۡ شَW<اۤ>ءَ لَجَعَلَهُT<ۥ> سَاكِنN<ࣰ>ا ثُN<مَّ> جَعَلۡنَا ٱX<ل>شَّمۡسَ عَلَیۡهِ دَلِیلࣰا A<۝>D<٤٥>",
"ثُN<مَّ> قَبَضۡنَT<ـٰ>هُ إِلَیۡنَا قَQ<بۡ>ضX<ࣰ>ا N<یَ>سِیR<ر>ࣰا A<۝>D<٤٦>",
"وَهُوَ ٱلَّذِی جَعَلَ لَكُمُ ٱلَّیۡلَ لِبَاسX<ࣰ>ا N<وَ>X<ٱ>X<ل>N<نَّ>وۡمَ سُبَاتX<ࣰ>ا N<وَ>جَعَلَ ٱX<ل>N<نَّ>هَاR<رَ> نُشُوR<ر>ࣰا A<۝>D<٤٧>",
"وَهُوَ ٱلَّذِW<یۤ> أَR<رۡ>سَلَ ٱX<ل>رِّیَT<ـٰ>حَ بُشۡR<رَ>N<ۢ>ا بَیۡنَ یَدَیۡ R<رَ>حۡمَتِهِT<ۦ>ۚ وَأَN<ن>زَلۡنَا مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> طَهُوR<ر>ࣰا A<۝>D<٤٨>",
"لِّنُحۡT<ـۧ>ِیَ بِهِT<ۦ> بَلۡدَةX<ࣰ> N<مَّ>یۡتX<ࣰ>ا N<وَ>نُسۡقِیَهُT<ۥ> مِN<مَّ>ا خَلَQ<قۡ>نَW<اۤ> أَنۡعَT<ـٰ>مX<ࣰ>ا N<وَ>أَنَاسِیَّ كَثِیR<ر>ࣰا A<۝>D<٤٩>",
"وَلَقَQ<دۡ> صَR<رَّ>فۡنَT<ـٰ>هُ بَیۡنَهُمۡ لِیَذَّكَّR<رُ>وX<ا۟> فَأَبَىW<ٰۤ> أَكۡثَR<رُ> ٱX<ل>N<نَّ>اسِ إِلَّا كُفُوR<ر>ࣰا A<۝>D<٥٠>",
"وَلَوۡ شِئۡنَا لَبَعَثۡنَا فِی كُلِّ قَR<رۡ>یَةX<ࣲ> N<نَّ>ذِیR<ر>ࣰا A<۝>D<٥١>",
"فَلَا تُطِعِ ٱلۡكَT<ـٰ>فِرِینَ وَجَT<ـٰ>هِQ<دۡ>هُN<م> بِهِT<ۦ> جِهَادN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٥٢>",
"۞ وَهُوَ ٱلَّذِی مَR<رَ>جَ ٱلۡبَحۡR<رَ>یۡنِ هَT<ـٰ>ذَا عَذۡبN<ࣱ> فُR<رَ>اتX<ࣱ> N<وَ>هَT<ـٰ>ذَا مِلۡحٌ أُجَاجX<ࣱ> N<وَ>جَعَلَ بَیۡنَهُمَا بَR<رۡ>زَخX<ࣰ>ا N<وَ>حِQ<جۡ>R<ر>X<ࣰ>ا N<مَّ>حۡجُوR<ر>ࣰا A<۝>D<٥٣>",
"وَهُوَ ٱلَّذِی خَلَقَ مِنَ ٱلۡمَW<اۤ>ءِ بَشَR<ر>N<ࣰ>ا فَجَعَلَهُT<ۥ> نَسَبX<ࣰ>ا N<وَ>صِهۡR<ر>X<ࣰ>اۗ N<وَ>كَانَ R<رَ>بُّكَ قَدِیR<ر>ࣰا A<۝>D<٥٤>",
"وَیَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ مَا لَا یَN<ن>فَعُهُمۡ وَلَا یَضُR<رُّ>هُمۡۗ وَكَانَ ٱلۡكَافِR<رُ> عَلَىٰ R<رَ>بِّهِT<ۦ> ظَهِیR<ر>ࣰا A<۝>D<٥٥>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ إِلَّا مُبَشِّR<ر>X<ࣰ>ا N<وَ>نَذِیR<ر>ࣰا A<۝>D<٥٦>",
"قُلۡ مَW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَQ<جۡ>رٍ إِلَّا مَN<ن> شَW<اۤ>ءَ أَX<ن> N<یَ>تَّخِذَ إِلَىٰ R<رَ>بِّهِT<ۦ> سَبِیلࣰا A<۝>D<٥٧>",
"وَتَوَكَّلۡ عَلَى ٱلۡحَیِّ ٱلَّذِی لَا یَمُوتُ وَسَبِّحۡ بِحَمۡدِهِT<ۦ>ۚ وَكَفَىٰ بِهِT<ۦ> بِذُنُوبِ عِبَادِهِT<ۦ> خَبِیR<ر>ًا A<۝>D<٥٨>",
"ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَمَا بَیۡنَهُمَا فِی سِتَّةِ أَیَّامN<ࣲ> ثُN<مَّ> ٱسۡتَوَىٰ عَلَى ٱلۡعَR<رۡ>شِۖ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ فَسۡءَلۡ بِهِT<ۦ> خَبِیR<ر>ࣰا A<۝>D<٥٩>",
"وَإِذَا قِیلَ لَهُمُ ٱ̅سۡ̅جُ̅دُ̅و̅X<ا۟>̅ لِX<ل>R<رَّ>حۡمَT<ـٰ>نِ قَالُوX<ا۟> وَمَا ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ أَنَسۡجُدُ لِمَا تَأۡمُR<رُ>نَا وَزَادَهُمۡ نُفُوR<ر>ࣰا A<۝>D<٦٠> ۩",
"تَبَاR<رَ>كَ ٱلَّذِی جَعَلَ فِی ٱX<ل>سَّمَW<اۤ>ءِ بُR<رُ>وجX<ࣰ>ا N<وَ>جَعَلَ فِیهَا سِR<رَ>T< ٰ>جX<ࣰ>ا N<وَ>قَمَR<ر>X<ࣰ>ا N<مُّ>نِیR<ر>ࣰا A<۝>D<٦١>",
"وَهُوَ ٱلَّذِی جَعَلَ ٱلَّیۡلَ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ> خِلۡفَةX<ࣰ> لِّمَنۡ أَR<رَ>ادَ أَX<ن> N<یَ>ذَّكَّR<رَ> أَوۡ أَR<رَ>ادَ شُكُوR<ر>ࣰا A<۝>D<٦٢>",
"وَعِبَادُ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ ٱلَّذِینَ یَمۡشُونَ عَلَى ٱلۡأَR<رۡ>ضِ هَوۡنX<ࣰ>ا N<وَ>إِذَا خَاطَبَهُمُ ٱلۡجَT<ـٰ>هِلُونَ قَالُوX<ا۟> سَلَT<ـٰ>مࣰا A<۝>D<٦٣>",
"وَX<ٱ>لَّذِینَ یَبِیتُونَ لِR<رَ>بِّهِمۡ سُجَّدX<ࣰ>ا N<وَ>قِیَT<ـٰ>مࣰا A<۝>D<٦٤>",
"وَX<ٱ>لَّذِینَ یَقُولُونَ R<رَ>بَّنَا ٱصۡرِفۡ عَN<نَّ>ا عَذَابَ جَهَN<نَّ>مَۖ إِN<نَّ> عَذَابَهَا كَانَ غَR<رَ>امًا A<۝>D<٦٥>",
"إِN<نَّ>هَا سَW<اۤ>ءَتۡ مُسۡتَقَR<رّ>X<ࣰ>ا N<وَ>مُقَامࣰا A<۝>D<٦٦>",
"وَX<ٱ>لَّذِینَ إِذَW<اۤ> أَN<ن>فَقُوX<ا۟> لَمۡ یُسۡرِفُوX<ا۟> وَلَمۡ یَQ<قۡ>تُR<رُ>وX<ا۟> وَكَانَ بَیۡنَ ذَT< ٰ>لِكَ قَوَامࣰا A<۝>D<٦٧>",
"وَX<ٱ>لَّذِینَ لَا یَQ<دۡ>عُونَ مَعَ ٱللَّهِ إِلَT<ـٰ>هًا ءَاخَR<رَ> وَلَا یَQ<قۡ>تُلُونَ ٱX<ل>N<نَّ>فۡسَ ٱلَّتِی حَR<رَّ>مَ ٱللَّهُ إِلَّا بِX<ٱ>لۡحَقِّ وَلَا یَزۡنُونَۚ وَمَX<ن> N<یَ>فۡعَلۡ ذَT< ٰ>لِكَ یَلۡقَ أَثَامࣰا A<۝>D<٦٨>",
"یُضَT<ـٰ>عَفۡ لَهُ ٱلۡعَذَابُ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ وَیَخۡلُQ<دۡ> فِیهِT<ۦ> مُهَانًا A<۝>D<٦٩>",
"إِلَّا مَN<ن> تَابَ وَءَامَنَ وَعَمِلَ عَمَلN<ࣰ>ا صَT<ـٰ>لِحN<ࣰ>ا فَأُX<و۟>لَW<ـٰۤ>ئِكَ یُبَدِّلُ ٱللَّهُ سَیِّءَاتِهِمۡ حَسَنَT<ـٰ>تX<ࣲ>ۗ N<وَ>كَانَ ٱللَّهُ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<٧٠>",
"وَمَN<ن> تَابَ وَعَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَإِN<نَّ>هُT<ۥ> یَتُوبُ إِلَى ٱللَّهِ مَتَابࣰا A<۝>D<٧١>",
"وَX<ٱ>لَّذِینَ لَا یَشۡهَدُونَ ٱX<ل>زُّوR<رَ> وَإِذَا مَR<رُّ>وX<ا۟> بِX<ٱ>X<ل>لَّغۡوِ مَR<رُّ>وX<ا۟> كِR<رَ>امࣰا A<۝>D<٧٢>",
"وَX<ٱ>لَّذِینَ إِذَا ذُكِّR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِ R<رَ>بِّهِمۡ لَمۡ یَخِR<رُّ>وX<ا۟> عَلَیۡهَا صُN<مّ>X<ࣰ>ا N<وَ>عُمۡیَانࣰا A<۝>D<٧٣>",
"وَX<ٱ>لَّذِینَ یَقُولُونَ R<رَ>بَّنَا هَQ<بۡ> لَنَا مِنۡ أَزۡوَT< ٰ>جِنَا وَذُرِّیَّT<ـٰ>تِنَا قُR<رَّ>ةَ أَعۡیُنX<ࣲ> N<وَ>X<ٱ>Q<جۡ>عَلۡنَا لِلۡمُتَّقِینَ إِمَامًا A<۝>D<٧٤>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ یُQ<جۡ>زَوۡنَ ٱلۡغُR<رۡ>فَةَ بِمَا صَبَR<رُ>وX<ا۟> وَیُلَقَّوۡنَ فِیهَا تَحِیَّةX<ࣰ> N<وَ>سَلَT<ـٰ>مًا A<۝>D<٧٥>",
"خَT<ـٰ>لِدِینَ فِیهَاۚ حَسُنَتۡ مُسۡتَقَR<رّ>X<ࣰ>ا N<وَ>مُقَامࣰا A<۝>D<٧٦>",
"قُلۡ مَا یَعۡبَؤُX<ا۟> بِكُمۡ R<رَ>بِّی لَوۡلَا دُعَW<اۤ>ؤُكُمۡۖ فَقَQ<دۡ> كَذَّQ<بۡ>تُمۡ فَسَوۡفَ یَكُونُ لِزَامَۢا A<۝>D<٧٧>",
"#۞ طL<سۤ>L<مۤ> A<۝>D<١>",
"تِلۡكَ ءَایَT<ـٰ>تُ ٱلۡكِتَT<ـٰ>بِ ٱلۡمُبِJ<ی>نِ A<۝>D<٢>",
"لَعَلَّكَ بَT<ـٰ>خِعX<ࣱ> N<نَّ>فۡسَكَ أَلَّا یَكُونُوX<ا۟> مُؤۡمِنِJ<ی>نَ A<۝>D<٣>",
"إِن N<نَّ>شَأۡ نُنَزِّلۡ عَلَیۡهِم N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ ءَایَةN<ࣰ> فَظَلَّتۡ أَعۡنَT<ـٰ>قُهُمۡ لَهَا خَT<ـٰ>ضِعِJ<ی>نَ A<۝>D<٤>",
"وَمَا یَأۡتِیهِم N<مِّ>N<ن> ذِكۡرX<ࣲ> N<مِّ>نَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ مُحۡدَثٍ إِلَّا كَانُوX<ا۟> عَنۡهُ مُعۡرِضِJ<ی>نَ A<۝>D<٥>",
"فَقَQ<دۡ> كَذَّبُوX<ا۟> فَسَیَأۡتِیهِمۡ أَX<ن>N<ۢ>بَW<ـٰۤ>ؤُX<ا۟> مَا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٦>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> إِلَى ٱلۡأَR<رۡ>ضِ كَمۡ أَX<ن>N<ۢ>بَتۡنَا فِیهَا مِN<ن> كُلِّ زَوۡجN<ࣲ> كَرِJ<ی>مٍ A<۝>D<٧>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ>ۖ N<وَ>مَا كَانَ أَكۡثَR<رُ>هُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٨>",
"وَإِN<نَّ> R<رَ>بَّكَ لَهُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٩>",
"وَإِذۡ نَادَىٰ R<رَ>بُّكَ مُوسَىW<ٰۤ> أَنِ ٱئۡتِ ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٠>",
"قَوۡمَ فِرۡعَوۡنَۚ أَلَا یَتَّقُJ<و>نَ A<۝>D<١١>",
"قَالَ R<رَ>بِّ إِN<نِّ>W<یۤ> أَخَافُ أَX<ن> N<یُ>كَذِّبُJ<و>نِ A<۝>D<١٢>",
"وَیَضِیقُ صَQ<دۡ>رِی وَلَا یَN<ن>طَلِقُ لِسَانِی فَأَR<رۡ>سِلۡ إِلَىٰ هَT<ـٰ>R<رُ>J<و>نَ A<۝>D<١٣>",
"وَلَهُمۡ عَلَیَّ ذَX<ن>N<ۢ>بN<ࣱ> فَأَخَافُ أَX<ن> N<یَ>Q<قۡ>تُلُJ<و>نِ A<۝>D<١٤>",
"قَالَ كَلَّاۖ فَX<ٱ>ذۡهَبَا بِءَایَT<ـٰ>تِنَW<اۤ>ۖ إِN<نَّ>ا مَعَكُم N<مُّ>سۡتَمِعُJ<و>نَ A<۝>D<١٥>",
"فَأۡتِیَا فِرۡعَوۡنَ فَقُولَW<اۤ> إِN<نَّ>ا R<رَ>سُولُ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٦>",
"أَنۡ أَR<رۡ>سِلۡ مَعَنَا بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِJ<ی>لَ A<۝>D<١٧>",
"قَالَ أَلَمۡ نُR<رَ>بِّكَ فِینَا وَلِیدX<ࣰ>ا N<وَ>لَبِثۡتَ فِینَا مِنۡ عُمُرِكَ سِنِJ<ی>نَ A<۝>D<١٨>",
"وَفَعَلۡتَ فَعۡلَتَكَ ٱلَّتِی فَعَلۡتَ وَأَN<ن>تَ مِنَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٩>",
"قَالَ فَعَلۡتُهَW<اۤ> إِذX<ࣰ>ا N<وَ>أَنَا۠ مِنَ ٱX<ل>ضَّL<اۤ>لِّJ<ی>نَ A<۝>D<٢٠>",
"فَفَR<رَ>R<رۡ>تُ مِN<ن>كُمۡ لَN<مَّ>ا خِفۡتُكُمۡ فَوَهَبَ لِی R<رَ>بِّی حُكۡمX<ࣰ>ا N<وَ>جَعَلَنِی مِنَ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٢١>",
"وَتِلۡكَ نِعۡمَةN<ࣱ> تَمُN<نُّ>هَا عَلَیَّ أَنۡ عَبَّX<د>تَّ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِJ<ی>لَ A<۝>D<٢٢>",
"قَالَ فِرۡعَوۡنُ وَمَا R<رَ>بُّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٢٣>",
"قَالَ R<رَ>بُّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَW<اۤ>ۖ إِN<ن> كُN<ن>تُم N<مُّ>وقِنِJ<ی>نَ A<۝>D<٢٤>",
"قَالَ لِمَنۡ حَوۡلَهُW<ۥۤ> أَلَا تَسۡتَمِعُJ<و>نَ A<۝>D<٢٥>",
"قَالَ R<رَ>بُّكُمۡ وَR<رَ>بُّ ءَابَW<اۤ>ئِكُمُ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٢٦>",
"قَالَ إِN<نَّ> R<رَ>سُولَكُمُ ٱلَّذِW<یۤ> أُR<رۡ>سِلَ إِلَیۡكُمۡ لَمَQ<جۡ>نُJ<و>نࣱ A<۝>D<٢٧>",
"قَالَ R<رَ>بُّ ٱلۡمَشۡرِقِ وَX<ٱ>لۡمَغۡرِبِ وَمَا بَیۡنَهُمَW<اۤ>ۖ إِN<ن> كُN<ن>تُمۡ تَعۡقِلُJ<و>نَ A<۝>D<٢٨>",
"قَالَ لَئِنِ ٱتَّخَذۡتَ إِلَT<ـٰ>هًا غَیۡرِی لَأَQ<جۡ>عَلَN<نَّ>كَ مِنَ ٱلۡمَسۡجُونِJ<ی>نَ A<۝>D<٢٩>",
"قَالَ أَوَ لَوۡ جِئۡتُكَ بِشَیۡءX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٣٠>",
"قَالَ فَأۡتِ بِهِW<ۦۤ> إِN<ن> كُN<ن>تَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٣١>",
"فَأَلۡقَىٰ عَصَاهُ فَإِذَا هِیَ ثُعۡبَانX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٣٢>",
"وَنَزَعَ یَدَهُT<ۥ> فَإِذَا هِیَ بَیۡضَW<اۤ>ءُ لِX<ل>N<نَّ>T<ـٰ>ظِرِJ<ی>نَ A<۝>D<٣٣>",
"قَالَ لِلۡمَلَإِ حَوۡلَهُW<ۥۤ> إِN<نَّ> هَT<ـٰ>ذَا لَسَT<ـٰ>حِR<ر>ٌ عَلِJ<ی>مࣱ A<۝>D<٣٤>",
"یُرِیدُ أَX<ن> N<یُ>خۡرِجَكُم N<مِّ>نۡ أَR<رۡ>ضِكُN<م> بِسِحۡرِهِT<ۦ> فَمَاذَا تَأۡمُR<رُ>J<و>نَ A<۝>D<٣٥>",
"قَالُW<وۤ>X<ا۟> أَR<رۡ>جِهۡ وَأَخَاهُ وَX<ٱ>Q<بۡ>عَثۡ فِی ٱلۡمَدَW<اۤ>ئِنِ حَT<ـٰ>شِرِJ<ی>نَ A<۝>D<٣٦>",
"یَأۡتُوكَ بِكُلِّ سَحَّارٍ عَلِJ<ی>مࣲ A<۝>D<٣٧>",
"فَجُمِعَ ٱX<ل>سَّحَR<رَ>ةُ لِمِیقَT<ـٰ>تِ یَوۡمX<ࣲ> N<مَّ>عۡلُJ<و>مࣲ A<۝>D<٣٨>",
"وَقِیلَ لِX<ل>N<نَّ>اسِ هَلۡ أَN<ن>تُم N<مُّ>Q<جۡ>تَمِعُJ<و>نَ A<۝>D<٣٩>",
"لَعَلَّنَا نَتَّبِعُ ٱX<ل>سَّحَR<رَ>ةَ إِN<ن> كَانُوX<ا۟> هُمُ ٱلۡغَT<ـٰ>لِبِJ<ی>نَ A<۝>D<٤٠>",
"فَلَN<مَّ>ا جَW<اۤ>ءَ ٱX<ل>سَّحَR<رَ>ةُ قَالُوX<ا۟> لِفِرۡعَوۡنَ أَئِN<نَّ> لَنَا لَأَQ<جۡ>R<ر>ًا إِN<ن> كُN<نَّ>ا نَحۡنُ ٱلۡغَT<ـٰ>لِبِJ<ی>نَ A<۝>D<٤١>",
"قَالَ نَعَمۡ وَإِN<نَّ>كُمۡ إِذX<ࣰ>ا لَّمِنَ ٱلۡمُقَR<رَّ>بِJ<ی>نَ A<۝>D<٤٢>",
"قَالَ لَهُم N<مُّ>وسَىW<ٰۤ> أَلۡقُوX<ا۟> مَW<اۤ> أَN<ن>تُم N<مُّ>لۡقُJ<و>نَ A<۝>D<٤٣>",
"فَأَلۡقَوۡX<ا۟> حِبَالَهُمۡ وَعِصِیَّهُمۡ وَقَالُوX<ا۟> بِعِزَّةِ فِرۡعَوۡنَ إِN<نَّ>ا لَنَحۡنُ ٱلۡغَT<ـٰ>لِبُJ<و>نَ A<۝>D<٤٤>",
"فَأَلۡقَىٰ مُوسَىٰ عَصَاهُ فَإِذَا هِیَ تَلۡقَفُ مَا یَأۡفِكُJ<و>نَ A<۝>D<٤٥>",
"فَأُلۡقِیَ ٱX<ل>سَّحَR<رَ>ةُ سَT<ـٰ>جِدِJ<ی>نَ A<۝>D<٤٦>",
"قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا بِR<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٤٧>",
"R<رَ>بِّ مُوسَىٰ وَهَT<ـٰ>R<رُ>J<و>نَ A<۝>D<٤٨>",
"قَالَ ءَامَN<ن>تُمۡ لَهُT<ۥ> قَQ<بۡ>لَ أَنۡ ءَاذَنَ لَكُمۡۖ إِN<نَّ>هُT<ۥ> لَكَبِیR<رُ>كُمُ ٱلَّذِی عَلَّمَكُمُ ٱX<ل>سِّحۡR<رَ> فَلَسَوۡفَ تَعۡلَمُونَۚ لَأُقَطِّعَN<نَّ> أَیۡدِیَكُمۡ وَأَR<رۡ>جُلَكُم N<مِّ>نۡ خِلَT<ـٰ>فX<ࣲ> N<وَ>لَأُصَلِّبَN<نَّ>كُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٤٩>",
"قَالُوX<ا۟> لَا ضَیۡR<رَ>ۖ إِN<نَّ>W<اۤ> إِلَىٰ R<رَ>بِّنَا مُN<ن>قَلِبُJ<و>نَ A<۝>D<٥٠>",
"إِN<نَّ>ا نَQ<طۡ>مَعُ أَX<ن> N<یَ>غۡفِR<رَ> لَنَا R<رَ>بُّنَا خَطَT<ـٰ>یَT<ـٰ>نَW<اۤ> أَN<ن> كُN<نَّ>W<اۤ> أَوَّلَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٥١>",
"۞ وَأَوۡحَیۡنَW<اۤ> إِلَىٰ مُوسَىW<ٰۤ> أَنۡ أَسۡرِ بِعِبَادِW<یۤ> إِN<نَّ>كُم N<مُّ>تَّبَعُJ<و>نَ A<۝>D<٥٢>",
"فَأَR<رۡ>سَلَ فِرۡعَوۡنُ فِی ٱلۡمَدَW<اۤ>ئِنِ حَT<ـٰ>شِرِJ<ی>نَ A<۝>D<٥٣>",
"إِN<نَّ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ لَشِرۡذِمَةN<ࣱ> قَلِیلُJ<و>نَ A<۝>D<٥٤>",
"وَإِN<نَّ>هُمۡ لَنَا لَغَW<اۤ>ئِظُJ<و>نَ A<۝>D<٥٥>",
"وَإِN<نَّ>ا لَجَمِیعٌ حَT<ـٰ>ذِR<رُ>J<و>نَ A<۝>D<٥٦>",
"فَأَخۡR<رَ>Q<جۡ>نَT<ـٰ>هُم N<مِّ>N<ن> جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>عُیُJ<و>نࣲ A<۝>D<٥٧>",
"وَكُنُوزX<ࣲ> N<وَ>مَقَامN<ࣲ> كَرِJ<ی>مࣲ A<۝>D<٥٨>",
"كَذَT< ٰ>لِكَۖ وَأَوۡR<رَ>ثۡنَT<ـٰ>هَا بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِJ<ی>لَ A<۝>D<٥٩>",
"فَأَتۡبَعُوهُم N<مُّ>شۡرِقِJ<ی>نَ A<۝>D<٦٠>",
"فَلَN<مَّ>ا تَR<رَ>W< ٰۤ>ءَا ٱلۡجَمۡعَانِ قَالَ أَصۡحَT<ـٰ>بُ مُوسَىW<ٰۤ> إِN<نَّ>ا لَمُQ<دۡ>R<رَ>كُJ<و>نَ A<۝>D<٦١>",
"قَالَ كَلَّW<اۤ>ۖ إِN<نَّ> مَعِیَ R<رَ>بِّی سَیَهۡدِJ<ی>نِ A<۝>D<٦٢>",
"فَأَوۡحَیۡنَW<اۤ> إِلَىٰ مُوسَىW<ٰۤ> أَنِ ٱضۡرِب بِّعَصَاكَ ٱلۡبَحۡR<رَ>ۖ فَX<ٱ>N<ن>فَلَقَ فَكَانَ كُلُّ فِرۡقN<ࣲ> كَX<ٱ>X<ل>طَّوۡدِ ٱلۡعَظِJ<ی>مِ A<۝>D<٦٣>",
"وَأَزۡلَفۡنَا ثَN<مَّ> ٱلۡءَاخَرِJ<ی>نَ A<۝>D<٦٤>",
"وَأَN<ن>جَیۡنَا مُوسَىٰ وَمَX<ن> N<مَّ>عَهُW<ۥۤ> أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٦٥>",
"ثُN<مَّ> أَغۡR<رَ>Q<قۡ>نَا ٱلۡءَاخَرِJ<ی>نَ A<۝>D<٦٦>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ>ۖ N<وَ>مَا كَانَ أَكۡثَR<رُ>هُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٦٧>",
"وَإِN<نَّ> R<رَ>بَّكَ لَهُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٦٨>",
"وَX<ٱ>تۡلُ عَلَیۡهِمۡ نَبَأَ إِQ<بۡ>R<رَ>T< ٰ>هِJ<ی>مَ A<۝>D<٦٩>",
"إِذۡ قَالَ لِأَبِیهِ وَقَوۡمِهِT<ۦ> مَا تَعۡبُدُJ<و>نَ A<۝>D<٧٠>",
"قَالُوX<ا۟> نَعۡبُدُ أَصۡنَامN<ࣰ>ا فَنَظَلُّ لَهَا عَT<ـٰ>كِفِJ<ی>نَ A<۝>D<٧١>",
"قَالَ هَلۡ یَسۡمَعُونَكُمۡ إِذۡ تَQ<دۡ>عُJ<و>نَ A<۝>D<٧٢>",
"أَوۡ یَN<ن>فَعُونَكُمۡ أَوۡ یَضُR<رُّ>J<و>نَ A<۝>D<٧٣>",
"قَالُوX<ا۟> بَلۡ وَجَQ<دۡ>نَW<اۤ> ءَابَW<اۤ>ءَنَا كَذَT< ٰ>لِكَ یَفۡعَلُJ<و>نَ A<۝>D<٧٤>",
"قَالَ أَفَR<رَ>ءَیۡتُم N<مَّ>ا كُN<ن>تُمۡ تَعۡبُدُJ<و>نَ A<۝>D<٧٥>",
"أَN<ن>تُمۡ وَءَابَW<اۤ>ؤُكُمُ ٱلۡأَQ<قۡ>دَمُJ<و>نَ A<۝>D<٧٦>",
"فَإِN<نَّ>هُمۡ عَدُوّX<ࣱ> لِّW<یۤ> إِلَّا R<رَ>بَّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٧٧>",
"ٱلَّذِی خَلَقَنِی فَهُوَ یَهۡدِJ<ی>نِ A<۝>D<٧٨>",
"وَX<ٱ>لَّذِی هُوَ یُQ<طۡ>عِمُنِی وَیَسۡقِJ<ی>نِ A<۝>D<٧٩>",
"وَإِذَا مَرِضۡتُ فَهُوَ یَشۡفِJ<ی>نِ A<۝>D<٨٠>",
"وَX<ٱ>لَّذِی یُمِیتُنِی ثُN<مَّ> یُحۡیِJ<ی>نِ A<۝>D<٨١>",
"وَX<ٱ>لَّذِW<یۤ> أَQ<طۡ>مَعُ أَX<ن> N<یَ>غۡفِR<رَ> لِی خَطِW<یۤ>ءَتِی یَوۡمَ ٱX<ل>دِّJ<ی>نِ A<۝>D<٨٢>",
"R<رَ>بِّ هَQ<بۡ> لِی حُكۡمX<ࣰ>ا N<وَ>أَلۡحِQ<قۡ>نِی بِX<ٱ>X<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٨٣>",
"وَX<ٱ>Q<جۡ>عَل لِّی لِسَانَ صِQ<دۡ>قN<ࣲ> فِی ٱلۡءَاخِرِJ<ی>نَ A<۝>D<٨٤>",
"وَX<ٱ>Q<جۡ>عَلۡنِی مِX<ن> N<وَ>R<رَ>ثَةِ جَN<نَّ>ةِ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<٨٥>",
"وَX<ٱ>غۡفِرۡ لِأَبِW<یۤ> إِN<نَّ>هُT<ۥ> كَانَ مِنَ ٱX<ل>ضَّL<اۤ>لِّJ<ی>نَ A<۝>D<٨٦>",
"وَلَا تُخۡزِنِی یَوۡمَ یُQ<بۡ>عَثُJ<و>نَ A<۝>D<٨٧>",
"یَوۡمَ لَا یَN<ن>فَعُ مَالX<ࣱ> N<وَ>لَا بَنُJ<و>نَ A<۝>D<٨٨>",
"إِلَّا مَنۡ أَتَى ٱللَّهَ بِقَلۡبN<ࣲ> سَلِJ<ی>مࣲ A<۝>D<٨٩>",
"وَأُزۡلِفَتِ ٱلۡجَN<نَّ>ةُ لِلۡمُتَّقِJ<ی>نَ A<۝>D<٩٠>",
"وَبُرِّزَتِ ٱلۡجَحِیمُ لِلۡغَاوِJ<ی>نَ A<۝>D<٩١>",
"وَقِیلَ لَهُمۡ أَیۡنَ مَا كُN<ن>تُمۡ تَعۡبُدُJ<و>نَ A<۝>D<٩٢>",
"مِN<ن> دُونِ ٱللَّهِ هَلۡ یَN<ن>صُR<رُ>ونَكُمۡ أَوۡ یَN<ن>تَصِR<رُ>J<و>نَ A<۝>D<٩٣>",
"فَكُQ<بۡ>كِبُوX<ا۟> فِیهَا هُمۡ وَX<ٱ>لۡغَاوُT<ۥ>نَ A<۝>D<٩٤>",
"وَجُنُودُ إِQ<بۡ>لِیسَ أَQ<جۡ>مَعُJ<و>نَ A<۝>D<٩٥>",
"قَالُوX<ا۟> وَهُمۡ فِیهَا یَخۡتَصِمُJ<و>نَ A<۝>D<٩٦>",
"تَٱللَّهِ إِN<ن> كُN<نَّ>ا لَفِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٩٧>",
"إِذۡ نُسَوِّیكُN<م> بِR<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٩٨>",
"وَمَW<اۤ> أَضَلَّنَW<اۤ> إِلَّا ٱلۡمُQ<جۡ>رِمُJ<و>نَ A<۝>D<٩٩>",
"فَمَا لَنَا مِN<ن> شَT<ـٰ>فِعِJ<ی>نَ A<۝>D<١٠٠>",
"وَلَا صَدِیقٍ حَمِJ<ی>مࣲ A<۝>D<١٠١>",
"فَلَوۡ أَN<نَّ> لَنَا كَR<رَّ>ةN<ࣰ> فَنَكُونَ مِنَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٠٢>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ>ۖ N<وَ>مَا كَانَ أَكۡثَR<رُ>هُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٠٣>",
"وَإِN<نَّ> R<رَ>بَّكَ لَهُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٠٤>",
"كَذَّبَتۡ قَوۡمُ نُوحٍ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٠٥>",
"إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ نُوحٌ أَلَا تَتَّقُJ<و>نَ A<۝>D<١٠٦>",
"إِN<نِّ>ی لَكُمۡ R<رَ>سُولٌ أَمِJ<ی>نࣱ A<۝>D<١٠٧>",
"فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<١٠٨>",
"وَمَW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَQ<جۡ>رٍۖ إِنۡ أَQ<جۡ>رِیَ إِلَّا عَلَىٰ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٠٩>",
"فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<١١٠>",
"۞ قَالُW<وۤ>X<ا۟> أَنُؤۡمِنُ لَكَ وَX<ٱ>تَّبَعَكَ ٱلۡأَR<رۡ>ذَلُJ<و>نَ A<۝>D<١١١>",
"قَالَ وَمَا عِلۡمِی بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١١٢>",
"إِنۡ حِسَابُهُمۡ إِلَّا عَلَىٰ R<رَ>بِّیۖ لَوۡ تَشۡعُR<رُ>J<و>نَ A<۝>D<١١٣>",
"وَمَW<اۤ> أَنَا۠ بِطَارِدِ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١١٤>",
"إِنۡ أَنَا۠ إِلَّا نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١١٥>",
"قَالُوX<ا۟> لَئِX<ن> لَّمۡ تَN<ن>تَهِ یَT<ـٰ>نُوحُ لَتَكُونَN<نَّ> مِنَ ٱلۡمَR<رۡ>جُومِJ<ی>نَ A<۝>D<١١٦>",
"قَالَ R<رَ>بِّ إِN<نَّ> قَوۡمِی كَذَّبُJ<و>نِ A<۝>D<١١٧>",
"فَX<ٱ>فۡتَحۡ بَیۡنِی وَبَیۡنَهُمۡ فَتۡحX<ࣰ>ا N<وَ>نَجِّنِی وَمَX<ن> N<مَّ>عِیَ مِنَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١١٨>",
"فَأَN<ن>جَیۡنَT<ـٰ>هُ وَمَX<ن> N<مَّ>عَهُT<ۥ> فِی ٱلۡفُلۡكِ ٱلۡمَشۡحُJ<و>نِ A<۝>D<١١٩>",
"ثُN<مَّ> أَغۡR<رَ>Q<قۡ>نَا بَعۡدُ ٱلۡبَاقِJ<ی>نَ A<۝>D<١٢٠>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ>ۖ N<وَ>مَا كَانَ أَكۡثَR<رُ>هُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٢١>",
"وَإِN<نَّ> R<رَ>بَّكَ لَهُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٢٢>",
"كَذَّبَتۡ عَادٌ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٢٣>",
"إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ هُودٌ أَلَا تَتَّقُJ<و>نَ A<۝>D<١٢٤>",
"إِN<نِّ>ی لَكُمۡ R<رَ>سُولٌ أَمِJ<ی>نࣱ A<۝>D<١٢٥>",
"فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<١٢٦>",
"وَمَW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَQ<جۡ>رٍۖ إِنۡ أَQ<جۡ>رِیَ إِلَّا عَلَىٰ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٢٧>",
"أَتَQ<بۡ>نُونَ بِكُلِّ رِیعٍ ءَایَةN<ࣰ> تَعۡبَثُJ<و>نَ A<۝>D<١٢٨>",
"وَتَتَّخِذُونَ مَصَانِعَ لَعَلَّكُمۡ تَخۡلُدُJ<و>نَ A<۝>D<١٢٩>",
"وَإِذَا بَطَشۡتُN<م> بَطَشۡتُمۡ جَبَّارِJ<ی>نَ A<۝>D<١٣٠>",
"فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<١٣١>",
"وَX<ٱ>تَّقُوX<ا۟> ٱلَّذِW<یۤ> أَمَدَّكُN<م> بِمَا تَعۡلَمُJ<و>نَ A<۝>D<١٣٢>",
"أَمَدَّكُN<م> بِأَنۡعَT<ـٰ>مX<ࣲ> N<وَ>بَنِJ<ی>نَ A<۝>D<١٣٣>",
"وَجَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>عُیُJ<و>نٍ A<۝>D<١٣٤>",
"إِN<نِّ>W<یۤ> أَخَافُ عَلَیۡكُمۡ عَذَابَ یَوۡمٍ عَظِJ<ی>مࣲ A<۝>D<١٣٥>",
"قَالُوX<ا۟> سَوَW<اۤ>ءٌ عَلَیۡنَW<اۤ> أَوَعَظۡتَ أَمۡ لَمۡ تَكُX<ن> N<مِّ>نَ ٱلۡوَT< ٰ>عِظِJ<ی>نَ A<۝>D<١٣٦>",
"إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا خُلُقُ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٣٧>",
"وَمَا نَحۡنُ بِمُعَذَّبِJ<ی>نَ A<۝>D<١٣٨>",
"فَكَذَّبُوهُ فَأَهۡلَكۡنَT<ـٰ>هُمۡۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ>ۖ N<وَ>مَا كَانَ أَكۡثَR<رُ>هُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٣٩>",
"وَإِN<نَّ> R<رَ>بَّكَ لَهُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٤٠>",
"كَذَّبَتۡ ثَمُودُ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٤١>",
"إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ صَT<ـٰ>لِحٌ أَلَا تَتَّقُJ<و>نَ A<۝>D<١٤٢>",
"إِN<نِّ>ی لَكُمۡ R<رَ>سُولٌ أَمِJ<ی>نࣱ A<۝>D<١٤٣>",
"فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<١٤٤>",
"وَمَW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَQ<جۡ>رٍۖ إِنۡ أَQ<جۡ>رِیَ إِلَّا عَلَىٰ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٤٥>",
"أَتُتۡR<رَ>كُونَ فِی مَا هَT<ـٰ>هُنَW<اۤ> ءَامِنِJ<ی>نَ A<۝>D<١٤٦>",
"فِی جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>عُیُJ<و>نࣲ A<۝>D<١٤٧>",
"وَزُR<رُ>وعX<ࣲ> N<وَ>نَخۡلN<ࣲ> طَلۡعُهَا هَضِJ<ی>مࣱ A<۝>D<١٤٨>",
"وَتَنۡحِتُونَ مِنَ ٱلۡجِبَالِ بُیُوتN<ࣰ>ا فَT<ـٰ>رِهِJ<ی>نَ A<۝>D<١٤٩>",
"فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<١٥٠>",
"وَلَا تُطِیعُW<وۤ>X<ا۟> أَمۡR<رَ> ٱلۡمُسۡرِفِJ<ی>نَ A<۝>D<١٥١>",
"ٱلَّذِینَ یُفۡسِدُونَ فِی ٱلۡأَR<رۡ>ضِ وَلَا یُصۡلِحُJ<و>نَ A<۝>D<١٥٢>",
"قَالُW<وۤ>X<ا۟> إِN<نَّ>مَW<اۤ> أَN<ن>تَ مِنَ ٱلۡمُسَحَّرِJ<ی>نَ A<۝>D<١٥٣>",
"مَW<اۤ> أَN<ن>تَ إِلَّا بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُنَا فَأۡتِ بِءَایَةٍ إِN<ن> كُN<ن>تَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٥٤>",
"قَالَ هَT<ـٰ>ذِهِT<ۦ> نَاقَةX<ࣱ> لَّهَا شِرۡبX<ࣱ> N<وَ>لَكُمۡ شِرۡبُ یَوۡمX<ࣲ> N<مَّ>عۡلُJ<و>مࣲ A<۝>D<١٥٥>",
"وَلَا تَمَسُّوهَا بِسُW<وۤ>ءN<ࣲ> فَیَأۡخُذَكُمۡ عَذَابُ یَوۡمٍ عَظِJ<ی>مࣲ A<۝>D<١٥٦>",
"فَعَقَR<رُ>وهَا فَأَصۡبَحُوX<ا۟> نَT<ـٰ>دِمِJ<ی>نَ A<۝>D<١٥٧>",
"فَأَخَذَهُمُ ٱلۡعَذَابُۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ>ۖ N<وَ>مَا كَانَ أَكۡثَR<رُ>هُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٥٨>",
"وَإِN<نَّ> R<رَ>بَّكَ لَهُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٥٩>",
"كَذَّبَتۡ قَوۡمُ لُوطٍ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٦٠>",
"إِذۡ قَالَ لَهُمۡ أَخُوهُمۡ لُوطٌ أَلَا تَتَّقُJ<و>نَ A<۝>D<١٦١>",
"إِN<نِّ>ی لَكُمۡ R<رَ>سُولٌ أَمِJ<ی>نࣱ A<۝>D<١٦٢>",
"فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<١٦٣>",
"وَمَW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَQ<جۡ>رٍۖ إِنۡ أَQ<جۡ>رِیَ إِلَّا عَلَىٰ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٦٤>",
"أَتَأۡتُونَ ٱX<ل>ذُّكۡR<رَ>انَ مِنَ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٦٥>",
"وَتَذَR<رُ>ونَ مَا خَلَقَ لَكُمۡ R<رَ>بُّكُم N<مِّ>نۡ أَزۡوَT< ٰ>جِكُN<م>ۚ بَلۡ أَN<ن>تُمۡ قَوۡمٌ عَادُJ<و>نَ A<۝>D<١٦٦>",
"قَالُوX<ا۟> لَئِX<ن> لَّمۡ تَN<ن>تَهِ یَT<ـٰ>لُوطُ لَتَكُونَN<نَّ> مِنَ ٱلۡمُخۡR<رَ>جِJ<ی>نَ A<۝>D<١٦٧>",
"قَالَ إِN<نِّ>ی لِعَمَلِكُم N<مِّ>نَ ٱلۡقَالِJ<ی>نَ A<۝>D<١٦٨>",
"R<رَ>بِّ نَجِّنِی وَأَهۡلِی مِN<مَّ>ا یَعۡمَلُJ<و>نَ A<۝>D<١٦٩>",
"فَنَجَّیۡنَT<ـٰ>هُ وَأَهۡلَهُW<ۥۤ> أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<١٧٠>",
"إِلَّا عَجُوزN<ࣰ>ا فِی ٱلۡغَT<ـٰ>بِرِJ<ی>نَ A<۝>D<١٧١>",
"ثُN<مَّ> دَN<مَّ>R<رۡ>نَا ٱلۡءَاخَرِJ<ی>نَ A<۝>D<١٧٢>",
"وَأَمۡطَR<رۡ>نَا عَلَیۡهِم N<مَّ>طَR<ر>N<ࣰ>اۖ فَسَW<اۤ>ءَ مَطَR<رُ> ٱلۡمُN<ن>ذَرِJ<ی>نَ A<۝>D<١٧٣>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ>ۖ N<وَ>مَا كَانَ أَكۡثَR<رُ>هُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٧٤>",
"وَإِN<نَّ> R<رَ>بَّكَ لَهُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٧٥>",
"كَذَّبَ أَصۡحَT<ـٰ>بُ لۡءَیۡكَةِ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٧٦>",
"إِذۡ قَالَ لَهُمۡ شُعَیۡبٌ أَلَا تَتَّقُJ<و>نَ A<۝>D<١٧٧>",
"إِN<نِّ>ی لَكُمۡ R<رَ>سُولٌ أَمِJ<ی>نࣱ A<۝>D<١٧٨>",
"فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<١٧٩>",
"وَمَW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَQ<جۡ>رٍۖ إِنۡ أَQ<جۡ>رِیَ إِلَّا عَلَىٰ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٨٠>",
"۞ أَوۡفُوX<ا۟> ٱلۡكَیۡلَ وَلَا تَكُونُوX<ا۟> مِنَ ٱلۡمُخۡسِرِJ<ی>نَ A<۝>D<١٨١>",
"وَزِنُوX<ا۟> بِX<ٱ>لۡقِسۡطَاسِ ٱلۡمُسۡتَقِJ<ی>مِ A<۝>D<١٨٢>",
"وَلَا تَQ<بۡ>خَسُوX<ا۟> ٱX<ل>N<نَّ>اسَ أَشۡیَW<اۤ>ءَهُمۡ وَلَا تَعۡثَوۡX<ا۟> فِی ٱلۡأَR<رۡ>ضِ مُفۡسِدِJ<ی>نَ A<۝>D<١٨٣>",
"وَX<ٱ>تَّقُوX<ا۟> ٱلَّذِی خَلَقَكُمۡ وَX<ٱ>لۡجِبِلَّةَ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٨٤>",
"قَالُW<وۤ>X<ا۟> إِN<نَّ>مَW<اۤ> أَN<ن>تَ مِنَ ٱلۡمُسَحَّرِJ<ی>نَ A<۝>D<١٨٥>",
"وَمَW<اۤ> أَN<ن>تَ إِلَّا بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُنَا وَإِن N<نَّ>ظُN<نُّ>كَ لَمِنَ ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<١٨٦>",
"فَأَسۡقِQ<طۡ> عَلَیۡنَا كِسَفX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ إِN<ن> كُN<ن>تَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٨٧>",
"قَالَ R<رَ>بِّW<یۤ> أَعۡلَمُ بِمَا تَعۡمَلُJ<و>نَ A<۝>D<١٨٨>",
"فَكَذَّبُوهُ فَأَخَذَهُمۡ عَذَابُ یَوۡمِ ٱX<ل>ظُّلَّةِۚ إِN<نَّ>هُT<ۥ> كَانَ عَذَابَ یَوۡمٍ عَظِJ<ی>مٍ A<۝>D<١٨٩>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ>ۖ N<وَ>مَا كَانَ أَكۡثَR<رُ>هُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<١٩٠>",
"وَإِN<نَّ> R<رَ>بَّكَ لَهُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٩١>",
"وَإِN<نَّ>هُT<ۥ> لَتَN<ن>زِیلُ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٩٢>",
"نَزَلَ بِهِ ٱX<ل>R<رُّ>وحُ ٱلۡأَمِJ<ی>نُ A<۝>D<١٩٣>",
"عَلَىٰ قَلۡبِكَ لِتَكُونَ مِنَ ٱلۡمُN<ن>ذِرِJ<ی>نَ A<۝>D<١٩٤>",
"بِلِسَانٍ عَR<رَ>بِیّX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<١٩٥>",
"وَإِN<نَّ>هُT<ۥ> لَفِی زُبُرِ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٩٦>",
"أَوَ لَمۡ یَكُX<ن> لَّهُمۡ ءَایَةً أَX<ن> N<یَ>عۡلَمَهُT<ۥ> عُلَمَW<ـٰۤ>ؤُX<ا۟> بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِJ<ی>لَ A<۝>D<١٩٧>",
"وَلَوۡ نَزَّلۡنَT<ـٰ>هُ عَلَىٰ بَعۡضِ ٱلۡأَعۡجَمِJ<ی>نَ A<۝>D<١٩٨>",
"فَقَR<رَ>أَهُT<ۥ> عَلَیۡهِم N<مَّ>ا كَانُوX<ا۟> بِهِT<ۦ> مُؤۡمِنِJ<ی>نَ A<۝>D<١٩٩>",
"كَذَT< ٰ>لِكَ سَلَكۡنَT<ـٰ>هُ فِی قُلُوبِ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٢٠٠>",
"لَا یُؤۡمِنُونَ بِهِT<ۦ> حَتَّىٰ یَR<رَ>وُX<ا۟> ٱلۡعَذَابَ ٱلۡأَلِJ<ی>مَ A<۝>D<٢٠١>",
"فَیَأۡتِیَهُN<م> بَغۡتَةX<ࣰ> N<وَ>هُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٢٠٢>",
"فَیَقُولُوX<ا۟> هَلۡ نَحۡنُ مُN<ن>ظَR<رُ>J<و>نَ A<۝>D<٢٠٣>",
"أَفَبِعَذَابِنَا یَسۡتَعۡجِلُJ<و>نَ A<۝>D<٢٠٤>",
"أَفَR<رَ>ءَیۡتَ إِX<ن> N<مَّ>تَّعۡنَT<ـٰ>هُمۡ سِنِJ<ی>نَ A<۝>D<٢٠٥>",
"ثُN<مَّ> جَW<اۤ>ءَهُم N<مَّ>ا كَانُوX<ا۟> یُوعَدُJ<و>نَ A<۝>D<٢٠٦>",
"مَW<اۤ> أَغۡنَىٰ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یُمَتَّعُJ<و>نَ A<۝>D<٢٠٧>",
"وَمَW<اۤ> أَهۡلَكۡنَا مِN<ن> قَR<رۡ>یَةٍ إِلَّا لَهَا مُN<ن>ذِR<رُ>J<و>نَ A<۝>D<٢٠٨>",
"ذِكۡR<رَ>ىٰ وَمَا كُN<نَّ>ا ظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢٠٩>",
"وَمَا تَنَزَّلَتۡ بِهِ ٱX<ل>شَّیَT<ـٰ>طِJ<ی>نُ A<۝>D<٢١٠>",
"وَمَا یَX<ن>N<ۢ>بَغِی لَهُمۡ وَمَا یَسۡتَطِیعُJ<و>نَ A<۝>D<٢١١>",
"إِN<نَّ>هُمۡ عَنِ ٱX<ل>سَّمۡعِ لَمَعۡزُولُJ<و>نَ A<۝>D<٢١٢>",
"فَلَا تَQ<دۡ>عُ مَعَ ٱللَّهِ إِلَT<ـٰ>هًا ءَاخَR<رَ> فَتَكُونَ مِنَ ٱلۡمُعَذَّبِJ<ی>نَ A<۝>D<٢١٣>",
"وَأَN<ن>ذِرۡ عَشِیR<رَ>تَكَ ٱلۡأَQ<قۡ>R<رَ>بِJ<ی>نَ A<۝>D<٢١٤>",
"وَX<ٱ>خۡفِضۡ جَنَاحَكَ لِمَنِ ٱتَّبَعَكَ مِنَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٢١٥>",
"فَإِنۡ عَصَوۡكَ فَقُلۡ إِN<نِّ>ی بَرِW<یۤ>ءX<ࣱ> N<مِّ>N<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<٢١٦>",
"وَتَوَكَّلۡ عَلَى ٱلۡعَزِیزِ ٱX<ل>R<رَّ>حِJ<ی>مِ A<۝>D<٢١٧>",
"ٱلَّذِی یَR<رَ>X<ى>T<ٰ>كَ حِینَ تَقُJ<و>مُ A<۝>D<٢١٨>",
"وَتَقَلُّبَكَ فِی ٱX<ل>سَّT<ـٰ>جِدِJ<ی>نَ A<۝>D<٢١٩>",
"إِN<نَّ>هُT<ۥ> هُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٢٢٠>",
"هَلۡ أُنَبِّئُكُمۡ عَلَىٰ مَN<ن> تَنَزَّلُ ٱX<ل>شَّیَT<ـٰ>طِJ<ی>نُ A<۝>D<٢٢١>",
"تَنَزَّلُ عَلَىٰ كُلِّ أَفَّاكٍ أَثِJ<ی>مࣲ A<۝>D<٢٢٢>",
"یُلۡقُونَ ٱX<ل>سَّمۡعَ وَأَكۡثَR<رُ>هُمۡ كَT<ـٰ>ذِبُJ<و>نَ A<۝>D<٢٢٣>",
"وَX<ٱ>X<ل>شُّعَR<رَ>W<اۤ>ءُ یَتَّبِعُهُمُ ٱلۡغَاوُT<ۥ>نَ A<۝>D<٢٢٤>",
"أَلَمۡ تَR<رَ> أَN<نَّ>هُمۡ فِی كُلِّ وَادX<ࣲ> N<یَ>هِیمُJ<و>نَ A<۝>D<٢٢٥>",
"وَأَN<نَّ>هُمۡ یَقُولُونَ مَا لَا یَفۡعَلُJ<و>نَ A<۝>D<٢٢٦>",
"إِلَّا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَذَكَR<رُ>وX<ا۟> ٱللَّهَ كَثِیR<ر>X<ࣰ>ا N<وَ>X<ٱ>N<ن>تَصَR<رُ>وX<ا۟> مِX<ن>N<ۢ> بَعۡدِ مَا ظُلِمُوX<ا۟>ۗ وَسَیَعۡلَمُ ٱلَّذِینَ ظَلَمُW<وۤ>X<ا۟> أَیَّ مُN<ن>قَلَبX<ࣲ> N<یَ>N<ن>قَلِبُJ<و>نَ A<۝>D<٢٢٧>",
"#۞ طL<سۤ>ۚ تِلۡكَ ءَایَT<ـٰ>تُ ٱلۡقُR<رۡ>ءَانِ وَكِتَابX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<١>",
"هُدX<ࣰ>ى N<وَ>بُشۡR<رَ>ىٰ لِلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٢>",
"ٱلَّذِینَ یُقِیمُونَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَیُؤۡتُونَ ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَهُN<م> بِX<ٱ>لۡءَاخِR<رَ>ةِ هُمۡ یُوقِنُJ<و>نَ A<۝>D<٣>",
"إِN<نَّ> ٱلَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ زَیَّN<نَّ>ا لَهُمۡ أَعۡمَT<ـٰ>لَهُمۡ فَهُمۡ یَعۡمَهُJ<و>نَ A<۝>D<٤>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ لَهُمۡ سُW<وۤ>ءُ ٱلۡعَذَابِ وَهُمۡ فِی ٱلۡءَاخِR<رَ>ةِ هُمُ ٱلۡأَخۡسَR<رُ>J<و>نَ A<۝>D<٥>",
"وَإِN<نَّ>كَ لَتُلَقَّى ٱلۡقُR<رۡ>ءَانَ مِX<ن> لَّدُنۡ حَكِیمٍ عَلِJ<ی>مٍ A<۝>D<٦>",
"إِذۡ قَالَ مُوسَىٰ لِأَهۡلِهِW<ۦۤ> إِN<نِّ>W<یۤ> ءَانَسۡتُ نَاR<ر>N<ࣰ>ا سَءَاتِیكُم N<مِّ>نۡهَا بِخَبَرٍ أَوۡ ءَاتِیكُN<م> بِشِهَابN<ࣲ> قَبَسX<ࣲ> لَّعَلَّكُمۡ تَصۡطَلُJ<و>نَ A<۝>D<٧>",
"فَلَN<مَّ>ا جَW<اۤ>ءَهَا نُودِیَ أَX<ن>N<ۢ> بُورِكَ مَN<ن> فِی ٱX<ل>N<نَّ>ارِ وَمَنۡ حَوۡلَهَا وَسُQ<بۡ>حَT<ـٰ>نَ ٱللَّهِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٨>",
"یَT<ـٰ>مُوسَىW<ٰۤ> إِN<نَّ>هُW<ۥۤ> أَنَا ٱللَّهُ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٩>",
"وَأَلۡقِ عَصَاكَۚ فَلَN<مَّ>ا R<رَ>ءَاهَا تَهۡتَزُّ كَأَN<نَّ>هَا جَL<اۤ>N<نّ>X<ࣱ> N<وَ>لَّىٰ مُQ<دۡ>بِR<ر>X<ࣰ>ا N<وَ>لَمۡ یُعَقِّQ<بۡ>ۚ یَT<ـٰ>مُوسَىٰ لَا تَخَفۡ إِN<نِّ>ی لَا یَخَافُ لَدَیَّ ٱلۡمُR<رۡ>سَلُJ<و>نَ A<۝>D<١٠>",
"إِلَّا مَN<ن> ظَلَمَ ثُN<مَّ> بَدَّلَ حُسۡنَN<ۢ>ا بَعۡدَ سُW<وۤ>ءN<ࣲ> فَإِN<نِّ>ی غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١١>",
"وَأَQ<دۡ>خِلۡ یَدَكَ فِی جَیۡبِكَ تَخۡR<رُ>Q<جۡ> بَیۡضَW<اۤ>ءَ مِنۡ غَیۡرِ سُW<وۤ>ءN<ࣲ>ۖ فِی تِسۡعِ ءَایَT<ـٰ>تٍ إِلَىٰ فِرۡعَوۡنَ وَقَوۡمِهِW<ۦۤ>ۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> قَوۡمN<ࣰ>ا فَT<ـٰ>سِقِJ<ی>نَ A<۝>D<١٢>",
"فَلَN<مَّ>ا جَW<اۤ>ءَتۡهُمۡ ءَایَT<ـٰ>تُنَا مُQ<بۡ>صِR<رَ>ةN<ࣰ> قَالُوX<ا۟> هَT<ـٰ>ذَا سِحۡR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٣>",
"وَجَحَدُوX<ا۟> بِهَا وَX<ٱ>سۡتَیۡقَنَتۡهَW<اۤ> أَN<ن>فُسُهُمۡ ظُلۡمX<ࣰ>ا N<وَ>عُلُوّN<ࣰ>اۚ فَX<ٱ>N<ن>ظُR<رۡ> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<١٤>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا دَاوُT<ۥ>دَ وَسُلَیۡمَT<ـٰ>نَ عِلۡمX<ࣰ>اۖ N<وَ>قَالَا ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِی فَضَّلَنَا عَلَىٰ كَثِیرX<ࣲ> N<مِّ>نۡ عِبَادِهِ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٥>",
"وَوَرِثَ سُلَیۡمَT<ـٰ>نُ دَاوُT<ۥ>دَۖ وَقَالَ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ عُلِّمۡنَا مَN<ن>طِقَ ٱX<ل>طَّیۡرِ وَأُوتِینَا مِN<ن> كُلِّ شَیۡءٍۖ إِN<نَّ> هَT<ـٰ>ذَا لَهُوَ ٱلۡفَضۡلُ ٱلۡمُبِJ<ی>نُ A<۝>D<١٦>",
"وَحُشِR<رَ> لِسُلَیۡمَT<ـٰ>نَ جُنُودُهُT<ۥ> مِنَ ٱلۡجِN<نِّ> وَX<ٱ>لۡإِN<ن>سِ وَX<ٱ>X<ل>طَّیۡرِ فَهُمۡ یُوزَعُJ<و>نَ A<۝>D<١٧>",
"حَتَّىW<ٰۤ> إِذَW<اۤ> أَتَوۡX<ا۟> عَلَىٰ وَادِ ٱX<ل>N<نَّ>مۡلِ قَالَتۡ نَمۡلَةX<ࣱ> N<یَ>W<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>مۡلُ ٱQ<دۡ>خُلُوX<ا۟> مَسَT<ـٰ>كِنَكُمۡ لَا یَحۡطِمَN<نَّ>كُمۡ سُلَیۡمَT<ـٰ>نُ وَجُنُودُهُT<ۥ> وَهُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<١٨>",
"فَتَبَسَّمَ ضَاحِكX<ࣰ>ا N<مِّ>N<ن> قَوۡلِهَا وَقَالَ R<رَ>بِّ أَوۡزِعۡنِW<یۤ> أَنۡ أَشۡكُR<رَ> نِعۡمَتَكَ ٱلَّتِW<یۤ> أَنۡعَمۡتَ عَلَیَّ وَعَلَىٰ وَT< ٰ>لِدَیَّ وَأَنۡ أَعۡمَلَ صَT<ـٰ>لِحN<ࣰ>ا تَR<رۡ>ضَX<ى>T<ٰ>هُ وَأَQ<دۡ>خِلۡنِی بِR<رَ>حۡمَتِكَ فِی عِبَادِكَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<١٩>",
"وَتَفَقَّدَ ٱX<ل>طَّیۡR<رَ> فَقَالَ مَا لِیَ لَW<اۤ> أَR<رَ>ى ٱلۡهُQ<دۡ>هُدَ أَمۡ كَانَ مِنَ ٱلۡغَW<اۤ>ئِبِJ<ی>نَ A<۝>D<٢٠>",
"لَأُعَذِّبَN<نَّ>هُT<ۥ> عَذَابN<ࣰ>ا شَدِیدًا أَوۡ لَأَX<ا۟>ذۡبَحَN<نَّ>هُW<ۥۤ> أَوۡ لَیَأۡتِیَN<نِّ>ی بِسُلۡطَT<ـٰ>نX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٢١>",
"فَمَكَثَ غَیۡR<رَ> بَعِیدN<ࣲ> فَقَالَ أَحَطتُ بِمَا لَمۡ تُحِQ<طۡ> بِهِT<ۦ> وَجِئۡتُكَ مِN<ن> سَبَإِN<ۭ> بِنَبَإX<ࣲ> N<یَ>قِJ<ی>نٍ A<۝>D<٢٢>",
"إِN<نِّ>ی وَجَX<د>تُّ ٱمۡR<رَ>أَةN<ࣰ> تَمۡلِكُهُمۡ وَأُوتِیَتۡ مِN<ن> كُلِّ شَیۡءX<ࣲ> N<وَ>لَهَا عَR<رۡ>شٌ عَظِJ<ی>مࣱ A<۝>D<٢٣>",
"وَجَX<د>تُّهَا وَقَوۡمَهَا یَسۡجُدُونَ لِX<ل>شَّمۡسِ مِN<ن> دُونِ ٱللَّهِ وَزَیَّنَ لَهُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ أَعۡمَT<ـٰ>لَهُمۡ فَصَدَّهُمۡ عَنِ ٱX<ل>سَّبِیلِ فَهُمۡ لَا یَهۡتَدُJ<و>نَ A<۝>D<٢٤>",
"أَ̅لَّ̅ا̅ ̅یَ̅سۡ̅جُ̅دُ̅و̅X<ا۟>̅ ̅لِ̅لَّ̅هِ̅ ٱلَّذِی یُخۡرِجُ ٱلۡخَQ<بۡ>ءَ فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَیَعۡلَمُ مَا تُخۡفُونَ وَمَا تُعۡلِنُJ<و>نَ A<۝>D<٢٥>",
"ٱللَّهُ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ R<رَ>بُّ ٱلۡعَR<رۡ>شِ ٱلۡعَظِJ<ی>مِ A<۝>D<٢٦> ۩",
"۞ قَالَ سَنَN<ن>ظُR<رُ> أَصَدَQ<قۡ>تَ أَمۡ كُN<ن>تَ مِنَ ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٢٧>",
"ٱذۡهَب بِّكِتَT<ـٰ>بِی هَT<ـٰ>ذَا فَأَلۡقِهۡ إِلَیۡهِمۡ ثُN<مَّ> تَوَلَّ عَنۡهُمۡ فَX<ٱ>N<ن>ظُR<رۡ> مَاذَا یَR<رۡ>جِعُJ<و>نَ A<۝>D<٢٨>",
"قَالَتۡ یَW<ـٰۤ>أَیُّهَا ٱلۡمَلَؤُX<ا۟> إِN<نِّ>W<یۤ> أُلۡقِیَ إِلَیَّ كِتَT<ـٰ>بN<ࣱ> كَرِJ<ی>مٌ A<۝>D<٢٩>",
"إِN<نَّ>هُT<ۥ> مِN<ن> سُلَیۡمَT<ـٰ>نَ وَإِN<نَّ>هُT<ۥ> بِسۡمِ ٱللَّهِ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ ٱX<ل>R<رَّ>حِJ<ی>مِ A<۝>D<٣٠>",
"أَلَّا تَعۡلُوX<ا۟> عَلَیَّ وَأۡتُونِی مُسۡلِمِJ<ی>نَ A<۝>D<٣١>",
"قَالَتۡ یَW<ـٰۤ>أَیُّهَا ٱلۡمَلَؤُX<ا۟> أَفۡتُونِی فِW<یۤ> أَمۡرِی مَا كُN<ن>تُ قَاطِعَةً أَمۡR<ر>ًا حَتَّىٰ تَشۡهَدُJ<و>نِ A<۝>D<٣٢>",
"قَالُوX<ا۟> نَحۡنُ أُX<و۟>لُوX<ا۟> قُوَّةX<ࣲ> N<وَ>أُX<و۟>لُوX<ا۟> بَأۡسN<ࣲ> شَدِیدX<ࣲ> N<وَ>X<ٱ>لۡأَمۡR<رُ> إِلَیۡكِ فَX<ٱ>N<ن>ظُرِی مَاذَا تَأۡمُرِJ<ی>نَ A<۝>D<٣٣>",
"قَالَتۡ إِN<نَّ> ٱلۡمُلُوكَ إِذَا دَخَلُوX<ا۟> قَR<رۡ>یَةً أَفۡسَدُوهَا وَجَعَلُW<وۤ>X<ا۟> أَعِزَّةَ أَهۡلِهَW<اۤ> أَذِلَّةX<ࣰ>ۚ N<وَ>كَذَT< ٰ>لِكَ یَفۡعَلُJ<و>نَ A<۝>D<٣٤>",
"وَإِN<نِّ>ی مُR<رۡ>سِلَةٌ إِلَیۡهِN<م> بِهَدِیَّةN<ࣲ> فَنَاظِR<رَ>ةُN<ۢ> بِمَ یَR<رۡ>جِعُ ٱلۡمُR<رۡ>سَلُJ<و>نَ A<۝>D<٣٥>",
"فَلَN<مَّ>ا جَW<اۤ>ءَ سُلَیۡمَT<ـٰ>نَ قَالَ أَتُمِدُّونَنِ بِمَالN<ࣲ> فَمَW<اۤ> ءَاتَX<ى>T<ٰ>نِT<ۦ>َ ٱللَّهُ خَیۡR<ر>X<ࣱ> N<مِّ>N<مَّ>W<اۤ> ءَاتَX<ى>T<ٰ>كُN<م>ۚ بَلۡ أَN<ن>تُN<م> بِهَدِیَّتِكُمۡ تَفۡR<رَ>حُJ<و>نَ A<۝>D<٣٦>",
"ٱR<رۡ>جِعۡ إِلَیۡهِمۡ فَلَنَأۡتِیَN<نَّ>هُN<م> بِجُنُودX<ࣲ> لَّا قِبَلَ لَهُN<م> بِهَا وَلَنُخۡرِجَN<نَّ>هُم N<مِّ>نۡهَW<اۤ> أَذِلَّةX<ࣰ> N<وَ>هُمۡ صَT<ـٰ>غِR<رُ>J<و>نَ A<۝>D<٣٧>",
"قَالَ یَW<ـٰۤ>أَیُّهَا ٱلۡمَلَؤُX<ا۟> أَیُّكُمۡ یَأۡتِینِی بِعَR<رۡ>شِهَا قَQ<بۡ>لَ أَX<ن> N<یَ>أۡتُونِی مُسۡلِمِJ<ی>نَ A<۝>D<٣٨>",
"قَالَ عِفۡرِیتX<ࣱ> N<مِّ>نَ ٱلۡجِN<نِّ> أَنَا۠ ءَاتِیكَ بِهِT<ۦ> قَQ<بۡ>لَ أَN<ن> تَقُومَ مِX<ن> N<مَّ>قَامِكَۖ وَإِN<نِّ>ی عَلَیۡهِ لَقَوِیٌّ أَمِJ<ی>نࣱ A<۝>D<٣٩>",
"قَالَ ٱلَّذِی عِN<ن>دَهُT<ۥ> عِلۡمX<ࣱ> N<مِّ>نَ ٱلۡكِتَT<ـٰ>بِ أَنَا۠ ءَاتِیكَ بِهِT<ۦ> قَQ<بۡ>لَ أَX<ن> N<یَ>R<رۡ>تَدَّ إِلَیۡكَ طَR<رۡ>فُكَۚ فَلَN<مَّ>ا R<رَ>ءَاهُ مُسۡتَقِR<رّ>ًا عِN<ن>دَهُT<ۥ> قَالَ هَT<ـٰ>ذَا مِN<ن> فَضۡلِ R<رَ>بِّی لِیَQ<بۡ>لُوَنِW<یۤ> ءَأَشۡكُR<رُ> أَمۡ أَكۡفُR<رُ>ۖ وَمَN<ن> شَكَR<رَ> فَإِN<نَّ>مَا یَشۡكُR<رُ> لِنَفۡسِهِT<ۦ>ۖ وَمَN<ن> كَفَR<رَ> فَإِN<نَّ> R<رَ>بِّی غَنِیّN<ࣱ> كَرِJ<ی>مࣱ A<۝>D<٤٠>",
"قَالَ نَكِّR<رُ>وX<ا۟> لَهَا عَR<رۡ>شَهَا نَN<ن>ظُR<رۡ> أَتَهۡتَدِW<یۤ> أَمۡ تَكُونُ مِنَ ٱلَّذِینَ لَا یَهۡتَدُJ<و>نَ A<۝>D<٤١>",
"فَلَN<مَّ>ا جَW<اۤ>ءَتۡ قِیلَ أَهَT<ـٰ>كَذَا عَR<رۡ>شُكِۖ قَالَتۡ كَأَN<نَّ>هُT<ۥ> هُوَۚ وَأُوتِینَا ٱلۡعِلۡمَ مِN<ن> قَQ<بۡ>لِهَا وَكُN<نَّ>ا مُسۡلِمِJ<ی>نَ A<۝>D<٤٢>",
"وَصَدَّهَا مَا كَانَت تَّعۡبُدُ مِN<ن> دُونِ ٱللَّهِۖ إِN<نَّ>هَا كَانَتۡ مِN<ن> قَوۡمN<ࣲ> كَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٤٣>",
"قِیلَ لَهَا ٱQ<دۡ>خُلِی ٱX<ل>صَّR<رۡ>حَۖ فَلَN<مَّ>ا R<رَ>أَتۡهُ حَسِبَتۡهُ لُجَّةX<ࣰ> N<وَ>كَشَفَتۡ عَN<ن> سَاقَیۡهَاۚ قَالَ إِN<نَّ>هُT<ۥ> صَR<رۡ>حX<ࣱ> N<مُّ>مَR<رَّ>دX<ࣱ> N<مِّ>N<ن> قَوَارِیR<رَ>ۗ قَالَتۡ R<رَ>بِّ إِN<نِّ>ی ظَلَمۡتُ نَفۡسِی وَأَسۡلَمۡتُ مَعَ سُلَیۡمَT<ـٰ>نَ لِلَّهِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٤٤>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَW<اۤ> إِلَىٰ ثَمُودَ أَخَاهُمۡ صَT<ـٰ>لِحًا أَنِ ٱعۡبُدُوX<ا۟> ٱللَّهَ فَإِذَا هُمۡ فَرِیقَانِ یَخۡتَصِمُJ<و>نَ A<۝>D<٤٥>",
"قَالَ یَT<ـٰ>قَوۡمِ لِمَ تَسۡتَعۡجِلُونَ بِX<ٱ>X<ل>سَّیِّئَةِ قَQ<بۡ>لَ ٱلۡحَسَنَةِۖ لَوۡلَا تَسۡتَغۡفِR<رُ>ونَ ٱللَّهَ لَعَلَّكُمۡ تُR<رۡ>حَمُJ<و>نَ A<۝>D<٤٦>",
"قَالُوX<ا۟> ٱطَّیَّR<رۡ>نَا بِكَ وَبِمَX<ن> N<مَّ>عَكَۚ قَالَ طَW<ـٰۤ>ئِR<رُ>كُمۡ عِN<ن>دَ ٱللَّهِۖ بَلۡ أَN<ن>تُمۡ قَوۡمN<ࣱ> تُفۡتَنُJ<و>نَ A<۝>D<٤٧>",
"وَكَانَ فِی ٱلۡمَدِینَةِ تِسۡعَةُ R<رَ>هۡطX<ࣲ> N<یُ>فۡسِدُونَ فِی ٱلۡأَR<رۡ>ضِ وَلَا یُصۡلِحُJ<و>نَ A<۝>D<٤٨>",
"قَالُوX<ا۟> تَقَاسَمُوX<ا۟> بِٱللَّهِ لَنُبَیِّتَN<نَّ>هُT<ۥ> وَأَهۡلَهُT<ۥ> ثُN<مَّ> لَنَقُولَN<نَّ> لِوَلِیِّهِT<ۦ> مَا شَهِQ<دۡ>نَا مَهۡلِكَ أَهۡلِهِT<ۦ> وَإِN<نَّ>ا لَصَT<ـٰ>دِقُJ<و>نَ A<۝>D<٤٩>",
"وَمَكَR<رُ>وX<ا۟> مَكۡR<ر>X<ࣰ>ا N<وَ>مَكَR<رۡ>نَا مَكۡR<ر>X<ࣰ>ا N<وَ>هُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٥٠>",
"فَX<ٱ>N<ن>ظُR<رۡ> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ مَكۡرِهِمۡ أَN<نَّ>ا دَN<مَّ>R<رۡ>نَT<ـٰ>هُمۡ وَقَوۡمَهُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٥١>",
"فَتِلۡكَ بُیُوتُهُمۡ خَاوِیَةَN<ۢ> بِمَا ظَلَمُW<وۤ>X<ا۟>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یَ>عۡلَمُJ<و>نَ A<۝>D<٥٢>",
"وَأَN<ن>جَیۡنَا ٱلَّذِینَ ءَامَنُوX<ا۟> وَكَانُوX<ا۟> یَتَّقُJ<و>نَ A<۝>D<٥٣>",
"وَلُوطًا إِذۡ قَالَ لِقَوۡمِهِW<ۦۤ> أَتَأۡتُونَ ٱلۡفَT<ـٰ>حِشَةَ وَأَN<ن>تُمۡ تُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٥٤>",
"أَئِN<نَّ>كُمۡ لَتَأۡتُونَ ٱX<ل>رِّجَالَ شَهۡوَةX<ࣰ> N<مِّ>N<ن> دُونِ ٱX<ل>N<نِّ>سَW<اۤ>ءِۚ بَلۡ أَN<ن>تُمۡ قَوۡمN<ࣱ> تَQ<جۡ>هَلُJ<و>نَ A<۝>D<٥٥>",
"۞ فَمَا كَانَ جَوَابَ قَوۡمِهِW<ۦۤ> إِلَّW<اۤ> أَN<ن> قَالُW<وۤ>X<ا۟> أَخۡرِجُW<وۤ>X<ا۟> ءَالَ لُوطX<ࣲ> N<مِّ>N<ن> قَR<رۡ>یَتِكُمۡۖ إِN<نَّ>هُمۡ أُنَاسX<ࣱ> N<یَ>تَطَهَّR<رُ>J<و>نَ A<۝>D<٥٦>",
"فَأَN<ن>جَیۡنَT<ـٰ>هُ وَأَهۡلَهُW<ۥۤ> إِلَّا ٱمۡR<رَ>أَتَهُT<ۥ> قَدَّR<رۡ>نَT<ـٰ>هَا مِنَ ٱلۡغَT<ـٰ>بِرِJ<ی>نَ A<۝>D<٥٧>",
"وَأَمۡطَR<رۡ>نَا عَلَیۡهِم N<مَّ>طَR<ر>N<ࣰ>اۖ فَسَW<اۤ>ءَ مَطَR<رُ> ٱلۡمُN<ن>ذَرِJ<ی>نَ A<۝>D<٥٨>",
"قُلِ ٱلۡحَمۡدُ لِلَّهِ وَسَلَT<ـٰ>مٌ عَلَىٰ عِبَادِهِ ٱلَّذِینَ ٱصۡطَفَىW<ٰۤ>ۗ ءَL<اۤ>للَّهُ خَیۡR<ر>ٌ أَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٥٩>",
"أَN<مَّ>نۡ خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَأَN<ن>زَلَ لَكُم N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَX<ن>N<ۢ>بَتۡنَا بِهِT<ۦ> حَدَW<اۤ>ئِقَ ذَاتَ بَهۡجَةX<ࣲ> N<مَّ>ا كَانَ لَكُمۡ أَN<ن> تُX<ن>N<ۢ>بِتُوX<ا۟> شَجَR<رَ>هَW<اۤ>ۗ أَءِلَT<ـٰ>هX<ࣱ> N<مَّ>عَ ٱللَّهِۚ بَلۡ هُمۡ قَوۡمX<ࣱ> N<یَ>عۡدِلُJ<و>نَ A<۝>D<٦٠>",
"أَN<مَّ>N<ن> جَعَلَ ٱلۡأَR<رۡ>ضَ قَR<رَ>اR<ر>X<ࣰ>ا N<وَ>جَعَلَ خِلَT<ـٰ>لَهَW<اۤ> أَنۡهَT<ـٰ>R<ر>X<ࣰ>ا N<وَ>جَعَلَ لَهَا R<رَ>وَT< ٰ>سِیَ وَجَعَلَ بَیۡنَ ٱلۡبَحۡR<رَ>یۡنِ حَاجِزًاۗ أَءِلَT<ـٰ>هX<ࣱ> N<مَّ>عَ ٱللَّهِۚ بَلۡ أَكۡثَR<رُ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٦١>",
"أَN<مَّ>X<ن> N<یُ>جِیبُ ٱلۡمُضۡطَR<رَّ> إِذَا دَعَاهُ وَیَكۡشِفُ ٱX<ل>سُّW<وۤ>ءَ وَیَQ<جۡ>عَلُكُمۡ خُلَفَW<اۤ>ءَ ٱلۡأَR<رۡ>ضِۗ أَءِلَT<ـٰ>هX<ࣱ> N<مَّ>عَ ٱللَّهِۚ قَلِیلX<ࣰ>ا N<مَّ>ا تَذَكَّR<رُ>J<و>نَ A<۝>D<٦٢>",
"أَN<مَّ>X<ن> N<یَ>هۡدِیكُمۡ فِی ظُلُمَT<ـٰ>تِ ٱلۡبَرِّ وَX<ٱ>لۡبَحۡرِ وَمَX<ن> N<یُ>R<رۡ>سِلُ ٱX<ل>رِّیَT<ـٰ>حَ بُشۡR<رَ>N<ۢ>ا بَیۡنَ یَدَیۡ R<رَ>حۡمَتِهِW<ۦۤ>ۗ أَءِلَT<ـٰ>هX<ࣱ> N<مَّ>عَ ٱللَّهِۚ تَعَT<ـٰ>لَى ٱللَّهُ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٦٣>",
"أَN<مَّ>X<ن> N<یَ>Q<بۡ>دَؤُX<ا۟> ٱلۡخَلۡقَ ثُN<مَّ> یُعِیدُهُT<ۥ> وَمَX<ن> N<یَ>R<رۡ>زُقُكُم N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِۗ أَءِلَT<ـٰ>هX<ࣱ> N<مَّ>عَ ٱللَّهِۚ قُلۡ هَاتُوX<ا۟> بُR<رۡ>هَT<ـٰ>نَكُمۡ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٦٤>",
"قُل لَّا یَعۡلَمُ مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ ٱلۡغَیۡبَ إِلَّا ٱللَّهُۚ وَمَا یَشۡعُR<رُ>ونَ أَیَّانَ یُQ<بۡ>عَثُJ<و>نَ A<۝>D<٦٥>",
"بَلِ ٱدَّT< ٰ>R<رَ>كَ عِلۡمُهُمۡ فِی ٱلۡءَاخِR<رَ>ةِۚ بَلۡ هُمۡ فِی شَكّX<ࣲ> N<مِّ>نۡهَاۖ بَلۡ هُم N<مِّ>نۡهَا عَمُJ<و>نَ A<۝>D<٦٦>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَءِذَا كُN<نَّ>ا تُR<رَ>T< ٰ>بX<ࣰ>ا N<وَ>ءَابَW<اۤ>ؤُنَW<اۤ> أَئِN<نَّ>ا لَمُخۡR<رَ>جُJ<و>نَ A<۝>D<٦٧>",
"لَقَQ<دۡ> وُعِQ<دۡ>نَا هَT<ـٰ>ذَا نَحۡنُ وَءَابَW<اۤ>ؤُنَا مِN<ن> قَQ<بۡ>لُ إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّW<اۤ> أَسَT<ـٰ>طِیR<رُ> ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٦٨>",
"قُلۡ سِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَX<ٱ>N<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٦٩>",
"وَلَا تَحۡزَنۡ عَلَیۡهِمۡ وَلَا تَكُN<ن> فِی ضَیۡقX<ࣲ> N<مِّ>N<مَّ>ا یَمۡكُR<رُ>J<و>نَ A<۝>D<٧٠>",
"وَیَقُولُونَ مَتَىٰ هَT<ـٰ>ذَا ٱلۡوَعۡدُ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٧١>",
"قُلۡ عَسَىW<ٰۤ> أَX<ن> N<یَ>كُونَ R<رَ>دِفَ لَكُN<م> بَعۡضُ ٱلَّذِی تَسۡتَعۡجِلُJ<و>نَ A<۝>D<٧٢>",
"وَإِN<نَّ> R<رَ>بَّكَ لَذُو فَضۡلٍ عَلَى ٱX<ل>N<نَّ>اسِ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَشۡكُR<رُ>J<و>نَ A<۝>D<٧٣>",
"وَإِN<نَّ> R<رَ>بَّكَ لَیَعۡلَمُ مَا تُكِN<نُّ> صُدُوR<رُ>هُمۡ وَمَا یُعۡلِنُJ<و>نَ A<۝>D<٧٤>",
"وَمَا مِنۡ غَW<اۤ>ئِبَةN<ࣲ> فِی ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِ إِلَّا فِی كِتَT<ـٰ>بX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٧٥>",
"إِN<نَّ> هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانَ یَقُصُّ عَلَىٰ بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ أَكۡثَR<رَ> ٱلَّذِی هُمۡ فِیهِ یَخۡتَلِفُJ<و>نَ A<۝>D<٧٦>",
"وَإِN<نَّ>هُT<ۥ> لَهُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣱ> لِّلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٧٧>",
"إِN<نَّ> R<رَ>بَّكَ یَQ<قۡ>ضِی بَیۡنَهُN<م> بِحُكۡمِهِT<ۦ>ۚ وَهُوَ ٱلۡعَزِیزُ ٱلۡعَلِJ<ی>مُ A<۝>D<٧٨>",
"فَتَوَكَّلۡ عَلَى ٱللَّهِۖ إِN<نَّ>كَ عَلَى ٱلۡحَقِّ ٱلۡمُبِJ<ی>نِ A<۝>D<٧٩>",
"إِN<نَّ>كَ لَا تُسۡمِعُ ٱلۡمَوۡتَىٰ وَلَا تُسۡمِعُ ٱX<ل>صُّN<مَّ> ٱX<ل>دُّعَW<اۤ>ءَ إِذَا وَلَّوۡX<ا۟> مُQ<دۡ>بِرِJ<ی>نَ A<۝>D<٨٠>",
"وَمَW<اۤ> أَN<ن>تَ بِهَT<ـٰ>دِی ٱلۡعُمۡیِ عَN<ن> ضَلَT<ـٰ>لَتِهِمۡۖ إِN<ن> تُسۡمِعُ إِلَّا مَX<ن> N<یُ>ؤۡمِنُ بِءَایَT<ـٰ>تِنَا فَهُم N<مُّ>سۡلِمُJ<و>نَ A<۝>D<٨١>",
"۞ وَإِذَا وَقَعَ ٱلۡقَوۡلُ عَلَیۡهِمۡ أَخۡR<رَ>Q<جۡ>نَا لَهُمۡ دَL<اۤ>بَّةX<ࣰ> N<مِّ>نَ ٱلۡأَR<رۡ>ضِ تُكَلِّمُهُمۡ أَN<نَّ> ٱX<ل>N<نَّ>اسَ كَانُوX<ا۟> بِءَایَT<ـٰ>تِنَا لَا یُوقِنُJ<و>نَ A<۝>D<٨٢>",
"وَیَوۡمَ نَحۡشُR<رُ> مِN<ن> كُلِّ أُN<مَّ>ةN<ࣲ> فَوۡجX<ࣰ>ا N<مِّ>N<مَّ>X<ن> N<یُ>كَذِّبُ بِءَایَT<ـٰ>تِنَا فَهُمۡ یُوزَعُJ<و>نَ A<۝>D<٨٣>",
"حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءُو قَالَ أَكَذَّQ<بۡ>تُN<م> بِءَایَT<ـٰ>تِی وَلَمۡ تُحِیطُوX<ا۟> بِهَا عِلۡمًا أَN<مَّ>اذَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٨٤>",
"وَوَقَعَ ٱلۡقَوۡلُ عَلَیۡهِN<م> بِمَا ظَلَمُوX<ا۟> فَهُمۡ لَا یَN<ن>طِقُJ<و>نَ A<۝>D<٨٥>",
"أَلَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ>ا جَعَلۡنَا ٱلَّیۡلَ لِیَسۡكُنُوX<ا۟> فِیهِ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ> مُQ<بۡ>صِR<ر>ًاۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٨٦>",
"وَیَوۡمَ یُN<ن>فَخُ فِی ٱX<ل>صُّورِ فَفَزِعَ مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَN<ن> فِی ٱلۡأَR<رۡ>ضِ إِلَّا مَN<ن> شَW<اۤ>ءَ ٱللَّهُۚ وَكُلٌّ أَتَوۡهُ دَT< ٰ>خِرِJ<ی>نَ A<۝>D<٨٧>",
"وَتَR<رَ>ى ٱلۡجِبَالَ تَحۡسَبُهَا جَامِدَةX<ࣰ> N<وَ>هِیَ تَمُR<رُّ> مَR<رَّ> ٱX<ل>سَّحَابِۚ صُنۡعَ ٱللَّهِ ٱلَّذِW<یۤ> أَتۡقَنَ كُلَّ شَیۡءٍۚ إِN<نَّ>هُT<ۥ> خَبِیR<رُ>N<ۢ> بِمَا تَفۡعَلُJ<و>نَ A<۝>D<٨٨>",
"مَN<ن> جَW<اۤ>ءَ بِX<ٱ>لۡحَسَنَةِ فَلَهُT<ۥ> خَیۡR<ر>X<ࣱ> N<مِّ>نۡهَا وَهُم N<مِّ>N<ن> فَزَعX<ࣲ> N<یَ>وۡمَئِذٍ ءَامِنُJ<و>نَ A<۝>D<٨٩>",
"وَمَN<ن> جَW<اۤ>ءَ بِX<ٱ>X<ل>سَّیِّئَةِ فَكُبَّتۡ وُجُوهُهُمۡ فِی ٱX<ل>N<نَّ>ارِ هَلۡ تُQ<جۡ>زَوۡنَ إِلَّا مَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٩٠>",
"إِN<نَّ>مَW<اۤ> أُمِرۡتُ أَنۡ أَعۡبُدَ R<رَ>بَّ هَT<ـٰ>ذِهِ ٱلۡبَلۡدَةِ ٱلَّذِی حَR<رَّ>مَهَا وَلَهُT<ۥ> كُلُّ شَیۡءX<ࣲ>ۖ N<وَ>أُمِرۡتُ أَنۡ أَكُونَ مِنَ ٱلۡمُسۡلِمِJ<ی>نَ A<۝>D<٩١>",
"وَأَنۡ أَتۡلُوَX<ا۟> ٱلۡقُR<رۡ>ءَانَۖ فَمَنِ ٱهۡتَدَىٰ فَإِN<نَّ>مَا یَهۡتَدِی لِنَفۡسِهِT<ۦ>ۖ وَمَN<ن> ضَلَّ فَقُلۡ إِN<نَّ>مَW<اۤ> أَنَا۠ مِنَ ٱلۡمُN<ن>ذِرِJ<ی>نَ A<۝>D<٩٢>",
"وَقُلِ ٱلۡحَمۡدُ لِلَّهِ سَیُرِیكُمۡ ءَایَT<ـٰ>تِهِT<ۦ> فَتَعۡرِفُونَهَاۚ وَمَا R<رَ>بُّكَ بِغَT<ـٰ>فِلٍ عَN<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<٩٣>",
"#طL<سۤ>L<مۤ> A<۝>D<١>",
"تِلۡكَ ءَایَT<ـٰ>تُ ٱلۡكِتَT<ـٰ>بِ ٱلۡمُبِJ<ی>نِ A<۝>D<٢>",
"نَتۡلُوX<ا۟> عَلَیۡكَ مِن N<نَّ>بَإِ مُوسَىٰ وَفِرۡعَوۡنَ بِX<ٱ>لۡحَقِّ لِقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٣>",
"إِN<نَّ> فِرۡعَوۡنَ عَلَا فِی ٱلۡأَR<رۡ>ضِ وَجَعَلَ أَهۡلَهَا شِیَعX<ࣰ>ا N<یَ>سۡتَضۡعِفُ طَW<اۤ>ئِفَةX<ࣰ> N<مِّ>نۡهُمۡ یُذَبِّحُ أَQ<بۡ>نَW<اۤ>ءَهُمۡ وَیَسۡتَحۡیِT<ۦ> نِسَW<اۤ>ءَهُمۡۚ إِN<نَّ>هُT<ۥ> كَانَ مِنَ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<٤>",
"وَنُرِیدُ أَن N<نَّ>مُN<نَّ> عَلَى ٱلَّذِینَ ٱسۡتُضۡعِفُوX<ا۟> فِی ٱلۡأَR<رۡ>ضِ وَنَQ<جۡ>عَلَهُمۡ أَئِN<مَّ>ةX<ࣰ> N<وَ>نَQ<جۡ>عَلَهُمُ ٱلۡوَT< ٰ>رِثِJ<ی>نَ A<۝>D<٥>",
"وَنُمَكِّنَ لَهُمۡ فِی ٱلۡأَR<رۡ>ضِ وَنُرِیَ فِرۡعَوۡنَ وَهَT<ـٰ>مَT<ـٰ>نَ وَجُنُودَهُمَا مِنۡهُم N<مَّ>ا كَانُوX<ا۟> یَحۡذَR<رُ>J<و>نَ A<۝>D<٦>",
"وَأَوۡحَیۡنَW<اۤ> إِلَىW<ٰۤ> أُN<مِّ> مُوسَىW<ٰۤ> أَنۡ أَR<رۡ>ضِعِیهِۖ فَإِذَا خِفۡتِ عَلَیۡهِ فَأَلۡقِیهِ فِی ٱلۡیَN<مِّ> وَلَا تَخَافِی وَلَا تَحۡزَنِW<یۤ>ۖ إِN<نَّ>ا R<رَ>L<اۤ>دُّوهُ إِلَیۡكِ وَجَاعِلُوهُ مِنَ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٧>",
"فَX<ٱ>لۡتَقَطَهُW<ۥۤ> ءَالُ فِرۡعَوۡنَ لِیَكُونَ لَهُمۡ عَدُوّX<ࣰ>ا N<وَ>حَزَنًاۗ إِN<نَّ> فِرۡعَوۡنَ وَهَT<ـٰ>مَT<ـٰ>نَ وَجُنُودَهُمَا كَانُوX<ا۟> خَT<ـٰ>طِءِJ<ی>نَ A<۝>D<٨>",
"وَقَالَتِ ٱمۡR<رَ>أَتُ فِرۡعَوۡنَ قُR<رَّ>تُ عَیۡنX<ࣲ> لِّی وَلَكَۖ لَا تَQ<قۡ>تُلُوهُ عَسَىW<ٰۤ> أَX<ن> N<یَ>N<ن>فَعَنَW<اۤ> أَوۡ نَتَّخِذَهُT<ۥ> وَلَدX<ࣰ>ا N<وَ>هُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٩>",
"وَأَصۡبَحَ فُؤَادُ أُN<مِّ> مُوسَىٰ فَT<ـٰ>رِغًاۖ إِN<ن> كَادَتۡ لَتُQ<بۡ>دِی بِهِT<ۦ> لَوۡلَW<اۤ> أَX<ن> R<رَّ>بَQ<طۡ>نَا عَلَىٰ قَلۡبِهَا لِتَكُونَ مِنَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٠>",
"وَقَالَتۡ لِأُخۡتِهِT<ۦ> قُصِّیهِۖ فَبَصُR<رَ>تۡ بِهِT<ۦ> عَN<ن> جُنُبX<ࣲ> N<وَ>هُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<١١>",
"۞ وَحَR<رَّ>مۡنَا عَلَیۡهِ ٱلۡمَR<رَ>اضِعَ مِN<ن> قَQ<بۡ>لُ فَقَالَتۡ هَلۡ أَدُلُّكُمۡ عَلَىW<ٰۤ> أَهۡلِ بَیۡتX<ࣲ> N<یَ>كۡفُلُونَهُT<ۥ> لَكُمۡ وَهُمۡ لَهُT<ۥ> نَT<ـٰ>صِحُJ<و>نَ A<۝>D<١٢>",
"فَR<رَ>دَQ<دۡ>نَT<ـٰ>هُ إِلَىW<ٰۤ> أُN<مِّ>هِT<ۦ> كَیۡ تَقَR<رَّ> عَیۡنُهَا وَلَا تَحۡزَنَ وَلِتَعۡلَمَ أَN<نَّ> وَعۡدَ ٱللَّهِ حَقّX<ࣱ> N<وَ>لَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<١٣>",
"وَلَN<مَّ>ا بَلَغَ أَشُدَّهُT<ۥ> وَX<ٱ>سۡتَوَىW<ٰۤ> ءَاتَیۡنَT<ـٰ>هُ حُكۡمX<ࣰ>ا N<وَ>عِلۡمX<ࣰ>اۚ N<وَ>كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٤>",
"وَدَخَلَ ٱلۡمَدِینَةَ عَلَىٰ حِینِ غَفۡلَةX<ࣲ> N<مِّ>نۡ أَهۡلِهَا فَوَجَدَ فِیهَا R<رَ>جُلَیۡنِ یَQ<قۡ>تَتِلَانِ هَT<ـٰ>ذَا مِN<ن> شِیعَتِهِT<ۦ> وَهَT<ـٰ>ذَا مِنۡ عَدُوِّهِT<ۦ>ۖ فَX<ٱ>سۡتَغَT<ـٰ>ثَهُ ٱلَّذِی مِN<ن> شِیعَتِهِT<ۦ> عَلَى ٱلَّذِی مِنۡ عَدُوِّهِT<ۦ> فَوَكَزَهُT<ۥ> مُوسَىٰ فَقَضَىٰ عَلَیۡهِۖ قَالَ هَT<ـٰ>ذَا مِنۡ عَمَلِ ٱX<ل>شَّیۡطَT<ـٰ>نِۖ إِN<نَّ>هُT<ۥ> عَدُوّX<ࣱ> N<مُّ>ضِلّX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٥>",
"قَالَ R<رَ>بِّ إِN<نِّ>ی ظَلَمۡتُ نَفۡسِی فَX<ٱ>غۡفِرۡ لِی فَغَفَR<رَ> لَهُW<ۥۤ>ۚ إِN<نَّ>هُT<ۥ> هُوَ ٱلۡغَفُوR<رُ> ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<١٦>",
"قَالَ R<رَ>بِّ بِمَW<اۤ> أَنۡعَمۡتَ عَلَیَّ فَلَنۡ أَكُونَ ظَهِیR<ر>X<ࣰ>ا لِّلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<١٧>",
"فَأَصۡبَحَ فِی ٱلۡمَدِینَةِ خَW<اۤ>ئِفX<ࣰ>ا N<یَ>تَR<رَ>قَّبُ فَإِذَا ٱلَّذِی ٱسۡتَN<ن>صَR<رَ>هُT<ۥ> بِX<ٱ>لۡأَمۡسِ یَسۡتَصۡرِخُهُT<ۥ>ۚ قَالَ لَهُT<ۥ> مُوسَىW<ٰۤ> إِN<نَّ>كَ لَغَوِیّX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٨>",
"فَلَN<مَّ>W<اۤ> أَنۡ أَR<رَ>ادَ أَX<ن> N<یَ>Q<بۡ>طِشَ بِX<ٱ>لَّذِی هُوَ عَدُوّX<ࣱ> لَّهُمَا قَالَ یَT<ـٰ>مُوسَىW<ٰۤ> أَتُرِیدُ أَN<ن> تَQ<قۡ>تُلَنِی كَمَا قَتَلۡتَ نَفۡسَN<ۢ>ا بِX<ٱ>لۡأَمۡسِۖ إِN<ن> تُرِیدُ إِلَّW<اۤ> أَN<ن> تَكُونَ جَبَّاR<ر>N<ࣰ>ا فِی ٱلۡأَR<رۡ>ضِ وَمَا تُرِیدُ أَN<ن> تَكُونَ مِنَ ٱلۡمُصۡلِحِJ<ی>نَ A<۝>D<١٩>",
"وَجَW<اۤ>ءَ R<رَ>جُلX<ࣱ> N<مِّ>نۡ أَQ<قۡ>صَا ٱلۡمَدِینَةِ یَسۡعَىٰ قَالَ یَT<ـٰ>مُوسَىW<ٰۤ> إِN<نَّ> ٱلۡمَلَأَ یَأۡتَمِR<رُ>ونَ بِكَ لِیَQ<قۡ>تُلُوكَ فَX<ٱ>خۡR<رُ>Q<جۡ> إِN<نِّ>ی لَكَ مِنَ ٱX<ل>N<نَّ>T<ـٰ>صِحِJ<ی>نَ A<۝>D<٢٠>",
"فَخَR<رَ>جَ مِنۡهَا خَW<اۤ>ئِفX<ࣰ>ا N<یَ>تَR<رَ>قَّبُۖ قَالَ R<رَ>بِّ نَجِّنِی مِنَ ٱلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢١>",
"وَلَN<مَّ>ا تَوَجَّهَ تِلۡقَW<اۤ>ءَ مَQ<دۡ>یَنَ قَالَ عَسَىٰ R<رَ>بِّW<یۤ> أَX<ن> N<یَ>هۡدِیَنِی سَوَW<اۤ>ءَ ٱX<ل>سَّبِJ<ی>لِ A<۝>D<٢٢>",
"وَلَN<مَّ>ا وَR<رَ>دَ مَW<اۤ>ءَ مَQ<دۡ>یَنَ وَجَدَ عَلَیۡهِ أُN<مَّ>ةX<ࣰ> N<مِّ>نَ ٱX<ل>N<نَّ>اسِ یَسۡقُونَ وَوَجَدَ مِN<ن> دُونِهِمُ ٱمۡR<رَ>أَتَیۡنِ تَذُودَانِۖ قَالَ مَا خَQ<طۡ>بُكُمَاۖ قَالَتَا لَا نَسۡقِی حَتَّىٰ یُصۡدِR<رَ> ٱX<ل>رِّعَW<اۤ>ءُۖ وَأَبُونَا شَیۡخN<ࣱ> كَبِJ<ی>رࣱ A<۝>D<٢٣>",
"فَسَقَىٰ لَهُمَا ثُN<مَّ> تَوَلَّىW<ٰۤ> إِلَى ٱX<ل>ظِّلِّ فَقَالَ R<رَ>بِّ إِN<نِّ>ی لِمَW<اۤ> أَN<ن>زَلۡتَ إِلَیَّ مِنۡ خَیۡرN<ࣲ> فَقِJ<ی>رࣱ A<۝>D<٢٤>",
"فَجَW<اۤ>ءَتۡهُ إِحۡدَX<ى>T<ٰ>هُمَا تَمۡشِی عَلَى ٱسۡتِحۡیَW<اۤ>ءN<ࣲ> قَالَتۡ إِN<نَّ> أَبِی یَQ<دۡ>عُوكَ لِیَQ<جۡ>زِیَكَ أَQ<جۡ>R<رَ> مَا سَقَیۡتَ لَنَاۚ فَلَN<مَّ>ا جَW<اۤ>ءَهُT<ۥ> وَقَصَّ عَلَیۡهِ ٱلۡقَصَصَ قَالَ لَا تَخَفۡۖ نَجَوۡتَ مِنَ ٱلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢٥>",
"قَالَتۡ إِحۡدَX<ى>T<ٰ>هُمَا یَW<ـٰۤ>أَبَتِ ٱسۡتَءۡجِرۡهُۖ إِN<نَّ> خَیۡR<رَ> مَنِ ٱسۡتَءۡجَR<رۡ>تَ ٱلۡقَوِیُّ ٱلۡأَمِJ<ی>نُ A<۝>D<٢٦>",
"قَالَ إِN<نِّ>W<یۤ> أُرِیدُ أَنۡ أُN<ن>كِحَكَ إِحۡدَى ٱQ<بۡ>نَتَیَّ هَT<ـٰ>تَیۡنِ عَلَىW<ٰۤ> أَN<ن> تَأۡجُR<رَ>نِی ثَمَT<ـٰ>نِیَ حِجَجN<ࣲ>ۖ فَإِنۡ أَتۡمَمۡتَ عَشۡR<ر>N<ࣰ>ا فَمِنۡ عِN<ن>دِكَۖ وَمَW<اۤ> أُرِیدُ أَنۡ أَشُقَّ عَلَیۡكَۚ سَتَجِدُنِW<یۤ> إِN<ن> شَW<اۤ>ءَ ٱللَّهُ مِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٢٧>",
"قَالَ ذَT< ٰ>لِكَ بَیۡنِی وَبَیۡنَكَۖ أَیَّمَا ٱلۡأَجَلَیۡنِ قَضَیۡتُ فَلَا عُQ<دۡ>وَT< ٰ>نَ عَلَیَّۖ وَٱللَّهُ عَلَىٰ مَا نَقُولُ وَكِJ<ی>لࣱ A<۝>D<٢٨>",
"۞ فَلَN<مَّ>ا قَضَىٰ مُوسَى ٱلۡأَجَلَ وَسَاR<رَ> بِأَهۡلِهِW<ۦۤ> ءَانَسَ مِN<ن> جَانِبِ ٱX<ل>طُّورِ نَاR<ر>N<ࣰ>اۖ قَالَ لِأَهۡلِهِ ٱمۡكُثُW<وۤ>X<ا۟> إِN<نِّ>W<یۤ> ءَانَسۡتُ نَاR<ر>X<ࣰ>ا لَّعَلِّW<یۤ> ءَاتِیكُم N<مِّ>نۡهَا بِخَبَرٍ أَوۡ جَذۡوَةX<ࣲ> N<مِّ>نَ ٱX<ل>N<نَّ>ارِ لَعَلَّكُمۡ تَصۡطَلُJ<و>نَ A<۝>D<٢٩>",
"فَلَN<مَّ>W<اۤ> أَتَX<ى>T<ٰ>هَا نُودِیَ مِN<ن> شَT<ـٰ>طِئِ ٱلۡوَادِ ٱلۡأَیۡمَنِ فِی ٱلۡبُQ<قۡ>عَةِ ٱلۡمُبَT<ـٰ>R<رَ>كَةِ مِنَ ٱX<ل>شَّجَR<رَ>ةِ أَX<ن> N<یَ>T<ـٰ>مُوسَىW<ٰۤ> إِN<نِّ>W<یۤ> أَنَا ٱللَّهُ R<رَ>بُّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٣٠>",
"وَأَنۡ أَلۡقِ عَصَاكَۚ فَلَN<مَّ>ا R<رَ>ءَاهَا تَهۡتَزُّ كَأَN<نَّ>هَا جَL<اۤ>N<نّ>X<ࣱ> N<وَ>لَّىٰ مُQ<دۡ>بِR<ر>X<ࣰ>ا N<وَ>لَمۡ یُعَقِّQ<بۡ>ۚ یَT<ـٰ>مُوسَىW<ٰۤ> أَQ<قۡ>بِلۡ وَلَا تَخَفۡۖ إِN<نَّ>كَ مِنَ ٱلۡءَامِنِJ<ی>نَ A<۝>D<٣١>",
"ٱسۡلُكۡ یَدَكَ فِی جَیۡبِكَ تَخۡR<رُ>Q<جۡ> بَیۡضَW<اۤ>ءَ مِنۡ غَیۡرِ سُW<وۤ>ءX<ࣲ> N<وَ>X<ٱ>ضۡمُمۡ إِلَیۡكَ جَنَاحَكَ مِنَ ٱX<ل>R<رَّ>هۡبِۖ فَذَT< ٰ>نِكَ بُR<رۡ>هَT<ـٰ>نَانِ مِX<ن> R<رَّ>بِّكَ إِلَىٰ فِرۡعَوۡنَ وَمَلَإِX<ی۟>هِW<ۦۤ>ۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> قَوۡمN<ࣰ>ا فَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٣٢>",
"قَالَ R<رَ>بِّ إِN<نِّ>ی قَتَلۡتُ مِنۡهُمۡ نَفۡسN<ࣰ>ا فَأَخَافُ أَX<ن> N<یَ>Q<قۡ>تُلُJ<و>نِ A<۝>D<٣٣>",
"وَأَخِی هَT<ـٰ>R<رُ>ونُ هُوَ أَفۡصَحُ مِN<نِّ>ی لِسَانN<ࣰ>ا فَأَR<رۡ>سِلۡهُ مَعِیَ رِQ<دۡ>ءX<ࣰ>ا N<یُ>صَدِّقُنِW<یۤ>ۖ إِN<نِّ>W<یۤ> أَخَافُ أَX<ن> N<یُ>كَذِّبُJ<و>نِ A<۝>D<٣٤>",
"قَالَ سَنَشُدُّ عَضُدَكَ بِأَخِیكَ وَنَQ<جۡ>عَلُ لَكُمَا سُلۡطَT<ـٰ>نN<ࣰ>ا فَلَا یَصِلُونَ إِلَیۡكُمَا بِءَایَT<ـٰ>تِنَW<اۤ>ۚ أَN<ن>تُمَا وَمَنِ ٱتَّبَعَكُمَا ٱلۡغَT<ـٰ>لِبُJ<و>نَ A<۝>D<٣٥>",
"فَلَN<مَّ>ا جَW<اۤ>ءَهُم N<مُّ>وسَىٰ بِءَایَT<ـٰ>تِنَا بَیِّنَT<ـٰ>تN<ࣲ> قَالُوX<ا۟> مَا هَT<ـٰ>ذَW<اۤ> إِلَّا سِحۡR<ر>X<ࣱ> N<مُّ>فۡتَR<ر>X<ࣰ>ى N<وَ>مَا سَمِعۡنَا بِهَT<ـٰ>ذَا فِW<یۤ> ءَابَW<اۤ>ئِنَا ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٣٦>",
"وَقَالَ مُوسَىٰ R<رَ>بِّW<یۤ> أَعۡلَمُ بِمَN<ن> جَW<اۤ>ءَ بِX<ٱ>لۡهُدَىٰ مِنۡ عِN<ن>دِهِT<ۦ> وَمَN<ن> تَكُونُ لَهُT<ۥ> عَT<ـٰ>قِبَةُ ٱX<ل>دَّارِۚ إِN<نَّ>هُT<ۥ> لَا یُفۡلِحُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٣٧>",
"وَقَالَ فِرۡعَوۡنُ یَW<ـٰۤ>أَیُّهَا ٱلۡمَلَأُ مَا عَلِمۡتُ لَكُم N<مِّ>نۡ إِلَT<ـٰ>هٍ غَیۡرِی فَأَوۡقِQ<دۡ> لِی یَT<ـٰ>هَT<ـٰ>مَT<ـٰ>نُ عَلَى ٱX<ل>طِّینِ فَX<ٱ>Q<جۡ>عَل لِّی صَR<رۡ>حX<ࣰ>ا لَّعَلِّW<یۤ> أَطَّلِعُ إِلَىW<ٰۤ> إِلَT<ـٰ>هِ مُوسَىٰ وَإِN<نِّ>ی لَأَظُN<نُّ>هُT<ۥ> مِنَ ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٣٨>",
"وَX<ٱ>سۡتَكۡبَR<رَ> هُوَ وَجُنُودُهُT<ۥ> فِی ٱلۡأَR<رۡ>ضِ بِغَیۡرِ ٱلۡحَقِّ وَظَN<نُّ>W<وۤ>X<ا۟> أَN<نَّ>هُمۡ إِلَیۡنَا لَا یُR<رۡ>جَعُJ<و>نَ A<۝>D<٣٩>",
"فَأَخَذۡنَT<ـٰ>هُ وَجُنُودَهُT<ۥ> فَنَبَذۡنَT<ـٰ>هُمۡ فِی ٱلۡیَN<مِّ>ۖ فَX<ٱ>N<ن>ظُR<رۡ> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤٠>",
"وَجَعَلۡنَT<ـٰ>هُمۡ أَئِN<مَّ>ةX<ࣰ> N<یَ>Q<دۡ>عُونَ إِلَى ٱX<ل>N<نَّ>ارِۖ وَیَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ لَا یُN<ن>صَR<رُ>J<و>نَ A<۝>D<٤١>",
"وَأَتۡبَعۡنَT<ـٰ>هُمۡ فِی هَT<ـٰ>ذِهِ ٱX<ل>دُّنۡیَا لَعۡنَةX<ࣰ>ۖ N<وَ>یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ هُم N<مِّ>نَ ٱلۡمَQ<قۡ>بُوحِJ<ی>نَ A<۝>D<٤٢>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ مِX<ن>N<ۢ> بَعۡدِ مَW<اۤ> أَهۡلَكۡنَا ٱلۡقُR<رُ>ونَ ٱلۡأُولَىٰ بَصَW<اۤ>ئِR<رَ> لِX<ل>N<نَّ>اسِ وَهُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣰ> لَّعَلَّهُمۡ یَتَذَكَّR<رُ>J<و>نَ A<۝>D<٤٣>",
"وَمَا كُN<ن>تَ بِجَانِبِ ٱلۡغَR<رۡ>بِیِّ إِذۡ قَضَیۡنَW<اۤ> إِلَىٰ مُوسَى ٱلۡأَمۡR<رَ> وَمَا كُN<ن>تَ مِنَ ٱX<ل>شَّT<ـٰ>هِدِJ<ی>نَ A<۝>D<٤٤>",
"وَلَT<ـٰ>كِN<نَّ>W<اۤ> أَN<ن>شَأۡنَا قُR<رُ>ونN<ࣰ>ا فَتَطَاوَلَ عَلَیۡهِمُ ٱلۡعُمُR<رُ>ۚ وَمَا كُN<ن>تَ ثَاوِیN<ࣰ>ا فِW<یۤ> أَهۡلِ مَQ<دۡ>یَنَ تَتۡلُوX<ا۟> عَلَیۡهِمۡ ءَایَT<ـٰ>تِنَا وَلَT<ـٰ>كِN<نَّ>ا كُN<نَّ>ا مُR<رۡ>سِلِJ<ی>نَ A<۝>D<٤٥>",
"وَمَا كُN<ن>تَ بِجَانِبِ ٱX<ل>طُّورِ إِذۡ نَادَیۡنَا وَلَT<ـٰ>كِX<ن> R<رَّ>حۡمَةX<ࣰ> N<مِّ>X<ن> R<رَّ>بِّكَ لِتُN<ن>ذِR<رَ> قَوۡمX<ࣰ>ا N<مَّ>W<اۤ> أَتَX<ى>T<ٰ>هُم N<مِّ>ن N<نَّ>ذِیرX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِكَ لَعَلَّهُمۡ یَتَذَكَّR<رُ>J<و>نَ A<۝>D<٤٦>",
"وَلَوۡلَW<اۤ> أَN<ن> تُصِیبَهُم N<مُّ>صِیبَةُN<ۢ> بِمَا قَدَّمَتۡ أَیۡدِیهِمۡ فَیَقُولُوX<ا۟> R<رَ>بَّنَا لَوۡلَW<اۤ> أَR<رۡ>سَلۡتَ إِلَیۡنَا R<رَ>سُولN<ࣰ>ا فَنَتَّبِعَ ءَایَT<ـٰ>تِكَ وَنَكُونَ مِنَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٤٧>",
"فَلَN<مَّ>ا جَW<اۤ>ءَهُمُ ٱلۡحَقُّ مِنۡ عِN<ن>دِنَا قَالُوX<ا۟> لَوۡلَW<اۤ> أُوتِیَ مِثۡلَ مَW<اۤ> أُوتِیَ مُوسَىW<ٰۤ>ۚ أَوَ لَمۡ یَكۡفُR<رُ>وX<ا۟> بِمَW<اۤ> أُوتِیَ مُوسَىٰ مِN<ن> قَQ<بۡ>لُۖ قَالُوX<ا۟> سِحۡR<رَ>انِ تَظَT<ـٰ>هَR<رَ>ا وَقَالُW<وۤ>X<ا۟> إِN<نَّ>ا بِكُلّN<ࣲ> كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٤٨>",
"قُلۡ فَأۡتُوX<ا۟> بِكِتَT<ـٰ>بX<ࣲ> N<مِّ>نۡ عِN<ن>دِ ٱللَّهِ هُوَ أَهۡدَىٰ مِنۡهُمَW<اۤ> أَتَّبِعۡهُ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٤٩>",
"فَإِX<ن> لَّمۡ یَسۡتَجِیبُوX<ا۟> لَكَ فَX<ٱ>عۡلَمۡ أَN<نَّ>مَا یَتَّبِعُونَ أَهۡوَW<اۤ>ءَهُمۡۚ وَمَنۡ أَضَلُّ مِN<مَّ>نِ ٱتَّبَعَ هَوَX<ى>T<ٰ>هُ بِغَیۡرِ هُدX<ࣰ>ى N<مِّ>نَ ٱللَّهِۚ إِN<نَّ> ٱللَّهَ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥٠>",
"۞ وَلَقَQ<دۡ> وَصَّلۡنَا لَهُمُ ٱلۡقَوۡلَ لَعَلَّهُمۡ یَتَذَكَّR<رُ>J<و>نَ A<۝>D<٥١>",
"ٱلَّذِینَ ءَاتَیۡنَT<ـٰ>هُمُ ٱلۡكِتَT<ـٰ>بَ مِN<ن> قَQ<بۡ>لِهِT<ۦ> هُN<م> بِهِT<ۦ> یُؤۡمِنُJ<و>نَ A<۝>D<٥٢>",
"وَإِذَا یُتۡلَىٰ عَلَیۡهِمۡ قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا بِهِW<ۦۤ> إِN<نَّ>هُ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّنَW<اۤ> إِN<نَّ>ا كُN<نَّ>ا مِN<ن> قَQ<بۡ>لِهِT<ۦ> مُسۡلِمِJ<ی>نَ A<۝>D<٥٣>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ یُؤۡتَوۡنَ أَQ<جۡ>R<رَ>هُم N<مَّ>R<رَّ>تَیۡنِ بِمَا صَبَR<رُ>وX<ا۟> وَیَQ<دۡ>R<رَ>ءُونَ بِX<ٱ>لۡحَسَنَةِ ٱX<ل>سَّیِّئَةَ وَمِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡ یُN<ن>فِقُJ<و>نَ A<۝>D<٥٤>",
"وَإِذَا سَمِعُوX<ا۟> ٱX<ل>لَّغۡوَ أَعۡR<رَ>ضُوX<ا۟> عَنۡهُ وَقَالُوX<ا۟> لَنَW<اۤ> أَعۡمَT<ـٰ>لُنَا وَلَكُمۡ أَعۡمَT<ـٰ>لُكُمۡ سَلَT<ـٰ>مٌ عَلَیۡكُمۡ لَا نَQ<بۡ>تَغِی ٱلۡجَT<ـٰ>هِلِJ<ی>نَ A<۝>D<٥٥>",
"إِN<نَّ>كَ لَا تَهۡدِی مَنۡ أَحۡبَQ<بۡ>تَ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ یَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَهُوَ أَعۡلَمُ بِX<ٱ>لۡمُهۡتَدِJ<ی>نَ A<۝>D<٥٦>",
"وَقَالُW<وۤ>X<ا۟> إِن N<نَّ>تَّبِعِ ٱلۡهُدَىٰ مَعَكَ نُتَخَطَّفۡ مِنۡ أَR<رۡ>ضِنَW<اۤ>ۚ أَوَ لَمۡ نُمَكِّX<ن> لَّهُمۡ حَR<رَ>مًا ءَامِنX<ࣰ>ا N<یُ>Q<جۡ>بَىW<ٰۤ> إِلَیۡهِ ثَمَR<رَ>T< ٰ>تُ كُلِّ شَیۡءX<ࣲ> رِّزۡقX<ࣰ>ا N<مِّ>X<ن> لَّدُN<نَّ>ا وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٥٧>",
"وَكَمۡ أَهۡلَكۡنَا مِN<ن> قَR<رۡ>یَةِN<ۭ> بَطِR<رَ>تۡ مَعِیشَتَهَاۖ فَتِلۡكَ مَسَT<ـٰ>كِنُهُمۡ لَمۡ تُسۡكَX<ن> N<مِّ>X<ن>N<ۢ> بَعۡدِهِمۡ إِلَّا قَلِیلX<ࣰ>اۖ N<وَ>كُN<نَّ>ا نَحۡنُ ٱلۡوَT< ٰ>رِثِJ<ی>نَ A<۝>D<٥٨>",
"وَمَا كَانَ R<رَ>بُّكَ مُهۡلِكَ ٱلۡقُR<رَ>ىٰ حَتَّىٰ یَQ<بۡ>عَثَ فِW<یۤ> أُN<مِّ>هَا R<رَ>سُولX<ࣰ>ا N<یَ>تۡلُوX<ا۟> عَلَیۡهِمۡ ءَایَT<ـٰ>تِنَاۚ وَمَا كُN<نَّ>ا مُهۡلِكِی ٱلۡقُR<رَ>ىW<ٰۤ> إِلَّا وَأَهۡلُهَا ظَT<ـٰ>لِمُJ<و>نَ A<۝>D<٥٩>",
"وَمَW<اۤ> أُوتِیتُم N<مِّ>N<ن> شَیۡءN<ࣲ> فَمَتَT<ـٰ>عُ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَزِینَتُهَاۚ وَمَا عِN<ن>دَ ٱللَّهِ خَیۡR<ر>X<ࣱ> N<وَ>أَQ<بۡ>قَىW<ٰۤ>ۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<٦٠>",
"أَفَمَX<ن> N<وَ>عَQ<دۡ>نَT<ـٰ>هُ وَعۡدًا حَسَنN<ࣰ>ا فَهُوَ لَT<ـٰ>قِیهِ كَمَX<ن> N<مَّ>تَّعۡنَT<ـٰ>هُ مَتَT<ـٰ>عَ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا ثُN<مَّ> هُوَ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ مِنَ ٱلۡمُحۡضَرِJ<ی>نَ A<۝>D<٦١>",
"وَیَوۡمَ یُنَادِیهِمۡ فَیَقُولُ أَیۡنَ شُR<رَ>كَW<اۤ>ءِیَ ٱلَّذِینَ كُN<ن>تُمۡ تَزۡعُمُJ<و>نَ A<۝>D<٦٢>",
"قَالَ ٱلَّذِینَ حَقَّ عَلَیۡهِمُ ٱلۡقَوۡلُ R<رَ>بَّنَا هَW<ـٰۤ>ؤُلَW<اۤ>ءِ ٱلَّذِینَ أَغۡوَیۡنَW<اۤ> أَغۡوَیۡنَT<ـٰ>هُمۡ كَمَا غَوَیۡنَاۖ تَبَR<رَّ>أۡنَW<اۤ> إِلَیۡكَۖ مَا كَانُW<وۤ>X<ا۟> إِیَّانَا یَعۡبُدُJ<و>نَ A<۝>D<٦٣>",
"وَقِیلَ ٱQ<دۡ>عُوX<ا۟> شُR<رَ>كَW<اۤ>ءَكُمۡ فَدَعَوۡهُمۡ فَلَمۡ یَسۡتَجِیبُوX<ا۟> لَهُمۡ وَR<رَ>أَوُX<ا۟> ٱلۡعَذَابَۚ لَوۡ أَN<نَّ>هُمۡ كَانُوX<ا۟> یَهۡتَدُJ<و>نَ A<۝>D<٦٤>",
"وَیَوۡمَ یُنَادِیهِمۡ فَیَقُولُ مَاذَW<اۤ> أَجَQ<بۡ>تُمُ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٦٥>",
"فَعَمِیَتۡ عَلَیۡهِمُ ٱلۡأَX<ن>N<ۢ>بَW<اۤ>ءُ یَوۡمَئِذN<ࣲ> فَهُمۡ لَا یَتَسَW<اۤ>ءَلُJ<و>نَ A<۝>D<٦٦>",
"فَأَN<مَّ>ا مَN<ن> تَابَ وَءَامَنَ وَعَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَعَسَىW<ٰۤ> أَX<ن> N<یَ>كُونَ مِنَ ٱلۡمُفۡلِحِJ<ی>نَ A<۝>D<٦٧>",
"وَR<رَ>بُّكَ یَخۡلُقُ مَا یَشَW<اۤ>ءُ وَیَخۡتَاR<رُ>ۗ مَا كَانَ لَهُمُ ٱلۡخِیَR<رَ>ةُۚ سُQ<بۡ>حَT<ـٰ>نَ ٱللَّهِ وَتَعَT<ـٰ>لَىٰ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٦٨>",
"وَR<رَ>بُّكَ یَعۡلَمُ مَا تُكِN<نُّ> صُدُوR<رُ>هُمۡ وَمَا یُعۡلِنُJ<و>نَ A<۝>D<٦٩>",
"وَهُوَ ٱللَّهُ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ لَهُ ٱلۡحَمۡدُ فِی ٱلۡأُولَىٰ وَX<ٱ>لۡءَاخِR<رَ>ةِۖ وَلَهُ ٱلۡحُكۡمُ وَإِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٧٠>",
"قُلۡ أَR<رَ>ءَیۡتُمۡ إِN<ن> جَعَلَ ٱللَّهُ عَلَیۡكُمُ ٱلَّیۡلَ سَR<رۡ>مَدًا إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ مَنۡ إِلَT<ـٰ>هٌ غَیۡR<رُ> ٱللَّهِ یَأۡتِیكُN<م> بِضِیَW<اۤ>ءٍۚ أَفَلَا تَسۡمَعُJ<و>نَ A<۝>D<٧١>",
"قُلۡ أَR<رَ>ءَیۡتُمۡ إِN<ن> جَعَلَ ٱللَّهُ عَلَیۡكُمُ ٱX<ل>N<نَّ>هَاR<رَ> سَR<رۡ>مَدًا إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ مَنۡ إِلَT<ـٰ>هٌ غَیۡR<رُ> ٱللَّهِ یَأۡتِیكُN<م> بِلَیۡلN<ࣲ> تَسۡكُنُونَ فِیهِۚ أَفَلَا تُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٧٢>",
"وَمِX<ن> R<رَّ>حۡمَتِهِT<ۦ> جَعَلَ لَكُمُ ٱلَّیۡلَ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ> لِتَسۡكُنُوX<ا۟> فِیهِ وَلِتَQ<بۡ>تَغُوX<ا۟> مِN<ن> فَضۡلِهِT<ۦ> وَلَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<٧٣>",
"وَیَوۡمَ یُنَادِیهِمۡ فَیَقُولُ أَیۡنَ شُR<رَ>كَW<اۤ>ءِیَ ٱلَّذِینَ كُN<ن>تُمۡ تَزۡعُمُJ<و>نَ A<۝>D<٧٤>",
"وَنَزَعۡنَا مِN<ن> كُلِّ أُN<مَّ>ةN<ࣲ> شَهِیدN<ࣰ>ا فَقُلۡنَا هَاتُوX<ا۟> بُR<رۡ>هَT<ـٰ>نَكُمۡ فَعَلِمُW<وۤ>X<ا۟> أَN<نَّ> ٱلۡحَقَّ لِلَّهِ وَضَلَّ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<٧٥>",
"۞ إِN<نَّ> قَT<ـٰ>R<رُ>ونَ كَانَ مِN<ن> قَوۡمِ مُوسَىٰ فَبَغَىٰ عَلَیۡهِمۡۖ وَءَاتَیۡنَT<ـٰ>هُ مِنَ ٱلۡكُنُوزِ مَW<اۤ> إِN<نَّ> مَفَاتِحَهُT<ۥ> لَتَنُW<وۤ>أُ بِX<ٱ>لۡعُصۡبَةِ أُX<و۟>لِی ٱلۡقُوَّةِ إِذۡ قَالَ لَهُT<ۥ> قَوۡمُهُT<ۥ> لَا تَفۡR<رَ>حۡۖ إِN<نَّ> ٱللَّهَ لَا یُحِبُّ ٱلۡفَرِحِJ<ی>نَ A<۝>D<٧٦>",
"وَX<ٱ>Q<بۡ>تَغِ فِیمَW<اۤ> ءَاتَX<ى>T<ٰ>كَ ٱللَّهُ ٱX<ل>دَّاR<رَ> ٱلۡءَاخِR<رَ>ةَۖ وَلَا تَN<ن>سَ نَصِیبَكَ مِنَ ٱX<ل>دُّنۡیَاۖ وَأَحۡسِN<ن> كَمَW<اۤ> أَحۡسَنَ ٱللَّهُ إِلَیۡكَۖ وَلَا تَQ<بۡ>غِ ٱلۡفَسَادَ فِی ٱلۡأَR<رۡ>ضِۖ إِN<نَّ> ٱللَّهَ لَا یُحِبُّ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<٧٧>",
"قَالَ إِN<نَّ>مَW<اۤ> أُوتِیتُهُT<ۥ> عَلَىٰ عِلۡمٍ عِN<ن>دِW<یۤ>ۚ أَوَ لَمۡ یَعۡلَمۡ أَN<نَّ> ٱللَّهَ قَQ<دۡ> أَهۡلَكَ مِN<ن> قَQ<بۡ>لِهِT<ۦ> مِنَ ٱلۡقُR<رُ>ونِ مَنۡ هُوَ أَشَدُّ مِنۡهُ قُوَّةX<ࣰ> N<وَ>أَكۡثَR<رُ> جَمۡعX<ࣰ>اۚ N<وَ>لَا یُسۡءَلُ عَN<ن> ذُنُوبِهِمُ ٱلۡمُQ<جۡ>رِمُJ<و>نَ A<۝>D<٧٨>",
"فَخَR<رَ>جَ عَلَىٰ قَوۡمِهِT<ۦ> فِی زِینَتِهِT<ۦ>ۖ قَالَ ٱلَّذِینَ یُرِیدُونَ ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا یَT<ـٰ>لَیۡتَ لَنَا مِثۡلَ مَW<اۤ> أُوتِیَ قَT<ـٰ>R<رُ>ونُ إِN<نَّ>هُT<ۥ> لَذُو حَظٍّ عَظِJ<ی>مࣲ A<۝>D<٧٩>",
"وَقَالَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡعِلۡمَ وَیۡلَكُمۡ ثَوَابُ ٱللَّهِ خَیۡR<ر>X<ࣱ> لِّمَنۡ ءَامَنَ وَعَمِلَ صَT<ـٰ>لِحX<ࣰ>اۚ N<وَ>لَا یُلَقَّX<ى>T<ٰ>هَW<اۤ> إِلَّا ٱX<ل>صَّT<ـٰ>بِR<رُ>J<و>نَ A<۝>D<٨٠>",
"فَخَسَفۡنَا بِهِT<ۦ> وَبِدَارِهِ ٱلۡأَR<رۡ>ضَ فَمَا كَانَ لَهُT<ۥ> مِN<ن> فِئَةX<ࣲ> N<یَ>N<ن>صُR<رُ>ونَهُT<ۥ> مِN<ن> دُونِ ٱللَّهِ وَمَا كَانَ مِنَ ٱلۡمُN<ن>تَصِرِJ<ی>نَ A<۝>D<٨١>",
"وَأَصۡبَحَ ٱلَّذِینَ تَمَN<نَّ>وۡX<ا۟> مَكَانَهُT<ۥ> بِX<ٱ>لۡأَمۡسِ یَقُولُونَ وَیۡكَأَN<نَّ> ٱللَّهَ یَQ<بۡ>سُطُ ٱX<ل>رِّزۡقَ لِمَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِT<ۦ> وَیَQ<قۡ>دِR<رُ>ۖ لَوۡلَW<اۤ> أَX<ن> N<مَّ>N<نَّ> ٱللَّهُ عَلَیۡنَا لَخَسَفَ بِنَاۖ وَیۡكَأَN<نَّ>هُT<ۥ> لَا یُفۡلِحُ ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٨٢>",
"تِلۡكَ ٱX<ل>دَّاR<رُ> ٱلۡءَاخِR<رَ>ةُ نَQ<جۡ>عَلُهَا لِلَّذِینَ لَا یُرِیدُونَ عُلُوّN<ࣰ>ا فِی ٱلۡأَR<رۡ>ضِ وَلَا فَسَادX<ࣰ>اۚ N<وَ>X<ٱ>لۡعَT<ـٰ>قِبَةُ لِلۡمُتَّقِJ<ی>نَ A<۝>D<٨٣>",
"مَN<ن> جَW<اۤ>ءَ بِX<ٱ>لۡحَسَنَةِ فَلَهُT<ۥ> خَیۡR<ر>X<ࣱ> N<مِّ>نۡهَاۖ وَمَN<ن> جَW<اۤ>ءَ بِX<ٱ>X<ل>سَّیِّئَةِ فَلَا یُQ<جۡ>زَى ٱلَّذِینَ عَمِلُوX<ا۟> ٱX<ل>سَّیِّءَاتِ إِلَّا مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٨٤>",
"إِN<نَّ> ٱلَّذِی فَR<رَ>ضَ عَلَیۡكَ ٱلۡقُR<رۡ>ءَانَ لَR<رَ>L<اۤ>دُّكَ إِلَىٰ مَعَادN<ࣲ>ۚ قُل R<رَّ>بِّW<یۤ> أَعۡلَمُ مَN<ن> جَW<اۤ>ءَ بِX<ٱ>لۡهُدَىٰ وَمَنۡ هُوَ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٨٥>",
"وَمَا كُN<ن>تَ تَR<رۡ>جُW<وۤ>X<ا۟> أَX<ن> N<یُ>لۡقَىW<ٰۤ> إِلَیۡكَ ٱلۡكِتَT<ـٰ>بُ إِلَّا R<رَ>حۡمَةX<ࣰ> N<مِّ>X<ن> R<رَّ>بِّكَۖ فَلَا تَكُونَN<نَّ> ظَهِیR<ر>X<ࣰ>ا لِّلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٨٦>",
"وَلَا یَصُدُّN<نَّ>كَ عَنۡ ءَایَT<ـٰ>تِ ٱللَّهِ بَعۡدَ إِذۡ أُN<ن>زِلَتۡ إِلَیۡكَۖ وَX<ٱ>Q<دۡ>عُ إِلَىٰ R<رَ>بِّكَۖ وَلَا تَكُونَN<نَّ> مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<٨٧>",
"وَلَا تَQ<دۡ>عُ مَعَ ٱللَّهِ إِلَT<ـٰ>هًا ءَاخَR<رَ>ۘ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۚ كُلُّ شَیۡءٍ هَالِكٌ إِلَّا وَQ<جۡ>هَهُT<ۥ>ۚ لَهُ ٱلۡحُكۡمُ وَإِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٨٨>",
"#۞ اL<لۤ>L<مۤ> A<۝>D<١>",
"أَحَسِبَ ٱX<ل>N<نَّ>اسُ أَX<ن> N<یُ>تۡR<رَ>كُW<وۤ>X<ا۟> أَX<ن> N<یَ>قُولُW<وۤ>X<ا۟> ءَامَN<نَّ>ا وَهُمۡ لَا یُفۡتَنُJ<و>نَ A<۝>D<٢>",
"وَلَقَQ<دۡ> فَتَN<نَّ>ا ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۖ فَلَیَعۡلَمَN<نَّ> ٱللَّهُ ٱلَّذِینَ صَدَقُوX<ا۟> وَلَیَعۡلَمَN<نَّ> ٱلۡكَT<ـٰ>ذِبِJ<ی>نَ A<۝>D<٣>",
"أَمۡ حَسِبَ ٱلَّذِینَ یَعۡمَلُونَ ٱX<ل>سَّیِّءَاتِ أَX<ن> N<یَ>سۡبِقُونَاۚ سَW<اۤ>ءَ مَا یَحۡكُمُJ<و>نَ A<۝>D<٤>",
"مَN<ن> كَانَ یَR<رۡ>جُوX<ا۟> لِقَW<اۤ>ءَ ٱللَّهِ فَإِN<نَّ> أَجَلَ ٱللَّهِ لَءَاتX<ࣲ>ۚ N<وَ>هُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٥>",
"وَمَN<ن> جَT<ـٰ>هَدَ فَإِN<نَّ>مَا یُجَT<ـٰ>هِدُ لِنَفۡسِهِW<ۦۤ>ۚ إِN<نَّ> ٱللَّهَ لَغَنِیٌّ عَنِ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٦>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَنُكَفِّR<رَ>N<نَّ> عَنۡهُمۡ سَیِّءَاتِهِمۡ وَلَنَQ<جۡ>زِیَN<نَّ>هُمۡ أَحۡسَنَ ٱلَّذِی كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٧>",
"وَوَصَّیۡنَا ٱلۡإِN<ن>سَT<ـٰ>نَ بِوَT< ٰ>لِدَیۡهِ حُسۡنX<ࣰ>اۖ N<وَ>إِN<ن> جَT<ـٰ>هَدَT< ٰ>كَ لِتُشۡرِكَ بِی مَا لَیۡسَ لَكَ بِهِT<ۦ> عِلۡمN<ࣱ> فَلَا تُطِعۡهُمَW<اۤ>ۚ إِلَیَّ مَR<رۡ>جِعُكُمۡ فَأُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٨>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَنُQ<دۡ>خِلَN<نَّ>هُمۡ فِی ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٩>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یَ>قُولُ ءَامَN<نَّ>ا بِٱللَّهِ فَإِذَW<اۤ> أُوذِیَ فِی ٱللَّهِ جَعَلَ فِتۡنَةَ ٱX<ل>N<نَّ>اسِ كَعَذَابِ ٱللَّهِۖ وَلَئِN<ن> جَW<اۤ>ءَ نَصۡR<ر>X<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكَ لَیَقُولُN<نَّ> إِN<نَّ>ا كُN<نَّ>ا مَعَكُمۡۚ أَوَ لَیۡسَ ٱللَّهُ بِأَعۡلَمَ بِمَا فِی صُدُورِ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٠>",
"وَلَیَعۡلَمَN<نَّ> ٱللَّهُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَلَیَعۡلَمَN<نَّ> ٱلۡمُنَT<ـٰ>فِقِJ<ی>نَ A<۝>D<١١>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لِلَّذِینَ ءَامَنُوX<ا۟> ٱتَّبِعُوX<ا۟> سَبِیلَنَا وَلۡنَحۡمِلۡ خَطَT<ـٰ>یَT<ـٰ>كُمۡ وَمَا هُN<م> بِحَT<ـٰ>مِلِینَ مِنۡ خَطَT<ـٰ>یَT<ـٰ>هُم N<مِّ>N<ن> شَیۡءٍۖ إِN<نَّ>هُمۡ لَكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<١٢>",
"وَلَیَحۡمِلُN<نَّ> أَثۡقَالَهُمۡ وَأَثۡقَالX<ࣰ>ا N<مَّ>عَ أَثۡقَالِهِمۡۖ وَلَیُسۡءَلُN<نَّ> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ عَN<مَّ>ا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<١٣>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا نُوحًا إِلَىٰ قَوۡمِهِT<ۦ> فَلَبِثَ فِیهِمۡ أَلۡفَ سَنَةٍ إِلَّا خَمۡسِینَ عَامN<ࣰ>ا فَأَخَذَهُمُ ٱX<ل>طُّوفَانُ وَهُمۡ ظَT<ـٰ>لِمُJ<و>نَ A<۝>D<١٤>",
"فَأَN<ن>جَیۡنَT<ـٰ>هُ وَأَصۡحَT<ـٰ>بَ ٱX<ل>سَّفِینَةِ وَجَعَلۡنَT<ـٰ>هَW<اۤ> ءَایَةX<ࣰ> لِّلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٥>",
"وَإِQ<بۡ>R<رَ>T< ٰ>هِیمَ إِذۡ قَالَ لِقَوۡمِهِ ٱعۡبُدُوX<ا۟> ٱللَّهَ وَX<ٱ>تَّقُوهُۖ ذَT< ٰ>لِكُمۡ خَیۡR<ر>X<ࣱ> لَّكُمۡ إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<١٦>",
"إِN<نَّ>مَا تَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ أَوۡثَT<ـٰ>نX<ࣰ>ا N<وَ>تَخۡلُقُونَ إِفۡكًاۚ إِN<نَّ> ٱلَّذِینَ تَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ لَا یَمۡلِكُونَ لَكُمۡ رِزۡقN<ࣰ>ا فَX<ٱ>Q<بۡ>تَغُوX<ا۟> عِN<ن>دَ ٱللَّهِ ٱX<ل>رِّزۡقَ وَX<ٱ>عۡبُدُوهُ وَX<ٱ>شۡكُR<رُ>وX<ا۟> لَهُW<ۥۤ>ۖ إِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<١٧>",
"وَإِN<ن> تُكَذِّبُوX<ا۟> فَقَQ<دۡ> كَذَّبَ أُمَمX<ࣱ> N<مِّ>N<ن> قَQ<بۡ>لِكُمۡۖ وَمَا عَلَى ٱX<ل>R<رَّ>سُولِ إِلَّا ٱلۡبَلَT<ـٰ>غُ ٱلۡمُبِJ<ی>نُ A<۝>D<١٨>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> كَیۡفَ یُQ<بۡ>دِئُ ٱللَّهُ ٱلۡخَلۡقَ ثُN<مَّ> یُعِیدُهُW<ۥۤ>ۚ إِN<نَّ> ذَT< ٰ>لِكَ عَلَى ٱللَّهِ یَسِJ<ی>رࣱ A<۝>D<١٩>",
"قُلۡ سِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَX<ٱ>N<ن>ظُR<رُ>وX<ا۟> كَیۡفَ بَدَأَ ٱلۡخَلۡقَۚ ثُN<مَّ> ٱللَّهُ یُN<ن>شِئُ ٱX<ل>N<نَّ>شۡأَةَ ٱلۡءَاخِR<رَ>ةَۚ إِN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٢٠>",
"یُعَذِّبُ مَX<ن> N<یَ>شَW<اۤ>ءُ وَیَR<رۡ>حَمُ مَX<ن> N<یَ>شَW<اۤ>ءُۖ وَإِلَیۡهِ تُQ<قۡ>لَبُJ<و>نَ A<۝>D<٢١>",
"وَمَW<اۤ> أَN<ن>تُN<م> بِمُعۡجِزِینَ فِی ٱلۡأَR<رۡ>ضِ وَلَا فِی ٱX<ل>سَّمَW<اۤ>ءِۖ وَمَا لَكُم N<مِّ>N<ن> دُونِ ٱللَّهِ مِX<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا نَصِJ<ی>رࣲ A<۝>D<٢٢>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِ ٱللَّهِ وَلِقَW<اۤ>ئِهِW<ۦۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ یَئِسُوX<ا۟> مِX<ن> R<رَّ>حۡمَتِی وَأُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٢٣>",
"فَمَا كَانَ جَوَابَ قَوۡمِهِW<ۦۤ> إِلَّW<اۤ> أَN<ن> قَالُوX<ا۟> ٱQ<قۡ>تُلُوهُ أَوۡ حَرِّقُوهُ فَأَN<ن>جَX<ى>T<ٰ>هُ ٱللَّهُ مِنَ ٱX<ل>N<نَّ>ارِۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٢٤>",
"وَقَالَ إِN<نَّ>مَا ٱتَّخَذۡتُم N<مِّ>N<ن> دُونِ ٱللَّهِ أَوۡثَT<ـٰ>نX<ࣰ>ا N<مَّ>وَدَّةَ بَیۡنِكُمۡ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۖ ثُN<مَّ> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ یَكۡفُR<رُ> بَعۡضُكُN<م> بِبَعۡضX<ࣲ> N<وَ>یَلۡعَنُ بَعۡضُكُN<م> بَعۡضX<ࣰ>ا N<وَ>مَأۡوَX<ى>T<ٰ>كُمُ ٱX<ل>N<نَّ>اR<رُ> وَمَا لَكُم N<مِّ>ن N<نَّ>T<ـٰ>صِرِJ<ی>نَ A<۝>D<٢٥>",
"۞ فَءَامَنَ لَهُT<ۥ> لُوطX<ࣱ>ۘ N<وَ>قَالَ إِN<نِّ>ی مُهَاجِR<ر>ٌ إِلَىٰ R<رَ>بِّW<یۤ>ۖ إِN<نَّ>هُT<ۥ> هُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٢٦>",
"وَوَهَQ<بۡ>نَا لَهُW<ۥۤ> إِسۡحَT<ـٰ>قَ وَیَعۡقُوبَ وَجَعَلۡنَا فِی ذُرِّیَّتِهِ ٱX<ل>N<نُّ>بُوَّةَ وَX<ٱ>لۡكِتَT<ـٰ>بَ وَءَاتَیۡنَT<ـٰ>هُ أَQ<جۡ>R<رَ>هُT<ۥ> فِی ٱX<ل>دُّنۡیَاۖ وَإِN<نَّ>هُT<ۥ> فِی ٱلۡءَاخِR<رَ>ةِ لَمِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٢٧>",
"وَلُوطًا إِذۡ قَالَ لِقَوۡمِهِW<ۦۤ> إِN<نَّ>كُمۡ لَتَأۡتُونَ ٱلۡفَT<ـٰ>حِشَةَ مَا سَبَقَكُN<م> بِهَا مِنۡ أَحَدX<ࣲ> N<مِّ>نَ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٢٨>",
"أَئِN<نَّ>كُمۡ لَتَأۡتُونَ ٱX<ل>رِّجَالَ وَتَQ<قۡ>طَعُونَ ٱX<ل>سَّبِیلَ وَتَأۡتُونَ فِی نَادِیكُمُ ٱلۡمُN<ن>كَR<رَ>ۖ فَمَا كَانَ جَوَابَ قَوۡمِهِW<ۦۤ> إِلَّW<اۤ> أَN<ن> قَالُوX<ا۟> ٱئۡتِنَا بِعَذَابِ ٱللَّهِ إِN<ن> كُN<ن>تَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٢٩>",
"قَالَ R<رَ>بِّ ٱN<ن>صُR<رۡ>نِی عَلَى ٱلۡقَوۡمِ ٱلۡمُفۡسِدِJ<ی>نَ A<۝>D<٣٠>",
"وَلَN<مَّ>ا جَW<اۤ>ءَتۡ R<رُ>سُلُنَW<اۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ بِX<ٱ>لۡبُشۡR<رَ>ىٰ قَالُW<وۤ>X<ا۟> إِN<نَّ>ا مُهۡلِكُW<وۤ>X<ا۟> أَهۡلِ هَT<ـٰ>ذِهِ ٱلۡقَR<رۡ>یَةِۖ إِN<نَّ> أَهۡلَهَا كَانُوX<ا۟> ظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٣١>",
"قَالَ إِN<نَّ> فِیهَا لُوطN<ࣰ>اۚ قَالُوX<ا۟> نَحۡنُ أَعۡلَمُ بِمَN<ن> فِیهَاۖ لَنُنَجِّیَN<نَّ>هُT<ۥ> وَأَهۡلَهُW<ۥۤ> إِلَّا ٱمۡR<رَ>أَتَهُT<ۥ> كَانَتۡ مِنَ ٱلۡغَT<ـٰ>بِرِJ<ی>نَ A<۝>D<٣٢>",
"وَلَN<مَّ>W<اۤ> أَN<ن> جَW<اۤ>ءَتۡ R<رُ>سُلُنَا لُوطN<ࣰ>ا سِW<یۤ>ءَ بِهِمۡ وَضَاقَ بِهِمۡ ذَR<رۡ>عX<ࣰ>اۖ N<وَ>قَالُوX<ا۟> لَا تَخَفۡ وَلَا تَحۡزَنۡ إِN<نَّ>ا مُنَجُّوكَ وَأَهۡلَكَ إِلَّا ٱمۡR<رَ>أَتَكَ كَانَتۡ مِنَ ٱلۡغَT<ـٰ>بِرِJ<ی>نَ A<۝>D<٣٣>",
"إِN<نَّ>ا مُN<ن>زِلُونَ عَلَىW<ٰۤ> أَهۡلِ هَT<ـٰ>ذِهِ ٱلۡقَR<رۡ>یَةِ رِQ<جۡ>زX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ بِمَا كَانُوX<ا۟> یَفۡسُقُJ<و>نَ A<۝>D<٣٤>",
"وَلَقَد تَّR<رَ>كۡنَا مِنۡهَW<اۤ> ءَایَةَN<ۢ> بَیِّنَةX<ࣰ> لِّقَوۡمX<ࣲ> N<یَ>عۡقِلُJ<و>نَ A<۝>D<٣٥>",
"وَإِلَىٰ مَQ<دۡ>یَنَ أَخَاهُمۡ شُعَیۡبN<ࣰ>ا فَقَالَ یَT<ـٰ>قَوۡمِ ٱعۡبُدُوX<ا۟> ٱللَّهَ وَX<ٱ>R<رۡ>جُوX<ا۟> ٱلۡیَوۡمَ ٱلۡءَاخِR<رَ> وَلَا تَعۡثَوۡX<ا۟> فِی ٱلۡأَR<رۡ>ضِ مُفۡسِدِJ<ی>نَ A<۝>D<٣٦>",
"فَكَذَّبُوهُ فَأَخَذَتۡهُمُ ٱX<ل>R<رَّ>Q<جۡ>فَةُ فَأَصۡبَحُوX<ا۟> فِی دَارِهِمۡ جَT<ـٰ>ثِمِJ<ی>نَ A<۝>D<٣٧>",
"وَعَادX<ࣰ>ا N<وَ>ثَمُودَX<ا۟> وَقَد تَّبَیَّنَ لَكُم N<مِّ>X<ن> N<مَّ>سَT<ـٰ>كِنِهِمۡۖ وَزَیَّنَ لَهُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ أَعۡمَT<ـٰ>لَهُمۡ فَصَدَّهُمۡ عَنِ ٱX<ل>سَّبِیلِ وَكَانُوX<ا۟> مُسۡتَQ<بۡ>صِرِJ<ی>نَ A<۝>D<٣٨>",
"وَقَT<ـٰ>R<رُ>ونَ وَفِرۡعَوۡنَ وَهَT<ـٰ>مَT<ـٰ>نَۖ وَلَقَQ<دۡ> جَW<اۤ>ءَهُم N<مُّ>وسَىٰ بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَX<ٱ>سۡتَكۡبَR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ وَمَا كَانُوX<ا۟> سَT<ـٰ>بِقِJ<ی>نَ A<۝>D<٣٩>",
"فَكُلًّا أَخَذۡنَا بِذَX<ن>N<ۢ>بِهِT<ۦ>ۖ فَمِنۡهُم N<مَّ>نۡ أَR<رۡ>سَلۡنَا عَلَیۡهِ حَاصِبX<ࣰ>ا N<وَ>مِنۡهُم N<مَّ>نۡ أَخَذَتۡهُ ٱX<ل>صَّیۡحَةُ وَمِنۡهُم N<مَّ>نۡ خَسَفۡنَا بِهِ ٱلۡأَR<رۡ>ضَ وَمِنۡهُم N<مَّ>نۡ أَغۡR<رَ>Q<قۡ>نَاۚ وَمَا كَانَ ٱللَّهُ لِیَظۡلِمَهُمۡ وَلَT<ـٰ>كِN<ن> كَانُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ یَظۡلِمُJ<و>نَ A<۝>D<٤٠>",
"مَثَلُ ٱلَّذِینَ ٱتَّخَذُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ أَوۡلِیَW<اۤ>ءَ كَمَثَلِ ٱلۡعَN<ن>كَبُوتِ ٱتَّخَذَتۡ بَیۡتX<ࣰ>اۖ N<وَ>إِN<نَّ> أَوۡهَنَ ٱلۡبُیُوتِ لَبَیۡتُ ٱلۡعَN<ن>كَبُوتِۚ لَوۡ كَانُوX<ا۟> یَعۡلَمُJ<و>نَ A<۝>D<٤١>",
"إِN<نَّ> ٱللَّهَ یَعۡلَمُ مَا یَQ<دۡ>عُونَ مِN<ن> دُونِهِT<ۦ> مِN<ن> شَیۡءX<ࣲ>ۚ N<وَ>هُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٤٢>",
"وَتِلۡكَ ٱلۡأَمۡثَT<ـٰ>لُ نَضۡرِبُهَا لِX<ل>N<نَّ>اسِۖ وَمَا یَعۡقِلُهَW<اۤ> إِلَّا ٱلۡعَT<ـٰ>لِمُJ<و>نَ A<۝>D<٤٣>",
"خَلَقَ ٱللَّهُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ بِX<ٱ>لۡحَقِّۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٤٤>",
"ٱتۡلُ مَW<اۤ> أُوحِیَ إِلَیۡكَ مِنَ ٱلۡكِتَT<ـٰ>بِ وَأَقِمِ ٱX<ل>صَّلَX<و>T<ٰ>ةَۖ إِN<نَّ> ٱX<ل>صَّلَX<و>T<ٰ>ةَ تَنۡهَىٰ عَنِ ٱلۡفَحۡشَW<اۤ>ءِ وَX<ٱ>لۡمُN<ن>كَرِۗ وَلَذِكۡR<رُ> ٱللَّهِ أَكۡبَR<رُ>ۗ وَٱللَّهُ یَعۡلَمُ مَا تَصۡنَعُJ<و>نَ A<۝>D<٤٥>",
"۞ وَلَا تُجَT<ـٰ>دِلُW<وۤ>X<ا۟> أَهۡلَ ٱلۡكِتَT<ـٰ>بِ إِلَّا بِX<ٱ>لَّتِی هِیَ أَحۡسَنُ إِلَّا ٱلَّذِینَ ظَلَمُوX<ا۟> مِنۡهُمۡۖ وَقُولُW<وۤ>X<ا۟> ءَامَN<نَّ>ا بِX<ٱ>لَّذِW<یۤ> أُN<ن>زِلَ إِلَیۡنَا وَأُN<ن>زِلَ إِلَیۡكُمۡ وَإِلَT<ـٰ>هُنَا وَإِلَT<ـٰ>هُكُمۡ وَT< ٰ>حِدX<ࣱ> N<وَ>نَحۡنُ لَهُT<ۥ> مُسۡلِمُJ<و>نَ A<۝>D<٤٦>",
"وَكَذَT< ٰ>لِكَ أَN<ن>زَلۡنَW<اۤ> إِلَیۡكَ ٱلۡكِتَT<ـٰ>بَۚ فَX<ٱ>لَّذِینَ ءَاتَیۡنَT<ـٰ>هُمُ ٱلۡكِتَT<ـٰ>بَ یُؤۡمِنُونَ بِهِT<ۦ>ۖ وَمِنۡ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ مَX<ن> N<یُ>ؤۡمِنُ بِهِT<ۦ>ۚ وَمَا یَQ<جۡ>حَدُ بِءَایَT<ـٰ>تِنَW<اۤ> إِلَّا ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٤٧>",
"وَمَا كُN<ن>تَ تَتۡلُوX<ا۟> مِN<ن> قَQ<بۡ>لِهِT<ۦ> مِN<ن> كِتَT<ـٰ>بX<ࣲ> N<وَ>لَا تَخُطُّهُT<ۥ> بِیَمِینِكَۖ إِذX<ࣰ>ا لَّX<ٱ>R<رۡ>تَابَ ٱلۡمُQ<بۡ>طِلُJ<و>نَ A<۝>D<٤٨>",
"بَلۡ هُوَ ءَایَT<ـٰ>تُN<ۢ> بَیِّنَT<ـٰ>تN<ࣱ> فِی صُدُورِ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡعِلۡمَۚ وَمَا یَQ<جۡ>حَدُ بِءَایَT<ـٰ>تِنَW<اۤ> إِلَّا ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٤٩>",
"وَقَالُوX<ا۟> لَوۡلَW<اۤ> أُN<ن>زِلَ عَلَیۡهِ ءَایَT<ـٰ>تX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ>ۚ قُلۡ إِN<نَّ>مَا ٱلۡءَایَT<ـٰ>تُ عِN<ن>دَ ٱللَّهِ وَإِN<نَّ>مَW<اۤ> أَنَا۠ نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<٥٠>",
"أَوَ لَمۡ یَكۡفِهِمۡ أَN<نَّ>W<اۤ> أَN<ن>زَلۡنَا عَلَیۡكَ ٱلۡكِتَT<ـٰ>بَ یُتۡلَىٰ عَلَیۡهِمۡۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَR<رَ>حۡمَةX<ࣰ> N<وَ>ذِكۡR<رَ>ىٰ لِقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٥١>",
"قُلۡ كَفَىٰ بِٱللَّهِ بَیۡنِی وَبَیۡنَكُمۡ شَهِیدX<ࣰ>اۖ N<یَ>عۡلَمُ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۗ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> بِX<ٱ>لۡبَT<ـٰ>طِلِ وَكَفَR<رُ>وX<ا۟> بِٱللَّهِ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<٥٢>",
"وَیَسۡتَعۡجِلُونَكَ بِX<ٱ>لۡعَذَابِ وَلَوۡلَW<اۤ> أَجَلX<ࣱ> N<مُّ>سَN<مّ>X<ࣰ>ى لَّجَW<اۤ>ءَهُمُ ٱلۡعَذَابُۚ وَلَیَأۡتِیَN<نَّ>هُN<م> بَغۡتَةX<ࣰ> N<وَ>هُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٥٣>",
"یَسۡتَعۡجِلُونَكَ بِX<ٱ>لۡعَذَابِ وَإِN<نَّ> جَهَN<نَّ>مَ لَمُحِیطَةُN<ۢ> بِX<ٱ>لۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٥٤>",
"یَوۡمَ یَغۡشَX<ى>T<ٰ>هُمُ ٱلۡعَذَابُ مِN<ن> فَوۡقِهِمۡ وَمِN<ن> تَحۡتِ أَR<رۡ>جُلِهِمۡ وَیَقُولُ ذُوقُوX<ا۟> مَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٥٥>",
"یَT<ـٰ>عِبَادِیَ ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<نَّ> أَR<رۡ>ضِی وَT< ٰ>سِعَةN<ࣱ> فَإِیَّT<ـٰ>یَ فَX<ٱ>عۡبُدُJ<و>نِ A<۝>D<٥٦>",
"كُلُّ نَفۡسN<ࣲ> ذَW<اۤ>ئِقَةُ ٱلۡمَوۡتِۖ ثُN<مَّ> إِلَیۡنَا تُR<رۡ>جَعُJ<و>نَ A<۝>D<٥٧>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَنُبَوِّئَN<نَّ>هُم N<مِّ>نَ ٱلۡجَN<نَّ>ةِ غُR<رَ>فN<ࣰ>ا تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَاۚ نِعۡمَ أَQ<جۡ>R<رُ> ٱلۡعَT<ـٰ>مِلِJ<ی>نَ A<۝>D<٥٨>",
"ٱلَّذِینَ صَبَR<رُ>وX<ا۟> وَعَلَىٰ R<رَ>بِّهِمۡ یَتَوَكَّلُJ<و>نَ A<۝>D<٥٩>",
"وَكَأَیِّX<ن> N<مِّ>N<ن> دَL<اۤ>بَّةX<ࣲ> لَّا تَحۡمِلُ رِزۡقَهَا ٱللَّهُ یَR<رۡ>زُقُهَا وَإِیَّاكُمۡۚ وَهُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٦٠>",
"وَلَئِN<ن> سَأَلۡتَهُم N<مَّ>نۡ خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَسَخَّR<رَ> ٱX<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ> لَیَقُولُN<نَّ> ٱللَّهُۖ فَأَN<نَّ>ىٰ یُؤۡفَكُJ<و>نَ A<۝>D<٦١>",
"ٱللَّهُ یَQ<بۡ>سُطُ ٱX<ل>رِّزۡقَ لِمَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِT<ۦ> وَیَQ<قۡ>دِR<رُ> لَهُW<ۥۤ>ۚ إِN<نَّ> ٱللَّهَ بِكُلِّ شَیۡءٍ عَلِJ<ی>مࣱ A<۝>D<٦٢>",
"وَلَئِN<ن> سَأَلۡتَهُم N<مَّ>ن N<نَّ>زَّلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَحۡیَا بِهِ ٱلۡأَR<رۡ>ضَ مِX<ن>N<ۢ> بَعۡدِ مَوۡتِهَا لَیَقُولُN<نَّ> ٱللَّهُۚ قُلِ ٱلۡحَمۡدُ لِلَّهِۚ بَلۡ أَكۡثَR<رُ>هُمۡ لَا یَعۡقِلُJ<و>نَ A<۝>D<٦٣>",
"وَمَا هَT<ـٰ>ذِهِ ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَW<اۤ> إِلَّا لَهۡوX<ࣱ> N<وَ>لَعِبX<ࣱ>ۚ N<وَ>إِN<نَّ> ٱX<ل>دَّاR<رَ> ٱلۡءَاخِR<رَ>ةَ لَهِیَ ٱلۡحَیَوَانُۚ لَوۡ كَانُوX<ا۟> یَعۡلَمُJ<و>نَ A<۝>D<٦٤>",
"فَإِذَا R<رَ>كِبُوX<ا۟> فِی ٱلۡفُلۡكِ دَعَوُX<ا۟> ٱللَّهَ مُخۡلِصِینَ لَهُ ٱX<ل>دِّینَ فَلَN<مَّ>ا نَجَّX<ى>T<ٰ>هُمۡ إِلَى ٱلۡبَرِّ إِذَا هُمۡ یُشۡرِكُJ<و>نَ A<۝>D<٦٥>",
"لِیَكۡفُR<رُ>وX<ا۟> بِمَW<اۤ> ءَاتَیۡنَT<ـٰ>هُمۡ وَلِیَتَمَتَّعُوX<ا۟>ۚ فَسَوۡفَ یَعۡلَمُJ<و>نَ A<۝>D<٦٦>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ>ا جَعَلۡنَا حَR<رَ>مًا ءَامِنX<ࣰ>ا N<وَ>یُتَخَطَّفُ ٱX<ل>N<نَّ>اسُ مِنۡ حَوۡلِهِمۡۚ أَفَبِX<ٱ>لۡبَT<ـٰ>طِلِ یُؤۡمِنُونَ وَبِنِعۡمَةِ ٱللَّهِ یَكۡفُR<رُ>J<و>نَ A<۝>D<٦٧>",
"وَمَنۡ أَظۡلَمُ مِN<مَّ>نِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبًا أَوۡ كَذَّبَ بِX<ٱ>لۡحَقِّ لَN<مَّ>ا جَW<اۤ>ءَهُW<ۥۤ>ۚ أَلَیۡسَ فِی جَهَN<نَّ>مَ مَثۡوX<ࣰ>ى لِّلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٦٨>",
"وَX<ٱ>لَّذِینَ جَT<ـٰ>هَدُوX<ا۟> فِینَا لَنَهۡدِیَN<نَّ>هُمۡ سُبُلَنَاۚ وَإِN<نَّ> ٱللَّهَ لَمَعَ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٦٩>",
"#۞ اL<لۤ>L<مۤ> A<۝>D<١>",
"غُلِبَتِ ٱX<ل>R<رُّ>J<و>مُ A<۝>D<٢>",
"فِW<یۤ> أَQ<دۡ>نَى ٱلۡأَR<رۡ>ضِ وَهُم N<مِّ>X<ن>N<ۢ> بَعۡدِ غَلَبِهِمۡ سَیَغۡلِبُJ<و>نَ A<۝>D<٣>",
"فِی بِضۡعِ سِنِینَۗ لِلَّهِ ٱلۡأَمۡR<رُ> مِN<ن> قَQ<بۡ>لُ وَمِX<ن>N<ۢ> بَعۡدُۚ وَیَوۡمَئِذX<ࣲ> N<یَ>فۡR<رَ>حُ ٱلۡمُؤۡمِنُJ<و>نَ A<۝>D<٤>",
"بِنَصۡرِ ٱللَّهِۚ یَN<ن>صُR<رُ> مَX<ن> N<یَ>شَW<اۤ>ءُۖ وَهُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٥>",
"وَعۡدَ ٱللَّهِۖ لَا یُخۡلِفُ ٱللَّهُ وَعۡدَهُT<ۥ> وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٦>",
"یَعۡلَمُونَ ظَT<ـٰ>هِR<ر>X<ࣰ>ا N<مِّ>نَ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَهُمۡ عَنِ ٱلۡءَاخِR<رَ>ةِ هُمۡ غَT<ـٰ>فِلُJ<و>نَ A<۝>D<٧>",
"أَوَ لَمۡ یَتَفَكَّR<رُ>وX<ا۟> فِW<یۤ> أَN<ن>فُسِهِمۗ N<مَّ>ا خَلَقَ ٱللَّهُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَمَا بَیۡنَهُمَW<اۤ> إِلَّا بِX<ٱ>لۡحَقِّ وَأَجَلX<ࣲ> N<مُّ>سَN<مّ>X<ࣰ>ىۗ N<وَ>إِN<نَّ> كَثِیR<ر>X<ࣰ>ا N<مِّ>نَ ٱX<ل>N<نَّ>اسِ بِلِقَW<اۤ>ئِ R<رَ>بِّهِمۡ لَكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٨>",
"أَوَ لَمۡ یَسِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَیَN<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۚ كَانُW<وۤ>X<ا۟> أَشَدَّ مِنۡهُمۡ قُوَّةX<ࣰ> N<وَ>أَثَاR<رُ>وX<ا۟> ٱلۡأَR<رۡ>ضَ وَعَمَR<رُ>وهَW<اۤ> أَكۡثَR<رَ> مِN<مَّ>ا عَمَR<رُ>وهَا وَجَW<اۤ>ءَتۡهُمۡ R<رُ>سُلُهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِۖ فَمَا كَانَ ٱللَّهُ لِیَظۡلِمَهُمۡ وَلَT<ـٰ>كِN<ن> كَانُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ یَظۡلِمُJ<و>نَ A<۝>D<٩>",
"ثُN<مَّ> كَانَ عَT<ـٰ>قِبَةَ ٱلَّذِینَ أَسَW<ـٰۤ>ءُوX<ا۟> ٱX<ل>سُّW<وۤ>أَىW<ٰۤ> أَN<ن> كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِ ٱللَّهِ وَكَانُوX<ا۟> بِهَا یَسۡتَهۡزِءُJ<و>نَ A<۝>D<١٠>",
"ٱللَّهُ یَQ<بۡ>دَؤُX<ا۟> ٱلۡخَلۡقَ ثُN<مَّ> یُعِیدُهُT<ۥ> ثُN<مَّ> إِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<١١>",
"وَیَوۡمَ تَقُومُ ٱX<ل>سَّاعَةُ یُQ<بۡ>لِسُ ٱلۡمُQ<جۡ>رِمُJ<و>نَ A<۝>D<١٢>",
"وَلَمۡ یَكُX<ن> لَّهُم N<مِّ>N<ن> شُR<رَ>كَW<اۤ>ئِهِمۡ شُفَعَW<ـٰۤ>ؤُX<ا۟> وَكَانُوX<ا۟> بِشُR<رَ>كَW<اۤ>ئِهِمۡ كَT<ـٰ>فِرِJ<ی>نَ A<۝>D<١٣>",
"وَیَوۡمَ تَقُومُ ٱX<ل>سَّاعَةُ یَوۡمَئِذX<ࣲ> N<یَ>تَفَR<رَّ>قُJ<و>نَ A<۝>D<١٤>",
"فَأَN<مَّ>ا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ فَهُمۡ فِی R<رَ>وۡضَةX<ࣲ> N<یُ>حۡبَR<رُ>J<و>نَ A<۝>D<١٥>",
"وَأَN<مَّ>ا ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَكَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا وَلِقَW<اۤ>ئِ ٱلۡءَاخِR<رَ>ةِ فَأُX<و۟>لَW<ـٰۤ>ئِكَ فِی ٱلۡعَذَابِ مُحۡضَR<رُ>J<و>نَ A<۝>D<١٦>",
"فَسُQ<بۡ>حَT<ـٰ>نَ ٱللَّهِ حِینَ تُمۡسُونَ وَحِینَ تُصۡبِحُJ<و>نَ A<۝>D<١٧>",
"وَلَهُ ٱلۡحَمۡدُ فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَعَشِیّX<ࣰ>ا N<وَ>حِینَ تُظۡهِR<رُ>J<و>نَ A<۝>D<١٨>",
"یُخۡرِجُ ٱلۡحَیَّ مِنَ ٱلۡمَیِّتِ وَیُخۡرِجُ ٱلۡمَیِّتَ مِنَ ٱلۡحَیِّ وَیُحۡیِ ٱلۡأَR<رۡ>ضَ بَعۡدَ مَوۡتِهَاۚ وَكَذَT< ٰ>لِكَ تُخۡR<رَ>جُJ<و>نَ A<۝>D<١٩>",
"وَمِنۡ ءَایَT<ـٰ>تِهِW<ۦۤ> أَنۡ خَلَقَكُم N<مِّ>N<ن> تُR<رَ>ابN<ࣲ> ثُN<مَّ> إِذَW<اۤ> أَN<ن>تُN<م> بَشَR<ر>N<ࣱ> تَN<ن>تَشِR<رُ>J<و>نَ A<۝>D<٢٠>",
"وَمِنۡ ءَایَT<ـٰ>تِهِW<ۦۤ> أَنۡ خَلَقَ لَكُم N<مِّ>نۡ أَN<ن>فُسِكُمۡ أَزۡوَT< ٰ>جX<ࣰ>ا لِّتَسۡكُنُW<وۤ>X<ا۟> إِلَیۡهَا وَجَعَلَ بَیۡنَكُم N<مَّ>وَدَّةX<ࣰ> N<وَ>R<رَ>حۡمَةًۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>تَفَكَّR<رُ>J<و>نَ A<۝>D<٢١>",
"وَمِنۡ ءَایَT<ـٰ>تِهِT<ۦ> خَلۡقُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَX<ٱ>خۡتِلَT<ـٰ>فُ أَلۡسِنَتِكُمۡ وَأَلۡوَT< ٰ>نِكُمۡۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّلۡعَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢٢>",
"وَمِنۡ ءَایَT<ـٰ>تِهِT<ۦ> مَنَامُكُN<م> بِX<ٱ>لَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِ وَX<ٱ>Q<بۡ>تِغَW<اۤ>ؤُكُم N<مِّ>N<ن> فَضۡلِهِW<ۦۤ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>سۡمَعُJ<و>نَ A<۝>D<٢٣>",
"وَمِنۡ ءَایَT<ـٰ>تِهِT<ۦ> یُرِیكُمُ ٱلۡبَR<رۡ>قَ خَوۡفX<ࣰ>ا N<وَ>طَمَعX<ࣰ>ا N<وَ>یُنَزِّلُ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَیُحۡیِT<ۦ> بِهِ ٱلۡأَR<رۡ>ضَ بَعۡدَ مَوۡتِهَW<اۤ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>عۡقِلُJ<و>نَ A<۝>D<٢٤>",
"وَمِنۡ ءَایَT<ـٰ>تِهِW<ۦۤ> أَN<ن> تَقُومَ ٱX<ل>سَّمَW<اۤ>ءُ وَX<ٱ>لۡأَR<رۡ>ضُ بِأَمۡرِهِT<ۦ>ۚ ثُN<مَّ> إِذَا دَعَاكُمۡ دَعۡوَةX<ࣰ> N<مِّ>نَ ٱلۡأَR<رۡ>ضِ إِذَW<اۤ> أَN<ن>تُمۡ تَخۡR<رُ>جُJ<و>نَ A<۝>D<٢٥>",
"وَلَهُT<ۥ> مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ كُلّX<ࣱ> لَّهُT<ۥ> قَT<ـٰ>نِتُJ<و>نَ A<۝>D<٢٦>",
"وَهُوَ ٱلَّذِی یَQ<بۡ>دَؤُX<ا۟> ٱلۡخَلۡقَ ثُN<مَّ> یُعِیدُهُT<ۥ> وَهُوَ أَهۡوَنُ عَلَیۡهِۚ وَلَهُ ٱلۡمَثَلُ ٱلۡأَعۡلَىٰ فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٢٧>",
"ضَR<رَ>بَ لَكُم N<مَّ>ثَلX<ࣰ>ا N<مِّ>نۡ أَN<ن>فُسِكُمۡۖ هَل لَّكُم N<مِّ>X<ن> N<مَّ>ا مَلَكَتۡ أَیۡمَT<ـٰ>نُكُم N<مِّ>N<ن> شُR<رَ>كَW<اۤ>ءَ فِی مَا R<رَ>زَQ<قۡ>نَT<ـٰ>كُمۡ فَأَN<ن>تُمۡ فِیهِ سَوَW<اۤ>ءN<ࣱ> تَخَافُونَهُمۡ كَخِیفَتِكُمۡ أَN<ن>فُسَكُمۡۚ كَذَT< ٰ>لِكَ نُفَصِّلُ ٱلۡءَایَT<ـٰ>تِ لِقَوۡمX<ࣲ> N<یَ>عۡقِلُJ<و>نَ A<۝>D<٢٨>",
"بَلِ ٱتَّبَعَ ٱلَّذِینَ ظَلَمُW<وۤ>X<ا۟> أَهۡوَW<اۤ>ءَهُN<م> بِغَیۡرِ عِلۡمN<ࣲ>ۖ فَمَX<ن> N<یَ>هۡدِی مَنۡ أَضَلَّ ٱللَّهُۖ وَمَا لَهُم N<مِّ>ن N<نَّ>T<ـٰ>صِرِJ<ی>نَ A<۝>D<٢٩>",
"فَأَقِمۡ وَQ<جۡ>هَكَ لِX<ل>دِّینِ حَنِیفN<ࣰ>اۚ فِQ<طۡ>R<رَ>تَ ٱللَّهِ ٱلَّتِی فَطَR<رَ> ٱX<ل>N<نَّ>اسَ عَلَیۡهَاۚ لَا تَQ<بۡ>دِیلَ لِخَلۡقِ ٱللَّهِۚ ذَT< ٰ>لِكَ ٱX<ل>دِّینُ ٱلۡقَیِّمُ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٣٠>",
"۞ مُنِیبِینَ إِلَیۡهِ وَX<ٱ>تَّقُوهُ وَأَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَلَا تَكُونُوX<ا۟> مِنَ ٱلۡمُشۡرِكِJ<ی>نَ A<۝>D<٣١>",
"مِنَ ٱلَّذِینَ فَR<رَّ>قُوX<ا۟> دِینَهُمۡ وَكَانُوX<ا۟> شِیَعN<ࣰ>اۖ كُلُّ حِزۡبِN<ۭ> بِمَا لَدَیۡهِمۡ فَرِحُJ<و>نَ A<۝>D<٣٢>",
"وَإِذَا مَسَّ ٱX<ل>N<نَّ>اسَ ضُR<رّ>N<ࣱ> دَعَوۡX<ا۟> R<رَ>بَّهُم N<مُّ>نِیبِینَ إِلَیۡهِ ثُN<مَّ> إِذَW<اۤ> أَذَاقَهُم N<مِّ>نۡهُ R<رَ>حۡمَةً إِذَا فَرِیقX<ࣱ> N<مِّ>نۡهُN<م> بِR<رَ>بِّهِمۡ یُشۡرِكُJ<و>نَ A<۝>D<٣٣>",
"لِیَكۡفُR<رُ>وX<ا۟> بِمَW<اۤ> ءَاتَیۡنَT<ـٰ>هُمۡۚ فَتَمَتَّعُوX<ا۟> فَسَوۡفَ تَعۡلَمُJ<و>نَ A<۝>D<٣٤>",
"أَمۡ أَN<ن>زَلۡنَا عَلَیۡهِمۡ سُلۡطَT<ـٰ>نN<ࣰ>ا فَهُوَ یَتَكَلَّمُ بِمَا كَانُوX<ا۟> بِهِT<ۦ> یُشۡرِكُJ<و>نَ A<۝>D<٣٥>",
"وَإِذَW<اۤ> أَذَQ<قۡ>نَا ٱX<ل>N<نَّ>اسَ R<رَ>حۡمَةN<ࣰ> فَرِحُوX<ا۟> بِهَاۖ وَإِN<ن> تُصِQ<بۡ>هُمۡ سَیِّئَةُN<ۢ> بِمَا قَدَّمَتۡ أَیۡدِیهِمۡ إِذَا هُمۡ یَQ<قۡ>نَطُJ<و>نَ A<۝>D<٣٦>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ> ٱللَّهَ یَQ<بۡ>سُطُ ٱX<ل>رِّزۡقَ لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیَQ<قۡ>دِR<رُ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٣٧>",
"فَءَاتِ ذَا ٱلۡقُR<رۡ>بَىٰ حَقَّهُT<ۥ> وَX<ٱ>لۡمِسۡكِینَ وَX<ٱ>Q<بۡ>نَ ٱX<ل>سَّبِیلِۚ ذَT< ٰ>لِكَ خَیۡR<ر>X<ࣱ> لِّلَّذِینَ یُرِیدُونَ وَQ<جۡ>هَ ٱللَّهِۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<٣٨>",
"وَمَW<اۤ> ءَاتَیۡتُم N<مِّ>X<ن> رِّبX<ࣰ>ا لِّیَR<رۡ>بُوَX<ا۟> فِW<یۤ> أَمۡوَT< ٰ>لِ ٱX<ل>N<نَّ>اسِ فَلَا یَR<رۡ>بُوX<ا۟> عِN<ن>دَ ٱللَّهِۖ وَمَW<اۤ> ءَاتَیۡتُم N<مِّ>N<ن> زَكَX<و>T<ٰ>ةN<ࣲ> تُرِیدُونَ وَQ<جۡ>هَ ٱللَّهِ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُضۡعِفُJ<و>نَ A<۝>D<٣٩>",
"ٱللَّهُ ٱلَّذِی خَلَقَكُمۡ ثُN<مَّ> R<رَ>زَقَكُمۡ ثُN<مَّ> یُمِیتُكُمۡ ثُN<مَّ> یُحۡیِیكُمۡۖ هَلۡ مِN<ن> شُR<رَ>كَW<اۤ>ئِكُم N<مَّ>X<ن> N<یَ>فۡعَلُ مِN<ن> ذَT< ٰ>لِكُم N<مِّ>N<ن> شَیۡءN<ࣲ>ۚ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ> وَتَعَT<ـٰ>لَىٰ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٤٠>",
"ظَهَR<رَ> ٱلۡفَسَادُ فِی ٱلۡبَرِّ وَX<ٱ>لۡبَحۡرِ بِمَا كَسَبَتۡ أَیۡدِی ٱX<ل>N<نَّ>اسِ لِیُذِیقَهُN<م> بَعۡضَ ٱلَّذِی عَمِلُوX<ا۟> لَعَلَّهُمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<٤١>",
"قُلۡ سِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَX<ٱ>N<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لُۚ كَانَ أَكۡثَR<رُ>هُم N<مُّ>شۡرِكِJ<ی>نَ A<۝>D<٤٢>",
"فَأَقِمۡ وَQ<جۡ>هَكَ لِX<ل>دِّینِ ٱلۡقَیِّمِ مِN<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>أۡتِیَ یَوۡمX<ࣱ> لَّا مَR<رَ>دَّ لَهُT<ۥ> مِنَ ٱللَّهِۖ یَوۡمَئِذX<ࣲ> N<یَ>صَّدَّعُJ<و>نَ A<۝>D<٤٣>",
"مَN<ن> كَفَR<رَ> فَعَلَیۡهِ كُفۡR<رُ>هُT<ۥ>ۖ وَمَنۡ عَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَلِأَN<ن>فُسِهِمۡ یَمۡهَدُJ<و>نَ A<۝>D<٤٤>",
"لِیَQ<جۡ>زِیَ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ مِN<ن> فَضۡلِهِW<ۦۤ>ۚ إِN<نَّ>هُT<ۥ> لَا یُحِبُّ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٤٥>",
"وَمِنۡ ءَایَT<ـٰ>تِهِW<ۦۤ> أَX<ن> N<یُ>R<رۡ>سِلَ ٱX<ل>رِّیَاحَ مُبَشِّR<رَ>T< ٰ>تX<ࣲ> N<وَ>لِیُذِیقَكُم N<مِّ>X<ن> R<رَّ>حۡمَتِهِT<ۦ> وَلِتَQ<جۡ>رِیَ ٱلۡفُلۡكُ بِأَمۡرِهِT<ۦ> وَلِتَQ<بۡ>تَغُوX<ا۟> مِN<ن> فَضۡلِهِT<ۦ> وَلَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<٤٦>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا مِN<ن> قَQ<بۡ>لِكَ R<رُ>سُلًا إِلَىٰ قَوۡمِهِمۡ فَجَW<اۤ>ءُوهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَX<ٱ>N<ن>تَقَمۡنَا مِنَ ٱلَّذِینَ أَQ<جۡ>R<رَ>مُوX<ا۟>ۖ وَكَانَ حَقًّا عَلَیۡنَا نَصۡR<رُ> ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٤٧>",
"ٱللَّهُ ٱلَّذِی یُR<رۡ>سِلُ ٱX<ل>رِّیَT<ـٰ>حَ فَتُثِیR<رُ> سَحَابN<ࣰ>ا فَیَQ<بۡ>سُطُهُT<ۥ> فِی ٱX<ل>سَّمَW<اۤ>ءِ كَیۡفَ یَشَW<اۤ>ءُ وَیَQ<جۡ>عَلُهُT<ۥ> كِسَفN<ࣰ>ا فَتَR<رَ>ى ٱلۡوَQ<دۡ>قَ یَخۡR<رُ>جُ مِنۡ خِلَT<ـٰ>لِهِT<ۦ>ۖ فَإِذَW<اۤ> أَصَابَ بِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِW<ۦۤ> إِذَا هُمۡ یَسۡتَQ<بۡ>شِR<رُ>J<و>نَ A<۝>D<٤٨>",
"وَإِN<ن> كَانُوX<ا۟> مِN<ن> قَQ<بۡ>لِ أَX<ن> N<یُ>نَزَّلَ عَلَیۡهِم N<مِّ>N<ن> قَQ<بۡ>لِهِT<ۦ> لَمُQ<بۡ>لِسِJ<ی>نَ A<۝>D<٤٩>",
"فَX<ٱ>N<ن>ظُR<رۡ> إِلَىW<ٰۤ> ءَاثَT<ـٰ>رِ R<رَ>حۡمَتِ ٱللَّهِ كَیۡفَ یُحۡیِ ٱلۡأَR<رۡ>ضَ بَعۡدَ مَوۡتِهَW<اۤ>ۚ إِN<نَّ> ذَT< ٰ>لِكَ لَمُحۡیِ ٱلۡمَوۡتَىٰۖ وَهُوَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٥٠>",
"وَلَئِنۡ أَR<رۡ>سَلۡنَا رِیحN<ࣰ>ا فَR<رَ>أَوۡهُ مُصۡفَR<رّ>X<ࣰ>ا لَّظَلُّوX<ا۟> مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> یَكۡفُR<رُ>J<و>نَ A<۝>D<٥١>",
"فَإِN<نَّ>كَ لَا تُسۡمِعُ ٱلۡمَوۡتَىٰ وَلَا تُسۡمِعُ ٱX<ل>صُّN<مَّ> ٱX<ل>دُّعَW<اۤ>ءَ إِذَا وَلَّوۡX<ا۟> مُQ<دۡ>بِرِJ<ی>نَ A<۝>D<٥٢>",
"وَمَW<اۤ> أَN<ن>تَ بِهَT<ـٰ>دِ ٱلۡعُمۡیِ عَN<ن> ضَلَT<ـٰ>لَتِهِمۡۖ إِN<ن> تُسۡمِعُ إِلَّا مَX<ن> N<یُ>ؤۡمِنُ بِءَایَT<ـٰ>تِنَا فَهُم N<مُّ>سۡلِمُJ<و>نَ A<۝>D<٥٣>",
"۞ ٱللَّهُ ٱلَّذِی خَلَقَكُم N<مِّ>N<ن> ضَعۡفN<ࣲ> ثُN<مَّ> جَعَلَ مِX<ن>N<ۢ> بَعۡدِ ضَعۡفN<ࣲ> قُوَّةN<ࣰ> ثُN<مَّ> جَعَلَ مِX<ن>N<ۢ> بَعۡدِ قُوَّةN<ࣲ> ضَعۡفX<ࣰ>ا N<وَ>شَیۡبَةX<ࣰ>ۚ N<یَ>خۡلُقُ مَا یَشَW<اۤ>ءُۚ وَهُوَ ٱلۡعَلِیمُ ٱلۡقَدِJ<ی>رُ A<۝>D<٥٤>",
"وَیَوۡمَ تَقُومُ ٱX<ل>سَّاعَةُ یُQ<قۡ>سِمُ ٱلۡمُQ<جۡ>رِمُونَ مَا لَبِثُوX<ا۟> غَیۡR<رَ> سَاعَةN<ࣲ>ۚ كَذَT< ٰ>لِكَ كَانُوX<ا۟> یُؤۡفَكُJ<و>نَ A<۝>D<٥٥>",
"وَقَالَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡعِلۡمَ وَX<ٱ>لۡإِیمَT<ـٰ>نَ لَقَQ<دۡ> لَبِثۡتُمۡ فِی كِتَT<ـٰ>بِ ٱللَّهِ إِلَىٰ یَوۡمِ ٱلۡبَعۡثِۖ فَهَT<ـٰ>ذَا یَوۡمُ ٱلۡبَعۡثِ وَلَT<ـٰ>كِN<نَّ>كُمۡ كُN<ن>تُمۡ لَا تَعۡلَمُJ<و>نَ A<۝>D<٥٦>",
"فَیَوۡمَئِذX<ࣲ> لَّا یَN<ن>فَعُ ٱلَّذِینَ ظَلَمُوX<ا۟> مَعۡذِR<رَ>تُهُمۡ وَلَا هُمۡ یُسۡتَعۡتَبُJ<و>نَ A<۝>D<٥٧>",
"وَلَقَQ<دۡ> ضَR<رَ>Q<بۡ>نَا لِX<ل>N<نَّ>اسِ فِی هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانِ مِN<ن> كُلِّ مَثَلX<ࣲ>ۚ N<وَ>لَئِN<ن> جِئۡتَهُN<م> بِءَایَةX<ࣲ> لَّیَقُولَN<نَّ> ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِنۡ أَN<ن>تُمۡ إِلَّا مُQ<بۡ>طِلُJ<و>نَ A<۝>D<٥٨>",
"كَذَT< ٰ>لِكَ یَQ<طۡ>بَعُ ٱللَّهُ عَلَىٰ قُلُوبِ ٱلَّذِینَ لَا یَعۡلَمُJ<و>نَ A<۝>D<٥٩>",
"فَX<ٱ>صۡبِرۡ إِN<نَّ> وَعۡدَ ٱللَّهِ حَقّX<ࣱ>ۖ N<وَ>لَا یَسۡتَخِفَّN<نَّ>كَ ٱلَّذِینَ لَا یُوقِنُJ<و>نَ A<۝>D<٦٠>",
"#اL<لۤ>L<مۤ> A<۝>D<١>",
"تِلۡكَ ءَایَT<ـٰ>تُ ٱلۡكِتَT<ـٰ>بِ ٱلۡحَكِJ<ی>مِ A<۝>D<٢>",
"هُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣰ> لِّلۡمُحۡسِنِJ<ی>نَ A<۝>D<٣>",
"ٱلَّذِینَ یُقِیمُونَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَیُؤۡتُونَ ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَهُN<م> بِX<ٱ>لۡءَاخِR<رَ>ةِ هُمۡ یُوقِنُJ<و>نَ A<۝>D<٤>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ عَلَىٰ هُدX<ࣰ>ى N<مِّ>X<ن> R<رَّ>بِّهِمۡۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<٥>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یَ>شۡتَرِی لَهۡوَ ٱلۡحَدِیثِ لِیُضِلَّ عَN<ن> سَبِیلِ ٱللَّهِ بِغَیۡرِ عِلۡمX<ࣲ> N<وَ>یَتَّخِذَهَا هُزُوًاۚ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ عَذَابX<ࣱ> N<مُّ>هِJ<ی>نࣱ A<۝>D<٦>",
"وَإِذَا تُتۡلَىٰ عَلَیۡهِ ءَایَT<ـٰ>تُنَا وَلَّىٰ مُسۡتَكۡبِR<ر>N<ࣰ>ا كَأَX<ن> لَّمۡ یَسۡمَعۡهَا كَأَN<نَّ> فِW<یۤ> أُذُنَیۡهِ وَQ<قۡ>R<ر>N<ࣰ>اۖ فَبَشِّرۡهُ بِعَذَابٍ أَلِJ<ی>مٍ A<۝>D<٧>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَهُمۡ جَN<نَّ>T<ـٰ>تُ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<٨>",
"خَT<ـٰ>لِدِینَ فِیهَاۖ وَعۡدَ ٱللَّهِ حَقّX<ࣰ>اۚ N<وَ>هُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٩>",
"خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ بِغَیۡرِ عَمَدN<ࣲ> تَR<رَ>وۡنَهَاۖ وَأَلۡقَىٰ فِی ٱلۡأَR<رۡ>ضِ R<رَ>وَT< ٰ>سِیَ أَN<ن> تَمِیدَ بِكُمۡ وَبَثَّ فِیهَا مِN<ن> كُلِّ دَL<اۤ>بَّةX<ࣲ>ۚ N<وَ>أَN<ن>زَلۡنَا مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَX<ن>N<ۢ>بَتۡنَا فِیهَا مِN<ن> كُلِّ زَوۡجN<ࣲ> كَرِJ<ی>مٍ A<۝>D<١٠>",
"هَT<ـٰ>ذَا خَلۡقُ ٱللَّهِ فَأَR<رُ>ونِی مَاذَا خَلَقَ ٱلَّذِینَ مِN<ن> دُونِهِT<ۦ>ۚ بَلِ ٱX<ل>ظَّT<ـٰ>لِمُونَ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<١١>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا لُQ<قۡ>مَT<ـٰ>نَ ٱلۡحِكۡمَةَ أَنِ ٱشۡكُR<رۡ> لِلَّهِۚ وَمَX<ن> N<یَ>شۡكُR<رۡ> فَإِN<نَّ>مَا یَشۡكُR<رُ> لِنَفۡسِهِT<ۦ>ۖ وَمَN<ن> كَفَR<رَ> فَإِN<نَّ> ٱللَّهَ غَنِیٌّ حَمِJ<ی>Q<د>ࣱ A<۝>D<١٢>",
"وَإِذۡ قَالَ لُQ<قۡ>مَT<ـٰ>نُ لِX<ٱ>Q<بۡ>نِهِT<ۦ> وَهُوَ یَعِظُهُT<ۥ> یَT<ـٰ>بُنَیَّ لَا تُشۡرِكۡ بِٱللَّهِۖ إِN<نَّ> ٱX<ل>شِّرۡكَ لَظُلۡمٌ عَظِJ<ی>مࣱ A<۝>D<١٣>",
"وَوَصَّیۡنَا ٱلۡإِN<ن>سَT<ـٰ>نَ بِوَT< ٰ>لِدَیۡهِ حَمَلَتۡهُ أُN<مُّ>هُT<ۥ> وَهۡنًا عَلَىٰ وَهۡنX<ࣲ> N<وَ>فِصَT<ـٰ>لُهُT<ۥ> فِی عَامَیۡنِ أَنِ ٱشۡكُR<رۡ> لِی وَلِوَT< ٰ>لِدَیۡكَ إِلَیَّ ٱلۡمَصِJ<ی>رُ A<۝>D<١٤>",
"وَإِN<ن> جَT<ـٰ>هَدَاكَ عَلَىW<ٰۤ> أَN<ن> تُشۡرِكَ بِی مَا لَیۡسَ لَكَ بِهِT<ۦ> عِلۡمN<ࣱ> فَلَا تُطِعۡهُمَاۖ وَصَاحِQ<بۡ>هُمَا فِی ٱX<ل>دُّنۡیَا مَعۡR<رُ>وفX<ࣰ>اۖ N<وَ>X<ٱ>تَّبِعۡ سَبِیلَ مَنۡ أَنَابَ إِلَیَّۚ ثُN<مَّ> إِلَیَّ مَR<رۡ>جِعُكُمۡ فَأُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<١٥>",
"یَT<ـٰ>بُنَیَّ إِN<نَّ>هَW<اۤ> إِN<ن> تَكُ مِثۡقَالَ حَبَّةX<ࣲ> N<مِّ>نۡ خَR<رۡ>دَلN<ࣲ> فَتَكُN<ن> فِی صَخۡR<رَ>ةٍ أَوۡ فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ أَوۡ فِی ٱلۡأَR<رۡ>ضِ یَأۡتِ بِهَا ٱللَّهُۚ إِN<نَّ> ٱللَّهَ لَطِیفٌ خَبِJ<ی>رࣱ A<۝>D<١٦>",
"یَT<ـٰ>بُنَیَّ أَقِمِ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَأۡمُR<رۡ> بِX<ٱ>لۡمَعۡR<رُ>وفِ وَX<ٱ>نۡهَ عَنِ ٱلۡمُN<ن>كَرِ وَX<ٱ>صۡبِرۡ عَلَىٰ مَW<اۤ> أَصَابَكَۖ إِN<نَّ> ذَT< ٰ>لِكَ مِنۡ عَزۡمِ ٱلۡأُمُJ<و>R<ر>ِ A<۝>D<١٧>",
"وَلَا تُصَعِّرۡ خَدَّكَ لِX<ل>N<نَّ>اسِ وَلَا تَمۡشِ فِی ٱلۡأَR<رۡ>ضِ مَR<رَ>حًاۖ إِN<نَّ> ٱللَّهَ لَا یُحِبُّ كُلَّ مُخۡتَالN<ࣲ> فَخُJ<و>R<ر>ࣲ A<۝>D<١٨>",
"وَX<ٱ>Q<قۡ>صِQ<دۡ> فِی مَشۡیِكَ وَX<ٱ>غۡضُضۡ مِN<ن> صَوۡتِكَۚ إِN<نَّ> أَN<ن>كَR<رَ> ٱلۡأَصۡوَT< ٰ>تِ لَصَوۡتُ ٱلۡحَمِJ<ی>رِ A<۝>D<١٩>",
"أَلَمۡ تَR<رَ>وۡX<ا۟> أَN<نَّ> ٱللَّهَ سَخَّR<رَ> لَكُم N<مَّ>ا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِ وَأَسۡبَغَ عَلَیۡكُمۡ نِعَمَهُT<ۥ> ظَT<ـٰ>هِR<رَ>ةX<ࣰ> N<وَ>بَاطِنَةX<ࣰ>ۗ N<وَ>مِنَ ٱX<ل>N<نَّ>اسِ مَX<ن> N<یُ>جَT<ـٰ>دِلُ فِی ٱللَّهِ بِغَیۡرِ عِلۡمX<ࣲ> N<وَ>لَا هُدX<ࣰ>ى N<وَ>لَا كِتَT<ـٰ>بX<ࣲ> N<مُّ>نِJ<ی>رࣲ A<۝>D<٢٠>",
"وَإِذَا قِیلَ لَهُمُ ٱتَّبِعُوX<ا۟> مَW<اۤ> أَN<ن>زَلَ ٱللَّهُ قَالُوX<ا۟> بَلۡ نَتَّبِعُ مَا وَجَQ<دۡ>نَا عَلَیۡهِ ءَابَW<اۤ>ءَنَW<اۤ>ۚ أَوَ لَوۡ كَانَ ٱX<ل>شَّیۡطَT<ـٰ>نُ یَQ<دۡ>عُوهُمۡ إِلَىٰ عَذَابِ ٱX<ل>سَّعِJ<ی>رِ A<۝>D<٢١>",
"۞ وَمَX<ن> N<یُ>سۡلِمۡ وَQ<جۡ>هَهُW<ۥۤ> إِلَى ٱللَّهِ وَهُوَ مُحۡسِنN<ࣱ> فَقَدِ ٱسۡتَمۡسَكَ بِX<ٱ>لۡعُR<رۡ>وَةِ ٱلۡوُثۡقَىٰۗ وَإِلَى ٱللَّهِ عَT<ـٰ>قِبَةُ ٱلۡأُمُJ<و>R<ر>ِ A<۝>D<٢٢>",
"وَمَN<ن> كَفَR<رَ> فَلَا یَحۡزُN<ن>كَ كُفۡR<رُ>هُW<ۥۤ>ۚ إِلَیۡنَا مَR<رۡ>جِعُهُمۡ فَنُنَبِّئُهُN<م> بِمَا عَمِلُW<وۤ>X<ا۟>ۚ إِN<نَّ> ٱللَّهَ عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٢٣>",
"نُمَتِّعُهُمۡ قَلِیلN<ࣰ>ا ثُN<مَّ> نَضۡطَR<رُّ>هُمۡ إِلَىٰ عَذَابٍ غَلِJ<ی>ظࣲ A<۝>D<٢٤>",
"وَلَئِN<ن> سَأَلۡتَهُم N<مَّ>نۡ خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ لَیَقُولُN<نَّ> ٱللَّهُۚ قُلِ ٱلۡحَمۡدُ لِلَّهِۚ بَلۡ أَكۡثَR<رُ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٢٥>",
"لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ إِN<نَّ> ٱللَّهَ هُوَ ٱلۡغَنِیُّ ٱلۡحَمِJ<ی>Q<د>ُ A<۝>D<٢٦>",
"وَلَوۡ أَN<نَّ>مَا فِی ٱلۡأَR<رۡ>ضِ مِN<ن> شَجَR<رَ>ةٍ أَQ<قۡ>لَT<ـٰ>مX<ࣱ> N<وَ>X<ٱ>لۡبَحۡR<رُ> یَمُدُّهُT<ۥ> مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> سَQ<بۡ>عَةُ أَQ<بۡ>حُرX<ࣲ> N<مَّ>ا نَفِدَتۡ كَلِمَT<ـٰ>تُ ٱللَّهِۚ إِN<نَّ> ٱللَّهَ عَزِیزٌ حَكِJ<ی>مࣱ A<۝>D<٢٧>",
"مَّا خَلۡقُكُمۡ وَلَا بَعۡثُكُمۡ إِلَّا كَنَفۡسX<ࣲ> N<وَ>T< ٰ>حِدَةٍۚ إِN<نَّ> ٱللَّهَ سَمِیعُN<ۢ> بَصِJ<ی>رٌ A<۝>D<٢٨>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ یُولِجُ ٱلَّیۡلَ فِی ٱX<ل>N<نَّ>هَارِ وَیُولِجُ ٱX<ل>N<نَّ>هَاR<رَ> فِی ٱلَّیۡلِ وَسَخَّR<رَ> ٱX<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ>ۖ كُلّX<ࣱ> N<یَ>Q<جۡ>رِW<یۤ> إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>X<ࣰ>ى N<وَ>أَN<نَّ> ٱللَّهَ بِمَا تَعۡمَلُونَ خَبِJ<ی>رࣱ A<۝>D<٢٩>",
"ذَT< ٰ>لِكَ بِأَN<نَّ> ٱللَّهَ هُوَ ٱلۡحَقُّ وَأَN<نَّ> مَا یَQ<دۡ>عُونَ مِN<ن> دُونِهِ ٱلۡبَT<ـٰ>طِلُ وَأَN<نَّ> ٱللَّهَ هُوَ ٱلۡعَلِیُّ ٱلۡكَبِJ<ی>رُ A<۝>D<٣٠>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱلۡفُلۡكَ تَQ<جۡ>رِی فِی ٱلۡبَحۡرِ بِنِعۡمَتِ ٱللَّهِ لِیُرِیَكُم N<مِّ>نۡ ءَایَT<ـٰ>تِهِW<ۦۤ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّكُلِّ صَبَّارN<ࣲ> شَكُJ<و>R<ر>ࣲ A<۝>D<٣١>",
"وَإِذَا غَشِیَهُم N<مَّ>وۡجN<ࣱ> كَX<ٱ>X<ل>ظُّلَلِ دَعَوُX<ا۟> ٱللَّهَ مُخۡلِصِینَ لَهُ ٱX<ل>دِّینَ فَلَN<مَّ>ا نَجَّX<ى>T<ٰ>هُمۡ إِلَى ٱلۡبَرِّ فَمِنۡهُم N<مُّ>Q<قۡ>تَصِدX<ࣱ>ۚ N<وَ>مَا یَQ<جۡ>حَدُ بِءَایَT<ـٰ>تِنَW<اۤ> إِلَّا كُلُّ خَتَّارN<ࣲ> كَفُJ<و>R<ر>ࣲ A<۝>D<٣٢>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ ٱتَّقُوX<ا۟> R<رَ>بَّكُمۡ وَX<ٱ>خۡشَوۡX<ا۟> یَوۡمX<ࣰ>ا لَّا یَQ<جۡ>زِی وَالِدٌ عَX<ن> N<وَ>لَدِهِT<ۦ> وَلَا مَوۡلُودٌ هُوَ جَازٍ عَX<ن> N<وَ>الِدِهِT<ۦ> شَیۡءًاۚ إِN<نَّ> وَعۡدَ ٱللَّهِ حَقّN<ࣱ>ۖ فَلَا تَغُR<رَّ>N<نَّ>كُمُ ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَا وَلَا یَغُR<رَّ>N<نَّ>كُN<م> بِٱللَّهِ ٱلۡغَR<رُ>J<و>R<ر>ُ A<۝>D<٣٣>",
"إِN<نَّ> ٱللَّهَ عِN<ن>دَهُT<ۥ> عِلۡمُ ٱX<ل>سَّاعَةِ وَیُنَزِّلُ ٱلۡغَیۡثَ وَیَعۡلَمُ مَا فِی ٱلۡأَR<رۡ>حَامِۖ وَمَا تَQ<دۡ>رِی نَفۡسX<ࣱ> N<مَّ>اذَا تَكۡسِبُ غَدX<ࣰ>اۖ N<وَ>مَا تَQ<دۡ>رِی نَفۡسُN<ۢ> بِأَیِّ أَR<رۡ>ضN<ࣲ> تَمُوتُۚ إِN<نَّ> ٱللَّهَ عَلِیمٌ خَبِJ<ی>رُۢ A<۝>D<٣٤>",
"#اL<لۤ>L<مۤ> A<۝>D<١>",
"تَN<ن>زِیلُ ٱلۡكِتَT<ـٰ>بِ لَا R<رَ>یۡبَ فِیهِ مِX<ن> R<رَّ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٢>",
"أَمۡ یَقُولُونَ ٱفۡتَR<رَ>X<ى>T<ٰ>هُۚ بَلۡ هُوَ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّكَ لِتُN<ن>ذِR<رَ> قَوۡمX<ࣰ>ا N<مَّ>W<اۤ> أَتَX<ى>T<ٰ>هُم N<مِّ>ن N<نَّ>ذِیرX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِكَ لَعَلَّهُمۡ یَهۡتَدُJ<و>نَ A<۝>D<٣>",
"ٱللَّهُ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَمَا بَیۡنَهُمَا فِی سِتَّةِ أَیَّامN<ࣲ> ثُN<مَّ> ٱسۡتَوَىٰ عَلَى ٱلۡعَR<رۡ>شِۖ مَا لَكُم N<مِّ>N<ن> دُونِهِT<ۦ> مِX<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا شَفِیعٍۚ أَفَلَا تَتَذَكَّR<رُ>J<و>نَ A<۝>D<٤>",
"یُدَبِّR<رُ> ٱلۡأَمۡR<رَ> مِنَ ٱX<ل>سَّمَW<اۤ>ءِ إِلَى ٱلۡأَR<رۡ>ضِ ثُN<مَّ> یَعۡR<رُ>جُ إِلَیۡهِ فِی یَوۡمN<ࣲ> كَانَ مِQ<قۡ>دَاR<رُ>هُW<ۥۤ> أَلۡفَ سَنَةX<ࣲ> N<مِّ>N<مَّ>ا تَعُدُّJ<و>نَ A<۝>D<٥>",
"ذَT< ٰ>لِكَ عَT<ـٰ>لِمُ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٦>",
"ٱلَّذِW<یۤ> أَحۡسَنَ كُلَّ شَیۡءٍ خَلَقَهُT<ۥ>ۖ وَبَدَأَ خَلۡقَ ٱلۡإِN<ن>سَT<ـٰ>نِ مِN<ن> طِJ<ی>نࣲ A<۝>D<٧>",
"ثُN<مَّ> جَعَلَ نَسۡلَهُT<ۥ> مِN<ن> سُلَT<ـٰ>لَةX<ࣲ> N<مِّ>X<ن> N<مَّ>W<اۤ>ءX<ࣲ> N<مَّ>هِJ<ی>نࣲ A<۝>D<٨>",
"ثُN<مَّ> سَوَّX<ى>T<ٰ>هُ وَنَفَخَ فِیهِ مِX<ن> R<رُّ>وحِهِT<ۦ>ۖ وَجَعَلَ لَكُمُ ٱX<ل>سَّمۡعَ وَX<ٱ>لۡأَQ<بۡ>صَT<ـٰ>R<رَ> وَX<ٱ>لۡأَفۡءِدَةَۚ قَلِیلX<ࣰ>ا N<مَّ>ا تَشۡكُR<رُ>J<و>نَ A<۝>D<٩>",
"وَقَالُW<وۤ>X<ا۟> أَءِذَا ضَلَلۡنَا فِی ٱلۡأَR<رۡ>ضِ أَءِN<نَّ>ا لَفِی خَلۡقN<ࣲ> جَدِیدِN<ۭ>ۚ بَلۡ هُN<م> بِلِقَW<اۤ>ءِ R<رَ>بِّهِمۡ كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<١٠>",
"۞ قُلۡ یَتَوَفَّX<ى>T<ٰ>كُم N<مَّ>لَكُ ٱلۡمَوۡتِ ٱلَّذِی وُكِّلَ بِكُمۡ ثُN<مَّ> إِلَىٰ R<رَ>بِّكُمۡ تُR<رۡ>جَعُJ<و>نَ A<۝>D<١١>",
"وَلَوۡ تَR<رَ>ىW<ٰۤ> إِذِ ٱلۡمُQ<جۡ>رِمُونَ نَاكِسُوX<ا۟> R<رُ>ءُوسِهِمۡ عِN<ن>دَ R<رَ>بِّهِمۡ R<رَ>بَّنَW<اۤ> أَQ<بۡ>صَR<رۡ>نَا وَسَمِعۡنَا فَX<ٱ>R<رۡ>جِعۡنَا نَعۡمَلۡ صَT<ـٰ>لِحًا إِN<نَّ>ا مُوقِنُJ<و>نَ A<۝>D<١٢>",
"وَلَوۡ شِئۡنَا لَءَاتَیۡنَا كُلَّ نَفۡسٍ هُدَX<ى>T<ٰ>هَا وَلَT<ـٰ>كِنۡ حَقَّ ٱلۡقَوۡلُ مِN<نِّ>ی لَأَمۡلَأَN<نَّ> جَهَN<نَّ>مَ مِنَ ٱلۡجِN<نَّ>ةِ وَX<ٱ>X<ل>N<نَّ>اسِ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<١٣>",
"فَذُوقُوX<ا۟> بِمَا نَسِیتُمۡ لِقَW<اۤ>ءَ یَوۡمِكُمۡ هَT<ـٰ>ذَW<اۤ> إِN<نَّ>ا نَسِینَT<ـٰ>كُمۡۖ وَذُوقُوX<ا۟> عَذَابَ ٱلۡخُلۡدِ بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<١٤>",
"إِN<نَّ>مَا یُؤۡمِنُ بِءَایَT<ـٰ>تِنَا ٱلَّذِینَ إِذَا ذُكِّR<رُ>وX<ا۟> بِهَا خَ̅R<رُّ>̅و̅X<ا۟>̅ ̅سُ̅جَّ̅دX<ࣰ>̅ا̅ N<وَ>سَبَّحُوX<ا۟> بِحَمۡدِ R<رَ>بِّهِمۡ وَهُمۡ لَا یَسۡتَكۡبِR<رُ>J<و>نَ A<۝>D<١٥> ۩",
"تَتَجَافَىٰ جُنُوبُهُمۡ عَنِ ٱلۡمَضَاجِعِ یَQ<دۡ>عُونَ R<رَ>بَّهُمۡ خَوۡفX<ࣰ>ا N<وَ>طَمَعX<ࣰ>ا N<وَ>مِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡ یُN<ن>فِقُJ<و>نَ A<۝>D<١٦>",
"فَلَا تَعۡلَمُ نَفۡسX<ࣱ> N<مَّ>W<اۤ> أُخۡفِیَ لَهُم N<مِّ>N<ن> قُR<رَّ>ةِ أَعۡیُنN<ࣲ> جَزَW<اۤ>ءَN<ۢ> بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٧>",
"أَفَمَN<ن> كَانَ مُؤۡمِنN<ࣰ>ا كَمَN<ن> كَانَ فَاسِقX<ࣰ>اۚ لَّا یَسۡتَوُT<ۥ>نَ A<۝>D<١٨>",
"أَN<مَّ>ا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ فَلَهُمۡ جَN<نَّ>T<ـٰ>تُ ٱلۡمَأۡوَىٰ نُزُلَN<ۢ>ا بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٩>",
"وَأَN<مَّ>ا ٱلَّذِینَ فَسَقُوX<ا۟> فَمَأۡوَX<ى>T<ٰ>هُمُ ٱX<ل>N<نَّ>اR<رُ>ۖ كُلَّمَW<اۤ> أَR<رَ>ادُW<وۤ>X<ا۟> أَX<ن> N<یَ>خۡR<رُ>جُوX<ا۟> مِنۡهَW<اۤ> أُعِیدُوX<ا۟> فِیهَا وَقِیلَ لَهُمۡ ذُوقُوX<ا۟> عَذَابَ ٱX<ل>N<نَّ>ارِ ٱلَّذِی كُN<ن>تُN<م> بِهِT<ۦ> تُكَذِّبُJ<و>نَ A<۝>D<٢٠>",
"وَلَنُذِیقَN<نَّ>هُم N<مِّ>نَ ٱلۡعَذَابِ ٱلۡأَQ<دۡ>نَىٰ دُونَ ٱلۡعَذَابِ ٱلۡأَكۡبَرِ لَعَلَّهُمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<٢١>",
"وَمَنۡ أَظۡلَمُ مِN<مَّ>N<ن> ذُكِّR<رَ> بِءَایَT<ـٰ>تِ R<رَ>بِّهِT<ۦ> ثُN<مَّ> أَعۡR<رَ>ضَ عَنۡهَW<اۤ>ۚ إِN<نَّ>ا مِنَ ٱلۡمُQ<جۡ>رِمِینَ مُN<ن>تَقِمُJ<و>نَ A<۝>D<٢٢>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ فَلَا تَكُN<ن> فِی مِرۡیَةX<ࣲ> N<مِّ>X<ن> لِّقَW<اۤ>ئِهِT<ۦ>ۖ وَجَعَلۡنَT<ـٰ>هُ هُدX<ࣰ>ى لِّبَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِJ<ی>لَ A<۝>D<٢٣>",
"وَجَعَلۡنَا مِنۡهُمۡ أَئِN<مَّ>ةX<ࣰ> N<یَ>هۡدُونَ بِأَمۡرِنَا لَN<مَّ>ا صَبَR<رُ>وX<ا۟>ۖ وَكَانُوX<ا۟> بِءَایَT<ـٰ>تِنَا یُوقِنُJ<و>نَ A<۝>D<٢٤>",
"إِN<نَّ> R<رَ>بَّكَ هُوَ یَفۡصِلُ بَیۡنَهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ فِیمَا كَانُوX<ا۟> فِیهِ یَخۡتَلِفُJ<و>نَ A<۝>D<٢٥>",
"أَوَ لَمۡ یَهۡدِ لَهُمۡ كَمۡ أَهۡلَكۡنَا مِN<ن> قَQ<بۡ>لِهِم N<مِّ>نَ ٱلۡقُR<رُ>ونِ یَمۡشُونَ فِی مَسَT<ـٰ>كِنِهِمۡۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تٍۚ أَفَلَا یَسۡمَعُJ<و>نَ A<۝>D<٢٦>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ>ا نَسُوقُ ٱلۡمَW<اۤ>ءَ إِلَى ٱلۡأَR<رۡ>ضِ ٱلۡجُR<رُ>زِ فَنُخۡرِجُ بِهِT<ۦ> زَR<رۡ>عN<ࣰ>ا تَأۡكُلُ مِنۡهُ أَنۡعَT<ـٰ>مُهُمۡ وَأَN<ن>فُسُهُمۡۚ أَفَلَا یُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٢٧>",
"وَیَقُولُونَ مَتَىٰ هَT<ـٰ>ذَا ٱلۡفَتۡحُ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٢٨>",
"قُلۡ یَوۡمَ ٱلۡفَتۡحِ لَا یَN<ن>فَعُ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِیمَT<ـٰ>نُهُمۡ وَلَا هُمۡ یُN<ن>ظَR<رُ>J<و>نَ A<۝>D<٢٩>",
"فَأَعۡرِضۡ عَنۡهُمۡ وَX<ٱ>N<ن>تَظِرۡ إِN<نَّ>هُم N<مُّ>N<ن>تَظِR<رُ>J<و>نَ A<۝>D<٣٠>",
"#۞ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ ٱتَّقِ ٱللَّهَ وَلَا تُطِعِ ٱلۡكَT<ـٰ>فِرِینَ وَX<ٱ>لۡمُنَT<ـٰ>فِقِینَۚ إِN<نَّ> ٱللَّهَ كَانَ عَلِیمًا حَكِیمࣰا A<۝>D<١>",
"وَX<ٱ>تَّبِعۡ مَا یُوحَىW<ٰۤ> إِلَیۡكَ مِX<ن> R<رَّ>بِّكَۚ إِN<نَّ> ٱللَّهَ كَانَ بِمَا تَعۡمَلُونَ خَبِیR<ر>ࣰا A<۝>D<٢>",
"وَتَوَكَّلۡ عَلَى ٱللَّهِۚ وَكَفَىٰ بِٱللَّهِ وَكِیلࣰا A<۝>D<٣>",
"مَّا جَعَلَ ٱللَّهُ لِR<رَ>جُلX<ࣲ> N<مِّ>N<ن> قَلۡبَیۡنِ فِی جَوۡفِهِT<ۦ>ۚ وَمَا جَعَلَ أَزۡوَT< ٰ>جَكُمُ ٱلَّW<ـٰۤ>ءِی تُظَT<ـٰ>هِR<رُ>ونَ مِنۡهُN<نَّ> أُN<مَّ>هَT<ـٰ>تِكُمۡۚ وَمَا جَعَلَ أَQ<دۡ>عِیَW<اۤ>ءَكُمۡ أَQ<بۡ>نَW<اۤ>ءَكُمۡۚ ذَT< ٰ>لِكُمۡ قَوۡلُكُN<م> بِأَفۡوَT< ٰ>هِكُمۡۖ وَٱللَّهُ یَقُولُ ٱلۡحَقَّ وَهُوَ یَهۡدِی ٱX<ل>سَّبِJ<ی>لَ A<۝>D<٤>",
"ٱQ<دۡ>عُوهُمۡ لِءَابَW<اۤ>ئِهِمۡ هُوَ أَQ<قۡ>سَطُ عِN<ن>دَ ٱللَّهِۚ فَإِX<ن> لَّمۡ تَعۡلَمُW<وۤ>X<ا۟> ءَابَW<اۤ>ءَهُمۡ فَإِخۡوَT< ٰ>نُكُمۡ فِی ٱX<ل>دِّینِ وَمَوَT< ٰ>لِیكُمۡۚ وَلَیۡسَ عَلَیۡكُمۡ جُنَاحN<ࣱ> فِیمَW<اۤ> أَخۡطَأۡتُN<م> بِهِT<ۦ> وَلَT<ـٰ>كِX<ن> N<مَّ>ا تَعَN<مَّ>دَتۡ قُلُوبُكُمۡۚ وَكَانَ ٱللَّهُ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمًا A<۝>D<٥>",
"ٱX<ل>N<نَّ>بِیُّ أَوۡلَىٰ بِX<ٱ>لۡمُؤۡمِنِینَ مِنۡ أَN<ن>فُسِهِمۡۖ وَأَزۡوَT< ٰ>جُهُW<ۥۤ> أُN<مَّ>هَT<ـٰ>تُهُمۡۗ وَأُX<و۟>لُوX<ا۟> ٱلۡأَR<رۡ>حَامِ بَعۡضُهُمۡ أَوۡلَىٰ بِبَعۡضN<ࣲ> فِی كِتَT<ـٰ>بِ ٱللَّهِ مِنَ ٱلۡمُؤۡمِنِینَ وَX<ٱ>لۡمُهَT<ـٰ>جِرِینَ إِلَّW<اۤ> أَN<ن> تَفۡعَلُW<وۤ>X<ا۟> إِلَىW<ٰۤ> أَوۡلِیَW<اۤ>ئِكُم N<مَّ>عۡR<رُ>وفN<ࣰ>اۚ كَانَ ذَT< ٰ>لِكَ فِی ٱلۡكِتَT<ـٰ>بِ مَسۡطُوR<ر>ࣰا A<۝>D<٦>",
"وَإِذۡ أَخَذۡنَا مِنَ ٱX<ل>N<نَّ>بِیِّT<ـۧ>نَ مِیثَT<ـٰ>قَهُمۡ وَمِN<ن>كَ وَمِن N<نُّ>وحX<ࣲ> N<وَ>إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَمُوسَىٰ وَعِیسَى ٱQ<بۡ>نِ مَR<رۡ>یَمَۖ وَأَخَذۡنَا مِنۡهُم N<مِّ>یثَT<ـٰ>قًا غَلِیظࣰا A<۝>D<٧>",
"لِّیَسۡءَلَ ٱX<ل>صَّT<ـٰ>دِقِینَ عَN<ن> صِQ<دۡ>قِهِمۡۚ وَأَعَدَّ لِلۡكَT<ـٰ>فِرِینَ عَذَابًا أَلِیمࣰا A<۝>D<٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱذۡكُR<رُ>وX<ا۟> نِعۡمَةَ ٱللَّهِ عَلَیۡكُمۡ إِذۡ جَW<اۤ>ءَتۡكُمۡ جُنُودN<ࣱ> فَأَR<رۡ>سَلۡنَا عَلَیۡهِمۡ رِیحX<ࣰ>ا N<وَ>جُنُودX<ࣰ>ا لَّمۡ تَR<رَ>وۡهَاۚ وَكَانَ ٱللَّهُ بِمَا تَعۡمَلُونَ بَصِیR<ر>ًا A<۝>D<٩>",
"إِذۡ جَW<اۤ>ءُوكُم N<مِّ>N<ن> فَوۡقِكُمۡ وَمِنۡ أَسۡفَلَ مِN<ن>كُمۡ وَإِذۡ زَاغَتِ ٱلۡأَQ<بۡ>صَT<ـٰ>R<رُ> وَبَلَغَتِ ٱلۡقُلُوبُ ٱلۡحَنَاجِR<رَ> وَتَظُN<نُّ>ونَ بِٱللَّهِ ٱX<ل>ظُّنُونَا۠ A<۝>D<١٠>",
"هُنَالِكَ ٱQ<بۡ>تُلِیَ ٱلۡمُؤۡمِنُونَ وَزُلۡزِلُوX<ا۟> زِلۡزَالN<ࣰ>ا شَدِیدࣰا A<۝>D<١١>",
"وَإِذۡ یَقُولُ ٱلۡمُنَT<ـٰ>فِقُونَ وَX<ٱ>لَّذِینَ فِی قُلُوبِهِم N<مَّ>R<رَ>ضX<ࣱ> N<مَّ>ا وَعَدَنَا ٱللَّهُ وَR<رَ>سُولُهُW<ۥۤ> إِلَّا غُR<رُ>وR<ر>ࣰا A<۝>D<١٢>",
"وَإِذۡ قَالَت طَّW<اۤ>ئِفَةX<ࣱ> N<مِّ>نۡهُمۡ یَW<ـٰۤ>أَهۡلَ یَثۡرِبَ لَا مُقَامَ لَكُمۡ فَX<ٱ>R<رۡ>جِعُوX<ا۟>ۚ وَیَسۡتَءۡذِنُ فَرِیقX<ࣱ> N<مِّ>نۡهُمُ ٱX<ل>N<نَّ>بِیَّ یَقُولُونَ إِN<نَّ> بُیُوتَنَا عَوۡR<رَ>ةX<ࣱ> N<وَ>مَا هِیَ بِعَوۡR<رَ>ةٍۖ إِX<ن> N<یُ>رِیدُونَ إِلَّا فِR<رَ>اR<ر>ࣰا A<۝>D<١٣>",
"وَلَوۡ دُخِلَتۡ عَلَیۡهِم N<مِّ>نۡ أَQ<قۡ>طَارِهَا ثُN<مَّ> سُئِلُوX<ا۟> ٱلۡفِتۡنَةَ لَءَاتَوۡهَا وَمَا تَلَبَّثُوX<ا۟> بِهَW<اۤ> إِلَّا یَسِیR<ر>ࣰا A<۝>D<١٤>",
"وَلَقَQ<دۡ> كَانُوX<ا۟> عَT<ـٰ>هَدُوX<ا۟> ٱللَّهَ مِN<ن> قَQ<بۡ>لُ لَا یُوَلُّونَ ٱلۡأَQ<دۡ>بَT<ـٰ>R<رَ>ۚ وَكَانَ عَهۡدُ ٱللَّهِ مَسۡءُولࣰا A<۝>D<١٥>",
"قُل لَّX<ن> N<یَ>N<ن>فَعَكُمُ ٱلۡفِR<رَ>اR<رُ> إِN<ن> فَR<رَ>R<رۡ>تُم N<مِّ>نَ ٱلۡمَوۡتِ أَوِ ٱلۡقَتۡلِ وَإِذX<ࣰ>ا لَّا تُمَتَّعُونَ إِلَّا قَلِیلࣰا A<۝>D<١٦>",
"قُلۡ مَN<ن> ذَا ٱلَّذِی یَعۡصِمُكُم N<مِّ>نَ ٱللَّهِ إِنۡ أَR<رَ>ادَ بِكُمۡ سُW<وۤ>ءًا أَوۡ أَR<رَ>ادَ بِكُمۡ R<رَ>حۡمَةX<ࣰ>ۚ N<وَ>لَا یَجِدُونَ لَهُم N<مِّ>N<ن> دُونِ ٱللَّهِ وَلِیّX<ࣰ>ا N<وَ>لَا نَصِیR<ر>ࣰا A<۝>D<١٧>",
"۞ قَQ<دۡ> یَعۡلَمُ ٱللَّهُ ٱلۡمُعَوِّقِینَ مِN<ن>كُمۡ وَX<ٱ>لۡقَW<اۤ>ئِلِینَ لِإِخۡوَT< ٰ>نِهِمۡ هَلُN<مَّ> إِلَیۡنَاۖ وَلَا یَأۡتُونَ ٱلۡبَأۡسَ إِلَّا قَلِیلًا A<۝>D<١٨>",
"أَشِحَّةً عَلَیۡكُمۡۖ فَإِذَا جَW<اۤ>ءَ ٱلۡخَوۡفُ R<رَ>أَیۡتَهُمۡ یَN<ن>ظُR<رُ>ونَ إِلَیۡكَ تَدُوR<رُ> أَعۡیُنُهُمۡ كَX<ٱ>لَّذِی یُغۡشَىٰ عَلَیۡهِ مِنَ ٱلۡمَوۡتِۖ فَإِذَا ذَهَبَ ٱلۡخَوۡفُ سَلَقُوكُN<م> بِأَلۡسِنَةٍ حِدَادٍ أَشِحَّةً عَلَى ٱلۡخَیۡرِۚ أُX<و۟>لَW<ـٰۤ>ئِكَ لَمۡ یُؤۡمِنُوX<ا۟> فَأَحۡبَطَ ٱللَّهُ أَعۡمَT<ـٰ>لَهُمۡۚ وَكَانَ ذَT< ٰ>لِكَ عَلَى ٱللَّهِ یَسِیR<ر>ࣰا A<۝>D<١٩>",
"یَحۡسَبُونَ ٱلۡأَحۡزَابَ لَمۡ یَذۡهَبُوX<ا۟>ۖ وَإِX<ن> N<یَ>أۡتِ ٱلۡأَحۡزَابُ یَوَدُّوX<ا۟> لَوۡ أَN<نَّ>هُN<م> بَادُونَ فِی ٱلۡأَعۡR<رَ>ابِ یَسۡءَلُونَ عَنۡ أَX<ن>N<ۢ>بَW<اۤ>ئِكُمۡۖ وَلَوۡ كَانُوX<ا۟> فِیكُم N<مَّ>ا قَT<ـٰ>تَلُW<وۤ>X<ا۟> إِلَّا قَلِیلࣰا A<۝>D<٢٠>",
"لَّقَQ<دۡ> كَانَ لَكُمۡ فِی R<رَ>سُولِ ٱللَّهِ أُسۡوَةٌ حَسَنَةX<ࣱ> لِّمَN<ن> كَانَ یَR<رۡ>جُوX<ا۟> ٱللَّهَ وَX<ٱ>لۡیَوۡمَ ٱلۡءَاخِR<رَ> وَذَكَR<رَ> ٱللَّهَ كَثِیR<ر>ࣰا A<۝>D<٢١>",
"وَلَN<مَّ>ا R<رَ>ءَا ٱلۡمُؤۡمِنُونَ ٱلۡأَحۡزَابَ قَالُوX<ا۟> هَT<ـٰ>ذَا مَا وَعَدَنَا ٱللَّهُ وَR<رَ>سُولُهُT<ۥ> وَصَدَقَ ٱللَّهُ وَR<رَ>سُولُهُT<ۥ>ۚ وَمَا زَادَهُمۡ إِلَّW<اۤ> إِیمَT<ـٰ>نX<ࣰ>ا N<وَ>تَسۡلِیمࣰا A<۝>D<٢٢>",
"مِّنَ ٱلۡمُؤۡمِنِینَ رِجَالN<ࣱ> صَدَقُوX<ا۟> مَا عَT<ـٰ>هَدُوX<ا۟> ٱللَّهَ عَلَیۡهِۖ فَمِنۡهُم N<مَّ>N<ن> قَضَىٰ نَحۡبَهُT<ۥ> وَمِنۡهُم N<مَّ>X<ن> N<یَ>N<ن>تَظِR<رُ>ۖ وَمَا بَدَّلُوX<ا۟> تَQ<بۡ>دِیلࣰا A<۝>D<٢٣>",
"لِّیَQ<جۡ>زِیَ ٱللَّهُ ٱX<ل>صَّT<ـٰ>دِقِینَ بِصِQ<دۡ>قِهِمۡ وَیُعَذِّبَ ٱلۡمُنَT<ـٰ>فِقِینَ إِN<ن> شَW<اۤ>ءَ أَوۡ یَتُوبَ عَلَیۡهِمۡۚ إِN<نَّ> ٱللَّهَ كَانَ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<٢٤>",
"وَR<رَ>دَّ ٱللَّهُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِغَیۡظِهِمۡ لَمۡ یَنَالُوX<ا۟> خَیۡR<ر>X<ࣰ>اۚ N<وَ>كَفَى ٱللَّهُ ٱلۡمُؤۡمِنِینَ ٱلۡقِتَالَۚ وَكَانَ ٱللَّهُ قَوِیًّا عَزِیزࣰا A<۝>D<٢٥>",
"وَأَN<ن>زَلَ ٱلَّذِینَ ظَT<ـٰ>هَR<رُ>وهُم N<مِّ>نۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ مِN<ن> صَیَاصِیهِمۡ وَقَذَفَ فِی قُلُوبِهِمُ ٱX<ل>R<رُّ>عۡبَ فَرِیقN<ࣰ>ا تَQ<قۡ>تُلُونَ وَتَأۡسِR<رُ>ونَ فَرِیقࣰا A<۝>D<٢٦>",
"وَأَوۡR<رَ>ثَكُمۡ أَR<رۡ>ضَهُمۡ وَدِیَT<ـٰ>R<رَ>هُمۡ وَأَمۡوَT< ٰ>لَهُمۡ وَأَR<رۡ>ضX<ࣰ>ا لَّمۡ تَطَءُوهَاۚ وَكَانَ ٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِیR<ر>ࣰا A<۝>D<٢٧>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ قُل لِّأَزۡوَT< ٰ>جِكَ إِN<ن> كُN<ن>تُN<نَّ> تُرِQ<دۡ>نَ ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا وَزِینَتَهَا فَتَعَالَیۡنَ أُمَتِّعۡكُN<نَّ> وَأُسَرِّحۡكُN<نَّ> سَR<رَ>احN<ࣰ>ا جَمِیلࣰا A<۝>D<٢٨>",
"وَإِN<ن> كُN<ن>تُN<نَّ> تُرِQ<دۡ>نَ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> وَX<ٱ>X<ل>دَّاR<رَ> ٱلۡءَاخِR<رَ>ةَ فَإِN<نَّ> ٱللَّهَ أَعَدَّ لِلۡمُحۡسِنَT<ـٰ>تِ مِN<ن>كُN<نَّ> أَQ<جۡ>R<ر>ًا عَظِیمࣰا A<۝>D<٢٩>",
"یَT<ـٰ>نِسَW<اۤ>ءَ ٱX<ل>N<نَّ>بِیِّ مَX<ن> N<یَ>أۡتِ مِN<ن>كُN<نَّ> بِفَT<ـٰ>حِشَةX<ࣲ> N<مُّ>بَیِّنَةX<ࣲ> N<یُ>ضَT<ـٰ>عَفۡ لَهَا ٱلۡعَذَابُ ضِعۡفَیۡنِۚ وَكَانَ ذَT< ٰ>لِكَ عَلَى ٱللَّهِ یَسِیR<ر>ࣰا A<۝>D<٣٠>",
"۞ وَمَX<ن> N<یَ>Q<قۡ>نُتۡ مِN<ن>كُN<نَّ> لِلَّهِ وَR<رَ>سُولِهِT<ۦ> وَتَعۡمَلۡ صَT<ـٰ>لِحX<ࣰ>ا N<نُّ>ؤۡتِهَW<اۤ> أَQ<جۡ>R<رَ>هَا مَR<رَّ>تَیۡنِ وَأَعۡتَQ<دۡ>نَا لَهَا رِزۡقN<ࣰ>ا كَرِیمࣰا A<۝>D<٣١>",
"یَT<ـٰ>نِسَW<اۤ>ءَ ٱX<ل>N<نَّ>بِیِّ لَسۡتُN<نَّ> كَأَحَدX<ࣲ> N<مِّ>نَ ٱX<ل>N<نِّ>سَW<اۤ>ءِ إِنِ ٱتَّقَیۡتُN<نَّ>ۚ فَلَا تَخۡضَعۡنَ بِX<ٱ>لۡقَوۡلِ فَیَQ<طۡ>مَعَ ٱلَّذِی فِی قَلۡبِهِT<ۦ> مَR<رَ>ضX<ࣱ> N<وَ>قُلۡنَ قَوۡلX<ࣰ>ا N<مَّ>عۡR<رُ>وفࣰا A<۝>D<٣٢>",
"وَقَR<رۡ>نَ فِی بُیُوتِكُN<نَّ> وَلَا تَبَR<رَّ>Q<جۡ>نَ تَبَR<رُّ>جَ ٱلۡجَT<ـٰ>هِلِیَّةِ ٱلۡأُولَىٰۖ وَأَقِمۡنَ ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتِینَ ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَأَطِعۡنَ ٱللَّهَ وَR<رَ>سُولَهُW<ۥۤ>ۚ إِN<نَّ>مَا یُرِیدُ ٱللَّهُ لِیُذۡهِبَ عَN<ن>كُمُ ٱX<ل>رِّQ<جۡ>سَ أَهۡلَ ٱلۡبَیۡتِ وَیُطَهِّR<رَ>كُمۡ تَQ<طۡ>هِیR<ر>ࣰا A<۝>D<٣٣>",
"وَX<ٱ>ذۡكُR<رۡ>نَ مَا یُتۡلَىٰ فِی بُیُوتِكُN<نَّ> مِنۡ ءَایَT<ـٰ>تِ ٱللَّهِ وَX<ٱ>لۡحِكۡمَةِۚ إِN<نَّ> ٱللَّهَ كَانَ لَطِیفًا خَبِیR<ر>ًا A<۝>D<٣٤>",
"إِN<نَّ> ٱلۡمُسۡلِمِینَ وَX<ٱ>لۡمُسۡلِمَT<ـٰ>تِ وَX<ٱ>لۡمُؤۡمِنِینَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تِ وَX<ٱ>لۡقَT<ـٰ>نِتِینَ وَX<ٱ>لۡقَT<ـٰ>نِتَT<ـٰ>تِ وَX<ٱ>X<ل>صَّT<ـٰ>دِقِینَ وَX<ٱ>X<ل>صَّT<ـٰ>دِقَT<ـٰ>تِ وَX<ٱ>X<ل>صَّT<ـٰ>بِرِینَ وَX<ٱ>X<ل>صَّT<ـٰ>بِR<رَ>T< ٰ>تِ وَX<ٱ>لۡخَT<ـٰ>شِعِینَ وَX<ٱ>لۡخَT<ـٰ>شِعَT<ـٰ>تِ وَX<ٱ>لۡمُتَصَدِّقِینَ وَX<ٱ>لۡمُتَصَدِّقَT<ـٰ>تِ وَX<ٱ>X<ل>صَّW<ـٰۤ>ئِمِینَ وَX<ٱ>X<ل>صَّW<ـٰۤ>ئِمَT<ـٰ>تِ وَX<ٱ>لۡحَT<ـٰ>فِظِینَ فُR<رُ>وجَهُمۡ وَX<ٱ>لۡحَT<ـٰ>فِظَT<ـٰ>تِ وَX<ٱ>X<ل>ذَّT< ٰ>كِرِینَ ٱللَّهَ كَثِیR<ر>X<ࣰ>ا N<وَ>X<ٱ>X<ل>ذَّT< ٰ>كِR<رَ>T< ٰ>تِ أَعَدَّ ٱللَّهُ لَهُم N<مَّ>غۡفِR<رَ>ةX<ࣰ> N<وَ>أَQ<جۡ>R<ر>ًا عَظِیمࣰا A<۝>D<٣٥>",
"وَمَا كَانَ لِمُؤۡمِنX<ࣲ> N<وَ>لَا مُؤۡمِنَةٍ إِذَا قَضَى ٱللَّهُ وَR<رَ>سُولُهُW<ۥۤ> أَمۡR<ر>ًا أَX<ن> N<یَ>كُونَ لَهُمُ ٱلۡخِیَR<رَ>ةُ مِنۡ أَمۡرِهِمۡۗ وَمَX<ن> N<یَ>عۡصِ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> فَقَQ<دۡ> ضَلَّ ضَلَT<ـٰ>لX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<٣٦>",
"وَإِذۡ تَقُولُ لِلَّذِW<یۤ> أَنۡعَمَ ٱللَّهُ عَلَیۡهِ وَأَنۡعَمۡتَ عَلَیۡهِ أَمۡسِكۡ عَلَیۡكَ زَوۡجَكَ وَX<ٱ>تَّقِ ٱللَّهَ وَتُخۡفِی فِی نَفۡسِكَ مَا ٱللَّهُ مُQ<بۡ>دِیهِ وَتَخۡشَى ٱX<ل>N<نَّ>اسَ وَٱللَّهُ أَحَقُّ أَN<ن> تَخۡشَX<ى>T<ٰ>هُۖ فَلَN<مَّ>ا قَضَىٰ زَیۡدX<ࣱ> N<مِّ>نۡهَا وَطَR<ر>N<ࣰ>ا زَوَّQ<جۡ>نَT<ـٰ>كَهَا لِكَیۡ لَا یَكُونَ عَلَى ٱلۡمُؤۡمِنِینَ حَR<رَ>جN<ࣱ> فِW<یۤ> أَزۡوَT< ٰ>جِ أَQ<دۡ>عِیَW<اۤ>ئِهِمۡ إِذَا قَضَوۡX<ا۟> مِنۡهُN<نَّ> وَطَR<ر>X<ࣰ>اۚ N<وَ>كَانَ أَمۡR<رُ> ٱللَّهِ مَفۡعُولࣰا A<۝>D<٣٧>",
"مَّا كَانَ عَلَى ٱX<ل>N<نَّ>بِیِّ مِنۡ حَR<رَ>جN<ࣲ> فِیمَا فَR<رَ>ضَ ٱللَّهُ لَهُT<ۥ>ۖ سُN<نَّ>ةَ ٱللَّهِ فِی ٱلَّذِینَ خَلَوۡX<ا۟> مِN<ن> قَQ<بۡ>لُۚ وَكَانَ أَمۡR<رُ> ٱللَّهِ قَدَR<ر>X<ࣰ>ا N<مَّ>Q<قۡ>دُوR<ر>ًا A<۝>D<٣٨>",
"ٱلَّذِینَ یُبَلِّغُونَ رِسَT<ـٰ>لَT<ـٰ>تِ ٱللَّهِ وَیَخۡشَوۡنَهُT<ۥ> وَلَا یَخۡشَوۡنَ أَحَدًا إِلَّا ٱللَّهَۗ وَكَفَىٰ بِٱللَّهِ حَسِیبࣰا A<۝>D<٣٩>",
"مَّا كَانَ مُحَN<مَّ>دٌ أَبَW<اۤ> أَحَدX<ࣲ> N<مِّ>X<ن> رِّجَالِكُمۡ وَلَT<ـٰ>كِX<ن> R<رَّ>سُولَ ٱللَّهِ وَخَاتَمَ ٱX<ل>N<نَّ>بِیِّT<ـۧ>نَۗ وَكَانَ ٱللَّهُ بِكُلِّ شَیۡءٍ عَلِیمࣰا A<۝>D<٤٠>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱذۡكُR<رُ>وX<ا۟> ٱللَّهَ ذِكۡR<ر>N<ࣰ>ا كَثِیR<ر>ࣰا A<۝>D<٤١>",
"وَسَبِّحُوهُ بُكۡR<رَ>ةX<ࣰ> N<وَ>أَصِیلًا A<۝>D<٤٢>",
"هُوَ ٱلَّذِی یُصَلِّی عَلَیۡكُمۡ وَمَلَW<ـٰۤ>ئِكَتُهُT<ۥ> لِیُخۡرِجَكُم N<مِّ>نَ ٱX<ل>ظُّلُمَT<ـٰ>تِ إِلَى ٱX<ل>N<نُّ>ورِۚ وَكَانَ بِX<ٱ>لۡمُؤۡمِنِینَ R<رَ>حِیمࣰا A<۝>D<٤٣>",
"تَحِیَّتُهُمۡ یَوۡمَ یَلۡقَوۡنَهُT<ۥ> سَلَT<ـٰ>مX<ࣱ>ۚ N<وَ>أَعَدَّ لَهُمۡ أَQ<جۡ>R<ر>N<ࣰ>ا كَرِیمࣰا A<۝>D<٤٤>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ إِN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ شَT<ـٰ>هِدX<ࣰ>ا N<وَ>مُبَشِّR<ر>X<ࣰ>ا N<وَ>نَذِیR<ر>ࣰا A<۝>D<٤٥>",
"وَدَاعِیًا إِلَى ٱللَّهِ بِإِذۡنِهِT<ۦ> وَسِR<رَ>اجX<ࣰ>ا N<مُّ>نِیR<ر>ࣰا A<۝>D<٤٦>",
"وَبَشِّرِ ٱلۡمُؤۡمِنِینَ بِأَN<نَّ> لَهُم N<مِّ>نَ ٱللَّهِ فَضۡلN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٤٧>",
"وَلَا تُطِعِ ٱلۡكَT<ـٰ>فِرِینَ وَX<ٱ>لۡمُنَT<ـٰ>فِقِینَ وَدَعۡ أَذَX<ى>T<ٰ>هُمۡ وَتَوَكَّلۡ عَلَى ٱللَّهِۚ وَكَفَىٰ بِٱللَّهِ وَكِیلࣰا A<۝>D<٤٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا نَكَحۡتُمُ ٱلۡمُؤۡمِنَT<ـٰ>تِ ثُN<مَّ> طَلَّQ<قۡ>تُمُوهُN<نَّ> مِN<ن> قَQ<بۡ>لِ أَN<ن> تَمَسُّوهُN<نَّ> فَمَا لَكُمۡ عَلَیۡهِN<نَّ> مِنۡ عِدَّةN<ࣲ> تَعۡتَدُّونَهَاۖ فَمَتِّعُوهُN<نَّ> وَسَرِّحُوهُN<نَّ> سَR<رَ>احN<ࣰ>ا جَمِیلࣰا A<۝>D<٤٩>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ إِN<نَّ>W<اۤ> أَحۡلَلۡنَا لَكَ أَزۡوَT< ٰ>جَكَ ٱلَّT<ـٰ>تِW<یۤ> ءَاتَیۡتَ أُجُوR<رَ>هُN<نَّ> وَمَا مَلَكَتۡ یَمِینُكَ مِN<مَّ>W<اۤ> أَفَW<اۤ>ءَ ٱللَّهُ عَلَیۡكَ وَبَنَاتِ عَN<مِّ>كَ وَبَنَاتِ عَN<مَّ>T<ـٰ>تِكَ وَبَنَاتِ خَالِكَ وَبَنَاتِ خَT<ـٰ>لَT<ـٰ>تِكَ ٱلَّT<ـٰ>تِی هَاجَR<رۡ>نَ مَعَكَ وَX<ٱ>مۡR<رَ>أَةX<ࣰ> N<مُّ>ؤۡمِنَةً إِX<ن> N<وَ>هَبَتۡ نَفۡسَهَا لِX<ل>N<نَّ>بِیِّ إِنۡ أَR<رَ>ادَ ٱX<ل>N<نَّ>بِیُّ أَX<ن> N<یَ>سۡتَN<ن>كِحَهَا خَالِصَةX<ࣰ> لَّكَ مِN<ن> دُونِ ٱلۡمُؤۡمِنِینَۗ قَQ<دۡ> عَلِمۡنَا مَا فَR<رَ>ضۡنَا عَلَیۡهِمۡ فِW<یۤ> أَزۡوَT< ٰ>جِهِمۡ وَمَا مَلَكَتۡ أَیۡمَT<ـٰ>نُهُمۡ لِكَیۡلَا یَكُونَ عَلَیۡكَ حَR<رَ>جX<ࣱ>ۗ N<وَ>كَانَ ٱللَّهُ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<٥٠>",
"۞ تُR<رۡ>جِی مَN<ن> تَشَW<اۤ>ءُ مِنۡهُN<نَّ> وَتُءۡوِW<یۤ> إِلَیۡكَ مَN<ن> تَشَW<اۤ>ءُۖ وَمَنِ ٱQ<بۡ>تَغَیۡتَ مِN<مَّ>نۡ عَزَلۡتَ فَلَا جُنَاحَ عَلَیۡكَۚ ذَT< ٰ>لِكَ أَQ<دۡ>نَىW<ٰۤ> أَN<ن> تَقَR<رَّ> أَعۡیُنُهُN<نَّ> وَلَا یَحۡزَN<نَّ> وَیَR<رۡ>ضَیۡنَ بِمَW<اۤ> ءَاتَیۡتَهُN<نَّ> كُلُّهُN<نَّ>ۚ وَٱللَّهُ یَعۡلَمُ مَا فِی قُلُوبِكُمۡۚ وَكَانَ ٱللَّهُ عَلِیمًا حَلِیمࣰا A<۝>D<٥١>",
"لَّا یَحِلُّ لَكَ ٱX<ل>N<نِّ>سَW<اۤ>ءُ مِX<ن>N<ۢ> بَعۡدُ وَلَW<اۤ> أَN<ن> تَبَدَّلَ بِهِN<نَّ> مِنۡ أَزۡوَT< ٰ>جX<ࣲ> N<وَ>لَوۡ أَعۡجَبَكَ حُسۡنُهُN<نَّ> إِلَّا مَا مَلَكَتۡ یَمِینُكَۗ وَكَانَ ٱللَّهُ عَلَىٰ كُلِّ شَیۡءX<ࣲ> R<رَّ>قِیبࣰا A<۝>D<٥٢>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَQ<دۡ>خُلُوX<ا۟> بُیُوتَ ٱX<ل>N<نَّ>بِیِّ إِلَّW<اۤ> أَX<ن> N<یُ>ؤۡذَنَ لَكُمۡ إِلَىٰ طَعَامٍ غَیۡR<رَ> نَT<ـٰ>ظِرِینَ إِنَX<ى>T<ٰ>هُ وَلَT<ـٰ>كِنۡ إِذَا دُعِیتُمۡ فَX<ٱ>Q<دۡ>خُلُوX<ا۟> فَإِذَا طَعِمۡتُمۡ فَX<ٱ>N<ن>تَشِR<رُ>وX<ا۟> وَلَا مُسۡتَءۡنِسِینَ لِحَدِیثٍۚ إِN<نَّ> ذَT< ٰ>لِكُمۡ كَانَ یُؤۡذِی ٱX<ل>N<نَّ>بِیَّ فَیَسۡتَحۡیِT<ۦ> مِN<ن>كُمۡۖ وَٱللَّهُ لَا یَسۡتَحۡیِT<ۦ> مِنَ ٱلۡحَقِّۚ وَإِذَا سَأَلۡتُمُوهُN<نَّ> مَتَT<ـٰ>عN<ࣰ>ا فَسۡءَلُوهُN<نَّ> مِX<ن> N<وَ>R<رَ>W<اۤ>ءِ حِجَابN<ࣲ>ۚ ذَT< ٰ>لِكُمۡ أَQ<طۡ>هَR<رُ> لِقُلُوبِكُمۡ وَقُلُوبِهِN<نَّ>ۚ وَمَا كَانَ لَكُمۡ أَN<ن> تُؤۡذُوX<ا۟> R<رَ>سُولَ ٱللَّهِ وَلَW<اۤ> أَN<ن> تَN<ن>كِحُW<وۤ>X<ا۟> أَزۡوَT< ٰ>جَهُT<ۥ> مِX<ن>N<ۢ> بَعۡدِهِW<ۦۤ> أَبَدًاۚ إِN<نَّ> ذَT< ٰ>لِكُمۡ كَانَ عِN<ن>دَ ٱللَّهِ عَظِیمًا A<۝>D<٥٣>",
"إِN<ن> تُQ<بۡ>دُوX<ا۟> شَیۡءًا أَوۡ تُخۡفُوهُ فَإِN<نَّ> ٱللَّهَ كَانَ بِكُلِّ شَیۡءٍ عَلِیمࣰا A<۝>D<٥٤>",
"لَّا جُنَاحَ عَلَیۡهِN<نَّ> فِW<یۤ> ءَابَW<اۤ>ئِهِN<نَّ> وَلَW<اۤ> أَQ<بۡ>نَW<اۤ>ئِهِN<نَّ> وَلَW<اۤ> إِخۡوَT< ٰ>نِهِN<نَّ> وَلَW<اۤ> أَQ<بۡ>نَW<اۤ>ءِ إِخۡوَT< ٰ>نِهِN<نَّ> وَلَW<اۤ> أَQ<بۡ>نَW<اۤ>ءِ أَخَوَT< ٰ>تِهِN<نَّ> وَلَا نِسَW<اۤ>ئِهِN<نَّ> وَلَا مَا مَلَكَتۡ أَیۡمَT<ـٰ>نُهُN<نَّ>ۗ وَX<ٱ>تَّقِینَ ٱللَّهَۚ إِN<نَّ> ٱللَّهَ كَانَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> شَهِیدًا A<۝>D<٥٥>",
"إِN<نَّ> ٱللَّهَ وَمَلَW<ـٰۤ>ئِكَتَهُT<ۥ> یُصَلُّونَ عَلَى ٱX<ل>N<نَّ>بِیِّۚ یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> صَلُّوX<ا۟> عَلَیۡهِ وَسَلِّمُوX<ا۟> تَسۡلِیمًا A<۝>D<٥٦>",
"إِN<نَّ> ٱلَّذِینَ یُؤۡذُونَ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> لَعَنَهُمُ ٱللَّهُ فِی ٱX<ل>دُّنۡیَا وَX<ٱ>لۡءَاخِR<رَ>ةِ وَأَعَدَّ لَهُمۡ عَذَابX<ࣰ>ا N<مُّ>هِینࣰا A<۝>D<٥٧>",
"وَX<ٱ>لَّذِینَ یُؤۡذُونَ ٱلۡمُؤۡمِنِینَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تِ بِغَیۡرِ مَا ٱكۡتَسَبُوX<ا۟> فَقَدِ ٱحۡتَمَلُوX<ا۟> بُهۡتَT<ـٰ>نX<ࣰ>ا N<وَ>إِثۡمX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<٥٨>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ قُل لِّأَزۡوَT< ٰ>جِكَ وَبَنَاتِكَ وَنِسَW<اۤ>ءِ ٱلۡمُؤۡمِنِینَ یُQ<دۡ>نِینَ عَلَیۡهِN<نَّ> مِN<ن> جَلَT<ـٰ>بِیبِهِN<نَّ>ۚ ذَT< ٰ>لِكَ أَQ<دۡ>نَىW<ٰۤ> أَX<ن> N<یُ>عۡR<رَ>فۡنَ فَلَا یُؤۡذَیۡنَۗ وَكَانَ ٱللَّهُ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<٥٩>",
"۞ لَّئِX<ن> لَّمۡ یَN<ن>تَهِ ٱلۡمُنَT<ـٰ>فِقُونَ وَX<ٱ>لَّذِینَ فِی قُلُوبِهِم N<مَّ>R<رَ>ضX<ࣱ> N<وَ>X<ٱ>لۡمُR<رۡ>جِفُونَ فِی ٱلۡمَدِینَةِ لَنُغۡرِیَN<نَّ>كَ بِهِمۡ ثُN<مَّ> لَا یُجَاوِR<رُ>ونَكَ فِیهَW<اۤ> إِلَّا قَلِیلࣰا A<۝>D<٦٠>",
"مَّلۡعُونِینَۖ أَیۡنَمَا ثُقِفُW<وۤ>X<ا۟> أُخِذُوX<ا۟> وَقُتِّلُوX<ا۟> تَQ<قۡ>تِیلࣰا A<۝>D<٦١>",
"سُN<نَّ>ةَ ٱللَّهِ فِی ٱلَّذِینَ خَلَوۡX<ا۟> مِN<ن> قَQ<بۡ>لُۖ وَلَN<ن> تَجِدَ لِسُN<نَّ>ةِ ٱللَّهِ تَQ<بۡ>دِیلࣰا A<۝>D<٦٢>",
"یَسۡءَلُكَ ٱX<ل>N<نَّ>اسُ عَنِ ٱX<ل>سَّاعَةِۖ قُلۡ إِN<نَّ>مَا عِلۡمُهَا عِN<ن>دَ ٱللَّهِۚ وَمَا یُQ<دۡ>رِیكَ لَعَلَّ ٱX<ل>سَّاعَةَ تَكُونُ قَرِیبًا A<۝>D<٦٣>",
"إِN<نَّ> ٱللَّهَ لَعَنَ ٱلۡكَT<ـٰ>فِرِینَ وَأَعَدَّ لَهُمۡ سَعِیR<ر>ًا A<۝>D<٦٤>",
"خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدX<ࣰ>اۖ لَّا یَجِدُونَ وَلِیّX<ࣰ>ا N<وَ>لَا نَصِیR<ر>ࣰا A<۝>D<٦٥>",
"یَوۡمَ تُقَلَّبُ وُجُوهُهُمۡ فِی ٱX<ل>N<نَّ>ارِ یَقُولُونَ یَT<ـٰ>لَیۡتَنَW<اۤ> أَطَعۡنَا ٱللَّهَ وَأَطَعۡنَا ٱX<ل>R<رَّ>سُولَا۠ A<۝>D<٦٦>",
"وَقَالُوX<ا۟> R<رَ>بَّنَW<اۤ> إِN<نَّ>W<اۤ> أَطَعۡنَا سَادَتَنَا وَكُبَR<رَ>W<اۤ>ءَنَا فَأَضَلُّونَا ٱX<ل>سَّبِیلَا۠ A<۝>D<٦٧>",
"R<رَ>بَّنَW<اۤ> ءَاتِهِمۡ ضِعۡفَیۡنِ مِنَ ٱلۡعَذَابِ وَX<ٱ>لۡعَنۡهُمۡ لَعۡنN<ࣰ>ا كَبِیR<ر>ࣰا A<۝>D<٦٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَكُونُوX<ا۟> كَX<ٱ>لَّذِینَ ءَاذَوۡX<ا۟> مُوسَىٰ فَبَR<رَّ>أَهُ ٱللَّهُ مِN<مَّ>ا قَالُوX<ا۟>ۚ وَكَانَ عِN<ن>دَ ٱللَّهِ وَجِیهࣰا A<۝>D<٦٩>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱتَّقُوX<ا۟> ٱللَّهَ وَقُولُوX<ا۟> قَوۡلN<ࣰ>ا سَدِیدࣰا A<۝>D<٧٠>",
"یُصۡلِحۡ لَكُمۡ أَعۡمَT<ـٰ>لَكُمۡ وَیَغۡفِرۡ لَكُمۡ ذُنُوبَكُمۡۗ وَمَX<ن> N<یُ>طِعِ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> فَقَQ<دۡ> فَازَ فَوۡزًا عَظِیمًا A<۝>D<٧١>",
"إِN<نَّ>ا عَR<رَ>ضۡنَا ٱلۡأَمَانَةَ عَلَى ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَX<ٱ>لۡجِبَالِ فَأَبَیۡنَ أَX<ن> N<یَ>حۡمِلۡنَهَا وأَشۡفَQ<قۡ>نَ مِنۡهَا وَحَمَلَهَا ٱلۡإِN<ن>سَT<ـٰ>نُۖ إِN<نَّ>هُT<ۥ> كَانَ ظَلُومN<ࣰ>ا جَهُولࣰا A<۝>D<٧٢>",
"لِّیُعَذِّبَ ٱللَّهُ ٱلۡمُنَT<ـٰ>فِقِینَ وَX<ٱ>لۡمُنَT<ـٰ>فِقَT<ـٰ>تِ وَX<ٱ>لۡمُشۡرِكِینَ وَX<ٱ>لۡمُشۡرِكَT<ـٰ>تِ وَیَتُوبَ ٱللَّهُ عَلَى ٱلۡمُؤۡمِنِینَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تِۗ وَكَانَ ٱللَّهُ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمَۢا A<۝>D<٧٣>",
"#ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِی لَهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِ وَلَهُ ٱلۡحَمۡدُ فِی ٱلۡءَاخِR<رَ>ةِۚ وَهُوَ ٱلۡحَكِیمُ ٱلۡخَبِJ<ی>رُ A<۝>D<١>",
"یَعۡلَمُ مَا یَلِجُ فِی ٱلۡأَR<رۡ>ضِ وَمَا یَخۡR<رُ>جُ مِنۡهَا وَمَا یَN<ن>زِلُ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ وَمَا یَعۡR<رُ>جُ فِیهَاۚ وَهُوَ ٱX<ل>R<رَّ>حِیمُ ٱلۡغَفُJ<و>R<ر>ُ A<۝>D<٢>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَا تَأۡتِینَا ٱX<ل>سَّاعَةُۖ قُلۡ بَلَىٰ وَR<رَ>بِّی لَتَأۡتِیَN<نَّ>كُمۡ عَT<ـٰ>لِمِ ٱلۡغَیۡبِۖ لَا یَعۡزُبُ عَنۡهُ مِثۡقَالُ ذَR<رَّ>ةN<ࣲ> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَلَا فِی ٱلۡأَR<رۡ>ضِ وَلَW<اۤ> أَصۡغَR<رُ> مِN<ن> ذَT< ٰ>لِكَ وَلَW<اۤ> أَكۡبَR<رُ> إِلَّا فِی كِتَT<ـٰ>بX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٣>",
"لِّیَQ<جۡ>زِیَ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِۚ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<وَ>رِزۡقN<ࣱ> كَرِJ<ی>مࣱ A<۝>D<٤>",
"وَX<ٱ>لَّذِینَ سَعَوۡ فِW<یۤ> ءَایَT<ـٰ>تِنَا مُعَT<ـٰ>جِزِینَ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ عَذَابX<ࣱ> N<مِّ>X<ن> رِّQ<جۡ>زٍ أَلِJ<ی>مࣱ A<۝>D<٥>",
"وَیَR<رَ>ى ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡعِلۡمَ ٱلَّذِW<یۤ> أُN<ن>زِلَ إِلَیۡكَ مِX<ن> R<رَّ>بِّكَ هُوَ ٱلۡحَقَّ وَیَهۡدِW<یۤ> إِلَىٰ صِR<رَ>T< ٰ>طِ ٱلۡعَزِیزِ ٱلۡحَمِJ<ی>Q<د>ِ A<۝>D<٦>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> هَلۡ نَدُلُّكُمۡ عَلَىٰ R<رَ>جُلX<ࣲ> N<یُ>نَبِّئُكُمۡ إِذَا مُزِّQ<قۡ>تُمۡ كُلَّ مُمَزَّقٍ إِN<نَّ>كُمۡ لَفِی خَلۡقN<ࣲ> جَدِJ<ی>Q<د>ٍ A<۝>D<٧>",
"أَفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبًا أَN<م> بِهِT<ۦ> جِN<نَّ>ةُN<ۢ>ۗ بَلِ ٱلَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ فِی ٱلۡعَذَابِ وَX<ٱ>X<ل>ضَّلَT<ـٰ>لِ ٱلۡبَعِJ<ی>Q<د>ِ A<۝>D<٨>",
"أَفَلَمۡ یَR<رَ>وۡX<ا۟> إِلَىٰ مَا بَیۡنَ أَیۡدِیهِمۡ وَمَا خَلۡفَهُم N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِۚ إِن N<نَّ>شَأۡ نَخۡسِفۡ بِهِمُ ٱلۡأَR<رۡ>ضَ أَوۡ نُسۡقِQ<طۡ> عَلَیۡهِمۡ كِسَفX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَةX<ࣰ> لِّكُلِّ عَQ<بۡ>دX<ࣲ> N<مُّ>نِJ<ی>Q<ب>ࣲ A<۝>D<٩>",
"۞ وَلَقَQ<دۡ> ءَاتَیۡنَا دَاوُT<ۥ>دَ مِN<نَّ>ا فَضۡلX<ࣰ>اۖ N<یَ>T<ـٰ>جِبَالُ أَوِّبِی مَعَهُT<ۥ> وَX<ٱ>X<ل>طَّیۡR<رَ>ۖ وَأَلَN<نَّ>ا لَهُ ٱلۡحَدِJ<ی>Q<د>َ A<۝>D<١٠>",
"أَنِ ٱعۡمَلۡ سَT<ـٰ>بِغَT<ـٰ>تX<ࣲ> N<وَ>قَدِّرۡ فِی ٱX<ل>سَّR<رۡ>دِۖ وَX<ٱ>عۡمَلُوX<ا۟> صَT<ـٰ>لِحًاۖ إِN<نِّ>ی بِمَا تَعۡمَلُونَ بَصِJ<ی>رࣱ A<۝>D<١١>",
"وَلِسُلَیۡمَT<ـٰ>نَ ٱX<ل>رِّیحَ غُدُوُّهَا شَهۡR<ر>X<ࣱ> N<وَ>R<رَ>وَاحُهَا شَهۡR<ر>X<ࣱ>ۖ N<وَ>أَسَلۡنَا لَهُT<ۥ> عَیۡنَ ٱلۡقِQ<طۡ>رِۖ وَمِنَ ٱلۡجِN<نِّ> مَX<ن> N<یَ>عۡمَلُ بَیۡنَ یَدَیۡهِ بِإِذۡنِ R<رَ>بِّهِT<ۦ>ۖ وَمَX<ن> N<یَ>زِغۡ مِنۡهُمۡ عَنۡ أَمۡرِنَا نُذِQ<قۡ>هُ مِنۡ عَذَابِ ٱX<ل>سَّعِJ<ی>رِ A<۝>D<١٢>",
"یَعۡمَلُونَ لَهُT<ۥ> مَا یَشَW<اۤ>ءُ مِX<ن> N<مَّ>حَT<ـٰ>رِیبَ وَتَمَT<ـٰ>ثِیلَ وَجِفَانN<ࣲ> كَX<ٱ>لۡجَوَابِ وَقُدُورX<ࣲ> R<رَّ>اسِیَT<ـٰ>تٍۚ ٱعۡمَلُW<وۤ>X<ا۟> ءَالَ دَاوُT<ۥ>دَ شُكۡR<ر>X<ࣰ>اۚ N<وَ>قَلِیلX<ࣱ> N<مِّ>نۡ عِبَادِیَ ٱX<ل>شَّكُJ<و>R<ر>ُ A<۝>D<١٣>",
"فَلَN<مَّ>ا قَضَیۡنَا عَلَیۡهِ ٱلۡمَوۡتَ مَا دَلَّهُمۡ عَلَىٰ مَوۡتِهِW<ۦۤ> إِلَّا دَL<اۤ>بَّةُ ٱلۡأَR<رۡ>ضِ تَأۡكُلُ مِN<ن>سَأَتَهُT<ۥ>ۖ فَلَN<مَّ>ا خَR<رَّ> تَبَیَّنَتِ ٱلۡجِN<نُّ> أَX<ن> لَّوۡ كَانُوX<ا۟> یَعۡلَمُونَ ٱلۡغَیۡبَ مَا لَبِثُوX<ا۟> فِی ٱلۡعَذَابِ ٱلۡمُهِJ<ی>نِ A<۝>D<١٤>",
"لَقَQ<دۡ> كَانَ لِسَبَإN<ࣲ> فِی مَسۡكَنِهِمۡ ءَایَةN<ࣱ>ۖ جَN<نَّ>تَانِ عَX<ن> N<یَ>مِینX<ࣲ> N<وَ>شِمَالN<ࣲ>ۖ كُلُوX<ا۟> مِX<ن> رِّزۡقِ R<رَ>بِّكُمۡ وَX<ٱ>شۡكُR<رُ>وX<ا۟> لَهُT<ۥ>ۚ بَلۡدَةN<ࣱ> طَیِّبَةX<ࣱ> N<وَ>R<رَ>بٌّ غَفُJ<و>R<ر>ࣱ A<۝>D<١٥>",
"فَأَعۡR<رَ>ضُوX<ا۟> فَأَR<رۡ>سَلۡنَا عَلَیۡهِمۡ سَیۡلَ ٱلۡعَرِمِ وَبَدَّلۡنَT<ـٰ>هُN<م> بِجَN<نَّ>تَیۡهِمۡ جَN<نَّ>تَیۡنِ ذَوَاتَیۡ أُكُلٍ خَمۡطX<ࣲ> N<وَ>أَثۡلX<ࣲ> N<وَ>شَیۡءX<ࣲ> N<مِّ>N<ن> سِQ<دۡ>رN<ࣲ> قَلِJ<ی>لࣲ A<۝>D<١٦>",
"ذَT< ٰ>لِكَ جَزَیۡنَT<ـٰ>هُN<م> بِمَا كَفَR<رُ>وX<ا۟>ۖ وَهَلۡ نُجَT<ـٰ>زِW<یۤ> إِلَّا ٱلۡكَفُJ<و>R<ر>َ A<۝>D<١٧>",
"وَجَعَلۡنَا بَیۡنَهُمۡ وَبَیۡنَ ٱلۡقُR<رَ>ى ٱلَّتِی بَT<ـٰ>R<رَ>كۡنَا فِیهَا قُR<ر>N<ࣰ>ى ظَT<ـٰ>هِR<رَ>ةX<ࣰ> N<وَ>قَدَّR<رۡ>نَا فِیهَا ٱX<ل>سَّیۡR<رَ>ۖ سِیR<رُ>وX<ا۟> فِیهَا لَیَالِیَ وَأَیَّامًا ءَامِنِJ<ی>نَ A<۝>D<١٨>",
"فَقَالُوX<ا۟> R<رَ>بَّنَا بَT<ـٰ>عِQ<دۡ> بَیۡنَ أَسۡفَارِنَا وَظَلَمُW<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ فَجَعَلۡنَT<ـٰ>هُمۡ أَحَادِیثَ وَمَزَّQ<قۡ>نَT<ـٰ>هُمۡ كُلَّ مُمَزَّقٍۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّكُلِّ صَبَّارN<ࣲ> شَكُJ<و>R<ر>ࣲ A<۝>D<١٩>",
"وَلَقَQ<دۡ> صَدَّقَ عَلَیۡهِمۡ إِQ<بۡ>لِیسُ ظَN<نَّ>هُT<ۥ> فَX<ٱ>تَّبَعُوهُ إِلَّا فَرِیقX<ࣰ>ا N<مِّ>نَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٢٠>",
"وَمَا كَانَ لَهُT<ۥ> عَلَیۡهِم N<مِّ>N<ن> سُلۡطَT<ـٰ>نٍ إِلَّا لِنَعۡلَمَ مَX<ن> N<یُ>ؤۡمِنُ بِX<ٱ>لۡءَاخِR<رَ>ةِ مِN<مَّ>نۡ هُوَ مِنۡهَا فِی شَكّX<ࣲ>ۗ N<وَ>R<رَ>بُّكَ عَلَىٰ كُلِّ شَیۡءٍ حَفِJ<ی>ظࣱ A<۝>D<٢١>",
"قُلِ ٱQ<دۡ>عُوX<ا۟> ٱلَّذِینَ زَعَمۡتُم N<مِّ>N<ن> دُونِ ٱللَّهِ لَا یَمۡلِكُونَ مِثۡقَالَ ذَR<رَّ>ةN<ࣲ> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَلَا فِی ٱلۡأَR<رۡ>ضِ وَمَا لَهُمۡ فِیهِمَا مِN<ن> شِرۡكX<ࣲ> N<وَ>مَا لَهُT<ۥ> مِنۡهُم N<مِّ>N<ن> ظَهِJ<ی>رࣲ A<۝>D<٢٢>",
"وَلَا تَN<ن>فَعُ ٱX<ل>شَّفَT<ـٰ>عَةُ عِN<ن>دَهُW<ۥۤ> إِلَّا لِمَنۡ أَذِنَ لَهُT<ۥ>ۚ حَتَّىW<ٰۤ> إِذَا فُزِّعَ عَN<ن> قُلُوبِهِمۡ قَالُوX<ا۟> مَاذَا قَالَ R<رَ>بُّكُمۡۖ قَالُوX<ا۟> ٱلۡحَقَّۖ وَهُوَ ٱلۡعَلِیُّ ٱلۡكَبِJ<ی>رُ A<۝>D<٢٣>",
"۞ قُلۡ مَX<ن> N<یَ>R<رۡ>زُقُكُم N<مِّ>نَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ قُلِ ٱللَّهُۖ وَإِN<نَّ>W<اۤ> أَوۡ إِیَّاكُمۡ لَعَلَىٰ هُدًى أَوۡ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٢٤>",
"قُل لَّا تُسۡءَلُونَ عَN<مَّ>W<اۤ> أَQ<جۡ>R<رَ>مۡنَا وَلَا نُسۡءَلُ عَN<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<٢٥>",
"قُلۡ یَQ<جۡ>مَعُ بَیۡنَنَا R<رَ>بُّنَا ثُN<مَّ> یَفۡتَحُ بَیۡنَنَا بِX<ٱ>لۡحَقِّ وَهُوَ ٱلۡفَتَّاحُ ٱلۡعَلِJ<ی>مُ A<۝>D<٢٦>",
"قُلۡ أَR<رُ>ونِیَ ٱلَّذِینَ أَلۡحَQ<قۡ>تُN<م> بِهِT<ۦ> شُR<رَ>كَW<اۤ>ءَۖ كَلَّاۚ بَلۡ هُوَ ٱللَّهُ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٢٧>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ إِلَّا كَL<اۤ>فَّةX<ࣰ> لِّX<ل>N<نَّ>اسِ بَشِیR<ر>X<ࣰ>ا N<وَ>نَذِیR<ر>X<ࣰ>ا N<وَ>لَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٢٨>",
"وَیَقُولُونَ مَتَىٰ هَT<ـٰ>ذَا ٱلۡوَعۡدُ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٢٩>",
"قُل لَّكُم N<مِّ>یعَادُ یَوۡمX<ࣲ> لَّا تَسۡتَءۡخِR<رُ>ونَ عَنۡهُ سَاعَةX<ࣰ> N<وَ>لَا تَسۡتَQ<قۡ>دِمُJ<و>نَ A<۝>D<٣٠>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَن N<نُّ>ؤۡمِنَ بِهَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانِ وَلَا بِX<ٱ>لَّذِی بَیۡنَ یَدَیۡهِۗ وَلَوۡ تَR<رَ>ىW<ٰۤ> إِذِ ٱX<ل>ظَّT<ـٰ>لِمُونَ مَوۡقُوفُونَ عِN<ن>دَ R<رَ>بِّهِمۡ یَR<رۡ>جِعُ بَعۡضُهُمۡ إِلَىٰ بَعۡضٍ ٱلۡقَوۡلَ یَقُولُ ٱلَّذِینَ ٱسۡتُضۡعِفُوX<ا۟> لِلَّذِینَ ٱسۡتَكۡبَR<رُ>وX<ا۟> لَوۡلَW<اۤ> أَN<ن>تُمۡ لَكُN<نَّ>ا مُؤۡمِنِJ<ی>نَ A<۝>D<٣١>",
"قَالَ ٱلَّذِینَ ٱسۡتَكۡبَR<رُ>وX<ا۟> لِلَّذِینَ ٱسۡتُضۡعِفُW<وۤ>X<ا۟> أَنَحۡنُ صَدَQ<دۡ>نَT<ـٰ>كُمۡ عَنِ ٱلۡهُدَىٰ بَعۡدَ إِذۡ جَW<اۤ>ءَكُN<م>ۖ بَلۡ كُN<ن>تُم N<مُّ>Q<جۡ>رِمِJ<ی>نَ A<۝>D<٣٢>",
"وَقَالَ ٱلَّذِینَ ٱسۡتُضۡعِفُوX<ا۟> لِلَّذِینَ ٱسۡتَكۡبَR<رُ>وX<ا۟> بَلۡ مَكۡR<رُ> ٱلَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِ إِذۡ تَأۡمُR<رُ>ونَنَW<اۤ> أَن N<نَّ>كۡفُR<رَ> بِٱللَّهِ وَنَQ<جۡ>عَلَ لَهُW<ۥۤ> أَN<ن>دَادX<ࣰ>اۚ N<وَ>أَسَR<رُّ>وX<ا۟> ٱX<ل>N<نَّ>دَامَةَ لَN<مَّ>ا R<رَ>أَوُX<ا۟> ٱلۡعَذَابَۚ وَجَعَلۡنَا ٱلۡأَغۡلَT<ـٰ>لَ فِW<یۤ> أَعۡنَاقِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟>ۖ هَلۡ یُQ<جۡ>زَوۡنَ إِلَّا مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٣٣>",
"وَمَW<اۤ> أَR<رۡ>سَلۡنَا فِی قَR<رۡ>یَةX<ࣲ> N<مِّ>ن N<نَّ>ذِیرٍ إِلَّا قَالَ مُتۡR<رَ>فُوهَW<اۤ> إِN<نَّ>ا بِمَW<اۤ> أُR<رۡ>سِلۡتُN<م> بِهِT<ۦ> كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٣٤>",
"وَقَالُوX<ا۟> نَحۡنُ أَكۡثَR<رُ> أَمۡوَT< ٰ>لX<ࣰ>ا N<وَ>أَوۡلَT<ـٰ>دX<ࣰ>ا N<وَ>مَا نَحۡنُ بِمُعَذَّبِJ<ی>نَ A<۝>D<٣٥>",
"قُلۡ إِN<نَّ> R<رَ>بِّی یَQ<بۡ>سُطُ ٱX<ل>رِّزۡقَ لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیَQ<قۡ>دِR<رُ> وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٣٦>",
"وَمَW<اۤ> أَمۡوَT< ٰ>لُكُمۡ وَلَW<اۤ> أَوۡلَT<ـٰ>دُكُN<م> بِX<ٱ>لَّتِی تُقَرِّبُكُمۡ عِN<ن>دَنَا زُلۡفَىW<ٰۤ> إِلَّا مَنۡ ءَامَنَ وَعَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَأُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ جَزَW<اۤ>ءُ ٱX<ل>ضِّعۡفِ بِمَا عَمِلُوX<ا۟> وَهُمۡ فِی ٱلۡغُR<رُ>فَT<ـٰ>تِ ءَامِنُJ<و>نَ A<۝>D<٣٧>",
"وَX<ٱ>لَّذِینَ یَسۡعَوۡنَ فِW<یۤ> ءَایَT<ـٰ>تِنَا مُعَT<ـٰ>جِزِینَ أُX<و۟>لَW<ـٰۤ>ئِكَ فِی ٱلۡعَذَابِ مُحۡضَR<رُ>J<و>نَ A<۝>D<٣٨>",
"قُلۡ إِN<نَّ> R<رَ>بِّی یَQ<بۡ>سُطُ ٱX<ل>رِّزۡقَ لِمَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِT<ۦ> وَیَQ<قۡ>دِR<رُ> لَهُT<ۥ>ۚ وَمَW<اۤ> أَN<ن>فَQ<قۡ>تُم N<مِّ>N<ن> شَیۡءN<ࣲ> فَهُوَ یُخۡلِفُهُT<ۥ>ۖ وَهُوَ خَیۡR<رُ> ٱX<ل>R<رَّ>T< ٰ>زِقِJ<ی>نَ A<۝>D<٣٩>",
"وَیَوۡمَ یَحۡشُR<رُ>هُمۡ جَمِیعN<ࣰ>ا ثُN<مَّ> یَقُولُ لِلۡمَلَW<ـٰۤ>ئِكَةِ أَهَW<ـٰۤ>ؤُلَW<اۤ>ءِ إِیَّاكُمۡ كَانُوX<ا۟> یَعۡبُدُJ<و>نَ A<۝>D<٤٠>",
"قَالُوX<ا۟> سُQ<بۡ>حَT<ـٰ>نَكَ أَN<ن>تَ وَلِیُّنَا مِN<ن> دُونِهِN<م>ۖ بَلۡ كَانُوX<ا۟> یَعۡبُدُونَ ٱلۡجِN<نَّ>ۖ أَكۡثَR<رُ>هُN<م> بِهِم N<مُّ>ؤۡمِنُJ<و>نَ A<۝>D<٤١>",
"فَX<ٱ>لۡیَوۡمَ لَا یَمۡلِكُ بَعۡضُكُمۡ لِبَعۡضX<ࣲ> N<نَّ>فۡعX<ࣰ>ا N<وَ>لَا ضَR<رّ>X<ࣰ>ا N<وَ>نَقُولُ لِلَّذِینَ ظَلَمُوX<ا۟> ذُوقُوX<ا۟> عَذَابَ ٱX<ل>N<نَّ>ارِ ٱلَّتِی كُN<ن>تُN<م> بِهَا تُكَذِّبُJ<و>نَ A<۝>D<٤٢>",
"وَإِذَا تُتۡلَىٰ عَلَیۡهِمۡ ءَایَT<ـٰ>تُنَا بَیِّنَT<ـٰ>تN<ࣲ> قَالُوX<ا۟> مَا هَT<ـٰ>ذَW<اۤ> إِلَّا R<رَ>جُلX<ࣱ> N<یُ>رِیدُ أَX<ن> N<یَ>صُدَّكُمۡ عَN<مَّ>ا كَانَ یَعۡبُدُ ءَابَW<اۤ>ؤُكُمۡ وَقَالُوX<ا۟> مَا هَT<ـٰ>ذَW<اۤ> إِلَّW<اۤ> إِفۡكX<ࣱ> N<مُّ>فۡتَR<ر>X<ࣰ>ىۚ N<وَ>قَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لِلۡحَقِّ لَN<مَّ>ا جَW<اۤ>ءَهُمۡ إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا سِحۡR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٤٣>",
"وَمَW<اۤ> ءَاتَیۡنَT<ـٰ>هُم N<مِّ>N<ن> كُتُبX<ࣲ> N<یَ>Q<دۡ>R<رُ>سُونَهَاۖ وَمَW<اۤ> أَR<رۡ>سَلۡنَW<اۤ> إِلَیۡهِمۡ قَQ<بۡ>لَكَ مِن N<نَّ>ذِJ<ی>رࣲ A<۝>D<٤٤>",
"وَكَذَّبَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ وَمَا بَلَغُوX<ا۟> مِعۡشَاR<رَ> مَW<اۤ> ءَاتَیۡنَT<ـٰ>هُمۡ فَكَذَّبُوX<ا۟> R<رُ>سُلِیۖ فَكَیۡفَ كَانَ نَكِJ<ی>رِ A<۝>D<٤٥>",
"۞ قُلۡ إِN<نَّ>مَW<اۤ> أَعِظُكُN<م> بِوَT< ٰ>حِدَةٍۖ أَN<ن> تَقُومُوX<ا۟> لِلَّهِ مَثۡنَىٰ وَفُR<رَ>T< ٰ>دَىٰ ثُN<مَّ> تَتَفَكَّR<رُ>وX<ا۟>ۚ مَا بِصَاحِبِكُم N<مِّ>N<ن> جِN<نَّ>ةٍۚ إِنۡ هُوَ إِلَّا نَذِیR<ر>X<ࣱ> لَّكُN<م> بَیۡنَ یَدَیۡ عَذَابN<ࣲ> شَدِJ<ی>Q<د>ࣲ A<۝>D<٤٦>",
"قُلۡ مَا سَأَلۡتُكُم N<مِّ>نۡ أَQ<جۡ>رN<ࣲ> فَهُوَ لَكُمۡۖ إِنۡ أَQ<جۡ>رِیَ إِلَّا عَلَى ٱللَّهِۖ وَهُوَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> شَهِJ<ی>Q<د>ࣱ A<۝>D<٤٧>",
"قُلۡ إِN<نَّ> R<رَ>بِّی یَQ<قۡ>ذِفُ بِX<ٱ>لۡحَقِّ عَلَّT<ـٰ>مُ ٱلۡغُیُJ<و>Q<ب>ِ A<۝>D<٤٨>",
"قُلۡ جَW<اۤ>ءَ ٱلۡحَقُّ وَمَا یُQ<بۡ>دِئُ ٱلۡبَT<ـٰ>طِلُ وَمَا یُعِJ<ی>Q<د>ُ A<۝>D<٤٩>",
"قُلۡ إِN<ن> ضَلَلۡتُ فَإِN<نَّ>مَW<اۤ> أَضِلُّ عَلَىٰ نَفۡسِیۖ وَإِنِ ٱهۡتَدَیۡتُ فَبِمَا یُوحِW<یۤ> إِلَیَّ R<رَ>بِّW<یۤ>ۚ إِN<نَّ>هُT<ۥ> سَمِیعN<ࣱ> قَرِJ<ی>Q<ب>ࣱ A<۝>D<٥٠>",
"وَلَوۡ تَR<رَ>ىW<ٰۤ> إِذۡ فَزِعُوX<ا۟> فَلَا فَوۡتَ وَأُخِذُوX<ا۟> مِX<ن> N<مَّ>كَانN<ࣲ> قَرِJ<ی>Q<ب>ࣲ A<۝>D<٥١>",
"وَقَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا بِهِT<ۦ> وَأَN<نَّ>ىٰ لَهُمُ ٱX<ل>تَّنَاوُشُ مِX<ن> N<مَّ>كَانِN<ۭ> بَعِJ<ی>Q<د>ࣲ A<۝>D<٥٢>",
"وَقَQ<دۡ> كَفَR<رُ>وX<ا۟> بِهِT<ۦ> مِN<ن> قَQ<بۡ>لُۖ وَیَQ<قۡ>ذِفُونَ بِX<ٱ>لۡغَیۡبِ مِX<ن> N<مَّ>كَانِN<ۭ> بَعِJ<ی>Q<د>ࣲ A<۝>D<٥٣>",
"وَحِیلَ بَیۡنَهُمۡ وَبَیۡنَ مَا یَشۡتَهُونَ كَمَا فُعِلَ بِأَشۡیَاعِهِم N<مِّ>N<ن> قَQ<بۡ>لُۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> فِی شَكّX<ࣲ> N<مُّ>رِJ<ی>Q<ب>ِۭ A<۝>D<٥٤>",
"#ٱلۡحَمۡدُ لِلَّهِ فَاطِرِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ جَاعِلِ ٱلۡمَلَW<ـٰۤ>ئِكَةِ R<رُ>سُلًا أُX<و۟>لِW<یۤ> أَQ<جۡ>نِحَةX<ࣲ> N<مَّ>ثۡنَىٰ وَثُلَT<ـٰ>ثَ وَR<رُ>بَT<ـٰ>عَۚ یَزِیدُ فِی ٱلۡخَلۡقِ مَا یَشَW<اۤ>ءُۚ إِN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<١>",
"مَّا یَفۡتَحِ ٱللَّهُ لِX<ل>N<نَّ>اسِ مِX<ن> R<رَّ>حۡمَةN<ࣲ> فَلَا مُمۡسِكَ لَهَاۖ وَمَا یُمۡسِكۡ فَلَا مُR<رۡ>سِلَ لَهُT<ۥ> مِX<ن>N<ۢ> بَعۡدِهِT<ۦ>ۚ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٢>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ ٱذۡكُR<رُ>وX<ا۟> نِعۡمَتَ ٱللَّهِ عَلَیۡكُمۡۚ هَلۡ مِنۡ خَT<ـٰ>لِقٍ غَیۡR<رُ> ٱللَّهِ یَR<رۡ>زُقُكُم N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِۚ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ فَأَN<نَّ>ىٰ تُؤۡفَكُJ<و>نَ A<۝>D<٣>",
"وَإِX<ن> N<یُ>كَذِّبُوكَ فَقَQ<دۡ> كُذِّبَتۡ R<رُ>سُلX<ࣱ> N<مِّ>N<ن> قَQ<بۡ>لِكَۚ وَإِلَى ٱللَّهِ تُR<رۡ>جَعُ ٱلۡأُمُJ<و>R<ر>ُ A<۝>D<٤>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ إِN<نَّ> وَعۡدَ ٱللَّهِ حَقّN<ࣱ>ۖ فَلَا تَغُR<رَّ>N<نَّ>كُمُ ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَا وَلَا یَغُR<رَّ>N<نَّ>كُN<م> بِٱللَّهِ ٱلۡغَR<رُ>J<و>R<ر>ُ A<۝>D<٥>",
"إِN<نَّ> ٱX<ل>شَّیۡطَT<ـٰ>نَ لَكُمۡ عَدُوّN<ࣱ> فَX<ٱ>تَّخِذُوهُ عَدُوًّاۚ إِN<نَّ>مَا یَQ<دۡ>عُوX<ا۟> حِزۡبَهُT<ۥ> لِیَكُونُوX<ا۟> مِنۡ أَصۡحَT<ـٰ>بِ ٱX<ل>سَّعِJ<ی>رِ A<۝>D<٦>",
"ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَهُمۡ عَذَابN<ࣱ> شَدِیدX<ࣱ>ۖ N<وَ>X<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<وَ>أَQ<جۡ>R<ر>N<ࣱ> كَبِJ<ی>رٌ A<۝>D<٧>",
"أَفَمَN<ن> زُیِّنَ لَهُT<ۥ> سُW<وۤ>ءُ عَمَلِهِT<ۦ> فَR<رَ>ءَاهُ حَسَنN<ࣰ>اۖ فَإِN<نَّ> ٱللَّهَ یُضِلُّ مَX<ن> N<یَ>شَW<اۤ>ءُ وَیَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُۖ فَلَا تَذۡهَQ<بۡ> نَفۡسُكَ عَلَیۡهِمۡ حَسَR<رَ>T< ٰ>تٍۚ إِN<نَّ> ٱللَّهَ عَلِیمُN<ۢ> بِمَا یَصۡنَعُJ<و>نَ A<۝>D<٨>",
"وَٱللَّهُ ٱلَّذِW<یۤ> أَR<رۡ>سَلَ ٱX<ل>رِّیَT<ـٰ>حَ فَتُثِیR<رُ> سَحَابN<ࣰ>ا فَسُQ<قۡ>نَT<ـٰ>هُ إِلَىٰ بَلَدX<ࣲ> N<مَّ>یِّتN<ࣲ> فَأَحۡیَیۡنَا بِهِ ٱلۡأَR<رۡ>ضَ بَعۡدَ مَوۡتِهَاۚ كَذَT< ٰ>لِكَ ٱX<ل>N<نُّ>شُJ<و>R<ر>ُ A<۝>D<٩>",
"مَN<ن> كَانَ یُرِیدُ ٱلۡعِزَّةَ فَلِلَّهِ ٱلۡعِزَّةُ جَمِیعًاۚ إِلَیۡهِ یَصۡعَدُ ٱلۡكَلِمُ ٱX<ل>طَّیِّبُ وَX<ٱ>لۡعَمَلُ ٱX<ل>صَّT<ـٰ>لِحُ یَR<رۡ>فَعُهُT<ۥ>ۚ وَX<ٱ>لَّذِینَ یَمۡكُR<رُ>ونَ ٱX<ل>سَّیِّءَاتِ لَهُمۡ عَذَابN<ࣱ> شَدِیدX<ࣱ>ۖ N<وَ>مَكۡR<رُ> أُX<و۟>لَW<ـٰۤ>ئِكَ هُوَ یَبُJ<و>R<ر>ُ A<۝>D<١٠>",
"وَٱللَّهُ خَلَقَكُم N<مِّ>N<ن> تُR<رَ>ابN<ࣲ> ثُN<مَّ> مِن N<نُّ>Q<طۡ>فَةN<ࣲ> ثُN<مَّ> جَعَلَكُمۡ أَزۡوَT< ٰ>جX<ࣰ>اۚ N<وَ>مَا تَحۡمِلُ مِنۡ أُN<ن>ثَىٰ وَلَا تَضَعُ إِلَّا بِعِلۡمِهِT<ۦ>ۚ وَمَا یُعَN<مَّ>R<رُ> مِX<ن> N<مُّ>عَN<مَّ>رX<ࣲ> N<وَ>لَا یُN<ن>قَصُ مِنۡ عُمُرِهِW<ۦۤ> إِلَّا فِی كِتَT<ـٰ>بٍۚ إِN<نَّ> ذَT< ٰ>لِكَ عَلَى ٱللَّهِ یَسِJ<ی>رࣱ A<۝>D<١١>",
"وَمَا یَسۡتَوِی ٱلۡبَحۡR<رَ>انِ هَT<ـٰ>ذَا عَذۡبN<ࣱ> فُR<رَ>اتN<ࣱ> سَW<اۤ>ئِغN<ࣱ> شَR<رَ>ابُهُT<ۥ> وَهَT<ـٰ>ذَا مِلۡحٌ أُجَاجX<ࣱ>ۖ N<وَ>مِN<ن> كُلّN<ࣲ> تَأۡكُلُونَ لَحۡمN<ࣰ>ا طَرِیّX<ࣰ>ا N<وَ>تَسۡتَخۡرِجُونَ حِلۡیَةN<ࣰ> تَلۡبَسُونَهَاۖ وَتَR<رَ>ى ٱلۡفُلۡكَ فِیهِ مَوَاخِR<رَ> لِتَQ<بۡ>تَغُوX<ا۟> مِN<ن> فَضۡلِهِT<ۦ> وَلَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<١٢>",
"یُولِجُ ٱلَّیۡلَ فِی ٱX<ل>N<نَّ>هَارِ وَیُولِجُ ٱX<ل>N<نَّ>هَاR<رَ> فِی ٱلَّیۡلِ وَسَخَّR<رَ> ٱX<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ>ۖ كُلّX<ࣱ> N<یَ>Q<جۡ>رِی لِأَجَلX<ࣲ> N<مُّ>سَN<مّ>N<ࣰ>ىۚ ذَT< ٰ>لِكُمُ ٱللَّهُ R<رَ>بُّكُمۡ لَهُ ٱلۡمُلۡكُۚ وَX<ٱ>لَّذِینَ تَQ<دۡ>عُونَ مِN<ن> دُونِهِT<ۦ> مَا یَمۡلِكُونَ مِN<ن> قِQ<طۡ>مِJ<ی>رٍ A<۝>D<١٣>",
"إِN<ن> تَQ<دۡ>عُوهُمۡ لَا یَسۡمَعُوX<ا۟> دُعَW<اۤ>ءَكُمۡ وَلَوۡ سَمِعُوX<ا۟> مَا ٱسۡتَجَابُوX<ا۟> لَكُمۡۖ وَیَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ یَكۡفُR<رُ>ونَ بِشِرۡكِكُمۡۚ وَلَا یُنَبِّئُكَ مِثۡلُ خَبِJ<ی>رࣲ A<۝>D<١٤>",
"۞ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ أَN<ن>تُمُ ٱلۡفُقَR<رَ>W<اۤ>ءُ إِلَى ٱللَّهِۖ وَٱللَّهُ هُوَ ٱلۡغَنِیُّ ٱلۡحَمِJ<ی>Q<د>ُ A<۝>D<١٥>",
"إِX<ن> N<یَ>شَأۡ یُذۡهِQ<بۡ>كُمۡ وَیَأۡتِ بِخَلۡقN<ࣲ> جَدِJ<ی>Q<د>ࣲ A<۝>D<١٦>",
"وَمَا ذَT< ٰ>لِكَ عَلَى ٱللَّهِ بِعَزِJ<ی>زࣲ A<۝>D<١٧>",
"وَلَا تَزِR<رُ> وَازِR<رَ>ةX<ࣱ> N<وِ>زۡR<رَ> أُخۡR<رَ>ىٰۚ وَإِN<ن> تَQ<دۡ>عُ مُثۡقَلَةٌ إِلَىٰ حِمۡلِهَا لَا یُحۡمَلۡ مِنۡهُ شَیۡءX<ࣱ> N<وَ>لَوۡ كَانَ ذَا قُR<رۡ>بَىW<ٰۤ>ۗ إِN<نَّ>مَا تُN<ن>ذِR<رُ> ٱلَّذِینَ یَخۡشَوۡنَ R<رَ>بَّهُN<م> بِX<ٱ>لۡغَیۡبِ وَأَقَامُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَۚ وَمَN<ن> تَزَكَّىٰ فَإِN<نَّ>مَا یَتَزَكَّىٰ لِنَفۡسِهِT<ۦ>ۚ وَإِلَى ٱللَّهِ ٱلۡمَصِJ<ی>رُ A<۝>D<١٨>",
"وَمَا یَسۡتَوِی ٱلۡأَعۡمَىٰ وَX<ٱ>لۡبَصِJ<ی>رُ A<۝>D<١٩>",
"وَلَا ٱX<ل>ظُّلُمَT<ـٰ>تُ وَلَا ٱX<ل>N<نُّ>J<و>R<ر>ُ A<۝>D<٢٠>",
"وَلَا ٱX<ل>ظِّلُّ وَلَا ٱلۡحَR<رُ>J<و>R<ر>ُ A<۝>D<٢١>",
"وَمَا یَسۡتَوِی ٱلۡأَحۡیَW<اۤ>ءُ وَلَا ٱلۡأَمۡوَT< ٰ>تُۚ إِN<نَّ> ٱللَّهَ یُسۡمِعُ مَX<ن> N<یَ>شَW<اۤ>ءُۖ وَمَW<اۤ> أَN<ن>تَ بِمُسۡمِعX<ࣲ> N<مَّ>N<ن> فِی ٱلۡقُبُJ<و>R<ر>ِ A<۝>D<٢٢>",
"إِنۡ أَN<ن>تَ إِلَّا نَذِJ<ی>رٌ A<۝>D<٢٣>",
"إِN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ بِX<ٱ>لۡحَقِّ بَشِیR<ر>X<ࣰ>ا N<وَ>نَذِیR<ر>X<ࣰ>اۚ N<وَ>إِX<ن> N<مِّ>نۡ أُN<مَّ>ةٍ إِلَّا خَلَا فِیهَا نَذِJ<ی>رࣱ A<۝>D<٢٤>",
"وَإِX<ن> N<یُ>كَذِّبُوكَ فَقَQ<دۡ> كَذَّبَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ جَW<اۤ>ءَتۡهُمۡ R<رُ>سُلُهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ وَبِX<ٱ>X<ل>زُّبُرِ وَبِX<ٱ>لۡكِتَT<ـٰ>بِ ٱلۡمُنِJ<ی>رِ A<۝>D<٢٥>",
"ثُN<مَّ> أَخَذۡتُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟>ۖ فَكَیۡفَ كَانَ نَكِJ<ی>رِ A<۝>D<٢٦>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ أَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَأَخۡR<رَ>Q<جۡ>نَا بِهِT<ۦ> ثَمَR<رَ>T< ٰ>تX<ࣲ> N<مُّ>خۡتَلِفًا أَلۡوَT< ٰ>نُهَاۚ وَمِنَ ٱلۡجِبَالِ جُدَدُN<ۢ> بِیضX<ࣱ> N<وَ>حُمۡR<ر>X<ࣱ> N<مُّ>خۡتَلِفٌ أَلۡوَT< ٰ>نُهَا وَغَR<رَ>ابِیبُ سُJ<و>Q<د>ࣱ A<۝>D<٢٧>",
"وَمِنَ ٱX<ل>N<نَّ>اسِ وَX<ٱ>X<ل>دَّوَL<اۤ>بِّ وَX<ٱ>لۡأَنۡعَT<ـٰ>مِ مُخۡتَلِفٌ أَلۡوَT< ٰ>نُهُT<ۥ> كَذَT< ٰ>لِكَۗ إِN<نَّ>مَا یَخۡشَى ٱللَّهَ مِنۡ عِبَادِهِ ٱلۡعُلَمَW<ـٰۤ>ؤُX<ا۟>ۗ إِN<نَّ> ٱللَّهَ عَزِیزٌ غَفُJ<و>R<ر>ٌ A<۝>D<٢٨>",
"إِN<نَّ> ٱلَّذِینَ یَتۡلُونَ كِتَT<ـٰ>بَ ٱللَّهِ وَأَقَامُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَأَN<ن>فَقُوX<ا۟> مِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡ سِR<رّ>X<ࣰ>ا N<وَ>عَلَانِیَةX<ࣰ> N<یَ>R<رۡ>جُونَ تِجَT<ـٰ>R<رَ>ةX<ࣰ> لَّN<ن> تَبُJ<و>R<ر>َ A<۝>D<٢٩>",
"لِیُوَفِّیَهُمۡ أُجُوR<رَ>هُمۡ وَیَزِیدَهُم N<مِّ>N<ن> فَضۡلِهِW<ۦۤ>ۚ إِN<نَّ>هُT<ۥ> غَفُوR<ر>N<ࣱ> شَكُJ<و>R<ر>ࣱ A<۝>D<٣٠>",
"وَX<ٱ>لَّذِW<یۤ> أَوۡحَیۡنَW<اۤ> إِلَیۡكَ مِنَ ٱلۡكِتَT<ـٰ>بِ هُوَ ٱلۡحَقُّ مُصَدِّقX<ࣰ>ا لِّمَا بَیۡنَ یَدَیۡهِۗ إِN<نَّ> ٱللَّهَ بِعِبَادِهِT<ۦ> لَخَبِیR<رُ>N<ۢ> بَصِJ<ی>رࣱ A<۝>D<٣١>",
"ثُN<مَّ> أَوۡR<رَ>ثۡنَا ٱلۡكِتَT<ـٰ>بَ ٱلَّذِینَ ٱصۡطَفَیۡنَا مِنۡ عِبَادِنَاۖ فَمِنۡهُمۡ ظَالِمX<ࣱ> لِّنَفۡسِهِT<ۦ> وَمِنۡهُم N<مُّ>Q<قۡ>تَصِدX<ࣱ> N<وَ>مِنۡهُمۡ سَابِقُN<ۢ> بِX<ٱ>لۡخَیۡR<رَ>T< ٰ>تِ بِإِذۡنِ ٱللَّهِۚ ذَT< ٰ>لِكَ هُوَ ٱلۡفَضۡلُ ٱلۡكَبِJ<ی>رُ A<۝>D<٣٢>",
"جَN<نَّ>T<ـٰ>تُ عَQ<دۡ>نX<ࣲ> N<یَ>Q<دۡ>خُلُونَهَا یُحَلَّوۡنَ فِیهَا مِنۡ أَسَاوِR<رَ> مِN<ن> ذَهَبX<ࣲ> N<وَ>لُؤۡلُؤX<ࣰ>اۖ N<وَ>لِبَاسُهُمۡ فِیهَا حَرِJ<ی>رࣱ A<۝>D<٣٣>",
"وَقَالُوX<ا۟> ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِW<یۤ> أَذۡهَبَ عَN<نَّ>ا ٱلۡحَزَنَۖ إِN<نَّ> R<رَ>بَّنَا لَغَفُوR<ر>N<ࣱ> شَكُJ<و>R<ر>ٌ A<۝>D<٣٤>",
"ٱلَّذِW<یۤ> أَحَلَّنَا دَاR<رَ> ٱلۡمُقَامَةِ مِN<ن> فَضۡلِهِT<ۦ> لَا یَمَسُّنَا فِیهَا نَصَبX<ࣱ> N<وَ>لَا یَمَسُّنَا فِیهَا لُغُJ<و>Q<ب>ࣱ A<۝>D<٣٥>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> لَهُمۡ نَاR<رُ> جَهَN<نَّ>مَ لَا یُQ<قۡ>ضَىٰ عَلَیۡهِمۡ فَیَمُوتُوX<ا۟> وَلَا یُخَفَّفُ عَنۡهُم N<مِّ>نۡ عَذَابِهَاۚ كَذَT< ٰ>لِكَ نَQ<جۡ>زِی كُلَّ كَفُJ<و>R<ر>ࣲ A<۝>D<٣٦>",
"وَهُمۡ یَصۡطَرِخُونَ فِیهَا R<رَ>بَّنَW<اۤ> أَخۡرِQ<جۡ>نَا نَعۡمَلۡ صَT<ـٰ>لِحًا غَیۡR<رَ> ٱلَّذِی كُN<نَّ>ا نَعۡمَلُۚ أَوَ لَمۡ نُعَN<مِّ>رۡكُم N<مَّ>ا یَتَذَكَّR<رُ> فِیهِ مَN<ن> تَذَكَّR<رَ> وَجَW<اۤ>ءَكُمُ ٱX<ل>N<نَّ>ذِیR<رُ>ۖ فَذُوقُوX<ا۟> فَمَا لِX<ل>ظَّT<ـٰ>لِمِینَ مِن N<نَّ>صِJ<ی>رٍ A<۝>D<٣٧>",
"إِN<نَّ> ٱللَّهَ عَT<ـٰ>لِمُ غَیۡبِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ إِN<نَّ>هُT<ۥ> عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٣٨>",
"هُوَ ٱلَّذِی جَعَلَكُمۡ خَلَW<ـٰۤ>ئِفَ فِی ٱلۡأَR<رۡ>ضِۚ فَمَN<ن> كَفَR<رَ> فَعَلَیۡهِ كُفۡR<رُ>هُT<ۥ>ۖ وَلَا یَزِیدُ ٱلۡكَT<ـٰ>فِرِینَ كُفۡR<رُ>هُمۡ عِN<ن>دَ R<رَ>بِّهِمۡ إِلَّا مَQ<قۡ>تX<ࣰ>اۖ N<وَ>لَا یَزِیدُ ٱلۡكَT<ـٰ>فِرِینَ كُفۡR<رُ>هُمۡ إِلَّا خَسَاR<ر>ࣰا A<۝>D<٣٩>",
"قُلۡ أَR<رَ>ءَیۡتُمۡ شُR<رَ>كَW<اۤ>ءَكُمُ ٱلَّذِینَ تَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ أَR<رُ>ونِی مَاذَا خَلَقُوX<ا۟> مِنَ ٱلۡأَR<رۡ>ضِ أَمۡ لَهُمۡ شِرۡكN<ࣱ> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ أَمۡ ءَاتَیۡنَT<ـٰ>هُمۡ كِتَT<ـٰ>بN<ࣰ>ا فَهُمۡ عَلَىٰ بَیِّنَتX<ࣲ> N<مِّ>نۡهُۚ بَلۡ إِX<ن> N<یَ>عِدُ ٱX<ل>ظَّT<ـٰ>لِمُونَ بَعۡضُهُN<م> بَعۡضًا إِلَّا غُR<رُ>وR<ر>ًا A<۝>D<٤٠>",
"۞ إِN<نَّ> ٱللَّهَ یُمۡسِكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ أَN<ن> تَزُولَاۚ وَلَئِN<ن> زَالَتَW<اۤ> إِنۡ أَمۡسَكَهُمَا مِنۡ أَحَدX<ࣲ> N<مِّ>X<ن>N<ۢ> بَعۡدِهِW<ۦۤ>ۚ إِN<نَّ>هُT<ۥ> كَانَ حَلِیمًا غَفُوR<ر>ࣰا A<۝>D<٤١>",
"وَأَQ<قۡ>سَمُوX<ا۟> بِٱللَّهِ جَهۡدَ أَیۡمَT<ـٰ>نِهِمۡ لَئِN<ن> جَW<اۤ>ءَهُمۡ نَذِیR<ر>X<ࣱ> لَّیَكُونُN<نَّ> أَهۡدَىٰ مِنۡ إِحۡدَى ٱلۡأُمَمِۖ فَلَN<مَّ>ا جَW<اۤ>ءَهُمۡ نَذِیR<ر>X<ࣱ> N<مَّ>ا زَادَهُمۡ إِلَّا نُفُوR<ر>ًا A<۝>D<٤٢>",
"ٱسۡتِكۡبَاR<ر>N<ࣰ>ا فِی ٱلۡأَR<رۡ>ضِ وَمَكۡR<رَ> ٱX<ل>سَّیِّئِۚ وَلَا یَحِیقُ ٱلۡمَكۡR<رُ> ٱX<ل>سَّیِّئُ إِلَّا بِأَهۡلِهِT<ۦ>ۚ فَهَلۡ یَN<ن>ظُR<رُ>ونَ إِلَّا سُN<نَّ>تَ ٱلۡأَوَّلِینَۚ فَلَN<ن> تَجِدَ لِسُN<نَّ>تِ ٱللَّهِ تَQ<بۡ>دِیلX<ࣰ>اۖ N<وَ>لَN<ن> تَجِدَ لِسُN<نَّ>تِ ٱللَّهِ تَحۡوِیلًا A<۝>D<٤٣>",
"أَوَ لَمۡ یَسِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَیَN<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ وَكَانُW<وۤ>X<ا۟> أَشَدَّ مِنۡهُمۡ قُوَّةX<ࣰ>ۚ N<وَ>مَا كَانَ ٱللَّهُ لِیُعۡجِزَهُT<ۥ> مِN<ن> شَیۡءN<ࣲ> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَلَا فِی ٱلۡأَR<رۡ>ضِۚ إِN<نَّ>هُT<ۥ> كَانَ عَلِیمN<ࣰ>ا قَدِیR<ر>ࣰا A<۝>D<٤٤>",
"وَلَوۡ یُؤَاخِذُ ٱللَّهُ ٱX<ل>N<نَّ>اسَ بِمَا كَسَبُوX<ا۟> مَا تَR<رَ>كَ عَلَىٰ ظَهۡرِهَا مِN<ن> دَL<اۤ>بَّةX<ࣲ> N<وَ>لَT<ـٰ>كِX<ن> N<یُ>ؤَخِّR<رُ>هُمۡ إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>N<ࣰ>ىۖ فَإِذَا جَW<اۤ>ءَ أَجَلُهُمۡ فَإِN<نَّ> ٱللَّهَ كَانَ بِعِبَادِهِT<ۦ> بَصِیR<رَ>ۢا A<۝>D<٤٥>",
"#یL<سۤ> A<۝>D<١>",
"وَX<ٱ>لۡقُR<رۡ>ءَانِ ٱلۡحَكِJ<ی>مِ A<۝>D<٢>",
"إِN<نَّ>كَ لَمِنَ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٣>",
"عَلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٤>",
"تَN<ن>زِیلَ ٱلۡعَزِیزِ ٱX<ل>R<رَّ>حِJ<ی>مِ A<۝>D<٥>",
"لِتُN<ن>ذِR<رَ> قَوۡمX<ࣰ>ا N<مَّ>W<اۤ> أُN<ن>ذِR<رَ> ءَابَW<اۤ>ؤُهُمۡ فَهُمۡ غَT<ـٰ>فِلُJ<و>نَ A<۝>D<٦>",
"لَقَQ<دۡ> حَقَّ ٱلۡقَوۡلُ عَلَىW<ٰۤ> أَكۡثَرِهِمۡ فَهُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٧>",
"إِN<نَّ>ا جَعَلۡنَا فِW<یۤ> أَعۡنَT<ـٰ>قِهِمۡ أَغۡلَT<ـٰ>لN<ࣰ>ا فَهِیَ إِلَى ٱلۡأَذۡقَانِ فَهُم N<مُّ>Q<قۡ>مَحُJ<و>نَ A<۝>D<٨>",
"وَجَعَلۡنَا مِX<ن>N<ۢ> بَیۡنِ أَیۡدِیهِمۡ سَدّX<ࣰ>ا N<وَ>مِنۡ خَلۡفِهِمۡ سَدّN<ࣰ>ا فَأَغۡشَیۡنَT<ـٰ>هُمۡ فَهُمۡ لَا یُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٩>",
"وَسَوَW<اۤ>ءٌ عَلَیۡهِمۡ ءَأَN<ن>ذَR<رۡ>تَهُمۡ أَمۡ لَمۡ تُN<ن>ذِرۡهُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<١٠>",
"إِN<نَّ>مَا تُN<ن>ذِR<رُ> مَنِ ٱتَّبَعَ ٱX<ل>ذِّكۡR<رَ> وَخَشِیَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نَ بِX<ٱ>لۡغَیۡبِۖ فَبَشِّرۡهُ بِمَغۡفِR<رَ>ةX<ࣲ> N<وَ>أَQ<جۡ>رN<ࣲ> كَرِJ<ی>مٍ A<۝>D<١١>",
"إِN<نَّ>ا نَحۡنُ نُحۡیِ ٱلۡمَوۡتَىٰ وَنَكۡتُبُ مَا قَدَّمُوX<ا۟> وَءَاثَT<ـٰ>R<رَ>هُمۡۚ وَكُلَّ شَیۡءٍ أَحۡصَیۡنَT<ـٰ>هُ فِW<یۤ> إِمَامX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<١٢>",
"وَX<ٱ>ضۡرِQ<بۡ> لَهُم N<مَّ>ثَلًا أَصۡحَT<ـٰ>بَ ٱلۡقَR<رۡ>یَةِ إِذۡ جَW<اۤ>ءَهَا ٱلۡمُR<رۡ>سَلُJ<و>نَ A<۝>D<١٣>",
"إِذۡ أَR<رۡ>سَلۡنَW<اۤ> إِلَیۡهِمُ ٱثۡنَیۡنِ فَكَذَّبُوهُمَا فَعَزَّزۡنَا بِثَالِثN<ࣲ> فَقَالُW<وۤ>X<ا۟> إِN<نَّ>W<اۤ> إِلَیۡكُم N<مُّ>R<رۡ>سَلُJ<و>نَ A<۝>D<١٤>",
"قَالُوX<ا۟> مَW<اۤ> أَN<ن>تُمۡ إِلَّا بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُنَا وَمَW<اۤ> أَN<ن>زَلَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ مِN<ن> شَیۡءٍ إِنۡ أَN<ن>تُمۡ إِلَّا تَكۡذِبُJ<و>نَ A<۝>D<١٥>",
"قَالُوX<ا۟> R<رَ>بُّنَا یَعۡلَمُ إِN<نَّ>W<اۤ> إِلَیۡكُمۡ لَمُR<رۡ>سَلُJ<و>نَ A<۝>D<١٦>",
"وَمَا عَلَیۡنَW<اۤ> إِلَّا ٱلۡبَلَT<ـٰ>غُ ٱلۡمُبِJ<ی>نُ A<۝>D<١٧>",
"قَالُW<وۤ>X<ا۟> إِN<نَّ>ا تَطَیَّR<رۡ>نَا بِكُمۡۖ لَئِX<ن> لَّمۡ تَN<ن>تَهُوX<ا۟> لَنَR<رۡ>جُمَN<نَّ>كُمۡ وَلَیَمَسَّN<نَّ>كُم N<مِّ>N<نَّ>ا عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<١٨>",
"قَالُوX<ا۟> طَW<ـٰۤ>ئِR<رُ>كُم N<مَّ>عَكُمۡ أَئِN<ن> ذُكِّرۡتُN<م>ۚ بَلۡ أَN<ن>تُمۡ قَوۡمX<ࣱ> N<مُّ>سۡرِفُJ<و>نَ A<۝>D<١٩>",
"وَجَW<اۤ>ءَ مِنۡ أَQ<قۡ>صَا ٱلۡمَدِینَةِ R<رَ>جُلX<ࣱ> N<یَ>سۡعَىٰ قَالَ یَT<ـٰ>قَوۡمِ ٱتَّبِعُوX<ا۟> ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٢٠>",
"ٱتَّبِعُوX<ا۟> مَX<ن> لَّا یَسۡءَلُكُمۡ أَQ<جۡ>R<ر>X<ࣰ>ا N<وَ>هُم N<مُّ>هۡتَدُJ<و>نَ A<۝>D<٢١>",
"وَمَا لِیَ لَW<اۤ> أَعۡبُدُ ٱلَّذِی فَطَR<رَ>نِی وَإِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٢٢>",
"ءَأَتَّخِذُ مِN<ن> دُونِهِW<ۦۤ> ءَالِهَةً إِX<ن> N<یُ>رِQ<دۡ>نِ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ بِضُرّX<ࣲ> لَّا تُغۡنِ عَN<نِّ>ی شَفَT<ـٰ>عَتُهُمۡ شَیۡءX<ࣰ>ا N<وَ>لَا یُN<ن>قِذُJ<و>نِ A<۝>D<٢٣>",
"إِN<نِّ>W<یۤ> إِذX<ࣰ>ا لَّفِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٢٤>",
"إِN<نِّ>W<یۤ> ءَامَN<ن>تُ بِR<رَ>بِّكُمۡ فَX<ٱ>سۡمَعُJ<و>نِ A<۝>D<٢٥>",
"قِیلَ ٱQ<دۡ>خُلِ ٱلۡجَN<نَّ>ةَۖ قَالَ یَT<ـٰ>لَیۡتَ قَوۡمِی یَعۡلَمُJ<و>نَ A<۝>D<٢٦>",
"بِمَا غَفَR<رَ> لِی R<رَ>بِّی وَجَعَلَنِی مِنَ ٱلۡمُكۡR<رَ>مِJ<ی>نَ A<۝>D<٢٧>",
"۞ وَمَW<اۤ> أَN<ن>زَلۡنَا عَلَىٰ قَوۡمِهِT<ۦ> مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> مِN<ن> جُN<ن>دX<ࣲ> N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ وَمَا كُN<نَّ>ا مُN<ن>زِلِJ<ی>نَ A<۝>D<٢٨>",
"إِN<ن> كَانَتۡ إِلَّا صَیۡحَةX<ࣰ> N<وَ>T< ٰ>حِدَةN<ࣰ> فَإِذَا هُمۡ خَT<ـٰ>مِدُJ<و>نَ A<۝>D<٢٩>",
"یَT<ـٰ>حَسۡR<رَ>ةً عَلَى ٱلۡعِبَادِۚ مَا یَأۡتِیهِم N<مِّ>X<ن> R<رَّ>سُولٍ إِلَّا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٣٠>",
"أَلَمۡ یَR<رَ>وۡX<ا۟> كَمۡ أَهۡلَكۡنَا قَQ<بۡ>لَهُم N<مِّ>نَ ٱلۡقُR<رُ>ونِ أَN<نَّ>هُمۡ إِلَیۡهِمۡ لَا یَR<رۡ>جِعُJ<و>نَ A<۝>D<٣١>",
"وَإِN<ن> كُلّX<ࣱ> لَّN<مَّ>ا جَمِیعX<ࣱ> لَّدَیۡنَا مُحۡضَR<رُ>J<و>نَ A<۝>D<٣٢>",
"وَءَایَةX<ࣱ> لَّهُمُ ٱلۡأَR<رۡ>ضُ ٱلۡمَیۡتَةُ أَحۡیَیۡنَT<ـٰ>هَا وَأَخۡR<رَ>Q<جۡ>نَا مِنۡهَا حَبّN<ࣰ>ا فَمِنۡهُ یَأۡكُلُJ<و>نَ A<۝>D<٣٣>",
"وَجَعَلۡنَا فِیهَا جَN<نَّ>T<ـٰ>تX<ࣲ> N<مِّ>ن N<نَّ>خِیلX<ࣲ> N<وَ>أَعۡنَT<ـٰ>بX<ࣲ> N<وَ>فَجَّR<رۡ>نَا فِیهَا مِنَ ٱلۡعُیُJ<و>نِ A<۝>D<٣٤>",
"لِیَأۡكُلُوX<ا۟> مِN<ن> ثَمَرِهِT<ۦ> وَمَا عَمِلَتۡهُ أَیۡدِیهِمۡۚ أَفَلَا یَشۡكُR<رُ>J<و>نَ A<۝>D<٣٥>",
"سُQ<بۡ>حَT<ـٰ>نَ ٱلَّذِی خَلَقَ ٱلۡأَزۡوَT< ٰ>جَ كُلَّهَا مِN<مَّ>ا تُX<ن>N<ۢ>بِتُ ٱلۡأَR<رۡ>ضُ وَمِنۡ أَN<ن>فُسِهِمۡ وَمِN<مَّ>ا لَا یَعۡلَمُJ<و>نَ A<۝>D<٣٦>",
"وَءَایَةX<ࣱ> لَّهُمُ ٱلَّیۡلُ نَسۡلَخُ مِنۡهُ ٱX<ل>N<نَّ>هَاR<رَ> فَإِذَا هُم N<مُّ>ظۡلِمُJ<و>نَ A<۝>D<٣٧>",
"وَX<ٱ>X<ل>شَّمۡسُ تَQ<جۡ>رِی لِمُسۡتَقَرّX<ࣲ> لَّهَاۚ ذَT< ٰ>لِكَ تَQ<قۡ>دِیR<رُ> ٱلۡعَزِیزِ ٱلۡعَلِJ<ی>مِ A<۝>D<٣٨>",
"وَX<ٱ>لۡقَمَR<رَ> قَدَّR<رۡ>نَT<ـٰ>هُ مَنَازِلَ حَتَّىٰ عَادَ كَX<ٱ>لۡعُR<رۡ>جُونِ ٱلۡقَدِJ<ی>مِ A<۝>D<٣٩>",
"لَا ٱX<ل>شَّمۡسُ یَX<ن>N<ۢ>بَغِی لَهَW<اۤ> أَN<ن> تُQ<دۡ>رِكَ ٱلۡقَمَR<رَ> وَلَا ٱلَّیۡلُ سَابِقُ ٱX<ل>N<نَّ>هَارِۚ وَكُلّN<ࣱ> فِی فَلَكX<ࣲ> N<یَ>سۡبَحُJ<و>نَ A<۝>D<٤٠>",
"وَءَایَةX<ࣱ> لَّهُمۡ أَN<نَّ>ا حَمَلۡنَا ذُرِّیَّتَهُمۡ فِی ٱلۡفُلۡكِ ٱلۡمَشۡحُJ<و>نِ A<۝>D<٤١>",
"وَخَلَQ<قۡ>نَا لَهُم N<مِّ>X<ن> N<مِّ>ثۡلِهِT<ۦ> مَا یَR<رۡ>كَبُJ<و>نَ A<۝>D<٤٢>",
"وَإِن N<نَّ>شَأۡ نُغۡرِQ<قۡ>هُمۡ فَلَا صَرِیخَ لَهُمۡ وَلَا هُمۡ یُN<ن>قَذُJ<و>نَ A<۝>D<٤٣>",
"إِلَّا R<رَ>حۡمَةX<ࣰ> N<مِّ>N<نَّ>ا وَمَتَT<ـٰ>عًا إِلَىٰ حِJ<ی>نࣲ A<۝>D<٤٤>",
"وَإِذَا قِیلَ لَهُمُ ٱتَّقُوX<ا۟> مَا بَیۡنَ أَیۡدِیكُمۡ وَمَا خَلۡفَكُمۡ لَعَلَّكُمۡ تُR<رۡ>حَمُJ<و>نَ A<۝>D<٤٥>",
"وَمَا تَأۡتِیهِم N<مِّ>نۡ ءَایَةX<ࣲ> N<مِّ>نۡ ءَایَT<ـٰ>تِ R<رَ>بِّهِمۡ إِلَّا كَانُوX<ا۟> عَنۡهَا مُعۡرِضِJ<ی>نَ A<۝>D<٤٦>",
"وَإِذَا قِیلَ لَهُمۡ أَN<ن>فِقُوX<ا۟> مِN<مَّ>ا R<رَ>زَقَكُمُ ٱللَّهُ قَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لِلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَنُQ<طۡ>عِمُ مَX<ن> لَّوۡ یَشَW<اۤ>ءُ ٱللَّهُ أَQ<طۡ>عَمَهُW<ۥۤ> إِنۡ أَN<ن>تُمۡ إِلَّا فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٤٧>",
"وَیَقُولُونَ مَتَىٰ هَT<ـٰ>ذَا ٱلۡوَعۡدُ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٤٨>",
"مَا یَN<ن>ظُR<رُ>ونَ إِلَّا صَیۡحَةX<ࣰ> N<وَ>T< ٰ>حِدَةN<ࣰ> تَأۡخُذُهُمۡ وَهُمۡ یَخِصِّمُJ<و>نَ A<۝>D<٤٩>",
"فَلَا یَسۡتَطِیعُونَ تَوۡصِیَةX<ࣰ> N<وَ>لَW<اۤ> إِلَىW<ٰۤ> أَهۡلِهِمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<٥٠>",
"وَنُفِخَ فِی ٱX<ل>صُّورِ فَإِذَا هُم N<مِّ>نَ ٱلۡأَQ<جۡ>دَاثِ إِلَىٰ R<رَ>بِّهِمۡ یَN<ن>سِلُJ<و>نَ A<۝>D<٥١>",
"قَالُوX<ا۟> یَT<ـٰ>وَیۡلَنَا مَX<ن>N<ۢ> بَعَثَنَا مِX<ن> N<مَّ>R<رۡ>قَدِنَاۜۗ هَT<ـٰ>ذَا مَا وَعَدَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ وَصَدَقَ ٱلۡمُR<رۡ>سَلُJ<و>نَ A<۝>D<٥٢>",
"إِN<ن> كَانَتۡ إِلَّا صَیۡحَةX<ࣰ> N<وَ>T< ٰ>حِدَةN<ࣰ> فَإِذَا هُمۡ جَمِیعX<ࣱ> لَّدَیۡنَا مُحۡضَR<رُ>J<و>نَ A<۝>D<٥٣>",
"فَX<ٱ>لۡیَوۡمَ لَا تُظۡلَمُ نَفۡسN<ࣱ> شَیۡءX<ࣰ>ا N<وَ>لَا تُQ<جۡ>زَوۡنَ إِلَّا مَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٥٤>",
"إِN<نَّ> أَصۡحَT<ـٰ>بَ ٱلۡجَN<نَّ>ةِ ٱلۡیَوۡمَ فِی شُغُلN<ࣲ> فَT<ـٰ>كِهُJ<و>نَ A<۝>D<٥٥>",
"هُمۡ وَأَزۡوَT< ٰ>جُهُمۡ فِی ظِلَT<ـٰ>لٍ عَلَى ٱلۡأَR<رَ>W<اۤ>ئِكِ مُتَّكِءُJ<و>نَ A<۝>D<٥٦>",
"لَهُمۡ فِیهَا فَT<ـٰ>كِهَةX<ࣱ> N<وَ>لَهُم N<مَّ>ا یَدَّعُJ<و>نَ A<۝>D<٥٧>",
"سَلَT<ـٰ>مN<ࣱ> قَوۡلX<ࣰ>ا N<مِّ>X<ن> R<رَّ>بّX<ࣲ> R<رَّ>حِJ<ی>مࣲ A<۝>D<٥٨>",
"وَX<ٱ>مۡتَT<ـٰ>زُوX<ا۟> ٱلۡیَوۡمَ أَیُّهَا ٱلۡمُQ<جۡ>رِمُJ<و>نَ A<۝>D<٥٩>",
"۞ أَلَمۡ أَعۡهَQ<دۡ> إِلَیۡكُمۡ یَT<ـٰ>بَنِW<یۤ> ءَادَمَ أَX<ن> لَّا تَعۡبُدُوX<ا۟> ٱX<ل>شَّیۡطَT<ـٰ>نَۖ إِN<نَّ>هُT<ۥ> لَكُمۡ عَدُوّX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٦٠>",
"وَأَنِ ٱعۡبُدُونِیۚ هَT<ـٰ>ذَا صِR<رَ>T< ٰ>طX<ࣱ> N<مُّ>سۡتَقِJ<ی>مࣱ A<۝>D<٦١>",
"وَلَقَQ<دۡ> أَضَلَّ مِN<ن>كُمۡ جِبِلّN<ࣰ>ا كَثِیR<ر>ًاۖ أَفَلَمۡ تَكُونُوX<ا۟> تَعۡقِلُJ<و>نَ A<۝>D<٦٢>",
"هَT<ـٰ>ذِهِT<ۦ> جَهَN<نَّ>مُ ٱلَّتِی كُN<ن>تُمۡ تُوعَدُJ<و>نَ A<۝>D<٦٣>",
"ٱصۡلَوۡهَا ٱلۡیَوۡمَ بِمَا كُN<ن>تُمۡ تَكۡفُR<رُ>J<و>نَ A<۝>D<٦٤>",
"ٱلۡیَوۡمَ نَخۡتِمُ عَلَىW<ٰۤ> أَفۡوَT< ٰ>هِهِمۡ وَتُكَلِّمُنَW<اۤ> أَیۡدِیهِمۡ وَتَشۡهَدُ أَR<رۡ>جُلُهُN<م> بِمَا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<٦٥>",
"وَلَوۡ نَشَW<اۤ>ءُ لَطَمَسۡنَا عَلَىW<ٰۤ> أَعۡیُنِهِمۡ فَX<ٱ>سۡتَبَقُوX<ا۟> ٱX<ل>صِّR<رَ>T< ٰ>طَ فَأَN<نَّ>ىٰ یُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٦٦>",
"وَلَوۡ نَشَW<اۤ>ءُ لَمَسَخۡنَT<ـٰ>هُمۡ عَلَىٰ مَكَانَتِهِمۡ فَمَا ٱسۡتَطَT<ـٰ>عُوX<ا۟> مُضِیّX<ࣰ>ا N<وَ>لَا یَR<رۡ>جِعُJ<و>نَ A<۝>D<٦٧>",
"وَمَن N<نُّ>عَN<مِّ>رۡهُ نُنَكِّسۡهُ فِی ٱلۡخَلۡقِۚ أَفَلَا یَعۡقِلُJ<و>نَ A<۝>D<٦٨>",
"وَمَا عَلَّمۡنَT<ـٰ>هُ ٱX<ل>شِّعۡR<رَ> وَمَا یَX<ن>N<ۢ>بَغِی لَهُW<ۥۤ>ۚ إِنۡ هُوَ إِلَّا ذِكۡR<ر>X<ࣱ> N<وَ>قُR<رۡ>ءَانX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٦٩>",
"لِّیُN<ن>ذِR<رَ> مَN<ن> كَانَ حَیّX<ࣰ>ا N<وَ>یَحِقَّ ٱلۡقَوۡلُ عَلَى ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٧٠>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ>ا خَلَQ<قۡ>نَا لَهُم N<مِّ>N<مَّ>ا عَمِلَتۡ أَیۡدِینَW<اۤ> أَنۡعَT<ـٰ>مN<ࣰ>ا فَهُمۡ لَهَا مَT<ـٰ>لِكُJ<و>نَ A<۝>D<٧١>",
"وَذَلَّلۡنَT<ـٰ>هَا لَهُمۡ فَمِنۡهَا R<رَ>كُوبُهُمۡ وَمِنۡهَا یَأۡكُلُJ<و>نَ A<۝>D<٧٢>",
"وَلَهُمۡ فِیهَا مَنَT<ـٰ>فِعُ وَمَشَارِبُۚ أَفَلَا یَشۡكُR<رُ>J<و>نَ A<۝>D<٧٣>",
"وَX<ٱ>تَّخَذُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ ءَالِهَةX<ࣰ> لَّعَلَّهُمۡ یُN<ن>صَR<رُ>J<و>نَ A<۝>D<٧٤>",
"لَا یَسۡتَطِیعُونَ نَصۡR<رَ>هُمۡ وَهُمۡ لَهُمۡ جُN<ن>دX<ࣱ> N<مُّ>حۡضَR<رُ>J<و>نَ A<۝>D<٧٥>",
"فَلَا یَحۡزُN<ن>كَ قَوۡلُهُمۡۘ إِN<نَّ>ا نَعۡلَمُ مَا یُسِR<رُّ>ونَ وَمَا یُعۡلِنُJ<و>نَ A<۝>D<٧٦>",
"أَوَ لَمۡ یَR<رَ> ٱلۡإِN<ن>سَT<ـٰ>نُ أَN<نَّ>ا خَلَQ<قۡ>نَT<ـٰ>هُ مِن N<نُّ>Q<طۡ>فَةN<ࣲ> فَإِذَا هُوَ خَصِیمX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٧٧>",
"وَضَR<رَ>بَ لَنَا مَثَلX<ࣰ>ا N<وَ>نَسِیَ خَلۡقَهُT<ۥ>ۖ قَالَ مَX<ن> N<یُ>حۡیِ ٱلۡعِظَT<ـٰ>مَ وَهِیَ R<رَ>مِJ<ی>مࣱ A<۝>D<٧٨>",
"قُلۡ یُحۡیِیهَا ٱلَّذِW<یۤ> أَN<ن>شَأَهَW<اۤ> أَوَّلَ مَR<رَّ>ةX<ࣲ>ۖ N<وَ>هُوَ بِكُلِّ خَلۡقٍ عَلِJ<ی>مٌ A<۝>D<٧٩>",
"ٱلَّذِی جَعَلَ لَكُم N<مِّ>نَ ٱX<ل>شَّجَرِ ٱلۡأَخۡضَرِ نَاR<ر>N<ࣰ>ا فَإِذَW<اۤ> أَN<ن>تُم N<مِّ>نۡهُ تُوقِدُJ<و>نَ A<۝>D<٨٠>",
"أَوَ لَیۡسَ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ بِقَT<ـٰ>دِرٍ عَلَىW<ٰۤ> أَX<ن> N<یَ>خۡلُقَ مِثۡلَهُN<م>ۚ بَلَىٰ وَهُوَ ٱلۡخَلَّT<ـٰ>قُ ٱلۡعَلِJ<ی>مُ A<۝>D<٨١>",
"إِN<نَّ>مَW<اۤ> أَمۡR<رُ>هُW<ۥۤ> إِذَW<اۤ> أَR<رَ>ادَ شَیۡءًا أَX<ن> N<یَ>قُولَ لَهُT<ۥ> كُN<ن> فَیَكُJ<و>نُ A<۝>D<٨٢>",
"فَسُQ<بۡ>حَT<ـٰ>نَ ٱلَّذِی بِیَدِهِT<ۦ> مَلَكُوتُ كُلِّ شَیۡءX<ࣲ> N<وَ>إِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٨٣>",
"#وَX<ٱ>X<ل>صَّL<ـٰۤ>فَّT<ـٰ>تِ صَفࣰّا A<۝>D<١>",
"فَX<ٱ>X<ل>زَّT< ٰ>جِR<رَ>T< ٰ>تِ زَQ<جۡ>R<ر>ࣰا A<۝>D<٢>",
"فَX<ٱ>X<ل>تَّT<ـٰ>لِیَT<ـٰ>تِ ذِكۡR<ر>ًا A<۝>D<٣>",
"إِN<نَّ> إِلَT<ـٰ>هَكُمۡ لَوَT< ٰ>حِQ<د>ࣱ A<۝>D<٤>",
"R<رَّ>بُّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَا وَR<رَ>بُّ ٱلۡمَشَT<ـٰ>رِQ<ق>ِ A<۝>D<٥>",
"إِN<نَّ>ا زَیَّN<نَّ>ا ٱX<ل>سَّمَW<اۤ>ءَ ٱX<ل>دُّنۡیَا بِزِینَةٍ ٱلۡكَوَاكِQ<ب>ِ A<۝>D<٦>",
"وَحِفۡظX<ࣰ>ا N<مِّ>N<ن> كُلِّ شَیۡطَT<ـٰ>نX<ࣲ> N<مَّ>ارِQ<د>ࣲ A<۝>D<٧>",
"لَّا یَسَّN<مَّ>عُونَ إِلَى ٱلۡمَلَإِ ٱلۡأَعۡلَىٰ وَیُQ<قۡ>ذَفُونَ مِN<ن> كُلِّ جَانِQ<ب>ࣲ A<۝>D<٨>",
"دُحُوR<ر>X<ࣰ>اۖ N<وَ>لَهُمۡ عَذَابX<ࣱ> N<وَ>اصِQ<ب>ٌ A<۝>D<٩>",
"إِلَّا مَنۡ خَطِفَ ٱلۡخَQ<طۡ>فَةَ فَأَتۡبَعَهُT<ۥ> شِهَابN<ࣱ> ثَاقِQ<ب>ࣱ A<۝>D<١٠>",
"فَX<ٱ>سۡتَفۡتِهِمۡ أَهُمۡ أَشَدُّ خَلۡقًا أَم N<مَّ>نۡ خَلَQ<قۡ>نَW<اۤ>ۚ إِN<نَّ>ا خَلَQ<قۡ>نَT<ـٰ>هُم N<مِّ>N<ن> طِینX<ࣲ> لَّازِQ<ب>ِۭ A<۝>D<١١>",
"بَلۡ عَجِQ<بۡ>تَ وَیَسۡخَR<رُ>J<و>نَ A<۝>D<١٢>",
"وَإِذَا ذُكِّR<رُ>وX<ا۟> لَا یَذۡكُR<رُ>J<و>نَ A<۝>D<١٣>",
"وَإِذَا R<رَ>أَوۡX<ا۟> ءَایَةX<ࣰ> N<یَ>سۡتَسۡخِR<رُ>J<و>نَ A<۝>D<١٤>",
"وَقَالُW<وۤ>X<ا۟> إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا سِحۡR<ر>X<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<١٥>",
"أَءِذَا مِتۡنَا وَكُN<نَّ>ا تُR<رَ>ابX<ࣰ>ا N<وَ>عِظَT<ـٰ>مًا أَءِN<نَّ>ا لَمَQ<بۡ>عُوثُJ<و>نَ A<۝>D<١٦>",
"أَوَ ءَابَW<اۤ>ؤُنَا ٱلۡأَوَّلُJ<و>نَ A<۝>D<١٧>",
"قُلۡ نَعَمۡ وَأَN<ن>تُمۡ دَT< ٰ>خِR<رُ>J<و>نَ A<۝>D<١٨>",
"فَإِN<نَّ>مَا هِیَ زَQ<جۡ>R<رَ>ةX<ࣱ> N<وَ>T< ٰ>حِدَةN<ࣱ> فَإِذَا هُمۡ یَN<ن>ظُR<رُ>J<و>نَ A<۝>D<١٩>",
"وَقَالُوX<ا۟> یَT<ـٰ>وَیۡلَنَا هَT<ـٰ>ذَا یَوۡمُ ٱX<ل>دِّJ<ی>نِ A<۝>D<٢٠>",
"هَT<ـٰ>ذَا یَوۡمُ ٱلۡفَصۡلِ ٱلَّذِی كُN<ن>تُN<م> بِهِT<ۦ> تُكَذِّبُJ<و>نَ A<۝>D<٢١>",
"۞ ٱحۡشُR<رُ>وX<ا۟> ٱلَّذِینَ ظَلَمُوX<ا۟> وَأَزۡوَT< ٰ>جَهُمۡ وَمَا كَانُوX<ا۟> یَعۡبُدُJ<و>نَ A<۝>D<٢٢>",
"مِN<ن> دُونِ ٱللَّهِ فَX<ٱ>هۡدُوهُمۡ إِلَىٰ صِR<رَ>T< ٰ>طِ ٱلۡجَحِJ<ی>مِ A<۝>D<٢٣>",
"وَقِفُوهُمۡۖ إِN<نَّ>هُم N<مَّ>سۡءُولُJ<و>نَ A<۝>D<٢٤>",
"مَا لَكُمۡ لَا تَنَاصَR<رُ>J<و>نَ A<۝>D<٢٥>",
"بَلۡ هُمُ ٱلۡیَوۡمَ مُسۡتَسۡلِمُJ<و>نَ A<۝>D<٢٦>",
"وَأَQ<قۡ>بَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضX<ࣲ> N<یَ>تَسَW<اۤ>ءَلُJ<و>نَ A<۝>D<٢٧>",
"قَالُW<وۤ>X<ا۟> إِN<نَّ>كُمۡ كُN<ن>تُمۡ تَأۡتُونَنَا عَنِ ٱلۡیَمِJ<ی>نِ A<۝>D<٢٨>",
"قَالُوX<ا۟> بَل لَّمۡ تَكُونُوX<ا۟> مُؤۡمِنِJ<ی>نَ A<۝>D<٢٩>",
"وَمَا كَانَ لَنَا عَلَیۡكُم N<مِّ>N<ن> سُلۡطَT<ـٰ>نِN<ۭ>ۖ بَلۡ كُN<ن>تُمۡ قَوۡمN<ࣰ>ا طَT<ـٰ>غِJ<ی>نَ A<۝>D<٣٠>",
"فَحَقَّ عَلَیۡنَا قَوۡلُ R<رَ>بِّنَW<اۤ>ۖ إِN<نَّ>ا لَذَW<اۤ>ئِقُJ<و>نَ A<۝>D<٣١>",
"فَأَغۡوَیۡنَT<ـٰ>كُمۡ إِN<نَّ>ا كُN<نَّ>ا غَT<ـٰ>وِJ<ی>نَ A<۝>D<٣٢>",
"فَإِN<نَّ>هُمۡ یَوۡمَئِذN<ࣲ> فِی ٱلۡعَذَابِ مُشۡتَرِكُJ<و>نَ A<۝>D<٣٣>",
"إِN<نَّ>ا كَذَT< ٰ>لِكَ نَفۡعَلُ بِX<ٱ>لۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٣٤>",
"إِN<نَّ>هُمۡ كَانُW<وۤ>X<ا۟> إِذَا قِیلَ لَهُمۡ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا ٱللَّهُ یَسۡتَكۡبِR<رُ>J<و>نَ A<۝>D<٣٥>",
"وَیَقُولُونَ أَئِN<نَّ>ا لَتَارِكُW<وۤ>X<ا۟> ءَالِهَتِنَا لِشَاعِرX<ࣲ> N<مَّ>Q<جۡ>نُJ<و>نِۭ A<۝>D<٣٦>",
"بَلۡ جَW<اۤ>ءَ بِX<ٱ>لۡحَقِّ وَصَدَّقَ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<٣٧>",
"إِN<نَّ>كُمۡ لَذَW<اۤ>ئِقُوX<ا۟> ٱلۡعَذَابِ ٱلۡأَلِJ<ی>مِ A<۝>D<٣٨>",
"وَمَا تُQ<جۡ>زَوۡنَ إِلَّا مَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٣٩>",
"إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِJ<ی>نَ A<۝>D<٤٠>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ رِزۡقX<ࣱ> N<مَّ>عۡلُJ<و>مࣱ A<۝>D<٤١>",
"فَوَT< ٰ>كِهُ وَهُم N<مُّ>كۡR<رَ>مُJ<و>نَ A<۝>D<٤٢>",
"فِی جَN<نَّ>T<ـٰ>تِ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<٤٣>",
"عَلَىٰ سُR<رُ>رX<ࣲ> N<مُّ>تَقَT<ـٰ>بِلِJ<ی>نَ A<۝>D<٤٤>",
"یُطَافُ عَلَیۡهِN<م> بِكَأۡسX<ࣲ> N<مِّ>X<ن> N<مَّ>عِJ<ی>نِۭ A<۝>D<٤٥>",
"بَیۡضَW<اۤ>ءَ لَذَّةX<ࣲ> لِّX<ل>شَّT<ـٰ>رِبِJ<ی>نَ A<۝>D<٤٦>",
"لَا فِیهَا غَوۡلX<ࣱ> N<وَ>لَا هُمۡ عَنۡهَا یُN<ن>زَفُJ<و>نَ A<۝>D<٤٧>",
"وَعِN<ن>دَهُمۡ قَT<ـٰ>صِR<رَ>T< ٰ>تُ ٱX<ل>طَّR<رۡ>فِ عِJ<ی>نࣱ A<۝>D<٤٨>",
"كَأَN<نَّ>هُN<نَّ> بَیۡضX<ࣱ> N<مَّ>كۡنُJ<و>نࣱ A<۝>D<٤٩>",
"فَأَQ<قۡ>بَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضX<ࣲ> N<یَ>تَسَW<اۤ>ءَلُJ<و>نَ A<۝>D<٥٠>",
"قَالَ قَW<اۤ>ئِلX<ࣱ> N<مِّ>نۡهُمۡ إِN<نِّ>ی كَانَ لِی قَرِJ<ی>نࣱ A<۝>D<٥١>",
"یَقُولُ أَءِN<نَّ>كَ لَمِنَ ٱلۡمُصَدِّقِJ<ی>نَ A<۝>D<٥٢>",
"أَءِذَا مِتۡنَا وَكُN<نَّ>ا تُR<رَ>ابX<ࣰ>ا N<وَ>عِظَT<ـٰ>مًا أَءِN<نَّ>ا لَمَدِینُJ<و>نَ A<۝>D<٥٣>",
"قَالَ هَلۡ أَN<ن>تُم N<مُّ>طَّلِعُJ<و>نَ A<۝>D<٥٤>",
"فَX<ٱ>طَّلَعَ فَR<رَ>ءَاهُ فِی سَوَW<اۤ>ءِ ٱلۡجَحِJ<ی>مِ A<۝>D<٥٥>",
"قَالَ تَٱللَّهِ إِN<ن> كِX<د>تَّ لَتُR<رۡ>دِJ<ی>نِ A<۝>D<٥٦>",
"وَلَوۡلَا نِعۡمَةُ R<رَ>بِّی لَكُN<ن>تُ مِنَ ٱلۡمُحۡضَرِJ<ی>نَ A<۝>D<٥٧>",
"أَفَمَا نَحۡنُ بِمَیِّتِJ<ی>نَ A<۝>D<٥٨>",
"إِلَّا مَوۡتَتَنَا ٱلۡأُولَىٰ وَمَا نَحۡنُ بِمُعَذَّبِJ<ی>نَ A<۝>D<٥٩>",
"إِN<نَّ> هَT<ـٰ>ذَا لَهُوَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<٦٠>",
"لِمِثۡلِ هَT<ـٰ>ذَا فَلۡیَعۡمَلِ ٱلۡعَT<ـٰ>مِلُJ<و>نَ A<۝>D<٦١>",
"أَذَT< ٰ>لِكَ خَیۡR<ر>X<ࣱ> N<نُّ>زُلًا أَمۡ شَجَR<رَ>ةُ ٱX<ل>زَّقُّJ<و>مِ A<۝>D<٦٢>",
"إِN<نَّ>ا جَعَلۡنَT<ـٰ>هَا فِتۡنَةX<ࣰ> لِّX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٦٣>",
"إِN<نَّ>هَا شَجَR<رَ>ةN<ࣱ> تَخۡR<رُ>جُ فِW<یۤ> أَصۡلِ ٱلۡجَحِJ<ی>مِ A<۝>D<٦٤>",
"طَلۡعُهَا كَأَN<نَّ>هُT<ۥ> R<رُ>ءُوسُ ٱX<ل>شَّیَT<ـٰ>طِJ<ی>نِ A<۝>D<٦٥>",
"فَإِN<نَّ>هُمۡ لَءَاكِلُونَ مِنۡهَا فَمَالِءُونَ مِنۡهَا ٱلۡبُطُJ<و>نَ A<۝>D<٦٦>",
"ثُN<مَّ> إِN<نَّ> لَهُمۡ عَلَیۡهَا لَشَوۡبX<ࣰ>ا N<مِّ>نۡ حَمِJ<ی>مࣲ A<۝>D<٦٧>",
"ثُN<مَّ> إِN<نَّ> مَR<رۡ>جِعَهُمۡ لَإِلَى ٱلۡجَحِJ<ی>مِ A<۝>D<٦٨>",
"إِN<نَّ>هُمۡ أَلۡفَوۡX<ا۟> ءَابَW<اۤ>ءَهُمۡ ضَL<اۤ>لِّJ<ی>نَ A<۝>D<٦٩>",
"فَهُمۡ عَلَىW<ٰۤ> ءَاثَT<ـٰ>رِهِمۡ یُهۡR<رَ>عُJ<و>نَ A<۝>D<٧٠>",
"وَلَقَQ<دۡ> ضَلَّ قَQ<بۡ>لَهُمۡ أَكۡثَR<رُ> ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٧١>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا فِیهِم N<مُّ>N<ن>ذِرِJ<ی>نَ A<۝>D<٧٢>",
"فَX<ٱ>N<ن>ظُR<رۡ> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُN<ن>ذَرِJ<ی>نَ A<۝>D<٧٣>",
"إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِJ<ی>نَ A<۝>D<٧٤>",
"وَلَقَQ<دۡ> نَادَX<ى>T<ٰ>نَا نُوحN<ࣱ> فَلَنِعۡمَ ٱلۡمُجِیبُJ<و>نَ A<۝>D<٧٥>",
"وَنَجَّیۡنَT<ـٰ>هُ وَأَهۡلَهُT<ۥ> مِنَ ٱلۡكَR<رۡ>بِ ٱلۡعَظِJ<ی>مِ A<۝>D<٧٦>",
"وَجَعَلۡنَا ذُرِّیَّتَهُT<ۥ> هُمُ ٱلۡبَاقِJ<ی>نَ A<۝>D<٧٧>",
"وَتَR<رَ>كۡنَا عَلَیۡهِ فِی ٱلۡءَاخِرِJ<ی>نَ A<۝>D<٧٨>",
"سَلَT<ـٰ>مٌ عَلَىٰ نُوحN<ࣲ> فِی ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٧٩>",
"إِN<نَّ>ا كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٨٠>",
"إِN<نَّ>هُT<ۥ> مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٨١>",
"ثُN<مَّ> أَغۡR<رَ>Q<قۡ>نَا ٱلۡءَاخَرِJ<ی>نَ A<۝>D<٨٢>",
"۞ وَإِN<نَّ> مِN<ن> شِیعَتِهِT<ۦ> لَإِQ<بۡ>R<رَ>T< ٰ>هِJ<ی>مَ A<۝>D<٨٣>",
"إِذۡ جَW<اۤ>ءَ R<رَ>بَّهُT<ۥ> بِقَلۡبN<ࣲ> سَلِJ<ی>مٍ A<۝>D<٨٤>",
"إِذۡ قَالَ لِأَبِیهِ وَقَوۡمِهِT<ۦ> مَاذَا تَعۡبُدُJ<و>نَ A<۝>D<٨٥>",
"أَئِفۡكًا ءَالِهَةN<ࣰ> دُونَ ٱللَّهِ تُرِیدُJ<و>نَ A<۝>D<٨٦>",
"فَمَا ظَN<نُّ>كُN<م> بِR<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٨٧>",
"فَنَظَR<رَ> نَظۡR<رَ>ةN<ࣰ> فِی ٱX<ل>N<نُّ>جُJ<و>مِ A<۝>D<٨٨>",
"فَقَالَ إِN<نِّ>ی سَقِJ<ی>مࣱ A<۝>D<٨٩>",
"فَتَوَلَّوۡX<ا۟> عَنۡهُ مُQ<دۡ>بِرِJ<ی>نَ A<۝>D<٩٠>",
"فَR<رَ>اغَ إِلَىW<ٰۤ> ءَالِهَتِهِمۡ فَقَالَ أَلَا تَأۡكُلُJ<و>نَ A<۝>D<٩١>",
"مَا لَكُمۡ لَا تَN<ن>طِقُJ<و>نَ A<۝>D<٩٢>",
"فَR<رَ>اغَ عَلَیۡهِمۡ ضَR<رۡ>بَN<ۢ>ا بِX<ٱ>لۡیَمِJ<ی>نِ A<۝>D<٩٣>",
"فَأَQ<قۡ>بَلُW<وۤ>X<ا۟> إِلَیۡهِ یَزِفُّJ<و>نَ A<۝>D<٩٤>",
"قَالَ أَتَعۡبُدُونَ مَا تَنۡحِتُJ<و>نَ A<۝>D<٩٥>",
"وَٱللَّهُ خَلَقَكُمۡ وَمَا تَعۡمَلُJ<و>نَ A<۝>D<٩٦>",
"قَالُوX<ا۟> ٱQ<بۡ>نُوX<ا۟> لَهُT<ۥ> بُنۡیَT<ـٰ>نN<ࣰ>ا فَأَلۡقُوهُ فِی ٱلۡجَحِJ<ی>مِ A<۝>D<٩٧>",
"فَأَR<رَ>ادُوX<ا۟> بِهِT<ۦ> كَیۡدN<ࣰ>ا فَجَعَلۡنَT<ـٰ>هُمُ ٱلۡأَسۡفَلِJ<ی>نَ A<۝>D<٩٨>",
"وَقَالَ إِN<نِّ>ی ذَاهِبٌ إِلَىٰ R<رَ>بِّی سَیَهۡدِJ<ی>نِ A<۝>D<٩٩>",
"R<رَ>بِّ هَQ<بۡ> لِی مِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<١٠٠>",
"فَبَشَّR<رۡ>نَT<ـٰ>هُ بِغُلَT<ـٰ>مٍ حَلِJ<ی>مࣲ A<۝>D<١٠١>",
"فَلَN<مَّ>ا بَلَغَ مَعَهُ ٱX<ل>سَّعۡیَ قَالَ یَT<ـٰ>بُنَیَّ إِN<نِّ>W<یۤ> أَR<رَ>ىٰ فِی ٱلۡمَنَامِ أَN<نِّ>W<یۤ> أَذۡبَحُكَ فَX<ٱ>N<ن>ظُR<رۡ> مَاذَا تَR<رَ>ىٰۚ قَالَ یَW<ـٰۤ>أَبَتِ ٱفۡعَلۡ مَا تُؤۡمَR<رُ>ۖ سَتَجِدُنِW<یۤ> إِN<ن> شَW<اۤ>ءَ ٱللَّهُ مِنَ ٱX<ل>صَّT<ـٰ>بِرِJ<ی>نَ A<۝>D<١٠٢>",
"فَلَN<مَّ>W<اۤ> أَسۡلَمَا وَتَلَّهُT<ۥ> لِلۡجَبِJ<ی>نِ A<۝>D<١٠٣>",
"وَنَT<ـٰ>دَیۡنَT<ـٰ>هُ أَX<ن> N<یَ>W<ـٰۤ>إِQ<بۡ>R<رَ>T< ٰ>هِJ<ی>مُ A<۝>D<١٠٤>",
"قَQ<دۡ> صَدَّQ<قۡ>تَ ٱX<ل>R<رُّ>ءۡیَW<اۤ>ۚ إِN<نَّ>ا كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٠٥>",
"إِN<نَّ> هَT<ـٰ>ذَا لَهُوَ ٱلۡبَلَW<ـٰۤ>ؤُX<ا۟> ٱلۡمُبِJ<ی>نُ A<۝>D<١٠٦>",
"وَفَدَیۡنَT<ـٰ>هُ بِذِQ<بۡ>حٍ عَظِJ<ی>مࣲ A<۝>D<١٠٧>",
"وَتَR<رَ>كۡنَا عَلَیۡهِ فِی ٱلۡءَاخِرِJ<ی>نَ A<۝>D<١٠٨>",
"سَلَT<ـٰ>مٌ عَلَىW<ٰۤ> إِQ<بۡ>R<رَ>T< ٰ>هِJ<ی>مَ A<۝>D<١٠٩>",
"كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١١٠>",
"إِN<نَّ>هُT<ۥ> مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١١١>",
"وَبَشَّR<رۡ>نَT<ـٰ>هُ بِإِسۡحَT<ـٰ>قَ نَبِیّX<ࣰ>ا N<مِّ>نَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<١١٢>",
"وَبَT<ـٰ>R<رَ>كۡنَا عَلَیۡهِ وَعَلَىW<ٰۤ> إِسۡحَT<ـٰ>قَۚ وَمِN<ن> ذُرِّیَّتِهِمَا مُحۡسِنX<ࣱ> N<وَ>ظَالِمX<ࣱ> لِّنَفۡسِهِT<ۦ> مُبِJ<ی>نࣱ A<۝>D<١١٣>",
"وَلَقَQ<دۡ> مَنَN<نَّ>ا عَلَىٰ مُوسَىٰ وَهَT<ـٰ>R<رُ>J<و>نَ A<۝>D<١١٤>",
"وَنَجَّیۡنَT<ـٰ>هُمَا وَقَوۡمَهُمَا مِنَ ٱلۡكَR<رۡ>بِ ٱلۡعَظِJ<ی>مِ A<۝>D<١١٥>",
"وَنَصَR<رۡ>نَT<ـٰ>هُمۡ فَكَانُوX<ا۟> هُمُ ٱلۡغَT<ـٰ>لِبِJ<ی>نَ A<۝>D<١١٦>",
"وَءَاتَیۡنَT<ـٰ>هُمَا ٱلۡكِتَT<ـٰ>بَ ٱلۡمُسۡتَبِJ<ی>نَ A<۝>D<١١٧>",
"وَهَدَیۡنَT<ـٰ>هُمَا ٱX<ل>صِّR<رَ>T< ٰ>طَ ٱلۡمُسۡتَقِJ<ی>مَ A<۝>D<١١٨>",
"وَتَR<رَ>كۡنَا عَلَیۡهِمَا فِی ٱلۡءَاخِرِJ<ی>نَ A<۝>D<١١٩>",
"سَلَT<ـٰ>مٌ عَلَىٰ مُوسَىٰ وَهَT<ـٰ>R<رُ>J<و>نَ A<۝>D<١٢٠>",
"إِN<نَّ>ا كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٢١>",
"إِN<نَّ>هُمَا مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٢٢>",
"وَإِN<نَّ> إِلۡیَاسَ لَمِنَ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٢٣>",
"إِذۡ قَالَ لِقَوۡمِهِW<ۦۤ> أَلَا تَتَّقُJ<و>نَ A<۝>D<١٢٤>",
"أَتَQ<دۡ>عُونَ بَعۡلX<ࣰ>ا N<وَ>تَذَR<رُ>ونَ أَحۡسَنَ ٱلۡخَT<ـٰ>لِقِJ<ی>نَ A<۝>D<١٢٥>",
"ٱللَّهَ R<رَ>بَّكُمۡ وَR<رَ>بَّ ءَابَW<اۤ>ئِكُمُ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٢٦>",
"فَكَذَّبُوهُ فَإِN<نَّ>هُمۡ لَمُحۡضَR<رُ>J<و>نَ A<۝>D<١٢٧>",
"إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِJ<ی>نَ A<۝>D<١٢٨>",
"وَتَR<رَ>كۡنَا عَلَیۡهِ فِی ٱلۡءَاخِرِJ<ی>نَ A<۝>D<١٢٩>",
"سَلَT<ـٰ>مٌ عَلَىW<ٰۤ> إِلۡ یَاسِJ<ی>نَ A<۝>D<١٣٠>",
"إِN<نَّ>ا كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٣١>",
"إِN<نَّ>هُT<ۥ> مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٣٢>",
"وَإِN<نَّ> لُوطX<ࣰ>ا لَّمِنَ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٣٣>",
"إِذۡ نَجَّیۡنَT<ـٰ>هُ وَأَهۡلَهُW<ۥۤ> أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<١٣٤>",
"إِلَّا عَجُوزN<ࣰ>ا فِی ٱلۡغَT<ـٰ>بِرِJ<ی>نَ A<۝>D<١٣٥>",
"ثُN<مَّ> دَN<مَّ>R<رۡ>نَا ٱلۡءَاخَرِJ<ی>نَ A<۝>D<١٣٦>",
"وَإِN<نَّ>كُمۡ لَتَمُR<رُّ>ونَ عَلَیۡهِم N<مُّ>صۡبِحِJ<ی>نَ A<۝>D<١٣٧>",
"وَبِX<ٱ>لَّیۡلِۚ أَفَلَا تَعۡقِلُJ<و>نَ A<۝>D<١٣٨>",
"وَإِN<نَّ> یُونُسَ لَمِنَ ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٣٩>",
"إِذۡ أَبَقَ إِلَى ٱلۡفُلۡكِ ٱلۡمَشۡحُJ<و>نِ A<۝>D<١٤٠>",
"فَسَاهَمَ فَكَانَ مِنَ ٱلۡمُQ<دۡ>حَضِJ<ی>نَ A<۝>D<١٤١>",
"فَX<ٱ>لۡتَقَمَهُ ٱلۡحُوتُ وَهُوَ مُلِJ<ی>مࣱ A<۝>D<١٤٢>",
"فَلَوۡلَW<اۤ> أَN<نَّ>هُT<ۥ> كَانَ مِنَ ٱلۡمُسَبِّحِJ<ی>نَ A<۝>D<١٤٣>",
"لَلَبِثَ فِی بَQ<طۡ>نِهِW<ۦۤ> إِلَىٰ یَوۡمِ یُQ<بۡ>عَثُJ<و>نَ A<۝>D<١٤٤>",
"۞ فَنَبَذۡنَT<ـٰ>هُ بِX<ٱ>لۡعَR<رَ>W<اۤ>ءِ وَهُوَ سَقِJ<ی>مࣱ A<۝>D<١٤٥>",
"وَأَX<ن>N<ۢ>بَتۡنَا عَلَیۡهِ شَجَR<رَ>ةX<ࣰ> N<مِّ>X<ن> N<یَ>Q<قۡ>طِJ<ی>نࣲ A<۝>D<١٤٦>",
"وَأَR<رۡ>سَلۡنَT<ـٰ>هُ إِلَىٰ مِX<ا۟>ئَةِ أَلۡفٍ أَوۡ یَزِیدُJ<و>نَ A<۝>D<١٤٧>",
"فَءَامَنُوX<ا۟> فَمَتَّعۡنَT<ـٰ>هُمۡ إِلَىٰ حِJ<ی>نࣲ A<۝>D<١٤٨>",
"فَX<ٱ>سۡتَفۡتِهِمۡ أَلِR<رَ>بِّكَ ٱلۡبَنَاتُ وَلَهُمُ ٱلۡبَنُJ<و>نَ A<۝>D<١٤٩>",
"أَمۡ خَلَQ<قۡ>نَا ٱلۡمَلَW<ـٰۤ>ئِكَةَ إِنَT<ـٰ>ثX<ࣰ>ا N<وَ>هُمۡ شَT<ـٰ>هِدُJ<و>نَ A<۝>D<١٥٠>",
"أَلَW<اۤ> إِN<نَّ>هُم N<مِّ>نۡ إِفۡكِهِمۡ لَیَقُولُJ<و>نَ A<۝>D<١٥١>",
"وَلَدَ ٱللَّهُ وَإِN<نَّ>هُمۡ لَكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<١٥٢>",
"أَصۡطَفَى ٱلۡبَنَاتِ عَلَى ٱلۡبَنِJ<ی>نَ A<۝>D<١٥٣>",
"مَا لَكُمۡ كَیۡفَ تَحۡكُمُJ<و>نَ A<۝>D<١٥٤>",
"أَفَلَا تَذَكَّR<رُ>J<و>نَ A<۝>D<١٥٥>",
"أَمۡ لَكُمۡ سُلۡطَT<ـٰ>نX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٥٦>",
"فَأۡتُوX<ا۟> بِكِتَT<ـٰ>بِكُمۡ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٥٧>",
"وَجَعَلُوX<ا۟> بَیۡنَهُT<ۥ> وَبَیۡنَ ٱلۡجِN<نَّ>ةِ نَسَبX<ࣰ>اۚ N<وَ>لَقَQ<دۡ> عَلِمَتِ ٱلۡجِN<نَّ>ةُ إِN<نَّ>هُمۡ لَمُحۡضَR<رُ>J<و>نَ A<۝>D<١٥٨>",
"سُQ<بۡ>حَT<ـٰ>نَ ٱللَّهِ عَN<مَّ>ا یَصِفُJ<و>نَ A<۝>D<١٥٩>",
"إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِJ<ی>نَ A<۝>D<١٦٠>",
"فَإِN<نَّ>كُمۡ وَمَا تَعۡبُدُJ<و>نَ A<۝>D<١٦١>",
"مَW<اۤ> أَN<ن>تُمۡ عَلَیۡهِ بِفَT<ـٰ>تِنِJ<ی>نَ A<۝>D<١٦٢>",
"إِلَّا مَنۡ هُوَ صَالِ ٱلۡجَحِJ<ی>مِ A<۝>D<١٦٣>",
"وَمَا مِN<نَّ>W<اۤ> إِلَّا لَهُT<ۥ> مَقَامX<ࣱ> N<مَّ>عۡلُJ<و>مࣱ A<۝>D<١٦٤>",
"وَإِN<نَّ>ا لَنَحۡنُ ٱX<ل>صَّL<اۤ>فُّJ<و>نَ A<۝>D<١٦٥>",
"وَإِN<نَّ>ا لَنَحۡنُ ٱلۡمُسَبِّحُJ<و>نَ A<۝>D<١٦٦>",
"وَإِN<ن> كَانُوX<ا۟> لَیَقُولُJ<و>نَ A<۝>D<١٦٧>",
"لَوۡ أَN<نَّ> عِN<ن>دَنَا ذِكۡR<ر>X<ࣰ>ا N<مِّ>نَ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٦٨>",
"لَكُN<نَّ>ا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِJ<ی>نَ A<۝>D<١٦٩>",
"فَكَفَR<رُ>وX<ا۟> بِهِT<ۦ>ۖ فَسَوۡفَ یَعۡلَمُJ<و>نَ A<۝>D<١٧٠>",
"وَلَقَQ<دۡ> سَبَقَتۡ كَلِمَتُنَا لِعِبَادِنَا ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٧١>",
"إِN<نَّ>هُمۡ لَهُمُ ٱلۡمَN<ن>صُوR<رُ>J<و>نَ A<۝>D<١٧٢>",
"وَإِN<نَّ> جُN<ن>دَنَا لَهُمُ ٱلۡغَT<ـٰ>لِبُJ<و>نَ A<۝>D<١٧٣>",
"فَتَوَلَّ عَنۡهُمۡ حَتَّىٰ حِJ<ی>نࣲ A<۝>D<١٧٤>",
"وَأَQ<بۡ>صِرۡهُمۡ فَسَوۡفَ یُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<١٧٥>",
"أَفَبِعَذَابِنَا یَسۡتَعۡجِلُJ<و>نَ A<۝>D<١٧٦>",
"فَإِذَا نَزَلَ بِسَاحَتِهِمۡ فَسَW<اۤ>ءَ صَبَاحُ ٱلۡمُN<ن>ذَرِJ<ی>نَ A<۝>D<١٧٧>",
"وَتَوَلَّ عَنۡهُمۡ حَتَّىٰ حِJ<ی>نࣲ A<۝>D<١٧٨>",
"وَأَQ<بۡ>صِرۡ فَسَوۡفَ یُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<١٧٩>",
"سُQ<بۡ>حَT<ـٰ>نَ R<رَ>بِّكَ R<رَ>بِّ ٱلۡعِزَّةِ عَN<مَّ>ا یَصِفُJ<و>نَ A<۝>D<١٨٠>",
"وَسَلَT<ـٰ>مٌ عَلَى ٱلۡمُR<رۡ>سَلِJ<ی>نَ A<۝>D<١٨١>",
"وَX<ٱ>لۡحَمۡدُ لِلَّهِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٨٢>",
"#L<صۤ>ۚ وَX<ٱ>لۡقُR<رۡ>ءَانِ ذِی ٱX<ل>ذِّكۡرِ A<۝>D<١>",
"بَلِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فِی عِزَّةX<ࣲ> N<وَ>شِقَJ<ا>Q<ق>ࣲ A<۝>D<٢>",
"كَمۡ أَهۡلَكۡنَا مِN<ن> قَQ<بۡ>لِهِم N<مِّ>N<ن> قَR<رۡ>نN<ࣲ> فَنَادَوX<ا۟> وَّلَاتَ حِینَ مَنَJ<ا>صࣲ A<۝>D<٣>",
"وَعَجِبُW<وۤ>X<ا۟> أَN<ن> جَW<اۤ>ءَهُم N<مُّ>N<ن>ذِR<ر>X<ࣱ> N<مِّ>نۡهُمۡۖ وَقَالَ ٱلۡكَT<ـٰ>فِR<رُ>ونَ هَT<ـٰ>ذَا سَT<ـٰ>حِR<ر>N<ࣱ> كَذَّJ<ا>Q<ب>ٌ A<۝>D<٤>",
"أَجَعَلَ ٱلۡءَالِهَةَ إِلَT<ـٰ>هX<ࣰ>ا N<وَ>T< ٰ>حِدًاۖ إِN<نَّ> هَT<ـٰ>ذَا لَشَیۡءٌ عُجَJ<ا>Q<ب>ࣱ A<۝>D<٥>",
"وَX<ٱ>N<ن>طَلَقَ ٱلۡمَلَأُ مِنۡهُمۡ أَنِ ٱمۡشُوX<ا۟> وَX<ٱ>صۡبِR<رُ>وX<ا۟> عَلَىW<ٰۤ> ءَالِهَتِكُمۡۖ إِN<نَّ> هَT<ـٰ>ذَا لَشَیۡءX<ࣱ> N<یُ>R<رَ>J<ا>Q<د>ُ A<۝>D<٦>",
"مَا سَمِعۡنَا بِهَT<ـٰ>ذَا فِی ٱلۡمِلَّةِ ٱلۡءَاخِR<رَ>ةِ إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا ٱخۡتِلَT<ـٰ>Q<ق>ٌ A<۝>D<٧>",
"أَءُN<ن>زِلَ عَلَیۡهِ ٱX<ل>ذِّكۡR<رُ> مِX<ن>N<ۢ> بَیۡنِنَاۚ بَلۡ هُمۡ فِی شَكّX<ࣲ> N<مِّ>N<ن> ذِكۡرِیۚ بَل لَّN<مَّ>ا یَذُوقُوX<ا۟> عَذَJ<ا>Q<ب>ِ A<۝>D<٨>",
"أَمۡ عِN<ن>دَهُمۡ خَزَW<اۤ>ئِنُ R<رَ>حۡمَةِ R<رَ>بِّكَ ٱلۡعَزِیزِ ٱلۡوَهَّJ<ا>Q<ب>ِ A<۝>D<٩>",
"أَمۡ لَهُم N<مُّ>لۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَاۖ فَلۡیَR<رۡ>تَقُوX<ا۟> فِی ٱلۡأَسۡبَT<ـٰ>Q<ب>ِ A<۝>D<١٠>",
"جُN<ن>دX<ࣱ> N<مَّ>ا هُنَالِكَ مَهۡزُومX<ࣱ> N<مِّ>نَ ٱلۡأَحۡزَJ<ا>Q<ب>ِ A<۝>D<١١>",
"كَذَّبَتۡ قَQ<بۡ>لَهُمۡ قَوۡمُ نُوحX<ࣲ> N<وَ>عَادX<ࣱ> N<وَ>فِرۡعَوۡنُ ذُو ٱلۡأَوۡتَJ<ا>Q<د>ِ A<۝>D<١٢>",
"وَثَمُودُ وَقَوۡمُ لُوطX<ࣲ> N<وَ>أَصۡحَT<ـٰ>بُ لۡءَیۡكَةِۚ أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلۡأَحۡزَJ<ا>Q<ب>ُ A<۝>D<١٣>",
"إِN<ن> كُلٌّ إِلَّا كَذَّبَ ٱX<ل>R<رُّ>سُلَ فَحَقَّ عِقَJ<ا>Q<ب>ِ A<۝>D<١٤>",
"وَمَا یَN<ن>ظُR<رُ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ إِلَّا صَیۡحَةX<ࣰ> N<وَ>T< ٰ>حِدَةX<ࣰ> N<مَّ>ا لَهَا مِN<ن> فَوَJ<ا>Q<ق>ࣲ A<۝>D<١٥>",
"وَقَالُوX<ا۟> R<رَ>بَّنَا عَجِّل لَّنَا قِطَّنَا قَQ<بۡ>لَ یَوۡمِ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<١٦>",
"ٱصۡبِرۡ عَلَىٰ مَا یَقُولُونَ وَX<ٱ>ذۡكُR<رۡ> عَQ<بۡ>دَنَا دَاوُT<ۥ>دَ ذَا ٱلۡأَیۡدِۖ إِN<نَّ>هُW<ۥۤ> أَوَّJ<ا>Q<ب>ٌ A<۝>D<١٧>",
"إِN<نَّ>ا سَخَّR<رۡ>نَا ٱلۡجِبَالَ مَعَهُT<ۥ> یُسَبِّحۡنَ بِX<ٱ>لۡعَشِیِّ وَX<ٱ>لۡإِشۡR<رَ>J<ا>Q<ق>ِ A<۝>D<١٨>",
"وَX<ٱ>X<ل>طَّیۡR<رَ> مَحۡشُوR<رَ>ةN<ࣰ>ۖ كُلّX<ࣱ> لَّهُW<ۥۤ> أَوَّJ<ا>Q<ب>ࣱ A<۝>D<١٩>",
"وَشَدَQ<دۡ>نَا مُلۡكَهُT<ۥ> وَءَاتَیۡنَT<ـٰ>هُ ٱلۡحِكۡمَةَ وَفَصۡلَ ٱلۡخِطَJ<ا>Q<ب>ِ A<۝>D<٢٠>",
"۞ وَهَلۡ أَتَX<ى>T<ٰ>كَ نَبَؤُX<ا۟> ٱلۡخَصۡمِ إِذۡ تَسَوَّR<رُ>وX<ا۟> ٱلۡمِحۡR<رَ>J<ا>Q<ب>َ A<۝>D<٢١>",
"إِذۡ دَخَلُوX<ا۟> عَلَىٰ دَاوُT<ۥ>دَ فَفَزِعَ مِنۡهُمۡۖ قَالُوX<ا۟> لَا تَخَفۡۖ خَصۡمَانِ بَغَىٰ بَعۡضُنَا عَلَىٰ بَعۡضN<ࣲ> فَX<ٱ>حۡكُN<م> بَیۡنَنَا بِX<ٱ>لۡحَقِّ وَلَا تُشۡطِQ<طۡ> وَX<ٱ>هۡدِنَW<اۤ> إِلَىٰ سَوَW<اۤ>ءِ ٱX<ل>صِّR<رَ>T< ٰ>Q<ط>ِ A<۝>D<٢٢>",
"إِN<نَّ> هَT<ـٰ>ذَW<اۤ> أَخِی لَهُT<ۥ> تِسۡعX<ࣱ> N<وَ>تِسۡعُونَ نَعۡجَةX<ࣰ> N<وَ>لِیَ نَعۡجَةX<ࣱ> N<وَ>T< ٰ>حِدَةN<ࣱ> فَقَالَ أَكۡفِلۡنِیهَا وَعَزَّنِی فِی ٱلۡخِطَJ<ا>Q<ب>ِ A<۝>D<٢٣>",
"قَالَ لَقَQ<دۡ> ظَلَمَكَ بِسُؤَالِ نَعۡجَتِكَ إِلَىٰ نِعَاجِهِT<ۦ>ۖ وَإِN<نَّ> كَثِیR<ر>X<ࣰ>ا N<مِّ>نَ ٱلۡخُلَطَW<اۤ>ءِ لَیَQ<بۡ>غِی بَعۡضُهُمۡ عَلَىٰ بَعۡضٍ إِلَّا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَقَلِیلX<ࣱ> N<مَّ>ا هُمۡۗ وَظَN<نَّ> دَاوُT<ۥ>دُ أَN<نَّ>مَا فَتَN<نَّ>T<ـٰ>هُ فَX<ٱ>سۡتَغۡفَR<رَ> R<رَ>بَّهُT<ۥ> وَ̅خَ̅R<رَّ>̅ ̅R<رَ>̅ا̅كِ̅عX<ࣰ>̅ا̅ N<وَ>أَنَJ<ا>Q<ب>َ A<۝>D<٢٤> ۩",
"فَغَفَR<رۡ>نَا لَهُT<ۥ> ذَT< ٰ>لِكَۖ وَإِN<نَّ> لَهُT<ۥ> عِN<ن>دَنَا لَزُلۡفَىٰ وَحُسۡنَ مَءَJ<ا>Q<ب>ࣲ A<۝>D<٢٥>",
"یَT<ـٰ>دَاوُT<ۥ>دُ إِN<نَّ>ا جَعَلۡنَT<ـٰ>كَ خَلِیفَةN<ࣰ> فِی ٱلۡأَR<رۡ>ضِ فَX<ٱ>حۡكُN<م> بَیۡنَ ٱX<ل>N<نَّ>اسِ بِX<ٱ>لۡحَقِّ وَلَا تَتَّبِعِ ٱلۡهَوَىٰ فَیُضِلَّكَ عَN<ن> سَبِیلِ ٱللَّهِۚ إِN<نَّ> ٱلَّذِینَ یَضِلُّونَ عَN<ن> سَبِیلِ ٱللَّهِ لَهُمۡ عَذَابN<ࣱ> شَدِیدُN<ۢ> بِمَا نَسُوX<ا۟> یَوۡمَ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<٢٦>",
"وَمَا خَلَQ<قۡ>نَا ٱX<ل>سَّمَW<اۤ>ءَ وَX<ٱ>لۡأَR<رۡ>ضَ وَمَا بَیۡنَهُمَا بَT<ـٰ>طِلN<ࣰ>اۚ ذَT< ٰ>لِكَ ظَN<نُّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟>ۚ فَوَیۡلX<ࣱ> لِّلَّذِینَ كَفَR<رُ>وX<ا۟> مِنَ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٢٧>",
"أَمۡ نَQ<جۡ>عَلُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ كَX<ٱ>لۡمُفۡسِدِینَ فِی ٱلۡأَR<رۡ>ضِ أَمۡ نَQ<جۡ>عَلُ ٱلۡمُتَّقِینَ كَX<ٱ>لۡفُجَّJ<ا>R<ر>ِ A<۝>D<٢٨>",
"كِتَT<ـٰ>بٌ أَN<ن>زَلۡنَT<ـٰ>هُ إِلَیۡكَ مُبَT<ـٰ>R<رَ>كX<ࣱ> لِّیَدَّبَّR<رُ>W<وۤ>X<ا۟> ءَایَT<ـٰ>تِهِT<ۦ> وَلِیَتَذَكَّR<رَ> أُX<و۟>لُوX<ا۟> ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<٢٩>",
"وَوَهَQ<بۡ>نَا لِدَاوُT<ۥ>دَ سُلَیۡمَT<ـٰ>نَۚ نِعۡمَ ٱلۡعَQ<بۡ>دُ إِN<نَّ>هُW<ۥۤ> أَوَّJ<ا>Q<ب>ٌ A<۝>D<٣٠>",
"إِذۡ عُرِضَ عَلَیۡهِ بِX<ٱ>لۡعَشِیِّ ٱX<ل>صَّT<ـٰ>فِنَT<ـٰ>تُ ٱلۡجِیَJ<ا>Q<د>ُ A<۝>D<٣١>",
"فَقَالَ إِN<نِّ>W<یۤ> أَحۡبَQ<بۡ>تُ حُبَّ ٱلۡخَیۡرِ عَN<ن> ذِكۡرِ R<رَ>بِّی حَتَّىٰ تَوَاR<رَ>تۡ بِX<ٱ>لۡحِجَJ<ا>Q<ب>ِ A<۝>D<٣٢>",
"R<رُ>دُّوهَا عَلَیَّۖ فَطَفِقَ مَسۡحَN<ۢ>ا بِX<ٱ>X<ل>سُّوقِ وَX<ٱ>لۡأَعۡنَJ<ا>Q<ق>ِ A<۝>D<٣٣>",
"وَلَقَQ<دۡ> فَتَN<نَّ>ا سُلَیۡمَT<ـٰ>نَ وَأَلۡقَیۡنَا عَلَىٰ كُR<رۡ>سِیِّهِT<ۦ> جَسَدN<ࣰ>ا ثُN<مَّ> أَنَJ<ا>Q<ب>َ A<۝>D<٣٤>",
"قَالَ R<رَ>بِّ ٱغۡفِرۡ لِی وَهَQ<بۡ> لِی مُلۡكX<ࣰ>ا لَّا یَX<ن>N<ۢ>بَغِی لِأَحَدX<ࣲ> N<مِّ>X<ن>N<ۢ> بَعۡدِW<یۤ>ۖ إِN<نَّ>كَ أَN<ن>تَ ٱلۡوَهَّJ<ا>Q<ب>ُ A<۝>D<٣٥>",
"فَسَخَّR<رۡ>نَا لَهُ ٱX<ل>رِّیحَ تَQ<جۡ>رِی بِأَمۡرِهِT<ۦ> R<رُ>خَW<اۤ>ءً حَیۡثُ أَصَJ<ا>Q<ب>َ A<۝>D<٣٦>",
"وَX<ٱ>X<ل>شَّیَT<ـٰ>طِینَ كُلَّ بَN<نَّ>W<اۤ>ءX<ࣲ> N<وَ>غَوَّJ<ا>صࣲ A<۝>D<٣٧>",
"وَءَاخَرِینَ مُقَR<رَّ>نِینَ فِی ٱلۡأَصۡفَJ<ا>Q<د>ِ A<۝>D<٣٨>",
"هَT<ـٰ>ذَا عَطَW<اۤ>ؤُنَا فَX<ٱ>مۡنُنۡ أَوۡ أَمۡسِكۡ بِغَیۡرِ حِسَJ<ا>Q<ب>ࣲ A<۝>D<٣٩>",
"وَإِN<نَّ> لَهُT<ۥ> عِN<ن>دَنَا لَزُلۡفَىٰ وَحُسۡنَ مَءَJ<ا>Q<ب>ࣲ A<۝>D<٤٠>",
"وَX<ٱ>ذۡكُR<رۡ> عَQ<بۡ>دَنَW<اۤ> أَیُّوبَ إِذۡ نَادَىٰ R<رَ>بَّهُW<ۥۤ> أَN<نِّ>ی مَسَّنِیَ ٱX<ل>شَّیۡطَT<ـٰ>نُ بِنُصۡبX<ࣲ> N<وَ>عَذَJ<ا>Q<ب>ٍ A<۝>D<٤١>",
"ٱR<رۡ>كُضۡ بِرِQ<جۡ>لِكَۖ هَT<ـٰ>ذَا مُغۡتَسَلُN<ۢ> بَارِدX<ࣱ> N<وَ>شَR<رَ>J<ا>Q<ب>ࣱ A<۝>D<٤٢>",
"وَوَهَQ<بۡ>نَا لَهُW<ۥۤ> أَهۡلَهُT<ۥ> وَمِثۡلَهُم N<مَّ>عَهُمۡ R<رَ>حۡمَةX<ࣰ> N<مِّ>N<نَّ>ا وَذِكۡR<رَ>ىٰ لِأُX<و۟>لِی ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<٤٣>",
"وَخُذۡ بِیَدِكَ ضِغۡثN<ࣰ>ا فَX<ٱ>ضۡرِب بِّهِT<ۦ> وَلَا تَحۡنَثۡۗ إِN<نَّ>ا وَجَQ<دۡ>نَT<ـٰ>هُ صَابِR<ر>X<ࣰ>اۚ N<نِّ>عۡمَ ٱلۡعَQ<بۡ>دُ إِN<نَّ>هُW<ۥۤ> أَوَّJ<ا>Q<ب>ࣱ A<۝>D<٤٤>",
"وَX<ٱ>ذۡكُR<رۡ> عِبَT<ـٰ>دَنَW<اۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَإِسۡحَT<ـٰ>قَ وَیَعۡقُوبَ أُX<و۟>لِی ٱلۡأَیۡدِی وَX<ٱ>لۡأَQ<بۡ>صَT<ـٰ>R<ر>ِ A<۝>D<٤٥>",
"إِN<نَّ>W<اۤ> أَخۡلَصۡنَT<ـٰ>هُN<م> بِخَالِصَةN<ࣲ> ذِكۡR<رَ>ى ٱX<ل>دَّJ<ا>R<ر>ِ A<۝>D<٤٦>",
"وَإِN<نَّ>هُمۡ عِN<ن>دَنَا لَمِنَ ٱلۡمُصۡطَفَیۡنَ ٱلۡأَخۡیَJ<ا>R<ر>ِ A<۝>D<٤٧>",
"وَX<ٱ>ذۡكُR<رۡ> إِسۡمَT<ـٰ>عِیلَ وَX<ٱ>لۡیَسَعَ وَذَا ٱلۡكِفۡلِۖ وَكُلّX<ࣱ> N<مِّ>نَ ٱلۡأَخۡیَJ<ا>R<ر>ِ A<۝>D<٤٨>",
"هَT<ـٰ>ذَا ذِكۡR<ر>X<ࣱ>ۚ N<وَ>إِN<نَّ> لِلۡمُتَّقِینَ لَحُسۡنَ مَءَJ<ا>Q<ب>ࣲ A<۝>D<٤٩>",
"جَN<نَّ>T<ـٰ>تِ عَQ<دۡ>نX<ࣲ> N<مُّ>فَتَّحَةX<ࣰ> لَّهُمُ ٱلۡأَQ<بۡ>وَT< ٰ>Q<ب>ُ A<۝>D<٥٠>",
"مُتَّكِءِینَ فِیهَا یَQ<دۡ>عُونَ فِیهَا بِفَT<ـٰ>كِهَةN<ࣲ> كَثِیR<رَ>ةX<ࣲ> N<وَ>شَR<رَ>J<ا>Q<ب>ࣲ A<۝>D<٥١>",
"۞ وَعِN<ن>دَهُمۡ قَT<ـٰ>صِR<رَ>T< ٰ>تُ ٱX<ل>طَّR<رۡ>فِ أَتۡR<رَ>J<ا>Q<ب>ٌ A<۝>D<٥٢>",
"هَT<ـٰ>ذَا مَا تُوعَدُونَ لِیَوۡمِ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<٥٣>",
"إِN<نَّ> هَT<ـٰ>ذَا لَرِزۡقُنَا مَا لَهُT<ۥ> مِن N<نَّ>فَJ<ا>Q<د>ٍ A<۝>D<٥٤>",
"هَT<ـٰ>ذَاۚ وَإِN<نَّ> لِX<ل>طَّT<ـٰ>غِینَ لَشَR<رَّ> مَءَJ<ا>Q<ب>ࣲ A<۝>D<٥٥>",
"جَهَN<نَّ>مَ یَصۡلَوۡنَهَا فَبِئۡسَ ٱلۡمِهَJ<ا>Q<د>ُ A<۝>D<٥٦>",
"هَT<ـٰ>ذَا فَلۡیَذُوقُوهُ حَمِیمX<ࣱ> N<وَ>غَسَّJ<ا>Q<ق>ࣱ A<۝>D<٥٧>",
"وَءَاخَR<رُ> مِN<ن> شَكۡلِهِW<ۦۤ> أَزۡوَT< ٰ>Q<ج>ٌ A<۝>D<٥٨>",
"هَT<ـٰ>ذَا فَوۡجX<ࣱ> N<مُّ>Q<قۡ>تَحِمX<ࣱ> N<مَّ>عَكُمۡ لَا مَR<رۡ>حَبَN<ۢ>ا بِهِمۡۚ إِN<نَّ>هُمۡ صَالُوX<ا۟> ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٥٩>",
"قَالُوX<ا۟> بَلۡ أَN<ن>تُمۡ لَا مَR<رۡ>حَبَN<ۢ>ا بِكُمۡۖ أَN<ن>تُمۡ قَدَّمۡتُمُوهُ لَنَاۖ فَبِئۡسَ ٱلۡقَR<رَ>J<ا>R<ر>ُ A<۝>D<٦٠>",
"قَالُوX<ا۟> R<رَ>بَّنَا مَN<ن> قَدَّمَ لَنَا هَT<ـٰ>ذَا فَزِQ<دۡ>هُ عَذَابN<ࣰ>ا ضِعۡفN<ࣰ>ا فِی ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٦١>",
"وَقَالُوX<ا۟> مَا لَنَا لَا نَR<رَ>ىٰ رِجَالN<ࣰ>ا كُN<نَّ>ا نَعُدُّهُم N<مِّ>نَ ٱلۡأَشۡR<رَ>J<ا>R<ر>ِ A<۝>D<٦٢>",
"أَتَّخَذۡنَT<ـٰ>هُمۡ سِخۡرِیًّا أَمۡ زَاغَتۡ عَنۡهُمُ ٱلۡأَQ<بۡ>صَT<ـٰ>R<ر>ُ A<۝>D<٦٣>",
"إِN<نَّ> ذَT< ٰ>لِكَ لَحَقّN<ࣱ> تَخَاصُمُ أَهۡلِ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٦٤>",
"قُلۡ إِN<نَّ>مَW<اۤ> أَنَا۠ مُN<ن>ذِR<ر>X<ࣱ>ۖ N<وَ>مَا مِنۡ إِلَT<ـٰ>هٍ إِلَّا ٱللَّهُ ٱلۡوَT< ٰ>حِدُ ٱلۡقَهَّJ<ا>R<ر>ُ A<۝>D<٦٥>",
"R<رَ>بُّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَا ٱلۡعَزِیزُ ٱلۡغَفَّT<ـٰ>R<ر>ُ A<۝>D<٦٦>",
"قُلۡ هُوَ نَبَؤٌX<ا۟> عَظِJ<ی>مٌ A<۝>D<٦٧>",
"أَN<ن>تُمۡ عَنۡهُ مُعۡرِضُJ<و>نَ A<۝>D<٦٨>",
"مَا كَانَ لِیَ مِنۡ عِلۡمِN<ۭ> بِX<ٱ>لۡمَلَإِ ٱلۡأَعۡلَىW<ٰۤ> إِذۡ یَخۡتَصِمُJ<و>نَ A<۝>D<٦٩>",
"إِX<ن> N<یُ>وحَىW<ٰۤ> إِلَیَّ إِلَّW<اۤ> أَN<نَّ>مَW<اۤ> أَنَا۠ نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<٧٠>",
"إِذۡ قَالَ R<رَ>بُّكَ لِلۡمَلَW<ـٰۤ>ئِكَةِ إِN<نِّ>ی خَT<ـٰ>لِقُN<ۢ> بَشَR<ر>X<ࣰ>ا N<مِّ>N<ن> طِJ<ی>نࣲ A<۝>D<٧١>",
"فَإِذَا سَوَّیۡتُهُT<ۥ> وَنَفَخۡتُ فِیهِ مِX<ن> R<رُّ>وحِی فَقَعُوX<ا۟> لَهُT<ۥ> سَT<ـٰ>جِدِJ<ی>نَ A<۝>D<٧٢>",
"فَسَجَدَ ٱلۡمَلَW<ـٰۤ>ئِكَةُ كُلُّهُمۡ أَQ<جۡ>مَعُJ<و>نَ A<۝>D<٧٣>",
"إِلَّW<اۤ> إِQ<بۡ>لِیسَ ٱسۡتَكۡبَR<رَ> وَكَانَ مِنَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٧٤>",
"قَالَ یَW<ـٰۤ>إِQ<بۡ>لِیسُ مَا مَنَعَكَ أَN<ن> تَسۡجُدَ لِمَا خَلَQ<قۡ>تُ بِیَدَیَّۖ أَسۡتَكۡبَR<رۡ>تَ أَمۡ كُN<ن>تَ مِنَ ٱلۡعَالِJ<ی>نَ A<۝>D<٧٥>",
"قَالَ أَنَا۠ خَیۡR<ر>X<ࣱ> N<مِّ>نۡهُ خَلَQ<قۡ>تَنِی مِن N<نَّ>ارX<ࣲ> N<وَ>خَلَQ<قۡ>تَهُT<ۥ> مِN<ن> طِJ<ی>نࣲ A<۝>D<٧٦>",
"قَالَ فَX<ٱ>خۡR<رُ>Q<جۡ> مِنۡهَا فَإِN<نَّ>كَ R<رَ>جِJ<ی>مࣱ A<۝>D<٧٧>",
"وَإِN<نَّ> عَلَیۡكَ لَعۡنَتِW<یۤ> إِلَىٰ یَوۡمِ ٱX<ل>دِّJ<ی>نِ A<۝>D<٧٨>",
"قَالَ R<رَ>بِّ فَأَN<ن>ظِرۡنِW<یۤ> إِلَىٰ یَوۡمِ یُQ<بۡ>عَثُJ<و>نَ A<۝>D<٧٩>",
"قَالَ فَإِN<نَّ>كَ مِنَ ٱلۡمُN<ن>ظَرِJ<ی>نَ A<۝>D<٨٠>",
"إِلَىٰ یَوۡمِ ٱلۡوَQ<قۡ>تِ ٱلۡمَعۡلُJ<و>مِ A<۝>D<٨١>",
"قَالَ فَبِعِزَّتِكَ لَأُغۡوِیَN<نَّ>هُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٨٢>",
"إِلَّا عِبَادَكَ مِنۡهُمُ ٱلۡمُخۡلَصِJ<ی>نَ A<۝>D<٨٣>",
"قَالَ فَX<ٱ>لۡحَقُّ وَX<ٱ>لۡحَقَّ أَقُJ<و>لُ A<۝>D<٨٤>",
"لَأَمۡلَأَN<نَّ> جَهَN<نَّ>مَ مِN<ن>كَ وَمِN<مَّ>N<ن> تَبِعَكَ مِنۡهُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٨٥>",
"قُلۡ مَW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ مِنۡ أَQ<جۡ>رX<ࣲ> N<وَ>مَW<اۤ> أَنَا۠ مِنَ ٱلۡمُتَكَلِّفِJ<ی>نَ A<۝>D<٨٦>",
"إِنۡ هُوَ إِلَّا ذِكۡR<ر>X<ࣱ> لِّلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٨٧>",
"وَلَتَعۡلَمُN<نَّ> نَبَأَهُT<ۥ> بَعۡدَ حِJ<ی>نِۭ A<۝>D<٨٨>",
"#تَN<ن>زِیلُ ٱلۡكِتَT<ـٰ>بِ مِنَ ٱللَّهِ ٱلۡعَزِیزِ ٱلۡحَكِJ<ی>مِ A<۝>D<١>",
"إِN<نَّ>W<اۤ> أَN<ن>زَلۡنَW<اۤ> إِلَیۡكَ ٱلۡكِتَT<ـٰ>بَ بِX<ٱ>لۡحَقِّ فَX<ٱ>عۡبُدِ ٱللَّهَ مُخۡلِصX<ࣰ>ا لَّهُ ٱX<ل>دِّJ<ی>نَ A<۝>D<٢>",
"أَلَا لِلَّهِ ٱX<ل>دِّینُ ٱلۡخَالِصُۚ وَX<ٱ>لَّذِینَ ٱتَّخَذُوX<ا۟> مِN<ن> دُونِهِW<ۦۤ> أَوۡلِیَW<اۤ>ءَ مَا نَعۡبُدُهُمۡ إِلَّا لِیُقَرِّبُونَW<اۤ> إِلَى ٱللَّهِ زُلۡفَىW<ٰۤ> إِN<نَّ> ٱللَّهَ یَحۡكُمُ بَیۡنَهُمۡ فِی مَا هُمۡ فِیهِ یَخۡتَلِفُونَۗ إِN<نَّ> ٱللَّهَ لَا یَهۡدِی مَنۡ هُوَ كَT<ـٰ>ذِبN<ࣱ> كَفَّJ<ا>R<ر>ࣱ A<۝>D<٣>",
"لَّوۡ أَR<رَ>ادَ ٱللَّهُ أَX<ن> N<یَ>تَّخِذَ وَلَدX<ࣰ>ا لَّX<ٱ>صۡطَفَىٰ مِN<مَّ>ا یَخۡلُقُ مَا یَشَW<اۤ>ءُۚ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ>ۖ هُوَ ٱللَّهُ ٱلۡوَT< ٰ>حِدُ ٱلۡقَهَّJ<ا>R<ر>ُ A<۝>D<٤>",
"خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ بِX<ٱ>لۡحَقِّۖ یُكَوِّR<رُ> ٱلَّیۡلَ عَلَى ٱX<ل>N<نَّ>هَارِ وَیُكَوِّR<رُ> ٱX<ل>N<نَّ>هَاR<رَ> عَلَى ٱلَّیۡلِۖ وَسَخَّR<رَ> ٱX<ل>شَّمۡسَ وَX<ٱ>لۡقَمَR<رَ>ۖ كُلّX<ࣱ> N<یَ>Q<جۡ>رِی لِأَجَلX<ࣲ> N<مُّ>سَN<مّ>ًىۗ أَلَا هُوَ ٱلۡعَزِیزُ ٱلۡغَفَّT<ـٰ>R<ر>ُ A<۝>D<٥>",
"خَلَقَكُم N<مِّ>ن N<نَّ>فۡسX<ࣲ> N<وَ>T< ٰ>حِدَةN<ࣲ> ثُN<مَّ> جَعَلَ مِنۡهَا زَوۡجَهَا وَأَN<ن>زَلَ لَكُم N<مِّ>نَ ٱلۡأَنۡعَT<ـٰ>مِ ثَمَT<ـٰ>نِیَةَ أَزۡوَT< ٰ>جX<ࣲ>ۚ N<یَ>خۡلُقُكُمۡ فِی بُطُونِ أُN<مَّ>هَT<ـٰ>تِكُمۡ خَلۡقX<ࣰ>ا N<مِّ>X<ن>N<ۢ> بَعۡدِ خَلۡقN<ࣲ> فِی ظُلُمَT<ـٰ>تN<ࣲ> ثَلَT<ـٰ>ثN<ࣲ>ۚ ذَT< ٰ>لِكُمُ ٱللَّهُ R<رَ>بُّكُمۡ لَهُ ٱلۡمُلۡكُۖ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ فَأَN<نَّ>ىٰ تُصۡR<رَ>فُJ<و>نَ A<۝>D<٦>",
"إِN<ن> تَكۡفُR<رُ>وX<ا۟> فَإِN<نَّ> ٱللَّهَ غَنِیٌّ عَN<ن>كُمۡۖ وَلَا یَR<رۡ>ضَىٰ لِعِبَادِهِ ٱلۡكُفۡR<رَ>ۖ وَإِN<ن> تَشۡكُR<رُ>وX<ا۟> یَR<رۡ>ضَهُ لَكُمۡۗ وَلَا تَزِR<رُ> وَازِR<رَ>ةX<ࣱ> N<وِ>زۡR<رَ> أُخۡR<رَ>ىٰۚ ثُN<مَّ> إِلَىٰ R<رَ>بِّكُم N<مَّ>R<رۡ>جِعُكُمۡ فَیُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ تَعۡمَلُونَۚ إِN<نَّ>هُT<ۥ> عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٧>",
"۞ وَإِذَا مَسَّ ٱلۡإِN<ن>سَT<ـٰ>نَ ضُR<رّ>N<ࣱ> دَعَا R<رَ>بَّهُT<ۥ> مُنِیبًا إِلَیۡهِ ثُN<مَّ> إِذَا خَوَّلَهُT<ۥ> نِعۡمَةX<ࣰ> N<مِّ>نۡهُ نَسِیَ مَا كَانَ یَQ<دۡ>عُW<وۤ>X<ا۟> إِلَیۡهِ مِN<ن> قَQ<بۡ>لُ وَجَعَلَ لِلَّهِ أَN<ن>دَادX<ࣰ>ا لِّیُضِلَّ عَN<ن> سَبِیلِهِT<ۦ>ۚ قُلۡ تَمَتَّعۡ بِكُفۡرِكَ قَلِیلًا إِN<نَّ>كَ مِنۡ أَصۡحَT<ـٰ>بِ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٨>",
"أَN<مَّ>نۡ هُوَ قَT<ـٰ>نِتٌ ءَانَW<اۤ>ءَ ٱلَّیۡلِ سَاجِدX<ࣰ>ا N<وَ>قَW<اۤ>ئِمX<ࣰ>ا N<یَ>حۡذَR<رُ> ٱلۡءَاخِR<رَ>ةَ وَیَR<رۡ>جُوX<ا۟> R<رَ>حۡمَةَ R<رَ>بِّهِT<ۦ>ۗ قُلۡ هَلۡ یَسۡتَوِی ٱلَّذِینَ یَعۡلَمُونَ وَX<ٱ>لَّذِینَ لَا یَعۡلَمُونَۗ إِN<نَّ>مَا یَتَذَكَّR<رُ> أُX<و۟>لُوX<ا۟> ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<٩>",
"قُلۡ یَT<ـٰ>عِبَادِ ٱلَّذِینَ ءَامَنُوX<ا۟> ٱتَّقُوX<ا۟> R<رَ>بَّكُمۡۚ لِلَّذِینَ أَحۡسَنُوX<ا۟> فِی هَT<ـٰ>ذِهِ ٱX<ل>دُّنۡیَا حَسَنَةX<ࣱ>ۗ N<وَ>أَR<رۡ>ضُ ٱللَّهِ وَT< ٰ>سِعَةٌۗ إِN<نَّ>مَا یُوَفَّى ٱX<ل>صَّT<ـٰ>بِR<رُ>ونَ أَQ<جۡ>R<رَ>هُN<م> بِغَیۡرِ حِسَJ<ا>Q<ب>ࣲ A<۝>D<١٠>",
"قُلۡ إِN<نِّ>W<یۤ> أُمِرۡتُ أَنۡ أَعۡبُدَ ٱللَّهَ مُخۡلِصX<ࣰ>ا لَّهُ ٱX<ل>دِّJ<ی>نَ A<۝>D<١١>",
"وَأُمِرۡتُ لِأَنۡ أَكُونَ أَوَّلَ ٱلۡمُسۡلِمِJ<ی>نَ A<۝>D<١٢>",
"قُلۡ إِN<نِّ>W<یۤ> أَخَافُ إِنۡ عَصَیۡتُ R<رَ>بِّی عَذَابَ یَوۡمٍ عَظِJ<ی>مࣲ A<۝>D<١٣>",
"قُلِ ٱللَّهَ أَعۡبُدُ مُخۡلِصX<ࣰ>ا لَّهُT<ۥ> دِینِی A<۝>D<١٤>",
"فَX<ٱ>عۡبُدُوX<ا۟> مَا شِئۡتُم N<مِّ>N<ن> دُونِهِT<ۦ>ۗ قُلۡ إِN<نَّ> ٱلۡخَT<ـٰ>سِرِینَ ٱلَّذِینَ خَسِR<رُ>W<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ وَأَهۡلِیهِمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۗ أَلَا ذَT< ٰ>لِكَ هُوَ ٱلۡخُسۡR<رَ>انُ ٱلۡمُبِJ<ی>نُ A<۝>D<١٥>",
"لَهُم N<مِّ>N<ن> فَوۡقِهِمۡ ظُلَلX<ࣱ> N<مِّ>نَ ٱX<ل>N<نَّ>ارِ وَمِN<ن> تَحۡتِهِمۡ ظُلَلN<ࣱ>ۚ ذَT< ٰ>لِكَ یُخَوِّفُ ٱللَّهُ بِهِT<ۦ> عِبَادَهُT<ۥ>ۚ یَT<ـٰ>عِبَادِ فَX<ٱ>تَّقُJ<و>نِ A<۝>D<١٦>",
"وَX<ٱ>لَّذِینَ ٱQ<جۡ>تَنَبُوX<ا۟> ٱX<ل>طَّT<ـٰ>غُوتَ أَX<ن> N<یَ>عۡبُدُوهَا وَأَنَابُW<وۤ>X<ا۟> إِلَى ٱللَّهِ لَهُمُ ٱلۡبُشۡR<رَ>ىٰۚ فَبَشِّرۡ عِبَJ<ا>Q<د>ِ A<۝>D<١٧>",
"ٱلَّذِینَ یَسۡتَمِعُونَ ٱلۡقَوۡلَ فَیَتَّبِعُونَ أَحۡسَنَهُW<ۥۤ>ۚ أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ هَدَX<ى>T<ٰ>هُمُ ٱللَّهُۖ وَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمۡ أُX<و۟>لُوX<ا۟> ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<١٨>",
"أَفَمَنۡ حَقَّ عَلَیۡهِ كَلِمَةُ ٱلۡعَذَابِ أَفَأَN<ن>تَ تُN<ن>قِذُ مَN<ن> فِی ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<١٩>",
"لَT<ـٰ>كِنِ ٱلَّذِینَ ٱتَّقَوۡX<ا۟> R<رَ>بَّهُمۡ لَهُمۡ غُR<رَ>فX<ࣱ> N<مِّ>N<ن> فَوۡقِهَا غُR<رَ>فX<ࣱ> N<مَّ>Q<بۡ>نِیَّةN<ࣱ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ>ۖ وَعۡدَ ٱللَّهِ لَا یُخۡلِفُ ٱللَّهُ ٱلۡمِیعَJ<ا>Q<د>َ A<۝>D<٢٠>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ أَN<ن>زَلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءN<ࣰ> فَسَلَكَهُT<ۥ> یَنَT<ـٰ>بِیعَ فِی ٱلۡأَR<رۡ>ضِ ثُN<مَّ> یُخۡرِجُ بِهِT<ۦ> زَR<رۡ>عX<ࣰ>ا N<مُّ>خۡتَلِفًا أَلۡوَT< ٰ>نُهُT<ۥ> ثُN<مَّ> یَهِیجُ فَتَR<رَ>X<ى>T<ٰ>هُ مُصۡفَR<رّ>N<ࣰ>ا ثُN<مَّ> یَQ<جۡ>عَلُهُT<ۥ> حُطَT<ـٰ>مًاۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَذِكۡR<رَ>ىٰ لِأُX<و۟>لِی ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<٢١>",
"أَفَمَN<ن> شَR<رَ>حَ ٱللَّهُ صَQ<دۡ>R<رَ>هُT<ۥ> لِلۡإِسۡلَT<ـٰ>مِ فَهُوَ عَلَىٰ نُورX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ>ۚ فَوَیۡلX<ࣱ> لِّلۡقَT<ـٰ>سِیَةِ قُلُوبُهُم N<مِّ>N<ن> ذِكۡرِ ٱللَّهِۚ أُX<و۟>لَW<ـٰۤ>ئِكَ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٢٢>",
"ٱللَّهُ نَزَّلَ أَحۡسَنَ ٱلۡحَدِیثِ كِتَT<ـٰ>بX<ࣰ>ا N<مُّ>تَشَT<ـٰ>بِهX<ࣰ>ا N<مَّ>ثَانِیَ تَQ<قۡ>شَعِR<رُّ> مِنۡهُ جُلُودُ ٱلَّذِینَ یَخۡشَوۡنَ R<رَ>بَّهُمۡ ثُN<مَّ> تَلِینُ جُلُودُهُمۡ وَقُلُوبُهُمۡ إِلَىٰ ذِكۡرِ ٱللَّهِۚ ذَT< ٰ>لِكَ هُدَى ٱللَّهِ یَهۡدِی بِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَمَX<ن> N<یُ>ضۡلِلِ ٱللَّهُ فَمَا لَهُT<ۥ> مِنۡ هَJ<ا>Q<د>ٍ A<۝>D<٢٣>",
"أَفَمَX<ن> N<یَ>تَّقِی بِوَQ<جۡ>هِهِT<ۦ> سُW<وۤ>ءَ ٱلۡعَذَابِ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۚ وَقِیلَ لِX<ل>ظَّT<ـٰ>لِمِینَ ذُوقُوX<ا۟> مَا كُN<ن>تُمۡ تَكۡسِبُJ<و>نَ A<۝>D<٢٤>",
"كَذَّبَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ فَأَتَX<ى>T<ٰ>هُمُ ٱلۡعَذَابُ مِنۡ حَیۡثُ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٢٥>",
"فَأَذَاقَهُمُ ٱللَّهُ ٱلۡخِزۡیَ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۖ وَلَعَذَابُ ٱلۡءَاخِR<رَ>ةِ أَكۡبَR<رُ>ۚ لَوۡ كَانُوX<ا۟> یَعۡلَمُJ<و>نَ A<۝>D<٢٦>",
"وَلَقَQ<دۡ> ضَR<رَ>Q<بۡ>نَا لِX<ل>N<نَّ>اسِ فِی هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانِ مِN<ن> كُلِّ مَثَلX<ࣲ> لَّعَلَّهُمۡ یَتَذَكَّR<رُ>J<و>نَ A<۝>D<٢٧>",
"قُR<رۡ>ءَانًا عَR<رَ>بِیًّا غَیۡR<رَ> ذِی عِوَجX<ࣲ> لَّعَلَّهُمۡ یَتَّقُJ<و>نَ A<۝>D<٢٨>",
"ضَR<رَ>بَ ٱللَّهُ مَثَلX<ࣰ>ا R<رَّ>جُلN<ࣰ>ا فِیهِ شُR<رَ>كَW<اۤ>ءُ مُتَشَT<ـٰ>كِسُونَ وَR<رَ>جُلN<ࣰ>ا سَلَمX<ࣰ>ا لِّR<رَ>جُلٍ هَلۡ یَسۡتَوِیَانِ مَثَلًاۚ ٱلۡحَمۡدُ لِلَّهِۚ بَلۡ أَكۡثَR<رُ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٢٩>",
"إِN<نَّ>كَ مَیِّتX<ࣱ> N<وَ>إِN<نَّ>هُم N<مَّ>یِّتُJ<و>نَ A<۝>D<٣٠>",
"ثُN<مَّ> إِN<نَّ>كُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ عِN<ن>دَ R<رَ>بِّكُمۡ تَخۡتَصِمُJ<و>نَ A<۝>D<٣١>",
"۞ فَمَنۡ أَظۡلَمُ مِN<مَّ>N<ن> كَذَبَ عَلَى ٱللَّهِ وَكَذَّبَ بِX<ٱ>X<ل>صِّQ<دۡ>قِ إِذۡ جَW<اۤ>ءَهُW<ۥۤ>ۚ أَلَیۡسَ فِی جَهَN<نَّ>مَ مَثۡوX<ࣰ>ى لِّلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٣٢>",
"وَX<ٱ>لَّذِی جَW<اۤ>ءَ بِX<ٱ>X<ل>صِّQ<دۡ>قِ وَصَدَّقَ بِهِW<ۦۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُتَّقُJ<و>نَ A<۝>D<٣٣>",
"لَهُم N<مَّ>ا یَشَW<اۤ>ءُونَ عِN<ن>دَ R<رَ>بِّهِمۡۚ ذَT< ٰ>لِكَ جَزَW<اۤ>ءُ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٣٤>",
"لِیُكَفِّR<رَ> ٱللَّهُ عَنۡهُمۡ أَسۡوَأَ ٱلَّذِی عَمِلُوX<ا۟> وَیَQ<جۡ>زِیَهُمۡ أَQ<جۡ>R<رَ>هُN<م> بِأَحۡسَنِ ٱلَّذِی كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٣٥>",
"أَلَیۡسَ ٱللَّهُ بِكَافٍ عَQ<بۡ>دَهُT<ۥ>ۖ وَیُخَوِّفُونَكَ بِX<ٱ>لَّذِینَ مِN<ن> دُونِهِT<ۦ>ۚ وَمَX<ن> N<یُ>ضۡلِلِ ٱللَّهُ فَمَا لَهُT<ۥ> مِنۡ هَJ<ا>Q<د>ࣲ A<۝>D<٣٦>",
"وَمَX<ن> N<یَ>هۡدِ ٱللَّهُ فَمَا لَهُT<ۥ> مِX<ن> N<مُّ>ضِلٍّۗ أَلَیۡسَ ٱللَّهُ بِعَزِیزN<ࣲ> ذِی ٱN<ن>تِقَJ<ا>مࣲ A<۝>D<٣٧>",
"وَلَئِN<ن> سَأَلۡتَهُم N<مَّ>نۡ خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ لَیَقُولُN<نَّ> ٱللَّهُۚ قُلۡ أَفَR<رَ>ءَیۡتُم N<مَّ>ا تَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ إِنۡ أَR<رَ>ادَنِیَ ٱللَّهُ بِضُرٍّ هَلۡ هُN<نَّ> كَT<ـٰ>شِفَT<ـٰ>تُ ضُرِّهِW<ۦۤ> أَوۡ أَR<رَ>ادَنِی بِR<رَ>حۡمَةٍ هَلۡ هُN<نَّ> مُمۡسِكَT<ـٰ>تُ R<رَ>حۡمَتِهِT<ۦ>ۚ قُلۡ حَسۡبِیَ ٱللَّهُۖ عَلَیۡهِ یَتَوَكَّلُ ٱلۡمُتَوَكِّلُJ<و>نَ A<۝>D<٣٨>",
"قُلۡ یَT<ـٰ>قَوۡمِ ٱعۡمَلُوX<ا۟> عَلَىٰ مَكَانَتِكُمۡ إِN<نِّ>ی عَT<ـٰ>مِلN<ࣱ>ۖ فَسَوۡفَ تَعۡلَمُJ<و>نَ A<۝>D<٣٩>",
"مَX<ن> N<یَ>أۡتِیهِ عَذَابX<ࣱ> N<یُ>خۡزِیهِ وَیَحِلُّ عَلَیۡهِ عَذَابX<ࣱ> N<مُّ>قِJ<ی>مٌ A<۝>D<٤٠>",
"إِN<نَّ>W<اۤ> أَN<ن>زَلۡنَا عَلَیۡكَ ٱلۡكِتَT<ـٰ>بَ لِX<ل>N<نَّ>اسِ بِX<ٱ>لۡحَقِّۖ فَمَنِ ٱهۡتَدَىٰ فَلِنَفۡسِهِT<ۦ>ۖ وَمَN<ن> ضَلَّ فَإِN<نَّ>مَا یَضِلُّ عَلَیۡهَاۖ وَمَW<اۤ> أَN<ن>تَ عَلَیۡهِN<م> بِوَكِJ<ی>لٍ A<۝>D<٤١>",
"ٱللَّهُ یَتَوَفَّى ٱلۡأَN<ن>فُسَ حِینَ مَوۡتِهَا وَX<ٱ>لَّتِی لَمۡ تَمُتۡ فِی مَنَامِهَاۖ فَیُمۡسِكُ ٱلَّتِی قَضَىٰ عَلَیۡهَا ٱلۡمَوۡتَ وَیُR<رۡ>سِلُ ٱلۡأُخۡR<رَ>ىW<ٰۤ> إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>ًىۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>تَفَكَّR<رُ>J<و>نَ A<۝>D<٤٢>",
"أَمِ ٱتَّخَذُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ شُفَعَW<اۤ>ءَۚ قُلۡ أَوَ لَوۡ كَانُوX<ا۟> لَا یَمۡلِكُونَ شَیۡءX<ࣰ>ا N<وَ>لَا یَعۡقِلُJ<و>نَ A<۝>D<٤٣>",
"قُل لِّلَّهِ ٱX<ل>شَّفَT<ـٰ>عَةُ جَمِیعX<ࣰ>اۖ لَّهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ ثُN<مَّ> إِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٤٤>",
"وَإِذَا ذُكِR<رَ> ٱللَّهُ وَحۡدَهُ ٱشۡمَأَزَّتۡ قُلُوبُ ٱلَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِۖ وَإِذَا ذُكِR<رَ> ٱلَّذِینَ مِN<ن> دُونِهِW<ۦۤ> إِذَا هُمۡ یَسۡتَQ<بۡ>شِR<رُ>J<و>نَ A<۝>D<٤٥>",
"قُلِ ٱللَّهُN<مَّ> فَاطِR<رَ> ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ عَT<ـٰ>لِمَ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِ أَN<ن>تَ تَحۡكُمُ بَیۡنَ عِبَادِكَ فِی مَا كَانُوX<ا۟> فِیهِ یَخۡتَلِفُJ<و>نَ A<۝>D<٤٦>",
"وَلَوۡ أَN<نَّ> لِلَّذِینَ ظَلَمُوX<ا۟> مَا فِی ٱلۡأَR<رۡ>ضِ جَمِیعX<ࣰ>ا N<وَ>مِثۡلَهُT<ۥ> مَعَهُT<ۥ> لَX<ٱ>فۡتَدَوۡX<ا۟> بِهِT<ۦ> مِN<ن> سُW<وۤ>ءِ ٱلۡعَذَابِ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۚ وَبَدَا لَهُم N<مِّ>نَ ٱللَّهِ مَا لَمۡ یَكُونُوX<ا۟> یَحۡتَسِبُJ<و>نَ A<۝>D<٤٧>",
"وَبَدَا لَهُمۡ سَیِّءَاتُ مَا كَسَبُوX<ا۟> وَحَاقَ بِهِم N<مَّ>ا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٤٨>",
"فَإِذَا مَسَّ ٱلۡإِN<ن>سَT<ـٰ>نَ ضُR<رّ>N<ࣱ> دَعَانَا ثُN<مَّ> إِذَا خَوَّلۡنَT<ـٰ>هُ نِعۡمَةX<ࣰ> N<مِّ>N<نَّ>ا قَالَ إِN<نَّ>مَW<اۤ> أُوتِیتُهُT<ۥ> عَلَىٰ عِلۡمِN<ۭ>ۚ بَلۡ هِیَ فِتۡنَةX<ࣱ> N<وَ>لَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٤٩>",
"قَQ<دۡ> قَالَهَا ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ فَمَW<اۤ> أَغۡنَىٰ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<٥٠>",
"فَأَصَابَهُمۡ سَیِّءَاتُ مَا كَسَبُوX<ا۟>ۚ وَX<ٱ>لَّذِینَ ظَلَمُوX<ا۟> مِنۡ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ سَیُصِیبُهُمۡ سَیِّءَاتُ مَا كَسَبُوX<ا۟> وَمَا هُN<م> بِمُعۡجِزِJ<ی>نَ A<۝>D<٥١>",
"أَوَ لَمۡ یَعۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ یَQ<بۡ>سُطُ ٱX<ل>رِّزۡقَ لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیَQ<قۡ>دِR<رُ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یُ>ؤۡمِنُJ<و>نَ A<۝>D<٥٢>",
"۞ قُلۡ یَT<ـٰ>عِبَادِیَ ٱلَّذِینَ أَسۡR<رَ>فُوX<ا۟> عَلَىW<ٰۤ> أَN<ن>فُسِهِمۡ لَا تَQ<قۡ>نَطُوX<ا۟> مِX<ن> R<رَّ>حۡمَةِ ٱللَّهِۚ إِN<نَّ> ٱللَّهَ یَغۡفِR<رُ> ٱX<ل>ذُّنُوبَ جَمِیعًاۚ إِN<نَّ>هُT<ۥ> هُوَ ٱلۡغَفُوR<رُ> ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٥٣>",
"وَأَنِیبُW<وۤ>X<ا۟> إِلَىٰ R<رَ>بِّكُمۡ وَأَسۡلِمُوX<ا۟> لَهُT<ۥ> مِN<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>أۡتِیَكُمُ ٱلۡعَذَابُ ثُN<مَّ> لَا تُN<ن>صَR<رُ>J<و>نَ A<۝>D<٥٤>",
"وَX<ٱ>تَّبِعُW<وۤ>X<ا۟> أَحۡسَنَ مَW<اۤ> أُN<ن>زِلَ إِلَیۡكُم N<مِّ>X<ن> R<رَّ>بِّكُم N<مِّ>N<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>أۡتِیَكُمُ ٱلۡعَذَابُ بَغۡتَةX<ࣰ> N<وَ>أَN<ن>تُمۡ لَا تَشۡعُR<رُ>J<و>نَ A<۝>D<٥٥>",
"أَN<ن> تَقُولَ نَفۡسX<ࣱ> N<یَ>T<ـٰ>حَسۡR<رَ>تَىٰ عَلَىٰ مَا فَR<رَّ>طتُ فِی جَX<ن>N<ۢ>بِ ٱللَّهِ وَإِN<ن> كُN<ن>تُ لَمِنَ ٱX<ل>سَّT<ـٰ>خِرِJ<ی>نَ A<۝>D<٥٦>",
"أَوۡ تَقُولَ لَوۡ أَN<نَّ> ٱللَّهَ هَدَX<ى>T<ٰ>نِی لَكُN<ن>تُ مِنَ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٥٧>",
"أَوۡ تَقُولَ حِینَ تَR<رَ>ى ٱلۡعَذَابَ لَوۡ أَN<نَّ> لِی كَR<رَّ>ةN<ࣰ> فَأَكُونَ مِنَ ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٥٨>",
"بَلَىٰ قَQ<دۡ> جَW<اۤ>ءَتۡكَ ءَایَT<ـٰ>تِی فَكَذَّQ<بۡ>تَ بِهَا وَX<ٱ>سۡتَكۡبَR<رۡ>تَ وَكُN<ن>تَ مِنَ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٥٩>",
"وَیَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ تَR<رَ>ى ٱلَّذِینَ كَذَبُوX<ا۟> عَلَى ٱللَّهِ وُجُوهُهُم N<مُّ>سۡوَدَّةٌۚ أَلَیۡسَ فِی جَهَN<نَّ>مَ مَثۡوX<ࣰ>ى لِّلۡمُتَكَبِّرِJ<ی>نَ A<۝>D<٦٠>",
"وَیُنَجِّی ٱللَّهُ ٱلَّذِینَ ٱتَّقَوۡX<ا۟> بِمَفَازَتِهِمۡ لَا یَمَسُّهُمُ ٱX<ل>سُّW<وۤ>ءُ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<٦١>",
"ٱللَّهُ خَT<ـٰ>لِقُ كُلِّ شَیۡءX<ࣲ>ۖ N<وَ>هُوَ عَلَىٰ كُلِّ شَیۡءX<ࣲ> N<وَ>كِJ<ی>لࣱ A<۝>D<٦٢>",
"لَّهُT<ۥ> مَقَالِیدُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۗ وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِ ٱللَّهِ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<٦٣>",
"قُلۡ أَفَغَیۡR<رَ> ٱللَّهِ تَأۡمُR<رُ>L<وۤ>N<نِّ>W<یۤ> أَعۡبُدُ أَیُّهَا ٱلۡجَT<ـٰ>هِلُJ<و>نَ A<۝>D<٦٤>",
"وَلَقَQ<دۡ> أُوحِیَ إِلَیۡكَ وَإِلَى ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِكَ لَئِنۡ أَشۡR<رَ>كۡتَ لَیَحۡبَطَN<نَّ> عَمَلُكَ وَلَتَكُونَN<نَّ> مِنَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٦٥>",
"بَلِ ٱللَّهَ فَX<ٱ>عۡبُQ<دۡ> وَكُX<ن> N<مِّ>نَ ٱX<ل>شَّT<ـٰ>كِرِJ<ی>نَ A<۝>D<٦٦>",
"وَمَا قَدَR<رُ>وX<ا۟> ٱللَّهَ حَقَّ قَQ<دۡ>رِهِT<ۦ> وَX<ٱ>لۡأَR<رۡ>ضُ جَمِیعN<ࣰ>ا قَQ<بۡ>ضَتُهُT<ۥ> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ وَX<ٱ>X<ل>سَّمَT<ـٰ>وَT< ٰ>تُ مَQ<طۡ>وِیَّT<ـٰ>تُN<ۢ> بِیَمِینِهِT<ۦ>ۚ سُQ<بۡ>حَT<ـٰ>نَهُT<ۥ> وَتَعَT<ـٰ>لَىٰ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٦٧>",
"وَنُفِخَ فِی ٱX<ل>صُّورِ فَصَعِقَ مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَN<ن> فِی ٱلۡأَR<رۡ>ضِ إِلَّا مَN<ن> شَW<اۤ>ءَ ٱللَّهُۖ ثُN<مَّ> نُفِخَ فِیهِ أُخۡR<رَ>ىٰ فَإِذَا هُمۡ قِیَامX<ࣱ> N<یَ>N<ن>ظُR<رُ>J<و>نَ A<۝>D<٦٨>",
"وَأَشۡR<رَ>قَتِ ٱلۡأَR<رۡ>ضُ بِنُورِ R<رَ>بِّهَا وَوُضِعَ ٱلۡكِتَT<ـٰ>بُ وَجِX<ا۟>W<یۤ>ءَ بِX<ٱ>X<ل>N<نَّ>بِیِّT<ـۧ>نَ وَX<ٱ>X<ل>شُّهَدَW<اۤ>ءِ وَقُضِیَ بَیۡنَهُN<م> بِX<ٱ>لۡحَقِّ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<٦٩>",
"وَوُفِّیَتۡ كُلُّ نَفۡسX<ࣲ> N<مَّ>ا عَمِلَتۡ وَهُوَ أَعۡلَمُ بِمَا یَفۡعَلُJ<و>نَ A<۝>D<٧٠>",
"وَسِیقَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> إِلَىٰ جَهَN<نَّ>مَ زُمَR<ر>ًاۖ حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءُوهَا فُتِحَتۡ أَQ<بۡ>وَT< ٰ>بُهَا وَقَالَ لَهُمۡ خَزَنَتُهَW<اۤ> أَلَمۡ یَأۡتِكُمۡ R<رُ>سُلX<ࣱ> N<مِّ>N<ن>كُمۡ یَتۡلُونَ عَلَیۡكُمۡ ءَایَT<ـٰ>تِ R<رَ>بِّكُمۡ وَیُN<ن>ذِR<رُ>ونَكُمۡ لِقَW<اۤ>ءَ یَوۡمِكُمۡ هَT<ـٰ>ذَاۚ قَالُوX<ا۟> بَلَىٰ وَلَT<ـٰ>كِنۡ حَقَّتۡ كَلِمَةُ ٱلۡعَذَابِ عَلَى ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٧١>",
"قِیلَ ٱQ<دۡ>خُلُW<وۤ>X<ا۟> أَQ<بۡ>وَT< ٰ>بَ جَهَN<نَّ>مَ خَT<ـٰ>لِدِینَ فِیهَاۖ فَبِئۡسَ مَثۡوَى ٱلۡمُتَكَبِّرِJ<ی>نَ A<۝>D<٧٢>",
"وَسِیقَ ٱلَّذِینَ ٱتَّقَوۡX<ا۟> R<رَ>بَّهُمۡ إِلَى ٱلۡجَN<نَّ>ةِ زُمَR<ر>ًاۖ حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءُوهَا وَفُتِحَتۡ أَQ<بۡ>وَT< ٰ>بُهَا وَقَالَ لَهُمۡ خَزَنَتُهَا سَلَT<ـٰ>مٌ عَلَیۡكُمۡ طِQ<بۡ>تُمۡ فَX<ٱ>Q<دۡ>خُلُوهَا خَT<ـٰ>لِدِJ<ی>نَ A<۝>D<٧٣>",
"وَقَالُوX<ا۟> ٱلۡحَمۡدُ لِلَّهِ ٱلَّذِی صَدَقَنَا وَعۡدَهُT<ۥ> وَأَوۡR<رَ>ثَنَا ٱلۡأَR<رۡ>ضَ نَتَبَوَّأُ مِنَ ٱلۡجَN<نَّ>ةِ حَیۡثُ نَشَW<اۤ>ءُۖ فَنِعۡمَ أَQ<جۡ>R<رُ> ٱلۡعَT<ـٰ>مِلِJ<ی>نَ A<۝>D<٧٤>",
"وَتَR<رَ>ى ٱلۡمَلَW<ـٰۤ>ئِكَةَ حَL<اۤ>فِّینَ مِنۡ حَوۡلِ ٱلۡعَR<رۡ>شِ یُسَبِّحُونَ بِحَمۡدِ R<رَ>بِّهِمۡۚ وَقُضِیَ بَیۡنَهُN<م> بِX<ٱ>لۡحَقِّۚ وَقِیلَ ٱلۡحَمۡدُ لِلَّهِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٧٥>",
"#۞ حL<مۤ> A<۝>D<١>",
"تَN<ن>زِیلُ ٱلۡكِتَT<ـٰ>بِ مِنَ ٱللَّهِ ٱلۡعَزِیزِ ٱلۡعَلِJ<ی>مِ A<۝>D<٢>",
"غَافِرِ ٱX<ل>ذَّX<ن>N<ۢ>بِ وَقَابِلِ ٱX<ل>تَّوۡبِ شَدِیدِ ٱلۡعِقَابِ ذِی ٱX<ل>طَّوۡلِۖ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ إِلَیۡهِ ٱلۡمَصِJ<ی>رُ A<۝>D<٣>",
"مَا یُجَT<ـٰ>دِلُ فِW<یۤ> ءَایَT<ـٰ>تِ ٱللَّهِ إِلَّا ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فَلَا یَغۡR<رُ>R<رۡ>كَ تَقَلُّبُهُمۡ فِی ٱلۡبِلَT<ـٰ>Q<د>ِ A<۝>D<٤>",
"كَذَّبَتۡ قَQ<بۡ>لَهُمۡ قَوۡمُ نُوحX<ࣲ> N<وَ>X<ٱ>لۡأَحۡزَابُ مِX<ن>N<ۢ> بَعۡدِهِمۡۖ وَهَN<مَّ>تۡ كُلُّ أُN<مَّ>ةِN<ۭ> بِR<رَ>سُولِهِمۡ لِیَأۡخُذُوهُۖ وَجَT<ـٰ>دَلُوX<ا۟> بِX<ٱ>لۡبَT<ـٰ>طِلِ لِیُQ<دۡ>حِضُوX<ا۟> بِهِ ٱلۡحَقَّ فَأَخَذۡتُهُمۡۖ فَكَیۡفَ كَانَ عِقَJ<ا>Q<ب>ِ A<۝>D<٥>",
"وَكَذَT< ٰ>لِكَ حَقَّتۡ كَلِمَتُ R<رَ>بِّكَ عَلَى ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَN<نَّ>هُمۡ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٦>",
"ٱلَّذِینَ یَحۡمِلُونَ ٱلۡعَR<رۡ>شَ وَمَنۡ حَوۡلَهُT<ۥ> یُسَبِّحُونَ بِحَمۡدِ R<رَ>بِّهِمۡ وَیُؤۡمِنُونَ بِهِT<ۦ> وَیَسۡتَغۡفِR<رُ>ونَ لِلَّذِینَ ءَامَنُوX<ا۟>ۖ R<رَ>بَّنَا وَسِعۡتَ كُلَّ شَیۡءX<ࣲ> R<رَّ>حۡمَةX<ࣰ> N<وَ>عِلۡمN<ࣰ>ا فَX<ٱ>غۡفِرۡ لِلَّذِینَ تَابُوX<ا۟> وَX<ٱ>تَّبَعُوX<ا۟> سَبِیلَكَ وَقِهِمۡ عَذَابَ ٱلۡجَحِJ<ی>مِ A<۝>D<٧>",
"R<رَ>بَّنَا وَأَQ<دۡ>خِلۡهُمۡ جَN<نَّ>T<ـٰ>تِ عَQ<دۡ>نٍ ٱلَّتِی وَعَX<د>تَّهُمۡ وَمَN<ن> صَلَحَ مِنۡ ءَابَW<اۤ>ئِهِمۡ وَأَزۡوَT< ٰ>جِهِمۡ وَذُرِّیَّT<ـٰ>تِهِمۡۚ إِN<نَّ>كَ أَN<ن>تَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٨>",
"وَقِهِمُ ٱX<ل>سَّیِّءَاتِۚ وَمَN<ن> تَقِ ٱX<ل>سَّیِّءَاتِ یَوۡمَئِذN<ࣲ> فَقَQ<دۡ> R<رَ>حِمۡتَهُT<ۥ>ۚ وَذَT< ٰ>لِكَ هُوَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<٩>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> یُنَادَوۡنَ لَمَQ<قۡ>تُ ٱللَّهِ أَكۡبَR<رُ> مِX<ن> N<مَّ>Q<قۡ>تِكُمۡ أَN<ن>فُسَكُمۡ إِذۡ تُQ<دۡ>عَوۡنَ إِلَى ٱلۡإِیمَT<ـٰ>نِ فَتَكۡفُR<رُ>J<و>نَ A<۝>D<١٠>",
"قَالُوX<ا۟> R<رَ>بَّنَW<اۤ> أَمَتَّنَا ٱثۡنَتَیۡنِ وَأَحۡیَیۡتَنَا ٱثۡنَتَیۡنِ فَX<ٱ>عۡتَR<رَ>فۡنَا بِذُنُوبِنَا فَهَلۡ إِلَىٰ خُR<رُ>وجX<ࣲ> N<مِّ>N<ن> سَبِJ<ی>لࣲ A<۝>D<١١>",
"ذَT< ٰ>لِكُN<م> بِأَN<نَّ>هُW<ۥۤ> إِذَا دُعِیَ ٱللَّهُ وَحۡدَهُT<ۥ> كَفَR<رۡ>تُمۡ وَإِX<ن> N<یُ>شۡR<رَ>كۡ بِهِT<ۦ> تُؤۡمِنُوX<ا۟>ۚ فَX<ٱ>لۡحُكۡمُ لِلَّهِ ٱلۡعَلِیِّ ٱلۡكَبِJ<ی>رِ A<۝>D<١٢>",
"هُوَ ٱلَّذِی یُرِیكُمۡ ءَایَT<ـٰ>تِهِT<ۦ> وَیُنَزِّلُ لَكُم N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ رِزۡقX<ࣰ>اۚ N<وَ>مَا یَتَذَكَّR<رُ> إِلَّا مَX<ن> N<یُ>نِJ<ی>Q<ب>ُ A<۝>D<١٣>",
"فَX<ٱ>Q<دۡ>عُوX<ا۟> ٱللَّهَ مُخۡلِصِینَ لَهُ ٱX<ل>دِّینَ وَلَوۡ كَرِهَ ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<١٤>",
"R<رَ>فِیعُ ٱX<ل>دَّR<رَ>جَT<ـٰ>تِ ذُو ٱلۡعَR<رۡ>شِ یُلۡقِی ٱX<ل>R<رُّ>وحَ مِنۡ أَمۡرِهِT<ۦ> عَلَىٰ مَX<ن> N<یَ>شَW<اۤ>ءُ مِنۡ عِبَادِهِT<ۦ> لِیُN<ن>ذِR<رَ> یَوۡمَ ٱX<ل>تَّلَJ<ا>Q<ق>ِ A<۝>D<١٥>",
"یَوۡمَ هُN<م> بَT<ـٰ>رِزُونَۖ لَا یَخۡفَىٰ عَلَى ٱللَّهِ مِنۡهُمۡ شَیۡءX<ࣱ>ۚ لِّمَنِ ٱلۡمُلۡكُ ٱلۡیَوۡمَۖ لِلَّهِ ٱلۡوَT< ٰ>حِدِ ٱلۡقَهَّJ<ا>R<ر>ِ A<۝>D<١٦>",
"ٱلۡیَوۡمَ تُQ<جۡ>زَىٰ كُلُّ نَفۡسِN<ۭ> بِمَا كَسَبَتۡۚ لَا ظُلۡمَ ٱلۡیَوۡمَۚ إِN<نَّ> ٱللَّهَ سَرِیعُ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<١٧>",
"وَأَN<ن>ذِرۡهُمۡ یَوۡمَ ٱلۡءَازِفَةِ إِذِ ٱلۡقُلُوبُ لَدَى ٱلۡحَنَاجِرِ كَT<ـٰ>ظِمِینَۚ مَا لِX<ل>ظَّT<ـٰ>لِمِینَ مِنۡ حَمِیمX<ࣲ> N<وَ>لَا شَفِیعX<ࣲ> N<یُ>طَJ<ا>عُ A<۝>D<١٨>",
"یَعۡلَمُ خَW<اۤ>ئِنَةَ ٱلۡأَعۡیُنِ وَمَا تُخۡفِی ٱX<ل>صُّدُJ<و>R<ر>ُ A<۝>D<١٩>",
"وَٱللَّهُ یَQ<قۡ>ضِی بِX<ٱ>لۡحَقِّۖ وَX<ٱ>لَّذِینَ یَQ<دۡ>عُونَ مِN<ن> دُونِهِT<ۦ> لَا یَQ<قۡ>ضُونَ بِشَیۡءٍۗ إِN<نَّ> ٱللَّهَ هُوَ ٱX<ل>سَّمِیعُ ٱلۡبَصِJ<ی>رُ A<۝>D<٢٠>",
"۞ أَوَ لَمۡ یَسِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَیَN<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلَّذِینَ كَانُوX<ا۟> مِN<ن> قَQ<بۡ>لِهِمۡۚ كَانُوX<ا۟> هُمۡ أَشَدَّ مِنۡهُمۡ قُوَّةX<ࣰ> N<وَ>ءَاثَاR<ر>N<ࣰ>ا فِی ٱلۡأَR<رۡ>ضِ فَأَخَذَهُمُ ٱللَّهُ بِذُنُوبِهِمۡ وَمَا كَانَ لَهُم N<مِّ>نَ ٱللَّهِ مِX<ن> N<وَ>J<ا>Q<ق>ࣲ A<۝>D<٢١>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ كَانَت تَّأۡتِیهِمۡ R<رُ>سُلُهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَكَفَR<رُ>وX<ا۟> فَأَخَذَهُمُ ٱللَّهُۚ إِN<نَّ>هُT<ۥ> قَوِیّN<ࣱ> شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<٢٢>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا مُوسَىٰ بِءَایَT<ـٰ>تِنَا وَسُلۡطَT<ـٰ>نX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٢٣>",
"إِلَىٰ فِرۡعَوۡنَ وَهَT<ـٰ>مَT<ـٰ>نَ وَقَT<ـٰ>R<رُ>ونَ فَقَالُوX<ا۟> سَT<ـٰ>حِR<ر>N<ࣱ> كَذَّJ<ا>Q<ب>ࣱ A<۝>D<٢٤>",
"فَلَN<مَّ>ا جَW<اۤ>ءَهُN<م> بِX<ٱ>لۡحَقِّ مِنۡ عِN<ن>دِنَا قَالُوX<ا۟> ٱQ<قۡ>تُلُW<وۤ>X<ا۟> أَQ<بۡ>نَW<اۤ>ءَ ٱلَّذِینَ ءَامَنُوX<ا۟> مَعَهُT<ۥ> وَX<ٱ>سۡتَحۡیُوX<ا۟> نِسَW<اۤ>ءَهُمۡۚ وَمَا كَیۡدُ ٱلۡكَT<ـٰ>فِرِینَ إِلَّا فِی ضَلَT<ـٰ>لࣲ A<۝>D<٢٥>",
"وَقَالَ فِرۡعَوۡنُ ذَR<رُ>ونِW<یۤ> أَQ<قۡ>تُلۡ مُوسَىٰ وَلۡیَQ<دۡ>عُ R<رَ>بَّهُW<ۥۤ>ۖ إِN<نِّ>W<یۤ> أَخَافُ أَX<ن> N<یُ>بَدِّلَ دِینَكُمۡ أَوۡ أَX<ن> N<یُ>ظۡهِR<رَ> فِی ٱلۡأَR<رۡ>ضِ ٱلۡفَسَJ<ا>Q<د>َ A<۝>D<٢٦>",
"وَقَالَ مُوسَىW<ٰۤ> إِN<نِّ>ی عُذۡتُ بِR<رَ>بِّی وَR<رَ>بِّكُم N<مِّ>N<ن> كُلِّ مُتَكَبِّرX<ࣲ> لَّا یُؤۡمِنُ بِیَوۡمِ ٱلۡحِسَJ<ا>Q<ب>ِ A<۝>D<٢٧>",
"وَقَالَ R<رَ>جُلX<ࣱ> N<مُّ>ؤۡمِنX<ࣱ> N<مِّ>نۡ ءَالِ فِرۡعَوۡنَ یَكۡتُمُ إِیمَT<ـٰ>نَهُW<ۥۤ> أَتَQ<قۡ>تُلُونَ R<رَ>جُلًا أَX<ن> N<یَ>قُولَ R<رَ>بِّیَ ٱللَّهُ وَقَQ<دۡ> جَW<اۤ>ءَكُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ مِX<ن> R<رَّ>بِّكُمۡۖ وَإِX<ن> N<یَ>كُ كَT<ـٰ>ذِبN<ࣰ>ا فَعَلَیۡهِ كَذِبُهُT<ۥ>ۖ وَإِX<ن> N<یَ>كُ صَادِقX<ࣰ>ا N<یُ>صِQ<بۡ>كُN<م> بَعۡضُ ٱلَّذِی یَعِدُكُمۡۖ إِN<نَّ> ٱللَّهَ لَا یَهۡدِی مَنۡ هُوَ مُسۡرِفN<ࣱ> كَذَّJ<ا>Q<ب>ࣱ A<۝>D<٢٨>",
"یَT<ـٰ>قَوۡمِ لَكُمُ ٱلۡمُلۡكُ ٱلۡیَوۡمَ ظَT<ـٰ>هِرِینَ فِی ٱلۡأَR<رۡ>ضِ فَمَX<ن> N<یَ>N<ن>صُR<رُ>نَا مِX<ن>N<ۢ> بَأۡسِ ٱللَّهِ إِN<ن> جَW<اۤ>ءَنَاۚ قَالَ فِرۡعَوۡنُ مَW<اۤ> أُرِیكُمۡ إِلَّا مَW<اۤ> أَR<رَ>ىٰ وَمَW<اۤ> أَهۡدِیكُمۡ إِلَّا سَبِیلَ ٱX<ل>R<رَّ>شَJ<ا>Q<د>ِ A<۝>D<٢٩>",
"وَقَالَ ٱلَّذِW<یۤ> ءَامَنَ یَT<ـٰ>قَوۡمِ إِN<نِّ>W<یۤ> أَخَافُ عَلَیۡكُم N<مِّ>ثۡلَ یَوۡمِ ٱلۡأَحۡزَJ<ا>Q<ب>ِ A<۝>D<٣٠>",
"مِثۡلَ دَأۡبِ قَوۡمِ نُوحX<ࣲ> N<وَ>عَادX<ࣲ> N<وَ>ثَمُودَ وَX<ٱ>لَّذِینَ مِX<ن>N<ۢ> بَعۡدِهِمۡۚ وَمَا ٱللَّهُ یُرِیدُ ظُلۡمX<ࣰ>ا لِّلۡعِبَJ<ا>Q<د>ِ A<۝>D<٣١>",
"وَیَT<ـٰ>قَوۡمِ إِN<نِّ>W<یۤ> أَخَافُ عَلَیۡكُمۡ یَوۡمَ ٱX<ل>تَّنَJ<ا>Q<د>ِ A<۝>D<٣٢>",
"یَوۡمَ تُوَلُّونَ مُQ<دۡ>بِرِینَ مَا لَكُم N<مِّ>نَ ٱللَّهِ مِنۡ عَاصِمX<ࣲ>ۗ N<وَ>مَX<ن> N<یُ>ضۡلِلِ ٱللَّهُ فَمَا لَهُT<ۥ> مِنۡ هَJ<ا>Q<د>ࣲ A<۝>D<٣٣>",
"وَلَقَQ<دۡ> جَW<اۤ>ءَكُمۡ یُوسُفُ مِN<ن> قَQ<بۡ>لُ بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَمَا زِلۡتُمۡ فِی شَكّX<ࣲ> N<مِّ>N<مَّ>ا جَW<اۤ>ءَكُN<م> بِهِT<ۦ>ۖ حَتَّىW<ٰۤ> إِذَا هَلَكَ قُلۡتُمۡ لَX<ن> N<یَ>Q<بۡ>عَثَ ٱللَّهُ مِX<ن>N<ۢ> بَعۡدِهِT<ۦ> R<رَ>سُولN<ࣰ>اۚ كَذَT< ٰ>لِكَ یُضِلُّ ٱللَّهُ مَنۡ هُوَ مُسۡرِفX<ࣱ> N<مُّ>R<رۡ>تَJ<ا>Q<ب>ٌ A<۝>D<٣٤>",
"ٱلَّذِینَ یُجَT<ـٰ>دِلُونَ فِW<یۤ> ءَایَT<ـٰ>تِ ٱللَّهِ بِغَیۡرِ سُلۡطَT<ـٰ>نٍ أَتَX<ى>T<ٰ>هُمۡۖ كَبُR<رَ> مَQ<قۡ>تًا عِN<ن>دَ ٱللَّهِ وَعِN<ن>دَ ٱلَّذِینَ ءَامَنُوX<ا۟>ۚ كَذَT< ٰ>لِكَ یَQ<طۡ>بَعُ ٱللَّهُ عَلَىٰ كُلِّ قَلۡبِ مُتَكَبِّرN<ࣲ> جَبَّJ<ا>R<ر>ࣲ A<۝>D<٣٥>",
"وَقَالَ فِرۡعَوۡنُ یَT<ـٰ>هَT<ـٰ>مَT<ـٰ>نُ ٱQ<بۡ>نِ لِی صَR<رۡ>حX<ࣰ>ا لَّعَلِّW<یۤ> أَQ<بۡ>لُغُ ٱلۡأَسۡبَT<ـٰ>Q<ب>َ A<۝>D<٣٦>",
"أَسۡبَT<ـٰ>بَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ فَأَطَّلِعَ إِلَىW<ٰۤ> إِلَT<ـٰ>هِ مُوسَىٰ وَإِN<نِّ>ی لَأَظُN<نُّ>هُT<ۥ> كَT<ـٰ>ذِبX<ࣰ>اۚ N<وَ>كَذَT< ٰ>لِكَ زُیِّنَ لِفِرۡعَوۡنَ سُW<وۤ>ءُ عَمَلِهِT<ۦ> وَصُدَّ عَنِ ٱX<ل>سَّبِیلِۚ وَمَا كَیۡدُ فِرۡعَوۡنَ إِلَّا فِی تَبَJ<ا>Q<ب>ࣲ A<۝>D<٣٧>",
"وَقَالَ ٱلَّذِW<یۤ> ءَامَنَ یَT<ـٰ>قَوۡمِ ٱتَّبِعُونِ أَهۡدِكُمۡ سَبِیلَ ٱX<ل>R<رَّ>شَJ<ا>Q<د>ِ A<۝>D<٣٨>",
"یَT<ـٰ>قَوۡمِ إِN<نَّ>مَا هَT<ـٰ>ذِهِ ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَا مَتَT<ـٰ>عX<ࣱ> N<وَ>إِN<نَّ> ٱلۡءَاخِR<رَ>ةَ هِیَ دَاR<رُ> ٱلۡقَR<رَ>J<ا>R<ر>ِ A<۝>D<٣٩>",
"مَنۡ عَمِلَ سَیِّئَةN<ࣰ> فَلَا یُQ<جۡ>زَىW<ٰۤ> إِلَّا مِثۡلَهَاۖ وَمَنۡ عَمِلَ صَT<ـٰ>لِحX<ࣰ>ا N<مِّ>N<ن> ذَكَرٍ أَوۡ أُN<ن>ثَىٰ وَهُوَ مُؤۡمِنN<ࣱ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ یَQ<دۡ>خُلُونَ ٱلۡجَN<نَّ>ةَ یُR<رۡ>زَقُونَ فِیهَا بِغَیۡرِ حِسَJ<ا>Q<ب>ࣲ A<۝>D<٤٠>",
"۞ وَیَT<ـٰ>قَوۡمِ مَا لِW<یۤ> أَQ<دۡ>عُوكُمۡ إِلَى ٱX<ل>N<نَّ>جَX<و>T<ٰ>ةِ وَتَQ<دۡ>عُونَنِW<یۤ> إِلَى ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٤١>",
"تَQ<دۡ>عُونَنِی لِأَكۡفُR<رَ> بِٱللَّهِ وَأُشۡرِكَ بِهِT<ۦ> مَا لَیۡسَ لِی بِهِT<ۦ> عِلۡمX<ࣱ> N<وَ>أَنَا۠ أَQ<دۡ>عُوكُمۡ إِلَى ٱلۡعَزِیزِ ٱلۡغَفَّT<ـٰ>R<ر>ِ A<۝>D<٤٢>",
"لَا جَR<رَ>مَ أَN<نَّ>مَا تَQ<دۡ>عُونَنِW<یۤ> إِلَیۡهِ لَیۡسَ لَهُT<ۥ> دَعۡوَةN<ࣱ> فِی ٱX<ل>دُّنۡیَا وَلَا فِی ٱلۡءَاخِR<رَ>ةِ وَأَN<نَّ> مَR<رَ>دَّنَW<اۤ> إِلَى ٱللَّهِ وَأَN<نَّ> ٱلۡمُسۡرِفِینَ هُمۡ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٤٣>",
"فَسَتَذۡكُR<رُ>ونَ مَW<اۤ> أَقُولُ لَكُمۡۚ وَأُفَوِّضُ أَمۡرِW<یۤ> إِلَى ٱللَّهِۚ إِN<نَّ> ٱللَّهَ بَصِیR<رُ>N<ۢ> بِX<ٱ>لۡعِبَJ<ا>Q<د>ِ A<۝>D<٤٤>",
"فَوَقَX<ى>T<ٰ>هُ ٱللَّهُ سَیِّءَاتِ مَا مَكَR<رُ>وX<ا۟>ۖ وَحَاقَ بِءَالِ فِرۡعَوۡنَ سُW<وۤ>ءُ ٱلۡعَذَJ<ا>Q<ب>ِ A<۝>D<٤٥>",
"ٱX<ل>N<نَّ>اR<رُ> یُعۡR<رَ>ضُونَ عَلَیۡهَا غُدُوّX<ࣰ>ا N<وَ>عَشِیّX<ࣰ>اۚ N<وَ>یَوۡمَ تَقُومُ ٱX<ل>سَّاعَةُ أَQ<دۡ>خِلُW<وۤ>X<ا۟> ءَالَ فِرۡعَوۡنَ أَشَدَّ ٱلۡعَذَJ<ا>Q<ب>ِ A<۝>D<٤٦>",
"وَإِذۡ یَتَحَL<اۤ>جُّونَ فِی ٱX<ل>N<نَّ>ارِ فَیَقُولُ ٱX<ل>ضُّعَفَW<ـٰۤ>ؤُX<ا۟> لِلَّذِینَ ٱسۡتَكۡبَR<رُ>W<وۤ>X<ا۟> إِN<نَّ>ا كُN<نَّ>ا لَكُمۡ تَبَعN<ࣰ>ا فَهَلۡ أَN<ن>تُم N<مُّ>غۡنُونَ عَN<نَّ>ا نَصِیبX<ࣰ>ا N<مِّ>نَ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٤٧>",
"قَالَ ٱلَّذِینَ ٱسۡتَكۡبَR<رُ>W<وۤ>X<ا۟> إِN<نَّ>ا كُلّN<ࣱ> فِیهَW<اۤ> إِN<نَّ> ٱللَّهَ قَQ<دۡ> حَكَمَ بَیۡنَ ٱلۡعِبَJ<ا>Q<د>ِ A<۝>D<٤٨>",
"وَقَالَ ٱلَّذِینَ فِی ٱX<ل>N<نَّ>ارِ لِخَزَنَةِ جَهَN<نَّ>مَ ٱQ<دۡ>عُوX<ا۟> R<رَ>بَّكُمۡ یُخَفِّفۡ عَN<نَّ>ا یَوۡمX<ࣰ>ا N<مِّ>نَ ٱلۡعَذَJ<ا>Q<ب>ِ A<۝>D<٤٩>",
"قَالُW<وۤ>X<ا۟> أَوَ لَمۡ تَكُ تَأۡتِیكُمۡ R<رُ>سُلُكُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِۖ قَالُوX<ا۟> بَلَىٰۚ قَالُوX<ا۟> فَX<ٱ>Q<دۡ>عُوX<ا۟>ۗ وَمَا دُعَW<ـٰۤ>ؤُX<ا۟> ٱلۡكَT<ـٰ>فِرِینَ إِلَّا فِی ضَلَT<ـٰ>لٍ A<۝>D<٥٠>",
"إِN<نَّ>ا لَنَN<ن>صُR<رُ> R<رُ>سُلَنَا وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَیَوۡمَ یَقُومُ ٱلۡأَشۡهَT<ـٰ>Q<د>ُ A<۝>D<٥١>",
"یَوۡمَ لَا یَN<ن>فَعُ ٱX<ل>ظَّT<ـٰ>لِمِینَ مَعۡذِR<رَ>تُهُمۡۖ وَلَهُمُ ٱX<ل>لَّعۡنَةُ وَلَهُمۡ سُW<وۤ>ءُ ٱX<ل>دَّJ<ا>R<ر>ِ A<۝>D<٥٢>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَى ٱلۡهُدَىٰ وَأَوۡR<رَ>ثۡنَا بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ ٱلۡكِتَT<ـٰ>Q<ب>َ A<۝>D<٥٣>",
"هُدX<ࣰ>ى N<وَ>ذِكۡR<رَ>ىٰ لِأُX<و۟>لِی ٱلۡأَلۡبَT<ـٰ>Q<ب>ِ A<۝>D<٥٤>",
"فَX<ٱ>صۡبِرۡ إِN<نَّ> وَعۡدَ ٱللَّهِ حَقّX<ࣱ> N<وَ>X<ٱ>سۡتَغۡفِرۡ لِذَX<ن>N<ۢ>بِكَ وَسَبِّحۡ بِحَمۡدِ R<رَ>بِّكَ بِX<ٱ>لۡعَشِیِّ وَX<ٱ>لۡإِQ<بۡ>كَT<ـٰ>R<ر>ِ A<۝>D<٥٥>",
"إِN<نَّ> ٱلَّذِینَ یُجَT<ـٰ>دِلُونَ فِW<یۤ> ءَایَT<ـٰ>تِ ٱللَّهِ بِغَیۡرِ سُلۡطَT<ـٰ>نٍ أَتَX<ى>T<ٰ>هُمۡ إِN<ن> فِی صُدُورِهِمۡ إِلَّا كِQ<بۡ>R<ر>X<ࣱ> N<مَّ>ا هُN<م> بِبَT<ـٰ>لِغِیهِۚ فَX<ٱ>سۡتَعِذۡ بِٱللَّهِۖ إِN<نَّ>هُT<ۥ> هُوَ ٱX<ل>سَّمِیعُ ٱلۡبَصِJ<ی>رُ A<۝>D<٥٦>",
"لَخَلۡقُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ أَكۡبَR<رُ> مِنۡ خَلۡقِ ٱX<ل>N<نَّ>اسِ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٥٧>",
"وَمَا یَسۡتَوِی ٱلۡأَعۡمَىٰ وَX<ٱ>لۡبَصِیR<رُ> وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَلَا ٱلۡمُسِW<یۤ>ءُۚ قَلِیلX<ࣰ>ا N<مَّ>ا تَتَذَكَّR<رُ>J<و>نَ A<۝>D<٥٨>",
"إِN<نَّ> ٱX<ل>سَّاعَةَ لَءَاتِیَةX<ࣱ> لَّا R<رَ>یۡبَ فِیهَا وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٥٩>",
"وَقَالَ R<رَ>بُّكُمُ ٱQ<دۡ>عُونِW<یۤ> أَسۡتَجِQ<بۡ> لَكُمۡۚ إِN<نَّ> ٱلَّذِینَ یَسۡتَكۡبِR<رُ>ونَ عَنۡ عِبَادَتِی سَیَQ<دۡ>خُلُونَ جَهَN<نَّ>مَ دَاخِرِJ<ی>نَ A<۝>D<٦٠>",
"ٱللَّهُ ٱلَّذِی جَعَلَ لَكُمُ ٱلَّیۡلَ لِتَسۡكُنُوX<ا۟> فِیهِ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ> مُQ<بۡ>صِR<ر>ًاۚ إِN<نَّ> ٱللَّهَ لَذُو فَضۡلٍ عَلَى ٱX<ل>N<نَّ>اسِ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَشۡكُR<رُ>J<و>نَ A<۝>D<٦١>",
"ذَT< ٰ>لِكُمُ ٱللَّهُ R<رَ>بُّكُمۡ خَT<ـٰ>لِقُ كُلِّ شَیۡءX<ࣲ> لَّW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ فَأَN<نَّ>ىٰ تُؤۡفَكُJ<و>نَ A<۝>D<٦٢>",
"كَذَT< ٰ>لِكَ یُؤۡفَكُ ٱلَّذِینَ كَانُوX<ا۟> بِءَایَT<ـٰ>تِ ٱللَّهِ یَQ<جۡ>حَدُJ<و>نَ A<۝>D<٦٣>",
"ٱللَّهُ ٱلَّذِی جَعَلَ لَكُمُ ٱلۡأَR<رۡ>ضَ قَR<رَ>اR<ر>X<ࣰ>ا N<وَ>X<ٱ>X<ل>سَّمَW<اۤ>ءَ بِنَW<اۤ>ءX<ࣰ> N<وَ>صَوَّR<رَ>كُمۡ فَأَحۡسَنَ صُوَR<رَ>كُمۡ وَR<رَ>زَقَكُم N<مِّ>نَ ٱX<ل>طَّیِّبَT<ـٰ>تِۚ ذَT< ٰ>لِكُمُ ٱللَّهُ R<رَ>بُّكُمۡۖ فَتَبَاR<رَ>كَ ٱللَّهُ R<رَ>بُّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٦٤>",
"هُوَ ٱلۡحَیُّ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ فَX<ٱ>Q<دۡ>عُوهُ مُخۡلِصِینَ لَهُ ٱX<ل>دِّینَۗ ٱلۡحَمۡدُ لِلَّهِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٦٥>",
"۞ قُلۡ إِN<نِّ>ی نُهِیتُ أَنۡ أَعۡبُدَ ٱلَّذِینَ تَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ لَN<مَّ>ا جَW<اۤ>ءَنِیَ ٱلۡبَیِّنَT<ـٰ>تُ مِX<ن> R<رَّ>بِّی وَأُمِرۡتُ أَنۡ أُسۡلِمَ لِR<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٦٦>",
"هُوَ ٱلَّذِی خَلَقَكُم N<مِّ>N<ن> تُR<رَ>ابN<ࣲ> ثُN<مَّ> مِن N<نُّ>Q<طۡ>فَةN<ࣲ> ثُN<مَّ> مِنۡ عَلَقَةN<ࣲ> ثُN<مَّ> یُخۡرِجُكُمۡ طِفۡلN<ࣰ>ا ثُN<مَّ> لِتَQ<بۡ>لُغُW<وۤ>X<ا۟> أَشُدَّكُمۡ ثُN<مَّ> لِتَكُونُوX<ا۟> شُیُوخX<ࣰ>اۚ N<وَ>مِN<ن>كُم N<مَّ>X<ن> N<یُ>تَوَفَّىٰ مِN<ن> قَQ<بۡ>لُۖ وَلِتَQ<بۡ>لُغُW<وۤ>X<ا۟> أَجَلX<ࣰ>ا N<مُّ>سَN<مّ>X<ࣰ>ى N<وَ>لَعَلَّكُمۡ تَعۡقِلُJ<و>نَ A<۝>D<٦٧>",
"هُوَ ٱلَّذِی یُحۡیِT<ۦ> وَیُمِیتُۖ فَإِذَا قَضَىW<ٰۤ> أَمۡR<ر>N<ࣰ>ا فَإِN<نَّ>مَا یَقُولُ لَهُT<ۥ> كُN<ن> فَیَكُJ<و>نُ A<۝>D<٦٨>",
"أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ یُجَT<ـٰ>دِلُونَ فِW<یۤ> ءَایَT<ـٰ>تِ ٱللَّهِ أَN<نَّ>ىٰ یُصۡR<رَ>فُJ<و>نَ A<۝>D<٦٩>",
"ٱلَّذِینَ كَذَّبُوX<ا۟> بِX<ٱ>لۡكِتَT<ـٰ>بِ وَبِمَW<اۤ> أَR<رۡ>سَلۡنَا بِهِT<ۦ> R<رُ>سُلَنَاۖ فَسَوۡفَ یَعۡلَمُJ<و>نَ A<۝>D<٧٠>",
"إِذِ ٱلۡأَغۡلَT<ـٰ>لُ فِW<یۤ> أَعۡنَT<ـٰ>قِهِمۡ وَX<ٱ>X<ل>سَّلَT<ـٰ>سِلُ یُسۡحَبُJ<و>نَ A<۝>D<٧١>",
"فِی ٱلۡحَمِیمِ ثُN<مَّ> فِی ٱX<ل>N<نَّ>ارِ یُسۡجَR<رُ>J<و>نَ A<۝>D<٧٢>",
"ثُN<مَّ> قِیلَ لَهُمۡ أَیۡنَ مَا كُN<ن>تُمۡ تُشۡرِكُJ<و>نَ A<۝>D<٧٣>",
"مِN<ن> دُونِ ٱللَّهِۖ قَالُوX<ا۟> ضَلُّوX<ا۟> عَN<نَّ>ا بَل لَّمۡ نَكُن N<نَّ>Q<دۡ>عُوX<ا۟> مِN<ن> قَQ<بۡ>لُ شَیۡءN<ࣰ>اۚ كَذَT< ٰ>لِكَ یُضِلُّ ٱللَّهُ ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٧٤>",
"ذَT< ٰ>لِكُN<م> بِمَا كُN<ن>تُمۡ تَفۡR<رَ>حُونَ فِی ٱلۡأَR<رۡ>ضِ بِغَیۡرِ ٱلۡحَقِّ وَبِمَا كُN<ن>تُمۡ تَمۡR<رَ>حُJ<و>نَ A<۝>D<٧٥>",
"ٱQ<دۡ>خُلُW<وۤ>X<ا۟> أَQ<بۡ>وَT< ٰ>بَ جَهَN<نَّ>مَ خَT<ـٰ>لِدِینَ فِیهَاۖ فَبِئۡسَ مَثۡوَى ٱلۡمُتَكَبِّرِJ<ی>نَ A<۝>D<٧٦>",
"فَX<ٱ>صۡبِرۡ إِN<نَّ> وَعۡدَ ٱللَّهِ حَقّN<ࣱ>ۚ فَإِN<مَّ>ا نُرِیَN<نَّ>كَ بَعۡضَ ٱلَّذِی نَعِدُهُمۡ أَوۡ نَتَوَفَّیَN<نَّ>كَ فَإِلَیۡنَا یُR<رۡ>جَعُJ<و>نَ A<۝>D<٧٧>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا R<رُ>سُلX<ࣰ>ا N<مِّ>N<ن> قَQ<بۡ>لِكَ مِنۡهُم N<مَّ>N<ن> قَصَصۡنَا عَلَیۡكَ وَمِنۡهُم N<مَّ>X<ن> لَّمۡ نَQ<قۡ>صُصۡ عَلَیۡكَۗ وَمَا كَانَ لِR<رَ>سُولٍ أَX<ن> N<یَ>أۡتِیَ بِءَایَةٍ إِلَّا بِإِذۡنِ ٱللَّهِۚ فَإِذَا جَW<اۤ>ءَ أَمۡR<رُ> ٱللَّهِ قُضِیَ بِX<ٱ>لۡحَقِّ وَخَسِR<رَ> هُنَالِكَ ٱلۡمُQ<بۡ>طِلُJ<و>نَ A<۝>D<٧٨>",
"ٱللَّهُ ٱلَّذِی جَعَلَ لَكُمُ ٱلۡأَنۡعَT<ـٰ>مَ لِتَR<رۡ>كَبُوX<ا۟> مِنۡهَا وَمِنۡهَا تَأۡكُلُJ<و>نَ A<۝>D<٧٩>",
"وَلَكُمۡ فِیهَا مَنَT<ـٰ>فِعُ وَلِتَQ<بۡ>لُغُوX<ا۟> عَلَیۡهَا حَاجَةN<ࣰ> فِی صُدُورِكُمۡ وَعَلَیۡهَا وَعَلَى ٱلۡفُلۡكِ تُحۡمَلُJ<و>نَ A<۝>D<٨٠>",
"وَیُرِیكُمۡ ءَایَT<ـٰ>تِهِT<ۦ> فَأَیَّ ءَایَT<ـٰ>تِ ٱللَّهِ تُN<ن>كِR<رُ>J<و>نَ A<۝>D<٨١>",
"أَفَلَمۡ یَسِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَیَN<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۚ كَانُW<وۤ>X<ا۟> أَكۡثَR<رَ> مِنۡهُمۡ وَأَشَدَّ قُوَّةX<ࣰ> N<وَ>ءَاثَاR<ر>N<ࣰ>ا فِی ٱلۡأَR<رۡ>ضِ فَمَW<اۤ> أَغۡنَىٰ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<٨٢>",
"فَلَN<مَّ>ا جَW<اۤ>ءَتۡهُمۡ R<رُ>سُلُهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَرِحُوX<ا۟> بِمَا عِN<ن>دَهُم N<مِّ>نَ ٱلۡعِلۡمِ وَحَاقَ بِهِم N<مَّ>ا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٨٣>",
"فَلَN<مَّ>ا R<رَ>أَوۡX<ا۟> بَأۡسَنَا قَالُW<وۤ>X<ا۟> ءَامَN<نَّ>ا بِٱللَّهِ وَحۡدَهُT<ۥ> وَكَفَR<رۡ>نَا بِمَا كُN<نَّ>ا بِهِT<ۦ> مُشۡرِكِJ<ی>نَ A<۝>D<٨٤>",
"فَلَمۡ یَكُ یَN<ن>فَعُهُمۡ إِیمَT<ـٰ>نُهُمۡ لَN<مَّ>ا R<رَ>أَوۡX<ا۟> بَأۡسَنَاۖ سُN<نَّ>تَ ٱللَّهِ ٱلَّتِی قَQ<دۡ> خَلَتۡ فِی عِبَادِهِT<ۦ>ۖ وَخَسِR<رَ> هُنَالِكَ ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٨٥>",
"#حL<مۤ> A<۝>D<١>",
"تَN<ن>زِیلX<ࣱ> N<مِّ>نَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ ٱX<ل>R<رَّ>حِJ<ی>مِ A<۝>D<٢>",
"كِتَT<ـٰ>بN<ࣱ> فُصِّلَتۡ ءَایَT<ـٰ>تُهُT<ۥ> قُR<رۡ>ءَانًا عَR<رَ>بِیّX<ࣰ>ا لِّقَوۡمX<ࣲ> N<یَ>عۡلَمُJ<و>نَ A<۝>D<٣>",
"بَشِیR<ر>X<ࣰ>ا N<وَ>نَذِیR<ر>N<ࣰ>ا فَأَعۡR<رَ>ضَ أَكۡثَR<رُ>هُمۡ فَهُمۡ لَا یَسۡمَعُJ<و>نَ A<۝>D<٤>",
"وَقَالُوX<ا۟> قُلُوبُنَا فِW<یۤ> أَكِN<نَّ>ةX<ࣲ> N<مِّ>N<مَّ>ا تَQ<دۡ>عُونَW<اۤ> إِلَیۡهِ وَفِW<یۤ> ءَاذَانِنَا وَQ<قۡ>R<ر>X<ࣱ> N<وَ>مِX<ن>N<ۢ> بَیۡنِنَا وَبَیۡنِكَ حِجَابN<ࣱ> فَX<ٱ>عۡمَلۡ إِN<نَّ>نَا عَT<ـٰ>مِلُJ<و>نَ A<۝>D<٥>",
"قُلۡ إِN<نَّ>مَW<اۤ> أَنَا۠ بَشَR<ر>X<ࣱ> N<مِّ>ثۡلُكُمۡ یُوحَىW<ٰۤ> إِلَیَّ أَN<نَّ>مَW<اۤ> إِلَT<ـٰ>هُكُمۡ إِلَT<ـٰ>هX<ࣱ> N<وَ>T< ٰ>حِدN<ࣱ> فَX<ٱ>سۡتَقِیمُW<وۤ>X<ا۟> إِلَیۡهِ وَX<ٱ>سۡتَغۡفِR<رُ>وهُۗ وَوَیۡلX<ࣱ> لِّلۡمُشۡرِكِJ<ی>نَ A<۝>D<٦>",
"ٱلَّذِینَ لَا یُؤۡتُونَ ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَهُN<م> بِX<ٱ>لۡءَاخِR<رَ>ةِ هُمۡ كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٧>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَهُمۡ أَQ<جۡ>R<ر>ٌ غَیۡR<رُ> مَمۡنُJ<و>نࣲ A<۝>D<٨>",
"۞ قُلۡ أَئِN<نَّ>كُمۡ لَتَكۡفُR<رُ>ونَ بِX<ٱ>لَّذِی خَلَقَ ٱلۡأَR<رۡ>ضَ فِی یَوۡمَیۡنِ وَتَQ<جۡ>عَلُونَ لَهُW<ۥۤ> أَN<ن>دَادN<ࣰ>اۚ ذَT< ٰ>لِكَ R<رَ>بُّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٩>",
"وَجَعَلَ فِیهَا R<رَ>وَT< ٰ>سِیَ مِN<ن> فَوۡقِهَا وَبَT<ـٰ>R<رَ>كَ فِیهَا وَقَدَّR<رَ> فِیهَW<اۤ> أَQ<قۡ>وَT< ٰ>تَهَا فِW<یۤ> أَR<رۡ>بَعَةِ أَیَّامN<ࣲ> سَوَW<اۤ>ءX<ࣰ> لِّX<ل>سَّW<اۤ>ئِلِJ<ی>نَ A<۝>D<١٠>",
"ثُN<مَّ> ٱسۡتَوَىW<ٰۤ> إِلَى ٱX<ل>سَّمَW<اۤ>ءِ وَهِیَ دُخَانN<ࣱ> فَقَالَ لَهَا وَلِلۡأَR<رۡ>ضِ ٱئۡتِیَا طَوۡعًا أَوۡ كَR<رۡ>هN<ࣰ>ا قَالَتَW<اۤ> أَتَیۡنَا طَW<اۤ>ئِعِJ<ی>نَ A<۝>D<١١>",
"فَقَضَX<ى>T<ٰ>هُN<نَّ> سَQ<بۡ>عَ سَمَT<ـٰ>وَاتN<ࣲ> فِی یَوۡمَیۡنِ وَأَوۡحَىٰ فِی كُلِّ سَمَW<اۤ>ءٍ أَمۡR<رَ>هَاۚ وَزَیَّN<نَّ>ا ٱX<ل>سَّمَW<اۤ>ءَ ٱX<ل>دُّنۡیَا بِمَصَT<ـٰ>بِیحَ وَحِفۡظN<ࣰ>اۚ ذَT< ٰ>لِكَ تَQ<قۡ>دِیR<رُ> ٱلۡعَزِیزِ ٱلۡعَلِJ<ی>مِ A<۝>D<١٢>",
"فَإِنۡ أَعۡR<رَ>ضُوX<ا۟> فَقُلۡ أَN<ن>ذَR<رۡ>تُكُمۡ صَT<ـٰ>عِقَةX<ࣰ> N<مِّ>ثۡلَ صَT<ـٰ>عِقَةِ عَادX<ࣲ> N<وَ>ثَمُJ<و>Q<د>َ A<۝>D<١٣>",
"إِذۡ جَW<اۤ>ءَتۡهُمُ ٱX<ل>R<رُّ>سُلُ مِX<ن>N<ۢ> بَیۡنِ أَیۡدِیهِمۡ وَمِنۡ خَلۡفِهِمۡ أَلَّا تَعۡبُدُW<وۤ>X<ا۟> إِلَّا ٱللَّهَۖ قَالُوX<ا۟> لَوۡ شَW<اۤ>ءَ R<رَ>بُّنَا لَأَN<ن>زَلَ مَلَW<ـٰۤ>ئِكَةN<ࣰ> فَإِN<نَّ>ا بِمَW<اۤ> أُR<رۡ>سِلۡتُN<م> بِهِT<ۦ> كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<١٤>",
"فَأَN<مَّ>ا عَادN<ࣱ> فَX<ٱ>سۡتَكۡبَR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ بِغَیۡرِ ٱلۡحَقِّ وَقَالُوX<ا۟> مَنۡ أَشَدُّ مِN<نَّ>ا قُوَّةًۖ أَوَ لَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ> ٱللَّهَ ٱلَّذِی خَلَقَهُمۡ هُوَ أَشَدُّ مِنۡهُمۡ قُوَّةX<ࣰ>ۖ N<وَ>كَانُوX<ا۟> بِءَایَT<ـٰ>تِنَا یَQ<جۡ>حَدُJ<و>نَ A<۝>D<١٥>",
"فَأَR<رۡ>سَلۡنَا عَلَیۡهِمۡ رِیحN<ࣰ>ا صَR<رۡ>صَR<ر>N<ࣰ>ا فِW<یۤ> أَیَّامX<ࣲ> N<نَّ>حِسَاتX<ࣲ> لِّنُذِیقَهُمۡ عَذَابَ ٱلۡخِزۡیِ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۖ وَلَعَذَابُ ٱلۡءَاخِR<رَ>ةِ أَخۡزَىٰۖ وَهُمۡ لَا یُN<ن>صَR<رُ>J<و>نَ A<۝>D<١٦>",
"وَأَN<مَّ>ا ثَمُودُ فَهَدَیۡنَT<ـٰ>هُمۡ فَX<ٱ>سۡتَحَبُّوX<ا۟> ٱلۡعَمَىٰ عَلَى ٱلۡهُدَىٰ فَأَخَذَتۡهُمۡ صَT<ـٰ>عِقَةُ ٱلۡعَذَابِ ٱلۡهُونِ بِمَا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<١٧>",
"وَنَجَّیۡنَا ٱلَّذِینَ ءَامَنُوX<ا۟> وَكَانُوX<ا۟> یَتَّقُJ<و>نَ A<۝>D<١٨>",
"وَیَوۡمَ یُحۡشَR<رُ> أَعۡدَW<اۤ>ءُ ٱللَّهِ إِلَى ٱX<ل>N<نَّ>ارِ فَهُمۡ یُوزَعُJ<و>نَ A<۝>D<١٩>",
"حَتَّىW<ٰۤ> إِذَا مَا جَW<اۤ>ءُوهَا شَهِدَ عَلَیۡهِمۡ سَمۡعُهُمۡ وَأَQ<بۡ>صَT<ـٰ>R<رُ>هُمۡ وَجُلُودُهُN<م> بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٢٠>",
"وَقَالُوX<ا۟> لِجُلُودِهِمۡ لِمَ شَهِX<د>تُّمۡ عَلَیۡنَاۖ قَالُW<وۤ>X<ا۟> أَN<ن>طَقَنَا ٱللَّهُ ٱلَّذِW<یۤ> أَN<ن>طَقَ كُلَّ شَیۡءX<ࣲ>ۚ N<وَ>هُوَ خَلَقَكُمۡ أَوَّلَ مَR<رَّ>ةX<ࣲ> N<وَ>إِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٢١>",
"وَمَا كُN<ن>تُمۡ تَسۡتَتِR<رُ>ونَ أَX<ن> N<یَ>شۡهَدَ عَلَیۡكُمۡ سَمۡعُكُمۡ وَلَW<اۤ> أَQ<بۡ>صَT<ـٰ>R<رُ>كُمۡ وَلَا جُلُودُكُمۡ وَلَT<ـٰ>كِN<ن> ظَنَN<ن>تُمۡ أَN<نَّ> ٱللَّهَ لَا یَعۡلَمُ كَثِیR<ر>X<ࣰ>ا N<مِّ>N<مَّ>ا تَعۡمَلُJ<و>نَ A<۝>D<٢٢>",
"وَذَT< ٰ>لِكُمۡ ظَN<نُّ>كُمُ ٱلَّذِی ظَنَN<ن>تُN<م> بِR<رَ>بِّكُمۡ أَR<رۡ>دَX<ى>T<ٰ>كُمۡ فَأَصۡبَحۡتُم N<مِّ>نَ ٱلۡخَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٢٣>",
"فَإِX<ن> N<یَ>صۡبِR<رُ>وX<ا۟> فَX<ٱ>X<ل>N<نَّ>اR<رُ> مَثۡوX<ࣰ>ى لَّهُمۡۖ وَإِX<ن> N<یَ>سۡتَعۡتِبُوX<ا۟> فَمَا هُم N<مِّ>نَ ٱلۡمُعۡتَبِJ<ی>نَ A<۝>D<٢٤>",
"۞ وَقَیَّضۡنَا لَهُمۡ قُR<رَ>نَW<اۤ>ءَ فَزَیَّنُوX<ا۟> لَهُم N<مَّ>ا بَیۡنَ أَیۡدِیهِمۡ وَمَا خَلۡفَهُمۡ وَحَقَّ عَلَیۡهِمُ ٱلۡقَوۡلُ فِW<یۤ> أُمَمN<ࣲ> قَQ<دۡ> خَلَتۡ مِN<ن> قَQ<بۡ>لِهِم N<مِّ>نَ ٱلۡجِN<نِّ> وَX<ٱ>لۡإِN<ن>سِۖ إِN<نَّ>هُمۡ كَانُوX<ا۟> خَT<ـٰ>سِرِJ<ی>نَ A<۝>D<٢٥>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَا تَسۡمَعُوX<ا۟> لِهَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانِ وَX<ٱ>لۡغَوۡX<ا۟> فِیهِ لَعَلَّكُمۡ تَغۡلِبُJ<و>نَ A<۝>D<٢٦>",
"فَلَنُذِیقَN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> عَذَابN<ࣰ>ا شَدِیدX<ࣰ>ا N<وَ>لَنَQ<جۡ>زِیَN<نَّ>هُمۡ أَسۡوَأَ ٱلَّذِی كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٢٧>",
"ذَT< ٰ>لِكَ جَزَW<اۤ>ءُ أَعۡدَW<اۤ>ءِ ٱللَّهِ ٱX<ل>N<نَّ>اR<رُ>ۖ لَهُمۡ فِیهَا دَاR<رُ> ٱلۡخُلۡدِ جَزَW<اۤ>ءَN<ۢ> بِمَا كَانُوX<ا۟> بِءَایَT<ـٰ>تِنَا یَQ<جۡ>حَدُJ<و>نَ A<۝>D<٢٨>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> R<رَ>بَّنَW<اۤ> أَرِنَا ٱلَّذَیۡنِ أَضَلَّانَا مِنَ ٱلۡجِN<نِّ> وَX<ٱ>لۡإِN<ن>سِ نَQ<جۡ>عَلۡهُمَا تَحۡتَ أَQ<قۡ>دَامِنَا لِیَكُونَا مِنَ ٱلۡأَسۡفَلِJ<ی>نَ A<۝>D<٢٩>",
"إِN<نَّ> ٱلَّذِینَ قَالُوX<ا۟> R<رَ>بُّنَا ٱللَّهُ ثُN<مَّ> ٱسۡتَقَT<ـٰ>مُوX<ا۟> تَتَنَزَّلُ عَلَیۡهِمُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ أَلَّا تَخَافُوX<ا۟> وَلَا تَحۡزَنُوX<ا۟> وَأَQ<بۡ>شِR<رُ>وX<ا۟> بِX<ٱ>لۡجَN<نَّ>ةِ ٱلَّتِی كُN<ن>تُمۡ تُوعَدُJ<و>نَ A<۝>D<٣٠>",
"نَحۡنُ أَوۡلِیَW<اۤ>ؤُكُمۡ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَا وَفِی ٱلۡءَاخِR<رَ>ةِۖ وَلَكُمۡ فِیهَا مَا تَشۡتَهِW<یۤ> أَN<ن>فُسُكُمۡ وَلَكُمۡ فِیهَا مَا تَدَّعُJ<و>نَ A<۝>D<٣١>",
"نُزُلX<ࣰ>ا N<مِّ>نۡ غَفُورX<ࣲ> R<رَّ>حِJ<ی>مࣲ A<۝>D<٣٢>",
"وَمَنۡ أَحۡسَنُ قَوۡلX<ࣰ>ا N<مِّ>N<مَّ>N<ن> دَعَW<اۤ> إِلَى ٱللَّهِ وَعَمِلَ صَT<ـٰ>لِحX<ࣰ>ا N<وَ>قَالَ إِN<نَّ>نِی مِنَ ٱلۡمُسۡلِمِJ<ی>نَ A<۝>D<٣٣>",
"وَلَا تَسۡتَوِی ٱلۡحَسَنَةُ وَلَا ٱX<ل>سَّیِّئَةُۚ ٱQ<دۡ>فَعۡ بِX<ٱ>لَّتِی هِیَ أَحۡسَنُ فَإِذَا ٱلَّذِی بَیۡنَكَ وَبَیۡنَهُT<ۥ> عَدَT< ٰ>وَةN<ࣱ> كَأَN<نَّ>هُT<ۥ> وَلِیٌّ حَمِJ<ی>مࣱ A<۝>D<٣٤>",
"وَمَا یُلَقَّX<ى>T<ٰ>هَW<اۤ> إِلَّا ٱلَّذِینَ صَبَR<رُ>وX<ا۟> وَمَا یُلَقَّX<ى>T<ٰ>هَW<اۤ> إِلَّا ذُو حَظٍّ عَظِJ<ی>مࣲ A<۝>D<٣٥>",
"وَإِN<مَّ>ا یَN<ن>زَغَN<نَّ>كَ مِنَ ٱX<ل>شَّیۡطَT<ـٰ>نِ نَزۡغN<ࣱ> فَX<ٱ>سۡتَعِذۡ بِٱللَّهِۖ إِN<نَّ>هُT<ۥ> هُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٣٦>",
"وَمِنۡ ءَایَT<ـٰ>تِهِ ٱلَّیۡلُ وَX<ٱ>X<ل>N<نَّ>هَاR<رُ> وَX<ٱ>X<ل>شَّمۡسُ وَX<ٱ>لۡقَمَR<رُ>ۚ لَا تَسۡجُدُوX<ا۟> لِX<ل>شَّمۡسِ وَلَا لِلۡقَمَرِ وَ̅X<ٱ>̅سۡ̅جُ̅دُ̅و̅X<ا۟>̅ ̅لِ̅لَّ̅هِ̅ ٱلَّذِی خَلَقَهُN<نَّ> إِN<ن> كُN<ن>تُمۡ إِیَّاهُ تَعۡبُدُJ<و>نَ A<۝>D<٣٧>",
"فَإِنِ ٱسۡتَكۡبَR<رُ>وX<ا۟> فَX<ٱ>لَّذِینَ عِN<ن>دَ R<رَ>بِّكَ یُسَبِّحُونَ لَهُT<ۥ> بِX<ٱ>لَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِ وَهُمۡ لَا یَسۡءَمُJ<و>نَ A<۝>D<٣٨> ۩",
"وَمِنۡ ءَایَT<ـٰ>تِهِW<ۦۤ> أَN<نَّ>كَ تَR<رَ>ى ٱلۡأَR<رۡ>ضَ خَT<ـٰ>شِعَةN<ࣰ> فَإِذَW<اۤ> أَN<ن>زَلۡنَا عَلَیۡهَا ٱلۡمَW<اۤ>ءَ ٱهۡتَزَّتۡ وَR<رَ>بَتۡۚ إِN<نَّ> ٱلَّذِW<یۤ> أَحۡیَاهَا لَمُحۡیِ ٱلۡمَوۡتَىW<ٰۤ>ۚ إِN<نَّ>هُT<ۥ> عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<٣٩>",
"إِN<نَّ> ٱلَّذِینَ یُلۡحِدُونَ فِW<یۤ> ءَایَT<ـٰ>تِنَا لَا یَخۡفَوۡنَ عَلَیۡنَW<اۤ>ۗ أَفَمَX<ن> N<یُ>لۡقَىٰ فِی ٱX<ل>N<نَّ>ارِ خَیۡR<ر>ٌ أَم N<مَّ>X<ن> N<یَ>أۡتِW<یۤ> ءَامِنX<ࣰ>ا N<یَ>وۡمَ ٱلۡقِیَT<ـٰ>مَةِۚ ٱعۡمَلُوX<ا۟> مَا شِئۡتُمۡ إِN<نَّ>هُT<ۥ> بِمَا تَعۡمَلُونَ بَصِJ<ی>رٌ A<۝>D<٤٠>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِX<ٱ>X<ل>ذِّكۡرِ لَN<مَّ>ا جَW<اۤ>ءَهُمۡۖ وَإِN<نَّ>هُT<ۥ> لَكِتَT<ـٰ>بٌ عَزِJ<ی>زࣱ A<۝>D<٤١>",
"لَّا یَأۡتِیهِ ٱلۡبَT<ـٰ>طِلُ مِX<ن>N<ۢ> بَیۡنِ یَدَیۡهِ وَلَا مِنۡ خَلۡفِهِT<ۦ>ۖ تَN<ن>زِیلX<ࣱ> N<مِّ>نۡ حَكِیمٍ حَمِJ<ی>Q<د>ࣲ A<۝>D<٤٢>",
"مَّا یُقَالُ لَكَ إِلَّا مَا قَQ<دۡ> قِیلَ لِX<ل>R<رُّ>سُلِ مِN<ن> قَQ<بۡ>لِكَۚ إِN<نَّ> R<رَ>بَّكَ لَذُو مَغۡفِR<رَ>ةX<ࣲ> N<وَ>ذُو عِقَابٍ أَلِJ<ی>مࣲ A<۝>D<٤٣>",
"وَلَوۡ جَعَلۡنَT<ـٰ>هُ قُR<رۡ>ءَانًا أَعۡجَمِیّX<ࣰ>ا لَّقَالُوX<ا۟> لَوۡلَا فُصِّلَتۡ ءَایَT<ـٰ>تُهُW<ۥۤ>ۖ ءَا۬عۡجَمِیّX<ࣱ> N<وَ>عَR<رَ>بِیّN<ࣱ>ۗ قُلۡ هُوَ لِلَّذِینَ ءَامَنُوX<ا۟> هُدX<ࣰ>ى N<وَ>شِفَW<اۤ>ءX<ࣱ>ۚ N<وَ>X<ٱ>لَّذِینَ لَا یُؤۡمِنُونَ فِW<یۤ> ءَاذَانِهِمۡ وَQ<قۡ>R<ر>X<ࣱ> N<وَ>هُوَ عَلَیۡهِمۡ عَمًىۚ أُX<و۟>لَW<ـٰۤ>ئِكَ یُنَادَوۡنَ مِX<ن> N<مَّ>كَانِN<ۭ> بَعِJ<ی>Q<د>ࣲ A<۝>D<٤٤>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا مُوسَى ٱلۡكِتَT<ـٰ>بَ فَX<ٱ>خۡتُلِفَ فِیهِۚ وَلَوۡلَا كَلِمَةN<ࣱ> سَبَقَتۡ مِX<ن> R<رَّ>بِّكَ لَقُضِیَ بَیۡنَهُمۡۚ وَإِN<نَّ>هُمۡ لَفِی شَكّX<ࣲ> N<مِّ>نۡهُ مُرِJ<ی>Q<ب>ࣲ A<۝>D<٤٥>",
"مَّنۡ عَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَلِنَفۡسِهِT<ۦ>ۖ وَمَنۡ أَسَW<اۤ>ءَ فَعَلَیۡهَاۗ وَمَا R<رَ>بُّكَ بِظَلَّT<ـٰ>مX<ࣲ> لِّلۡعَبِJ<ی>Q<د>ِ A<۝>D<٤٦>",
"۞ إِلَیۡهِ یُR<رَ>دُّ عِلۡمُ ٱX<ل>سَّاعَةِۚ وَمَا تَخۡR<رُ>جُ مِN<ن> ثَمَR<رَ>T< ٰ>تX<ࣲ> N<مِّ>نۡ أَكۡمَامِهَا وَمَا تَحۡمِلُ مِنۡ أُN<ن>ثَىٰ وَلَا تَضَعُ إِلَّا بِعِلۡمِهِT<ۦ>ۚ وَیَوۡمَ یُنَادِیهِمۡ أَیۡنَ شُR<رَ>كَW<اۤ>ءِی قَالُW<وۤ>X<ا۟> ءَاذَN<نَّ>T<ـٰ>كَ مَامِN<نَّ>ا مِN<ن> شَهِJ<ی>Q<د>ࣲ A<۝>D<٤٧>",
"وَضَلَّ عَنۡهُم N<مَّ>ا كَانُوX<ا۟> یَQ<دۡ>عُونَ مِN<ن> قَQ<بۡ>لُۖ وَظَN<نُّ>وX<ا۟> مَا لَهُم N<مِّ>X<ن> N<مَّ>حِJ<ی>صࣲ A<۝>D<٤٨>",
"لَّا یَسۡءَمُ ٱلۡإِN<ن>سَT<ـٰ>نُ مِN<ن> دُعَW<اۤ>ءِ ٱلۡخَیۡرِ وَإِX<ن> N<مَّ>سَّهُ ٱX<ل>شَّR<رُّ> فَیَءُوسN<ࣱ> قَنُJ<و>Q<ط>ࣱ A<۝>D<٤٩>",
"وَلَئِنۡ أَذَQ<قۡ>نَT<ـٰ>هُ R<رَ>حۡمَةX<ࣰ> N<مِّ>N<نَّ>ا مِX<ن>N<ۢ> بَعۡدِ ضَR<رَّ>W<اۤ>ءَ مَسَّتۡهُ لَیَقُولَN<نَّ> هَT<ـٰ>ذَا لِی وَمَW<اۤ> أَظُN<نُّ> ٱX<ل>سَّاعَةَ قَW<اۤ>ئِمَةX<ࣰ> N<وَ>لَئِX<ن> R<رُّ>جِعۡتُ إِلَىٰ R<رَ>بِّW<یۤ> إِN<نَّ> لِی عِN<ن>دَهُT<ۥ> لَلۡحُسۡنَىٰۚ فَلَنُنَبِّئَN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> بِمَا عَمِلُوX<ا۟> وَلَنُذِیقَN<نَّ>هُم N<مِّ>نۡ عَذَابٍ غَلِJ<ی>ظࣲ A<۝>D<٥٠>",
"وَإِذَW<اۤ> أَنۡعَمۡنَا عَلَى ٱلۡإِN<ن>سَT<ـٰ>نِ أَعۡR<رَ>ضَ وَنَءَا بِجَانِبِهِT<ۦ> وَإِذَا مَسَّهُ ٱX<ل>شَّR<رُّ> فَذُو دُعَW<اۤ>ءٍ عَرِJ<ی>ضࣲ A<۝>D<٥١>",
"قُلۡ أَR<رَ>ءَیۡتُمۡ إِN<ن> كَانَ مِنۡ عِN<ن>دِ ٱللَّهِ ثُN<مَّ> كَفَR<رۡ>تُN<م> بِهِT<ۦ> مَنۡ أَضَلُّ مِN<مَّ>نۡ هُوَ فِی شِقَاقِN<ۭ> بَعِJ<ی>Q<د>ࣲ A<۝>D<٥٢>",
"سَنُرِیهِمۡ ءَایَT<ـٰ>تِنَا فِی ٱلۡءَافَاقِ وَفِW<یۤ> أَN<ن>فُسِهِمۡ حَتَّىٰ یَتَبَیَّنَ لَهُمۡ أَN<نَّ>هُ ٱلۡحَقُّۗ أَوَ لَمۡ یَكۡفِ بِR<رَ>بِّكَ أَN<نَّ>هُT<ۥ> عَلَىٰ كُلِّ شَیۡءN<ࣲ> شَهِJ<ی>Q<د>ٌ A<۝>D<٥٣>",
"أَلَW<اۤ> إِN<نَّ>هُمۡ فِی مِرۡیَةX<ࣲ> N<مِّ>X<ن> لِّقَW<اۤ>ءِ R<رَ>بِّهِمۡۗ أَلَW<اۤ> إِN<نَّ>هُT<ۥ> بِكُلِّ شَیۡءX<ࣲ> N<مُّ>حِJ<ی>Q<ط>ُۢ A<۝>D<٥٤>",
"#حL<مۤ> A<۝>D<١>",
"L<عۤ>L<سۤ>L<قۤ> A<۝>D<٢>",
"كَذَT< ٰ>لِكَ یُوحِW<یۤ> إِلَیۡكَ وَإِلَى ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِكَ ٱللَّهُ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٣>",
"لَهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۖ وَهُوَ ٱلۡعَلِیُّ ٱلۡعَظِJ<ی>مُ A<۝>D<٤>",
"تَكَادُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تُ یَتَفَطَّR<رۡ>نَ مِN<ن> فَوۡقِهِN<نَّ>ۚ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ یُسَبِّحُونَ بِحَمۡدِ R<رَ>بِّهِمۡ وَیَسۡتَغۡفِR<رُ>ونَ لِمَN<ن> فِی ٱلۡأَR<رۡ>ضِۗ أَلَW<اۤ> إِN<نَّ> ٱللَّهَ هُوَ ٱلۡغَفُوR<رُ> ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٥>",
"وَX<ٱ>لَّذِینَ ٱتَّخَذُوX<ا۟> مِN<ن> دُونِهِW<ۦۤ> أَوۡلِیَW<اۤ>ءَ ٱللَّهُ حَفِیظٌ عَلَیۡهِمۡ وَمَW<اۤ> أَN<ن>تَ عَلَیۡهِN<م> بِوَكِJ<ی>لࣲ A<۝>D<٦>",
"وَكَذَT< ٰ>لِكَ أَوۡحَیۡنَW<اۤ> إِلَیۡكَ قُR<رۡ>ءَانًا عَR<رَ>بِیّX<ࣰ>ا لِّتُN<ن>ذِR<رَ> أُN<مَّ> ٱلۡقُR<رَ>ىٰ وَمَنۡ حَوۡلَهَا وَتُN<ن>ذِR<رَ> یَوۡمَ ٱلۡجَمۡعِ لَا R<رَ>یۡبَ فِیهِۚ فَرِیقN<ࣱ> فِی ٱلۡجَN<نَّ>ةِ وَفَرِیقN<ࣱ> فِی ٱX<ل>سَّعِJ<ی>رِ A<۝>D<٧>",
"وَلَوۡ شَW<اۤ>ءَ ٱللَّهُ لَجَعَلَهُمۡ أُN<مَّ>ةX<ࣰ> N<وَ>T< ٰ>حِدَةX<ࣰ> N<وَ>لَT<ـٰ>كِX<ن> N<یُ>Q<دۡ>خِلُ مَX<ن> N<یَ>شَW<اۤ>ءُ فِی R<رَ>حۡمَتِهِT<ۦ>ۚ وَX<ٱ>X<ل>ظَّT<ـٰ>لِمُونَ مَا لَهُم N<مِّ>X<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا نَصِJ<ی>رٍ A<۝>D<٨>",
"أَمِ ٱتَّخَذُوX<ا۟> مِN<ن> دُونِهِW<ۦۤ> أَوۡلِیَW<اۤ>ءَۖ فَٱللَّهُ هُوَ ٱلۡوَلِیُّ وَهُوَ یُحۡیِ ٱلۡمَوۡتَىٰ وَهُوَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٩>",
"وَمَا ٱخۡتَلَفۡتُمۡ فِیهِ مِN<ن> شَیۡءN<ࣲ> فَحُكۡمُهُW<ۥۤ> إِلَى ٱللَّهِۚ ذَT< ٰ>لِكُمُ ٱللَّهُ R<رَ>بِّی عَلَیۡهِ تَوَكَّلۡتُ وَإِلَیۡهِ أُنِJ<ی>Q<ب>ُ A<۝>D<١٠>",
"فَاطِR<رُ> ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ جَعَلَ لَكُم N<مِّ>نۡ أَN<ن>فُسِكُمۡ أَزۡوَT< ٰ>جX<ࣰ>ا N<وَ>مِنَ ٱلۡأَنۡعَT<ـٰ>مِ أَزۡوَT< ٰ>جX<ࣰ>ا N<یَ>ذۡR<رَ>ؤُكُمۡ فِیهِۚ لَیۡسَ كَمِثۡلِهِT<ۦ> شَیۡءX<ࣱ>ۖ N<وَ>هُوَ ٱX<ل>سَّمِیعُ ٱلۡبَصِJ<ی>رُ A<۝>D<١١>",
"لَهُT<ۥ> مَقَالِیدُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ یَQ<بۡ>سُطُ ٱX<ل>رِّزۡقَ لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیَQ<قۡ>دِR<رُ>ۚ إِN<نَّ>هُT<ۥ> بِكُلِّ شَیۡءٍ عَلِJ<ی>مࣱ A<۝>D<١٢>",
"۞ شَR<رَ>عَ لَكُم N<مِّ>نَ ٱX<ل>دِّینِ مَا وَصَّىٰ بِهِT<ۦ> نُوحX<ࣰ>ا N<وَ>X<ٱ>لَّذِW<یۤ> أَوۡحَیۡنَW<اۤ> إِلَیۡكَ وَمَا وَصَّیۡنَا بِهِW<ۦۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَمُوسَىٰ وَعِیسَىW<ٰۤ>ۖ أَنۡ أَقِیمُوX<ا۟> ٱX<ل>دِّینَ وَلَا تَتَفَR<رَّ>قُوX<ا۟> فِیهِۚ كَبُR<رَ> عَلَى ٱلۡمُشۡرِكِینَ مَا تَQ<دۡ>عُوهُمۡ إِلَیۡهِۚ ٱللَّهُ یَQ<جۡ>تَبِW<یۤ> إِلَیۡهِ مَX<ن> N<یَ>شَW<اۤ>ءُ وَیَهۡدِW<یۤ> إِلَیۡهِ مَX<ن> N<یُ>نِJ<ی>Q<ب>ُ A<۝>D<١٣>",
"وَمَا تَفَR<رَّ>قُW<وۤ>X<ا۟> إِلَّا مِX<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَهُمُ ٱلۡعِلۡمُ بَغۡیَN<ۢ>ا بَیۡنَهُمۡۚ وَلَوۡلَا كَلِمَةN<ࣱ> سَبَقَتۡ مِX<ن> R<رَّ>بِّكَ إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>X<ࣰ>ى لَّقُضِیَ بَیۡنَهُمۡۚ وَإِN<نَّ> ٱلَّذِینَ أُورِثُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ مِX<ن>N<ۢ> بَعۡدِهِمۡ لَفِی شَكّX<ࣲ> N<مِّ>نۡهُ مُرِJ<ی>Q<ب>ࣲ A<۝>D<١٤>",
"فَلِذَT< ٰ>لِكَ فَX<ٱ>Q<دۡ>عُۖ وَX<ٱ>سۡتَقِمۡ كَمَW<اۤ> أُمِرۡتَۖ وَلَا تَتَّبِعۡ أَهۡوَW<اۤ>ءَهُمۡۖ وَقُلۡ ءَامَN<ن>تُ بِمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ مِN<ن> كِتَT<ـٰ>بX<ࣲ>ۖ N<وَ>أُمِرۡتُ لِأَعۡدِلَ بَیۡنَكُمُۖ ٱللَّهُ R<رَ>بُّنَا وَR<رَ>بُّكُمۡۖ لَنَW<اۤ> أَعۡمَT<ـٰ>لُنَا وَلَكُمۡ أَعۡمَT<ـٰ>لُكُمۡۖ لَا حُجَّةَ بَیۡنَنَا وَبَیۡنَكُمُۖ ٱللَّهُ یَQ<جۡ>مَعُ بَیۡنَنَاۖ وَإِلَیۡهِ ٱلۡمَصِJ<ی>رُ A<۝>D<١٥>",
"وَX<ٱ>لَّذِینَ یُحَL<اۤ>جُّونَ فِی ٱللَّهِ مِX<ن>N<ۢ> بَعۡدِ مَا ٱسۡتُجِیبَ لَهُT<ۥ> حُجَّتُهُمۡ دَاحِضَةٌ عِN<ن>دَ R<رَ>بِّهِمۡ وَعَلَیۡهِمۡ غَضَبX<ࣱ> N<وَ>لَهُمۡ عَذَابN<ࣱ> شَدِJ<ی>Q<د>ٌ A<۝>D<١٦>",
"ٱللَّهُ ٱلَّذِW<یۤ> أَN<ن>زَلَ ٱلۡكِتَT<ـٰ>بَ بِX<ٱ>لۡحَقِّ وَX<ٱ>لۡمِیزَانَۗ وَمَا یُQ<دۡ>رِیكَ لَعَلَّ ٱX<ل>سَّاعَةَ قَرِJ<ی>Q<ب>ࣱ A<۝>D<١٧>",
"یَسۡتَعۡجِلُ بِهَا ٱلَّذِینَ لَا یُؤۡمِنُونَ بِهَاۖ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مُشۡفِقُونَ مِنۡهَا وَیَعۡلَمُونَ أَN<نَّ>هَا ٱلۡحَقُّۗ أَلَW<اۤ> إِN<نَّ> ٱلَّذِینَ یُمَاR<رُ>ونَ فِی ٱX<ل>سَّاعَةِ لَفِی ضَلَT<ـٰ>لِN<ۭ> بَعِJ<ی>Q<د>ٍ A<۝>D<١٨>",
"ٱللَّهُ لَطِیفُN<ۢ> بِعِبَادِهِT<ۦ> یَR<رۡ>زُقُ مَX<ن> N<یَ>شَW<اۤ>ءُۖ وَهُوَ ٱلۡقَوِیُّ ٱلۡعَزِJ<ی>زُ A<۝>D<١٩>",
"مَN<ن> كَانَ یُرِیدُ حَR<رۡ>ثَ ٱلۡءَاخِR<رَ>ةِ نَزِQ<دۡ> لَهُT<ۥ> فِی حَR<رۡ>ثِهِT<ۦ>ۖ وَمَN<ن> كَانَ یُرِیدُ حَR<رۡ>ثَ ٱX<ل>دُّنۡیَا نُؤۡتِهِT<ۦ> مِنۡهَا وَمَا لَهُT<ۥ> فِی ٱلۡءَاخِR<رَ>ةِ مِن N<نَّ>صِJ<ی>Q<ب>ٍ A<۝>D<٢٠>",
"أَمۡ لَهُمۡ شُR<رَ>كَW<ـٰۤ>ؤُX<ا۟> شَR<رَ>عُوX<ا۟> لَهُم N<مِّ>نَ ٱX<ل>دِّینِ مَا لَمۡ یَأۡذَX<ن>N<ۢ> بِهِ ٱللَّهُۚ وَلَوۡلَا كَلِمَةُ ٱلۡفَصۡلِ لَقُضِیَ بَیۡنَهُمۡۗ وَإِN<نَّ> ٱX<ل>ظَّT<ـٰ>لِمِینَ لَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٢١>",
"تَR<رَ>ى ٱX<ل>ظَّT<ـٰ>لِمِینَ مُشۡفِقِینَ مِN<مَّ>ا كَسَبُوX<ا۟> وَهُوَ وَاقِعُN<ۢ> بِهِمۡۗ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ فِی R<رَ>وۡضَاتِ ٱلۡجَN<نَّ>اتِۖ لَهُم N<مَّ>ا یَشَW<اۤ>ءُونَ عِN<ن>دَ R<رَ>بِّهِمۡۚ ذَT< ٰ>لِكَ هُوَ ٱلۡفَضۡلُ ٱلۡكَبِJ<ی>رُ A<۝>D<٢٢>",
"ذَT< ٰ>لِكَ ٱلَّذِی یُبَشِّR<رُ> ٱللَّهُ عِبَادَهُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِۗ قُل لَّW<اۤ> أَسۡءَلُكُمۡ عَلَیۡهِ أَQ<جۡ>R<ر>ًا إِلَّا ٱلۡمَوَدَّةَ فِی ٱلۡقُR<رۡ>بَىٰۗ وَمَX<ن> N<یَ>Q<قۡ>تَرِفۡ حَسَنَةX<ࣰ> N<نَّ>زِQ<دۡ> لَهُT<ۥ> فِیهَا حُسۡنًاۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>N<ࣱ> شَكُJ<و>R<ر>ٌ A<۝>D<٢٣>",
"أَمۡ یَقُولُونَ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ كَذِبN<ࣰ>اۖ فَإِX<ن> N<یَ>شَإِ ٱللَّهُ یَخۡتِمۡ عَلَىٰ قَلۡبِكَۗ وَیَمۡحُ ٱللَّهُ ٱلۡبَT<ـٰ>طِلَ وَیُحِقُّ ٱلۡحَقَّ بِكَلِمَT<ـٰ>تِهِW<ۦۤ>ۚ إِN<نَّ>هُT<ۥ> عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٢٤>",
"وَهُوَ ٱلَّذِی یَQ<قۡ>بَلُ ٱX<ل>تَّوۡبَةَ عَنۡ عِبَادِهِT<ۦ> وَیَعۡفُوX<ا۟> عَنِ ٱX<ل>سَّیِّءَاتِ وَیَعۡلَمُ مَا تَفۡعَلُJ<و>نَ A<۝>D<٢٥>",
"وَیَسۡتَجِیبُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَیَزِیدُهُم N<مِّ>N<ن> فَضۡلِهِT<ۦ>ۚ وَX<ٱ>لۡكَT<ـٰ>فِR<رُ>ونَ لَهُمۡ عَذَابN<ࣱ> شَدِJ<ی>Q<د>ࣱ A<۝>D<٢٦>",
"۞ وَلَوۡ بَسَطَ ٱللَّهُ ٱX<ل>رِّزۡقَ لِعِبَادِهِT<ۦ> لَبَغَوۡX<ا۟> فِی ٱلۡأَR<رۡ>ضِ وَلَT<ـٰ>كِX<ن> N<یُ>نَزِّلُ بِقَدَرX<ࣲ> N<مَّ>ا یَشَW<اۤ>ءُۚ إِN<نَّ>هُT<ۥ> بِعِبَادِهِT<ۦ> خَبِیR<رُ>N<ۢ> بَصِJ<ی>رࣱ A<۝>D<٢٧>",
"وَهُوَ ٱلَّذِی یُنَزِّلُ ٱلۡغَیۡثَ مِX<ن>N<ۢ> بَعۡدِ مَا قَنَطُوX<ا۟> وَیَN<ن>شُR<رُ> R<رَ>حۡمَتَهُT<ۥ>ۚ وَهُوَ ٱلۡوَلِیُّ ٱلۡحَمِJ<ی>Q<د>ُ A<۝>D<٢٨>",
"وَمِنۡ ءَایَT<ـٰ>تِهِT<ۦ> خَلۡقُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَثَّ فِیهِمَا مِN<ن> دَL<اۤ>بَّةX<ࣲ>ۚ N<وَ>هُوَ عَلَىٰ جَمۡعِهِمۡ إِذَا یَشَW<اۤ>ءُ قَدِJ<ی>رࣱ A<۝>D<٢٩>",
"وَمَW<اۤ> أَصَT<ـٰ>بَكُم N<مِّ>X<ن> N<مُّ>صِیبَةN<ࣲ> فَبِمَا كَسَبَتۡ أَیۡدِیكُمۡ وَیَعۡفُوX<ا۟> عَN<ن> كَثِJ<ی>رࣲ A<۝>D<٣٠>",
"وَمَW<اۤ> أَN<ن>تُN<م> بِمُعۡجِزِینَ فِی ٱلۡأَR<رۡ>ضِۖ وَمَا لَكُم N<مِّ>N<ن> دُونِ ٱللَّهِ مِX<ن> N<وَ>لِیّX<ࣲ> N<وَ>لَا نَصِJ<ی>رࣲ A<۝>D<٣١>",
"وَمِنۡ ءَایَT<ـٰ>تِهِ ٱلۡجَوَارِ فِی ٱلۡبَحۡرِ كَX<ٱ>لۡأَعۡلَT<ـٰ>مِ A<۝>D<٣٢>",
"إِX<ن> N<یَ>شَأۡ یُسۡكِنِ ٱX<ل>رِّیحَ فَیَظۡلَلۡنَ R<رَ>وَاكِدَ عَلَىٰ ظَهۡرِهِW<ۦۤ>ۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّكُلِّ صَبَّارN<ࣲ> شَكُJ<و>R<ر>ٍ A<۝>D<٣٣>",
"أَوۡ یُوبِQ<قۡ>هُN<نَّ> بِمَا كَسَبُوX<ا۟> وَیَعۡفُ عَN<ن> كَثِJ<ی>رࣲ A<۝>D<٣٤>",
"وَیَعۡلَمَ ٱلَّذِینَ یُجَT<ـٰ>دِلُونَ فِW<یۤ> ءَایَT<ـٰ>تِنَا مَا لَهُم N<مِّ>X<ن> N<مَّ>حِJ<ی>صࣲ A<۝>D<٣٥>",
"فَمَW<اۤ> أُوتِیتُم N<مِّ>N<ن> شَیۡءN<ࣲ> فَمَتَT<ـٰ>عُ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۚ وَمَا عِN<ن>دَ ٱللَّهِ خَیۡR<ر>X<ࣱ> N<وَ>أَQ<بۡ>قَىٰ لِلَّذِینَ ءَامَنُوX<ا۟> وَعَلَىٰ R<رَ>بِّهِمۡ یَتَوَكَّلُJ<و>نَ A<۝>D<٣٦>",
"وَX<ٱ>لَّذِینَ یَQ<جۡ>تَنِبُونَ كَبَW<ـٰۤ>ئِR<رَ> ٱلۡإِثۡمِ وَX<ٱ>لۡفَوَT< ٰ>حِشَ وَإِذَا مَا غَضِبُوX<ا۟> هُمۡ یَغۡفِR<رُ>J<و>نَ A<۝>D<٣٧>",
"وَX<ٱ>لَّذِینَ ٱسۡتَجَابُوX<ا۟> لِR<رَ>بِّهِمۡ وَأَقَامُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَأَمۡR<رُ>هُمۡ شُوR<رَ>ىٰ بَیۡنَهُمۡ وَمِN<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>هُمۡ یُN<ن>فِقُJ<و>نَ A<۝>D<٣٨>",
"وَX<ٱ>لَّذِینَ إِذَW<اۤ> أَصَابَهُمُ ٱلۡبَغۡیُ هُمۡ یَN<ن>تَصِR<رُ>J<و>نَ A<۝>D<٣٩>",
"وَجَزَW< ٰۤ>ؤُX<ا۟> سَیِّئَةN<ࣲ> سَیِّئَةX<ࣱ> N<مِّ>ثۡلُهَاۖ فَمَنۡ عَفَا وَأَصۡلَحَ فَأَQ<جۡ>R<رُ>هُT<ۥ> عَلَى ٱللَّهِۚ إِN<نَّ>هُT<ۥ> لَا یُحِبُّ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٤٠>",
"وَلَمَنِ ٱN<ن>تَصَR<رَ> بَعۡدَ ظُلۡمِهِT<ۦ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ مَا عَلَیۡهِم N<مِّ>N<ن> سَبِJ<ی>لٍ A<۝>D<٤١>",
"إِN<نَّ>مَا ٱX<ل>سَّبِیلُ عَلَى ٱلَّذِینَ یَظۡلِمُونَ ٱX<ل>N<نَّ>اسَ وَیَQ<بۡ>غُونَ فِی ٱلۡأَR<رۡ>ضِ بِغَیۡرِ ٱلۡحَقِّۚ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٤٢>",
"وَلَمَN<ن> صَبَR<رَ> وَغَفَR<رَ> إِN<نَّ> ذَT< ٰ>لِكَ لَمِنۡ عَزۡمِ ٱلۡأُمُJ<و>R<ر>ِ A<۝>D<٤٣>",
"وَمَX<ن> N<یُ>ضۡلِلِ ٱللَّهُ فَمَا لَهُT<ۥ> مِX<ن> N<وَ>لِیّX<ࣲ> N<مِّ>X<ن>N<ۢ> بَعۡدِهِT<ۦ>ۗ وَتَR<رَ>ى ٱX<ل>ظَّT<ـٰ>لِمِینَ لَN<مَّ>ا R<رَ>أَوُX<ا۟> ٱلۡعَذَابَ یَقُولُونَ هَلۡ إِلَىٰ مَR<رَ>دّX<ࣲ> N<مِّ>N<ن> سَبِJ<ی>لࣲ A<۝>D<٤٤>",
"وَتَR<رَ>X<ى>T<ٰ>هُمۡ یُعۡR<رَ>ضُونَ عَلَیۡهَا خَT<ـٰ>شِعِینَ مِنَ ٱX<ل>ذُّلِّ یَN<ن>ظُR<رُ>ونَ مِN<ن> طَR<رۡ>فٍ خَفِیّX<ࣲ>ۗ N<وَ>قَالَ ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<نَّ> ٱلۡخَT<ـٰ>سِرِینَ ٱلَّذِینَ خَسِR<رُ>W<وۤ>X<ا۟> أَN<ن>فُسَهُمۡ وَأَهۡلِیهِمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۗ أَلَW<اۤ> إِN<نَّ> ٱX<ل>ظَّT<ـٰ>لِمِینَ فِی عَذَابX<ࣲ> N<مُّ>قِJ<ی>مࣲ A<۝>D<٤٥>",
"وَمَا كَانَ لَهُم N<مِّ>نۡ أَوۡلِیَW<اۤ>ءَ یَN<ن>صُR<رُ>ونَهُم N<مِّ>N<ن> دُونِ ٱللَّهِۗ وَمَX<ن> N<یُ>ضۡلِلِ ٱللَّهُ فَمَا لَهُT<ۥ> مِN<ن> سَبِJ<ی>لٍ A<۝>D<٤٦>",
"ٱسۡتَجِیبُوX<ا۟> لِR<رَ>بِّكُم N<مِّ>N<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>أۡتِیَ یَوۡمX<ࣱ> لَّا مَR<رَ>دَّ لَهُT<ۥ> مِنَ ٱللَّهِۚ مَا لَكُم N<مِّ>X<ن> N<مَّ>لۡجَإX<ࣲ> N<یَ>وۡمَئِذX<ࣲ> N<وَ>مَا لَكُم N<مِّ>ن N<نَّ>كِJ<ی>رࣲ A<۝>D<٤٧>",
"فَإِنۡ أَعۡR<رَ>ضُوX<ا۟> فَمَW<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ عَلَیۡهِمۡ حَفِیظًاۖ إِنۡ عَلَیۡكَ إِلَّا ٱلۡبَلَT<ـٰ>غُۗ وَإِN<نَّ>W<اۤ> إِذَW<اۤ> أَذَQ<قۡ>نَا ٱلۡإِN<ن>سَT<ـٰ>نَ مِN<نَّ>ا R<رَ>حۡمَةN<ࣰ> فَرِحَ بِهَاۖ وَإِN<ن> تُصِQ<بۡ>هُمۡ سَیِّئَةُN<ۢ> بِمَا قَدَّمَتۡ أَیۡدِیهِمۡ فَإِN<نَّ> ٱلۡإِN<ن>سَT<ـٰ>نَ كَفُJ<و>R<ر>ࣱ A<۝>D<٤٨>",
"لِّلَّهِ مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ یَخۡلُقُ مَا یَشَW<اۤ>ءُۚ یَهَبُ لِمَX<ن> N<یَ>شَW<اۤ>ءُ إِنَT<ـٰ>ثX<ࣰ>ا N<وَ>یَهَبُ لِمَX<ن> N<یَ>شَW<اۤ>ءُ ٱX<ل>ذُّكُJ<و>R<ر>َ A<۝>D<٤٩>",
"أَوۡ یُزَوِّجُهُمۡ ذُكۡR<رَ>انX<ࣰ>ا N<وَ>إِنَT<ـٰ>ثX<ࣰ>اۖ N<وَ>یَQ<جۡ>عَلُ مَX<ن> N<یَ>شَW<اۤ>ءُ عَقِیمًاۚ إِN<نَّ>هُT<ۥ> عَلِیمN<ࣱ> قَدِJ<ی>رࣱ A<۝>D<٥٠>",
"۞ وَمَا كَانَ لِبَشَرٍ أَX<ن> N<یُ>كَلِّمَهُ ٱللَّهُ إِلَّا وَحۡیًا أَوۡ مِX<ن> N<وَ>R<رَ>W<اۤ>ئِ حِجَابٍ أَوۡ یُR<رۡ>سِلَ R<رَ>سُولN<ࣰ>ا فَیُوحِیَ بِإِذۡنِهِT<ۦ> مَا یَشَW<اۤ>ءُۚ إِN<نَّ>هُT<ۥ> عَلِیٌّ حَكِJ<ی>مࣱ A<۝>D<٥١>",
"وَكَذَT< ٰ>لِكَ أَوۡحَیۡنَW<اۤ> إِلَیۡكَ R<رُ>وحX<ࣰ>ا N<مِّ>نۡ أَمۡرِنَاۚ مَا كُN<ن>تَ تَQ<دۡ>رِی مَا ٱلۡكِتَT<ـٰ>بُ وَلَا ٱلۡإِیمَT<ـٰ>نُ وَلَT<ـٰ>كِN<ن> جَعَلۡنَT<ـٰ>هُ نُوR<ر>X<ࣰ>ا N<نَّ>هۡدِی بِهِT<ۦ> مَن N<نَّ>شَW<اۤ>ءُ مِنۡ عِبَادِنَاۚ وَإِN<نَّ>كَ لَتَهۡدِW<یۤ> إِلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٥٢>",
"صِR<رَ>T< ٰ>طِ ٱللَّهِ ٱلَّذِی لَهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۗ أَلَW<اۤ> إِلَى ٱللَّهِ تَصِیR<رُ> ٱلۡأُمُJ<و>R<ر>ُ A<۝>D<٥٣>",
"#حL<مۤ> A<۝>D<١>",
"وَX<ٱ>لۡكِتَT<ـٰ>بِ ٱلۡمُبِJ<ی>نِ A<۝>D<٢>",
"إِN<نَّ>ا جَعَلۡنَT<ـٰ>هُ قُR<رۡ>ءَT< ٰ>نًا عَR<رَ>بِیّX<ࣰ>ا لَّعَلَّكُمۡ تَعۡقِلُJ<و>نَ A<۝>D<٣>",
"وَإِN<نَّ>هُT<ۥ> فِW<یۤ> أُN<مِّ> ٱلۡكِتَT<ـٰ>بِ لَدَیۡنَا لَعَلِیٌّ حَكِJ<ی>مٌ A<۝>D<٤>",
"أَفَنَضۡرِبُ عَN<ن>كُمُ ٱX<ل>ذِّكۡR<رَ> صَفۡحًا أَN<ن> كُN<ن>تُمۡ قَوۡمX<ࣰ>ا N<مُّ>سۡرِفِJ<ی>نَ A<۝>D<٥>",
"وَكَمۡ أَR<رۡ>سَلۡنَا مِن N<نَّ>بِیّN<ࣲ> فِی ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٦>",
"وَمَا یَأۡتِیهِم N<مِّ>ن N<نَّ>بِیٍّ إِلَّا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٧>",
"فَأَهۡلَكۡنَW<اۤ> أَشَدَّ مِنۡهُN<م> بَQ<طۡ>شX<ࣰ>ا N<وَ>مَضَىٰ مَثَلُ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٨>",
"وَلَئِN<ن> سَأَلۡتَهُم N<مَّ>نۡ خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ لَیَقُولُN<نَّ> خَلَقَهُN<نَّ> ٱلۡعَزِیزُ ٱلۡعَلِJ<ی>مُ A<۝>D<٩>",
"ٱلَّذِی جَعَلَ لَكُمُ ٱلۡأَR<رۡ>ضَ مَهۡدX<ࣰ>ا N<وَ>جَعَلَ لَكُمۡ فِیهَا سُبُلX<ࣰ>ا لَّعَلَّكُمۡ تَهۡتَدُJ<و>نَ A<۝>D<١٠>",
"وَX<ٱ>لَّذِی نَزَّلَ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءَN<ۢ> بِقَدَرN<ࣲ> فَأَN<ن>شَR<رۡ>نَا بِهِT<ۦ> بَلۡدَةX<ࣰ> N<مَّ>یۡتN<ࣰ>اۚ كَذَT< ٰ>لِكَ تُخۡR<رَ>جُJ<و>نَ A<۝>D<١١>",
"وَX<ٱ>لَّذِی خَلَقَ ٱلۡأَزۡوَT< ٰ>جَ كُلَّهَا وَجَعَلَ لَكُم N<مِّ>نَ ٱلۡفُلۡكِ وَX<ٱ>لۡأَنۡعَT<ـٰ>مِ مَا تَR<رۡ>كَبُJ<و>نَ A<۝>D<١٢>",
"لِتَسۡتَوُT<ۥ>X<ا۟> عَلَىٰ ظُهُورِهِT<ۦ> ثُN<مَّ> تَذۡكُR<رُ>وX<ا۟> نِعۡمَةَ R<رَ>بِّكُمۡ إِذَا ٱسۡتَوَیۡتُمۡ عَلَیۡهِ وَتَقُولُوX<ا۟> سُQ<بۡ>حَT<ـٰ>نَ ٱلَّذِی سَخَّR<رَ> لَنَا هَT<ـٰ>ذَا وَمَا كُN<نَّ>ا لَهُT<ۥ> مُQ<قۡ>رِنِJ<ی>نَ A<۝>D<١٣>",
"وَإِN<نَّ>W<اۤ> إِلَىٰ R<رَ>بِّنَا لَمُN<ن>قَلِبُJ<و>نَ A<۝>D<١٤>",
"وَجَعَلُوX<ا۟> لَهُT<ۥ> مِنۡ عِبَادِهِT<ۦ> جُزۡءًاۚ إِN<نَّ> ٱلۡإِN<ن>سَT<ـٰ>نَ لَكَفُوR<ر>X<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<١٥>",
"أَمِ ٱتَّخَذَ مِN<مَّ>ا یَخۡلُقُ بَنَاتX<ࣲ> N<وَ>أَصۡفَX<ى>T<ٰ>كُN<م> بِX<ٱ>لۡبَنِJ<ی>نَ A<۝>D<١٦>",
"وَإِذَا بُشِّR<رَ> أَحَدُهُN<م> بِمَا ضَR<رَ>بَ لِX<ل>R<رَّ>حۡمَT<ـٰ>نِ مَثَلN<ࣰ>ا ظَلَّ وَQ<جۡ>هُهُT<ۥ> مُسۡوَدّX<ࣰ>ا N<وَ>هُوَ كَظِJ<ی>مٌ A<۝>D<١٧>",
"أَوَ مَX<ن> N<یُ>نَشَّؤُX<ا۟> فِی ٱلۡحِلۡیَةِ وَهُوَ فِی ٱلۡخِصَامِ غَیۡR<رُ> مُبِJ<ی>نࣲ A<۝>D<١٨>",
"وَجَعَلُوX<ا۟> ٱلۡمَلَW<ـٰۤ>ئِكَةَ ٱلَّذِینَ هُمۡ عِبَT<ـٰ>دُ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ إِنَT<ـٰ>ثًاۚ أَشَهِدُوX<ا۟> خَلۡقَهُمۡۚ سَتُكۡتَبُ شَهَT<ـٰ>دَتُهُمۡ وَیُسۡءَلُJ<و>نَ A<۝>D<١٩>",
"وَقَالُوX<ا۟> لَوۡ شَW<اۤ>ءَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ مَا عَبَQ<دۡ>نَT<ـٰ>هُمۗ N<مَّ>ا لَهُN<م> بِذَT< ٰ>لِكَ مِنۡ عِلۡمٍۖ إِنۡ هُمۡ إِلَّا یَخۡR<رُ>صُJ<و>نَ A<۝>D<٢٠>",
"أَمۡ ءَاتَیۡنَT<ـٰ>هُمۡ كِتَT<ـٰ>بX<ࣰ>ا N<مِّ>N<ن> قَQ<بۡ>لِهِT<ۦ> فَهُN<م> بِهِT<ۦ> مُسۡتَمۡسِكُJ<و>نَ A<۝>D<٢١>",
"بَلۡ قَالُW<وۤ>X<ا۟> إِN<نَّ>ا وَجَQ<دۡ>نَW<اۤ> ءَابَW<اۤ>ءَنَا عَلَىW<ٰۤ> أُN<مَّ>ةX<ࣲ> N<وَ>إِN<نَّ>ا عَلَىW<ٰۤ> ءَاثَT<ـٰ>رِهِم N<مُّ>هۡتَدُJ<و>نَ A<۝>D<٢٢>",
"وَكَذَT< ٰ>لِكَ مَW<اۤ> أَR<رۡ>سَلۡنَا مِN<ن> قَQ<بۡ>لِكَ فِی قَR<رۡ>یَةX<ࣲ> N<مِّ>ن N<نَّ>ذِیرٍ إِلَّا قَالَ مُتۡR<رَ>فُوهَW<اۤ> إِN<نَّ>ا وَجَQ<دۡ>نَW<اۤ> ءَابَW<اۤ>ءَنَا عَلَىW<ٰۤ> أُN<مَّ>ةX<ࣲ> N<وَ>إِN<نَّ>ا عَلَىW<ٰۤ> ءَاثَT<ـٰ>رِهِم N<مُّ>Q<قۡ>تَدُJ<و>نَ A<۝>D<٢٣>",
"۞ قَT<ـٰ>لَ أَوَ لَوۡ جِئۡتُكُN<م> بِأَهۡدَىٰ مِN<مَّ>ا وَجَX<د>تُّمۡ عَلَیۡهِ ءَابَW<اۤ>ءَكُمۡۖ قَالُW<وۤ>X<ا۟> إِN<نَّ>ا بِمَW<اۤ> أُR<رۡ>سِلۡتُN<م> بِهِT<ۦ> كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٢٤>",
"فَX<ٱ>N<ن>تَقَمۡنَا مِنۡهُمۡۖ فَX<ٱ>N<ن>ظُR<رۡ> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلۡمُكَذِّبِJ<ی>نَ A<۝>D<٢٥>",
"وَإِذۡ قَالَ إِQ<بۡ>R<رَ>T< ٰ>هِیمُ لِأَبِیهِ وَقَوۡمِهِW<ۦۤ> إِN<نَّ>نِی بَR<رَ>W<اۤ>ءX<ࣱ> N<مِّ>N<مَّ>ا تَعۡبُدُJ<و>نَ A<۝>D<٢٦>",
"إِلَّا ٱلَّذِی فَطَR<رَ>نِی فَإِN<نَّ>هُT<ۥ> سَیَهۡدِJ<ی>نِ A<۝>D<٢٧>",
"وَجَعَلَهَا كَلِمَةَN<ۢ> بَاقِیَةN<ࣰ> فِی عَقِبِهِT<ۦ> لَعَلَّهُمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<٢٨>",
"بَلۡ مَتَّعۡتُ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ وَءَابَW<اۤ>ءَهُمۡ حَتَّىٰ جَW<اۤ>ءَهُمُ ٱلۡحَقُّ وَR<رَ>سُولX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٢٩>",
"وَلَN<مَّ>ا جَW<اۤ>ءَهُمُ ٱلۡحَقُّ قَالُوX<ا۟> هَT<ـٰ>ذَا سِحۡR<ر>X<ࣱ> N<وَ>إِN<نَّ>ا بِهِT<ۦ> كَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٣٠>",
"وَقَالُوX<ا۟> لَوۡلَا نُزِّلَ هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانُ عَلَىٰ R<رَ>جُلX<ࣲ> N<مِّ>نَ ٱلۡقَR<رۡ>یَتَیۡنِ عَظِJ<ی>مٍ A<۝>D<٣١>",
"أَهُمۡ یَQ<قۡ>سِمُونَ R<رَ>حۡمَتَ R<رَ>بِّكَۚ نَحۡنُ قَسَمۡنَا بَیۡنَهُم N<مَّ>عِیشَتَهُمۡ فِی ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۚ وَR<رَ>فَعۡنَا بَعۡضَهُمۡ فَوۡقَ بَعۡضN<ࣲ> دَR<رَ>جَT<ـٰ>تX<ࣲ> لِّیَتَّخِذَ بَعۡضُهُN<م> بَعۡضN<ࣰ>ا سُخۡرِیّX<ࣰ>اۗ N<وَ>R<رَ>حۡمَتُ R<رَ>بِّكَ خَیۡR<ر>X<ࣱ> N<مِّ>N<مَّ>ا یَQ<جۡ>مَعُJ<و>نَ A<۝>D<٣٢>",
"وَلَوۡلَW<اۤ> أَX<ن> N<یَ>كُونَ ٱX<ل>N<نَّ>اسُ أُN<مَّ>ةX<ࣰ> N<وَ>T< ٰ>حِدَةX<ࣰ> لَّجَعَلۡنَا لِمَX<ن> N<یَ>كۡفُR<رُ> بِX<ٱ>X<ل>R<رَّ>حۡمَT<ـٰ>نِ لِبُیُوتِهِمۡ سُقُفX<ࣰ>ا N<مِّ>N<ن> فِضَّةX<ࣲ> N<وَ>مَعَارِجَ عَلَیۡهَا یَظۡهَR<رُ>J<و>نَ A<۝>D<٣٣>",
"وَلِبُیُوتِهِمۡ أَQ<بۡ>وَT< ٰ>بX<ࣰ>ا N<وَ>سُR<رُ>R<ر>ًا عَلَیۡهَا یَتَّكِءُJ<و>نَ A<۝>D<٣٤>",
"وَزُخۡR<رُ>فX<ࣰ>اۚ N<وَ>إِN<ن> كُلُّ ذَT< ٰ>لِكَ لَN<مَّ>ا مَتَT<ـٰ>عُ ٱلۡحَیَX<و>T<ٰ>ةِ ٱX<ل>دُّنۡیَاۚ وَX<ٱ>لۡءَاخِR<رَ>ةُ عِN<ن>دَ R<رَ>بِّكَ لِلۡمُتَّقِJ<ی>نَ A<۝>D<٣٥>",
"وَمَX<ن> N<یَ>عۡشُ عَN<ن> ذِكۡرِ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ نُقَیِّضۡ لَهُT<ۥ> شَیۡطَT<ـٰ>نN<ࣰ>ا فَهُوَ لَهُT<ۥ> قَرِJ<ی>نࣱ A<۝>D<٣٦>",
"وَإِN<نَّ>هُمۡ لَیَصُدُّونَهُمۡ عَنِ ٱX<ل>سَّبِیلِ وَیَحۡسَبُونَ أَN<نَّ>هُم N<مُّ>هۡتَدُJ<و>نَ A<۝>D<٣٧>",
"حَتَّىW<ٰۤ> إِذَا جَW<اۤ>ءَنَا قَالَ یَT<ـٰ>لَیۡتَ بَیۡنِی وَبَیۡنَكَ بُعۡدَ ٱلۡمَشۡرِقَیۡنِ فَبِئۡسَ ٱلۡقَرِJ<ی>نُ A<۝>D<٣٨>",
"وَلَX<ن> N<یَ>N<ن>فَعَكُمُ ٱلۡیَوۡمَ إِذ ظَّلَمۡتُمۡ أَN<نَّ>كُمۡ فِی ٱلۡعَذَابِ مُشۡتَرِكُJ<و>نَ A<۝>D<٣٩>",
"أَفَأَN<ن>تَ تُسۡمِعُ ٱX<ل>صُّN<مَّ> أَوۡ تَهۡدِی ٱلۡعُمۡیَ وَمَN<ن> كَانَ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٤٠>",
"فَإِN<مَّ>ا نَذۡهَبَN<نَّ> بِكَ فَإِN<نَّ>ا مِنۡهُم N<مُّ>N<ن>تَقِمُJ<و>نَ A<۝>D<٤١>",
"أَوۡ نُرِیَN<نَّ>كَ ٱلَّذِی وَعَQ<دۡ>نَT<ـٰ>هُمۡ فَإِN<نَّ>ا عَلَیۡهِم N<مُّ>Q<قۡ>تَدِR<رُ>J<و>نَ A<۝>D<٤٢>",
"فَX<ٱ>سۡتَمۡسِكۡ بِX<ٱ>لَّذِW<یۤ> أُوحِیَ إِلَیۡكَۖ إِN<نَّ>كَ عَلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٤٣>",
"وَإِN<نَّ>هُT<ۥ> لَذِكۡR<ر>X<ࣱ> لَّكَ وَلِقَوۡمِكَۖ وَسَوۡفَ تُسۡءَلُJ<و>نَ A<۝>D<٤٤>",
"وَسۡءَلۡ مَنۡ أَR<رۡ>سَلۡنَا مِN<ن> قَQ<بۡ>لِكَ مِX<ن> R<رُّ>سُلِنَW<اۤ> أَجَعَلۡنَا مِN<ن> دُونِ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ ءَالِهَةX<ࣰ> N<یُ>عۡبَدُJ<و>نَ A<۝>D<٤٥>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا مُوسَىٰ بِءَایَT<ـٰ>تِنَW<اۤ> إِلَىٰ فِرۡعَوۡنَ وَمَلَإِX<ی۟>هِT<ۦ> فَقَالَ إِN<نِّ>ی R<رَ>سُولُ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٤٦>",
"فَلَN<مَّ>ا جَW<اۤ>ءَهُN<م> بِءَایَT<ـٰ>تِنَW<اۤ> إِذَا هُم N<مِّ>نۡهَا یَضۡحَكُJ<و>نَ A<۝>D<٤٧>",
"وَمَا نُرِیهِم N<مِّ>نۡ ءَایَةٍ إِلَّا هِیَ أَكۡبَR<رُ> مِنۡ أُخۡتِهَاۖ وَأَخَذۡنَT<ـٰ>هُN<م> بِX<ٱ>لۡعَذَابِ لَعَلَّهُمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<٤٨>",
"وَقَالُوX<ا۟> یَW<ـٰۤ>أَیُّهَ ٱX<ل>سَّاحِR<رُ> ٱQ<دۡ>عُ لَنَا R<رَ>بَّكَ بِمَا عَهِدَ عِN<ن>دَكَ إِN<نَّ>نَا لَمُهۡتَدُJ<و>نَ A<۝>D<٤٩>",
"فَلَN<مَّ>ا كَشَفۡنَا عَنۡهُمُ ٱلۡعَذَابَ إِذَا هُمۡ یَN<ن>كُثُJ<و>نَ A<۝>D<٥٠>",
"وَنَادَىٰ فِرۡعَوۡنُ فِی قَوۡمِهِT<ۦ> قَالَ یَT<ـٰ>قَوۡمِ أَلَیۡسَ لِی مُلۡكُ مِصۡR<رَ> وَهَT<ـٰ>ذِهِ ٱلۡأَنۡهَT<ـٰ>R<رُ> تَQ<جۡ>رِی مِN<ن> تَحۡتِW<یۤ>ۚ أَفَلَا تُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٥١>",
"أَمۡ أَنَا۠ خَیۡR<ر>X<ࣱ> N<مِّ>نۡ هَT<ـٰ>ذَا ٱلَّذِی هُوَ مَهِینX<ࣱ> N<وَ>لَا یَكَادُ یُبِJ<ی>نُ A<۝>D<٥٢>",
"فَلَوۡلَW<اۤ> أُلۡقِیَ عَلَیۡهِ أَسۡوِR<رَ>ةX<ࣱ> N<مِّ>N<ن> ذَهَبٍ أَوۡ جَW<اۤ>ءَ مَعَهُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ مُQ<قۡ>تَرِنِJ<ی>نَ A<۝>D<٥٣>",
"فَX<ٱ>سۡتَخَفَّ قَوۡمَهُT<ۥ> فَأَطَاعُوهُۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> قَوۡمN<ࣰ>ا فَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٥٤>",
"فَلَN<مَّ>W<اۤ> ءَاسَفُونَا ٱN<ن>تَقَمۡنَا مِنۡهُمۡ فَأَغۡR<رَ>Q<قۡ>نَT<ـٰ>هُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٥٥>",
"فَجَعَلۡنَT<ـٰ>هُمۡ سَلَفX<ࣰ>ا N<وَ>مَثَلX<ࣰ>ا لِّلۡءَاخِرِJ<ی>نَ A<۝>D<٥٦>",
"۞ وَلَN<مَّ>ا ضُرِبَ ٱQ<بۡ>نُ مَR<رۡ>یَمَ مَثَلًا إِذَا قَوۡمُكَ مِنۡهُ یَصِدُّJ<و>نَ A<۝>D<٥٧>",
"وَقَالُW<وۤ>X<ا۟> ءَأَT< ٰ>لِهَتُنَا خَیۡR<ر>ٌ أَمۡ هُوَۚ مَا ضَR<رَ>بُوهُ لَكَ إِلَّا جَدَلَN<ۢ>اۚ بَلۡ هُمۡ قَوۡمٌ خَصِمُJ<و>نَ A<۝>D<٥٨>",
"إِنۡ هُوَ إِلَّا عَQ<بۡ>دٌ أَنۡعَمۡنَا عَلَیۡهِ وَجَعَلۡنَT<ـٰ>هُ مَثَلX<ࣰ>ا لِّبَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِJ<ی>لَ A<۝>D<٥٩>",
"وَلَوۡ نَشَW<اۤ>ءُ لَجَعَلۡنَا مِN<ن>كُم N<مَّ>لَW<ـٰۤ>ئِكَةN<ࣰ> فِی ٱلۡأَR<رۡ>ضِ یَخۡلُفُJ<و>نَ A<۝>D<٦٠>",
"وَإِN<نَّ>هُT<ۥ> لَعِلۡمX<ࣱ> لِّX<ل>سَّاعَةِ فَلَا تَمۡتَR<رُ>N<نَّ> بِهَا وَX<ٱ>تَّبِعُونِۚ هَT<ـٰ>ذَا صِR<رَ>T< ٰ>طX<ࣱ> N<مُّ>سۡتَقِJ<ی>مࣱ A<۝>D<٦١>",
"وَلَا یَصُدَّN<نَّ>كُمُ ٱX<ل>شَّیۡطَT<ـٰ>نُۖ إِN<نَّ>هُT<ۥ> لَكُمۡ عَدُوّX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٦٢>",
"وَلَN<مَّ>ا جَW<اۤ>ءَ عِیسَىٰ بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ قَالَ قَQ<دۡ> جِئۡتُكُN<م> بِX<ٱ>لۡحِكۡمَةِ وَلِأُبَیِّنَ لَكُN<م> بَعۡضَ ٱلَّذِی تَخۡتَلِفُونَ فِیهِۖ فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ وَأَطِیعُJ<و>نِ A<۝>D<٦٣>",
"إِN<نَّ> ٱللَّهَ هُوَ R<رَ>بِّی وَR<رَ>بُّكُمۡ فَX<ٱ>عۡبُدُوهُۚ هَT<ـٰ>ذَا صِR<رَ>T< ٰ>طX<ࣱ> N<مُّ>سۡتَقِJ<ی>مࣱ A<۝>D<٦٤>",
"فَX<ٱ>خۡتَلَفَ ٱلۡأَحۡزَابُ مِX<ن>N<ۢ> بَیۡنِهِمۡۖ فَوَیۡلX<ࣱ> لِّلَّذِینَ ظَلَمُوX<ا۟> مِنۡ عَذَابِ یَوۡمٍ أَلِJ<ی>مٍ A<۝>D<٦٥>",
"هَلۡ یَN<ن>ظُR<رُ>ونَ إِلَّا ٱX<ل>سَّاعَةَ أَN<ن> تَأۡتِیَهُN<م> بَغۡتَةX<ࣰ> N<وَ>هُمۡ لَا یَشۡعُR<رُ>J<و>نَ A<۝>D<٦٦>",
"ٱلۡأَخِلَّW<اۤ>ءُ یَوۡمَئِذِN<ۭ> بَعۡضُهُمۡ لِبَعۡضٍ عَدُوٌّ إِلَّا ٱلۡمُتَّقِJ<ی>نَ A<۝>D<٦٧>",
"یَT<ـٰ>عِبَادِ لَا خَوۡفٌ عَلَیۡكُمُ ٱلۡیَوۡمَ وَلَW<اۤ> أَN<ن>تُمۡ تَحۡزَنُJ<و>نَ A<۝>D<٦٨>",
"ٱلَّذِینَ ءَامَنُوX<ا۟> بِءَایَT<ـٰ>تِنَا وَكَانُوX<ا۟> مُسۡلِمِJ<ی>نَ A<۝>D<٦٩>",
"ٱQ<دۡ>خُلُوX<ا۟> ٱلۡجَN<نَّ>ةَ أَN<ن>تُمۡ وَأَزۡوَT< ٰ>جُكُمۡ تُحۡبَR<رُ>J<و>نَ A<۝>D<٧٠>",
"یُطَافُ عَلَیۡهِN<م> بِصِحَافX<ࣲ> N<مِّ>N<ن> ذَهَبX<ࣲ> N<وَ>أَكۡوَابX<ࣲ>ۖ N<وَ>فِیهَا مَا تَشۡتَهِیهِ ٱلۡأَN<ن>فُسُ وَتَلَذُّ ٱلۡأَعۡیُنُۖ وَأَN<ن>تُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٧١>",
"وَتِلۡكَ ٱلۡجَN<نَّ>ةُ ٱلَّتِW<یۤ> أُورِثۡتُمُوهَا بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٧٢>",
"لَكُمۡ فِیهَا فَT<ـٰ>كِهَةN<ࣱ> كَثِیR<رَ>ةX<ࣱ> N<مِّ>نۡهَا تَأۡكُلُJ<و>نَ A<۝>D<٧٣>",
"إِN<نَّ> ٱلۡمُQ<جۡ>رِمِینَ فِی عَذَابِ جَهَN<نَّ>مَ خَT<ـٰ>لِدُJ<و>نَ A<۝>D<٧٤>",
"لَا یُفَتَّR<رُ> عَنۡهُمۡ وَهُمۡ فِیهِ مُQ<بۡ>لِسُJ<و>نَ A<۝>D<٧٥>",
"وَمَا ظَلَمۡنَT<ـٰ>هُمۡ وَلَT<ـٰ>كِN<ن> كَانُوX<ا۟> هُمُ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٧٦>",
"وَنَادَوۡX<ا۟> یَT<ـٰ>مَT<ـٰ>لِكُ لِیَQ<قۡ>ضِ عَلَیۡنَا R<رَ>بُّكَۖ قَالَ إِN<نَّ>كُم N<مَّ>T<ـٰ>كِثُJ<و>نَ A<۝>D<٧٧>",
"لَقَQ<دۡ> جِئۡنَT<ـٰ>كُN<م> بِX<ٱ>لۡحَقِّ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>كُمۡ لِلۡحَقِّ كَT<ـٰ>رِهُJ<و>نَ A<۝>D<٧٨>",
"أَمۡ أَQ<بۡ>R<رَ>مُW<وۤ>X<ا۟> أَمۡR<ر>N<ࣰ>ا فَإِN<نَّ>ا مُQ<بۡ>رِمُJ<و>نَ A<۝>D<٧٩>",
"أَمۡ یَحۡسَبُونَ أَN<نَّ>ا لَا نَسۡمَعُ سِR<رَّ>هُمۡ وَنَQ<جۡ>وَX<ى>T<ٰ>هُN<م>ۚ بَلَىٰ وَR<رُ>سُلُنَا لَدَیۡهِمۡ یَكۡتُبُJ<و>نَ A<۝>D<٨٠>",
"قُلۡ إِN<ن> كَانَ لِX<ل>R<رَّ>حۡمَT<ـٰ>نِ وَلَدN<ࣱ> فَأَنَا۠ أَوَّلُ ٱلۡعَT<ـٰ>بِدِJ<ی>نَ A<۝>D<٨١>",
"سُQ<بۡ>حَT<ـٰ>نَ R<رَ>بِّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ R<رَ>بِّ ٱلۡعَR<رۡ>شِ عَN<مَّ>ا یَصِفُJ<و>نَ A<۝>D<٨٢>",
"فَذَR<رۡ>هُمۡ یَخُوضُوX<ا۟> وَیَلۡعَبُوX<ا۟> حَتَّىٰ یُلَT<ـٰ>قُوX<ا۟> یَوۡمَهُمُ ٱلَّذِی یُوعَدُJ<و>نَ A<۝>D<٨٣>",
"وَهُوَ ٱلَّذِی فِی ٱX<ل>سَّمَW<اۤ>ءِ إِلَT<ـٰ>هX<ࣱ> N<وَ>فِی ٱلۡأَR<رۡ>ضِ إِلَT<ـٰ>هX<ࣱ>ۚ N<وَ>هُوَ ٱلۡحَكِیمُ ٱلۡعَلِJ<ی>مُ A<۝>D<٨٤>",
"وَتَبَاR<رَ>كَ ٱلَّذِی لَهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَا وَعِN<ن>دَهُT<ۥ> عِلۡمُ ٱX<ل>سَّاعَةِ وَإِلَیۡهِ تُR<رۡ>جَعُJ<و>نَ A<۝>D<٨٥>",
"وَلَا یَمۡلِكُ ٱلَّذِینَ یَQ<دۡ>عُونَ مِN<ن> دُونِهِ ٱX<ل>شَّفَT<ـٰ>عَةَ إِلَّا مَN<ن> شَهِدَ بِX<ٱ>لۡحَقِّ وَهُمۡ یَعۡلَمُJ<و>نَ A<۝>D<٨٦>",
"وَلَئِN<ن> سَأَلۡتَهُم N<مَّ>نۡ خَلَقَهُمۡ لَیَقُولُN<نَّ> ٱللَّهُۖ فَأَN<نَّ>ىٰ یُؤۡفَكُJ<و>نَ A<۝>D<٨٧>",
"وَقِیلِهِT<ۦ> یَT<ـٰ>R<رَ>بِّ إِN<نَّ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ قَوۡمX<ࣱ> لَّا یُؤۡمِنُJ<و>نَ A<۝>D<٨٨>",
"فَX<ٱ>صۡفَحۡ عَنۡهُمۡ وَقُلۡ سَلَT<ـٰ>مN<ࣱ>ۚ فَسَوۡفَ یَعۡلَمُJ<و>نَ A<۝>D<٨٩>",
"#حL<مۤ> A<۝>D<١>",
"وَX<ٱ>لۡكِتَT<ـٰ>بِ ٱلۡمُبِJ<ی>نِ A<۝>D<٢>",
"إِN<نَّ>W<اۤ> أَN<ن>زَلۡنَT<ـٰ>هُ فِی لَیۡلَةX<ࣲ> N<مُّ>بَT<ـٰ>R<رَ>كَةٍۚ إِN<نَّ>ا كُN<نَّ>ا مُN<ن>ذِرِJ<ی>نَ A<۝>D<٣>",
"فِیهَا یُفۡR<رَ>قُ كُلُّ أَمۡرٍ حَكِJ<ی>مٍ A<۝>D<٤>",
"أَمۡR<ر>X<ࣰ>ا N<مِّ>نۡ عِN<ن>دِنَW<اۤ>ۚ إِN<نَّ>ا كُN<نَّ>ا مُR<رۡ>سِلِJ<ی>نَ A<۝>D<٥>",
"R<رَ>حۡمَةX<ࣰ> N<مِّ>X<ن> R<رَّ>بِّكَۚ إِN<نَّ>هُT<ۥ> هُوَ ٱX<ل>سَّمِیعُ ٱلۡعَلِJ<ی>مُ A<۝>D<٦>",
"R<رَ>بِّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَW<اۤ>ۖ إِN<ن> كُN<ن>تُم N<مُّ>وقِنِJ<ی>نَ A<۝>D<٧>",
"لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ یُحۡیِT<ۦ> وَیُمِیتُۖ R<رَ>بُّكُمۡ وَR<رَ>بُّ ءَابَW<اۤ>ئِكُمُ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٨>",
"بَلۡ هُمۡ فِی شَكّX<ࣲ> N<یَ>لۡعَبُJ<و>نَ A<۝>D<٩>",
"فَX<ٱ>R<رۡ>تَقِQ<بۡ> یَوۡمَ تَأۡتِی ٱX<ل>سَّمَW<اۤ>ءُ بِدُخَانX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<١٠>",
"یَغۡشَى ٱX<ل>N<نَّ>اسَۖ هَT<ـٰ>ذَا عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<١١>",
"R<رَّ>بَّنَا ٱكۡشِفۡ عَN<نَّ>ا ٱلۡعَذَابَ إِN<نَّ>ا مُؤۡمِنُJ<و>نَ A<۝>D<١٢>",
"أَN<نَّ>ىٰ لَهُمُ ٱX<ل>ذِّكۡR<رَ>ىٰ وَقَQ<دۡ> جَW<اۤ>ءَهُمۡ R<رَ>سُولX<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<١٣>",
"ثُN<مَّ> تَوَلَّوۡX<ا۟> عَنۡهُ وَقَالُوX<ا۟> مُعَلَّمX<ࣱ> N<مَّ>Q<جۡ>نُJ<و>نٌ A<۝>D<١٤>",
"إِN<نَّ>ا كَاشِفُوX<ا۟> ٱلۡعَذَابِ قَلِیلًاۚ إِN<نَّ>كُمۡ عَW<اۤ>ئِدُJ<و>نَ A<۝>D<١٥>",
"یَوۡمَ نَQ<بۡ>طِشُ ٱلۡبَQ<طۡ>شَةَ ٱلۡكُQ<بۡ>R<رَ>ىW<ٰۤ> إِN<نَّ>ا مُN<ن>تَقِمُJ<و>نَ A<۝>D<١٦>",
"۞ وَلَقَQ<دۡ> فَتَN<نَّ>ا قَQ<بۡ>لَهُمۡ قَوۡمَ فِرۡعَوۡنَ وَجَW<اۤ>ءَهُمۡ R<رَ>سُولN<ࣱ> كَرِJ<ی>مٌ A<۝>D<١٧>",
"أَنۡ أَدُّW<وۤ>X<ا۟> إِلَیَّ عِبَادَ ٱللَّهِۖ إِN<نِّ>ی لَكُمۡ R<رَ>سُولٌ أَمِJ<ی>نࣱ A<۝>D<١٨>",
"وَأَX<ن> لَّا تَعۡلُوX<ا۟> عَلَى ٱللَّهِۖ إِN<نِّ>W<یۤ> ءَاتِیكُN<م> بِسُلۡطَT<ـٰ>نX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<١٩>",
"وَإِN<نِّ>ی عُذۡتُ بِR<رَ>بِّی وَR<رَ>بِّكُمۡ أَN<ن> تَR<رۡ>جُمُJ<و>نِ A<۝>D<٢٠>",
"وَإِX<ن> لَّمۡ تُؤۡمِنُوX<ا۟> لِی فَX<ٱ>عۡتَزِلُJ<و>نِ A<۝>D<٢١>",
"فَدَعَا R<رَ>بَّهُW<ۥۤ> أَN<نَّ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ قَوۡمX<ࣱ> N<مُّ>Q<جۡ>رِمُJ<و>نَ A<۝>D<٢٢>",
"فَأَسۡرِ بِعِبَادِی لَیۡلًا إِN<نَّ>كُم N<مُّ>تَّبَعُJ<و>نَ A<۝>D<٢٣>",
"وَX<ٱ>تۡR<رُ>كِ ٱلۡبَحۡR<رَ> R<رَ>هۡوًاۖ إِN<نَّ>هُمۡ جُN<ن>دX<ࣱ> N<مُّ>غۡR<رَ>قُJ<و>نَ A<۝>D<٢٤>",
"كَمۡ تَR<رَ>كُوX<ا۟> مِN<ن> جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>عُیُJ<و>نࣲ A<۝>D<٢٥>",
"وَزُR<رُ>وعX<ࣲ> N<وَ>مَقَامN<ࣲ> كَرِJ<ی>مࣲ A<۝>D<٢٦>",
"وَنَعۡمَةN<ࣲ> كَانُوX<ا۟> فِیهَا فَT<ـٰ>كِهِJ<ی>نَ A<۝>D<٢٧>",
"كَذَT< ٰ>لِكَۖ وَأَوۡR<رَ>ثۡنَT<ـٰ>هَا قَوۡمًا ءَاخَرِJ<ی>نَ A<۝>D<٢٨>",
"فَمَا بَكَتۡ عَلَیۡهِمُ ٱX<ل>سَّمَW<اۤ>ءُ وَX<ٱ>لۡأَR<رۡ>ضُ وَمَا كَانُوX<ا۟> مُN<ن>ظَرِJ<ی>نَ A<۝>D<٢٩>",
"وَلَقَQ<دۡ> نَجَّیۡنَا بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ مِنَ ٱلۡعَذَابِ ٱلۡمُهِJ<ی>نِ A<۝>D<٣٠>",
"مِN<ن> فِرۡعَوۡنَۚ إِN<نَّ>هُT<ۥ> كَانَ عَالِیX<ࣰ>ا N<مِّ>نَ ٱلۡمُسۡرِفِJ<ی>نَ A<۝>D<٣١>",
"وَلَقَدِ ٱخۡتَR<رۡ>نَT<ـٰ>هُمۡ عَلَىٰ عِلۡمٍ عَلَى ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٣٢>",
"وَءَاتَیۡنَT<ـٰ>هُم N<مِّ>نَ ٱلۡءَایَT<ـٰ>تِ مَا فِیهِ بَلَW<ـٰۤ>ؤࣱX<ا۟> N<مُّ>بِJ<ی>نٌ A<۝>D<٣٣>",
"إِN<نَّ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ لَیَقُولُJ<و>نَ A<۝>D<٣٤>",
"إِنۡ هِیَ إِلَّا مَوۡتَتُنَا ٱلۡأُولَىٰ وَمَا نَحۡنُ بِمُN<ن>شَرِJ<ی>نَ A<۝>D<٣٥>",
"فَأۡتُوX<ا۟> بِءَابَW<اۤ>ئِنَW<اۤ> إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٣٦>",
"أَهُمۡ خَیۡR<ر>ٌ أَمۡ قَوۡمُ تُبَّعX<ࣲ> N<وَ>X<ٱ>لَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ أَهۡلَكۡنَT<ـٰ>هُمۡۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> مُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٣٧>",
"وَمَا خَلَQ<قۡ>نَا ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَمَا بَیۡنَهُمَا لَT<ـٰ>عِبِJ<ی>نَ A<۝>D<٣٨>",
"مَا خَلَQ<قۡ>نَT<ـٰ>هُمَW<اۤ> إِلَّا بِX<ٱ>لۡحَقِّ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٣٩>",
"إِN<نَّ> یَوۡمَ ٱلۡفَصۡلِ مِیقَT<ـٰ>تُهُمۡ أَQ<جۡ>مَعِJ<ی>نَ A<۝>D<٤٠>",
"یَوۡمَ لَا یُغۡنِی مَوۡلًى عَX<ن> N<مَّ>وۡلN<ࣰ>ى شَیۡءX<ࣰ>ا N<وَ>لَا هُمۡ یُN<ن>صَR<رُ>J<و>نَ A<۝>D<٤١>",
"إِلَّا مَX<ن> R<رَّ>حِمَ ٱللَّهُۚ إِN<نَّ>هُT<ۥ> هُوَ ٱلۡعَزِیزُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٤٢>",
"إِN<نَّ> شَجَR<رَ>تَ ٱX<ل>زَّقُّJ<و>مِ A<۝>D<٤٣>",
"طَعَامُ ٱلۡأَثِJ<ی>مِ A<۝>D<٤٤>",
"كَX<ٱ>لۡمُهۡلِ یَغۡلِی فِی ٱلۡبُطُJ<و>نِ A<۝>D<٤٥>",
"كَغَلۡیِ ٱلۡحَمِJ<ی>مِ A<۝>D<٤٦>",
"خُذُوهُ فَX<ٱ>عۡتِلُوهُ إِلَىٰ سَوَW<اۤ>ءِ ٱلۡجَحِJ<ی>مِ A<۝>D<٤٧>",
"ثُN<مَّ> صُبُّوX<ا۟> فَوۡقَ R<رَ>أۡسِهِT<ۦ> مِنۡ عَذَابِ ٱلۡحَمِJ<ی>مِ A<۝>D<٤٨>",
"ذُQ<قۡ> إِN<نَّ>كَ أَN<ن>تَ ٱلۡعَزِیزُ ٱلۡكَرِJ<ی>مُ A<۝>D<٤٩>",
"إِN<نَّ> هَT<ـٰ>ذَا مَا كُN<ن>تُN<م> بِهِT<ۦ> تَمۡتَR<رُ>J<و>نَ A<۝>D<٥٠>",
"إِN<نَّ> ٱلۡمُتَّقِینَ فِی مَقَامٍ أَمِJ<ی>نࣲ A<۝>D<٥١>",
"فِی جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>عُیُJ<و>نࣲ A<۝>D<٥٢>",
"یَلۡبَسُونَ مِN<ن> سُN<ن>دُسX<ࣲ> N<وَ>إِسۡتَQ<بۡ>R<رَ>قX<ࣲ> N<مُّ>تَقَT<ـٰ>بِلِJ<ی>نَ A<۝>D<٥٣>",
"كَذَT< ٰ>لِكَ وَزَوَّQ<جۡ>نَT<ـٰ>هُN<م> بِحُورٍ عِJ<ی>نࣲ A<۝>D<٥٤>",
"یَQ<دۡ>عُونَ فِیهَا بِكُلِّ فَT<ـٰ>كِهَةٍ ءَامِنِJ<ی>نَ A<۝>D<٥٥>",
"لَا یَذُوقُونَ فِیهَا ٱلۡمَوۡتَ إِلَّا ٱلۡمَوۡتَةَ ٱلۡأُولَىٰۖ وَوَقَX<ى>T<ٰ>هُمۡ عَذَابَ ٱلۡجَحِJ<ی>مِ A<۝>D<٥٦>",
"فَضۡلX<ࣰ>ا N<مِّ>X<ن> R<رَّ>بِّكَۚ ذَT< ٰ>لِكَ هُوَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<٥٧>",
"فَإِN<نَّ>مَا یَسَّR<رۡ>نَT<ـٰ>هُ بِلِسَانِكَ لَعَلَّهُمۡ یَتَذَكَّR<رُ>J<و>نَ A<۝>D<٥٨>",
"فَX<ٱ>R<رۡ>تَقِQ<بۡ> إِN<نَّ>هُم N<مُّ>R<رۡ>تَقِبُJ<و>نَ A<۝>D<٥٩>",
"#حL<مۤ> A<۝>D<١>",
"تَN<ن>زِیلُ ٱلۡكِتَT<ـٰ>بِ مِنَ ٱللَّهِ ٱلۡعَزِیزِ ٱلۡحَكِJ<ی>مِ A<۝>D<٢>",
"إِN<نَّ> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ لَءَایَT<ـٰ>تX<ࣲ> لِّلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٣>",
"وَفِی خَلۡقِكُمۡ وَمَا یَبُثُّ مِN<ن> دَL<اۤ>بَّةٍ ءَایَT<ـٰ>تX<ࣱ> لِّقَوۡمX<ࣲ> N<یُ>وقِنُJ<و>نَ A<۝>D<٤>",
"وَX<ٱ>خۡتِلَT<ـٰ>فِ ٱلَّیۡلِ وَX<ٱ>X<ل>N<نَّ>هَارِ وَمَW<اۤ> أَN<ن>زَلَ ٱللَّهُ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مِX<ن> رِّزۡقN<ࣲ> فَأَحۡیَا بِهِ ٱلۡأَR<رۡ>ضَ بَعۡدَ مَوۡتِهَا وَتَصۡرِیفِ ٱX<ل>رِّیَT<ـٰ>حِ ءَایَT<ـٰ>تX<ࣱ> لِّقَوۡمX<ࣲ> N<یَ>عۡقِلُJ<و>نَ A<۝>D<٥>",
"تِلۡكَ ءَایَT<ـٰ>تُ ٱللَّهِ نَتۡلُوهَا عَلَیۡكَ بِX<ٱ>لۡحَقِّۖ فَبِأَیِّ حَدِیثِN<ۭ> بَعۡدَ ٱللَّهِ وَءَایَT<ـٰ>تِهِT<ۦ> یُؤۡمِنُJ<و>نَ A<۝>D<٦>",
"وَیۡلX<ࣱ> لِّكُلِّ أَفَّاكٍ أَثِJ<ی>مࣲ A<۝>D<٧>",
"یَسۡمَعُ ءَایَT<ـٰ>تِ ٱللَّهِ تُتۡلَىٰ عَلَیۡهِ ثُN<مَّ> یُصِR<رُّ> مُسۡتَكۡبِR<ر>N<ࣰ>ا كَأَX<ن> لَّمۡ یَسۡمَعۡهَاۖ فَبَشِّرۡهُ بِعَذَابٍ أَلِJ<ی>مࣲ A<۝>D<٨>",
"وَإِذَا عَلِمَ مِنۡ ءَایَT<ـٰ>تِنَا شَیۡءًا ٱتَّخَذَهَا هُزُوًاۚ أُX<و۟>لَW<ـٰۤ>ئِكَ لَهُمۡ عَذَابX<ࣱ> N<مُّ>هِJ<ی>نࣱ A<۝>D<٩>",
"مِّX<ن> N<وَ>R<رَ>W<اۤ>ئِهِمۡ جَهَN<نَّ>مُۖ وَلَا یُغۡنِی عَنۡهُم N<مَّ>ا كَسَبُوX<ا۟> شَیۡءX<ࣰ>ا N<وَ>لَا مَا ٱتَّخَذُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ أَوۡلِیَW<اۤ>ءَۖ وَلَهُمۡ عَذَابٌ عَظِJ<ی>مٌ A<۝>D<١٠>",
"هَT<ـٰ>ذَا هُدX<ࣰ>ىۖ N<وَ>X<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِ R<رَ>بِّهِمۡ لَهُمۡ عَذَابX<ࣱ> N<مِّ>X<ن> رِّQ<جۡ>زٍ أَلِJ<ی>مٌ A<۝>D<١١>",
"۞ ٱللَّهُ ٱلَّذِی سَخَّR<رَ> لَكُمُ ٱلۡبَحۡR<رَ> لِتَQ<جۡ>رِیَ ٱلۡفُلۡكُ فِیهِ بِأَمۡرِهِT<ۦ> وَلِتَQ<بۡ>تَغُوX<ا۟> مِN<ن> فَضۡلِهِT<ۦ> وَلَعَلَّكُمۡ تَشۡكُR<رُ>J<و>نَ A<۝>D<١٢>",
"وَسَخَّR<رَ> لَكُم N<مَّ>ا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِ جَمِیعX<ࣰ>ا N<مِّ>نۡهُۚ إِN<نَّ> فِی ذَT< ٰ>لِكَ لَءَایَT<ـٰ>تX<ࣲ> لِّقَوۡمX<ࣲ> N<یَ>تَفَكَّR<رُ>J<و>نَ A<۝>D<١٣>",
"قُل لِّلَّذِینَ ءَامَنُوX<ا۟> یَغۡفِR<رُ>وX<ا۟> لِلَّذِینَ لَا یَR<رۡ>جُونَ أَیَّامَ ٱللَّهِ لِیَQ<جۡ>زِیَ قَوۡمَN<ۢ>ا بِمَا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<١٤>",
"مَنۡ عَمِلَ صَT<ـٰ>لِحN<ࣰ>ا فَلِنَفۡسِهِT<ۦ>ۖ وَمَنۡ أَسَW<اۤ>ءَ فَعَلَیۡهَاۖ ثُN<مَّ> إِلَىٰ R<رَ>بِّكُمۡ تُR<رۡ>جَعُJ<و>نَ A<۝>D<١٥>",
"وَلَقَQ<دۡ> ءَاتَیۡنَا بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحُكۡمَ وَX<ٱ>X<ل>N<نُّ>بُوَّةَ وَR<رَ>زَQ<قۡ>نَT<ـٰ>هُم N<مِّ>نَ ٱX<ل>طَّیِّبَT<ـٰ>تِ وَفَضَّلۡنَT<ـٰ>هُمۡ عَلَى ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٦>",
"وَءَاتَیۡنَT<ـٰ>هُN<م> بَیِّنَT<ـٰ>تX<ࣲ> N<مِّ>نَ ٱلۡأَمۡرِۖ فَمَا ٱخۡتَلَفُW<وۤ>X<ا۟> إِلَّا مِX<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَهُمُ ٱلۡعِلۡمُ بَغۡیَN<ۢ>ا بَیۡنَهُمۡۚ إِN<نَّ> R<رَ>بَّكَ یَQ<قۡ>ضِی بَیۡنَهُمۡ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ فِیمَا كَانُوX<ا۟> فِیهِ یَخۡتَلِفُJ<و>نَ A<۝>D<١٧>",
"ثُN<مَّ> جَعَلۡنَT<ـٰ>كَ عَلَىٰ شَرِیعَةX<ࣲ> N<مِّ>نَ ٱلۡأَمۡرِ فَX<ٱ>تَّبِعۡهَا وَلَا تَتَّبِعۡ أَهۡوَW<اۤ>ءَ ٱلَّذِینَ لَا یَعۡلَمُJ<و>نَ A<۝>D<١٨>",
"إِN<نَّ>هُمۡ لَX<ن> N<یُ>غۡنُوX<ا۟> عَN<ن>كَ مِنَ ٱللَّهِ شَیۡءX<ࣰ>اۚ N<وَ>إِN<نَّ> ٱX<ل>ظَّT<ـٰ>لِمِینَ بَعۡضُهُمۡ أَوۡلِیَW<اۤ>ءُ بَعۡضX<ࣲ>ۖ N<وَ>ٱللَّهُ وَلِیُّ ٱلۡمُتَّقِJ<ی>نَ A<۝>D<١٩>",
"هَT<ـٰ>ذَا بَصَW<ـٰۤ>ئِR<رُ> لِX<ل>N<نَّ>اسِ وَهُدX<ࣰ>ى N<وَ>R<رَ>حۡمَةX<ࣱ> لِّقَوۡمX<ࣲ> N<یُ>وقِنُJ<و>نَ A<۝>D<٢٠>",
"أَمۡ حَسِبَ ٱلَّذِینَ ٱQ<جۡ>تَR<رَ>حُوX<ا۟> ٱX<ل>سَّیِّءَاتِ أَن N<نَّ>Q<جۡ>عَلَهُمۡ كَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ سَوَW<اۤ>ءX<ࣰ> N<مَّ>حۡیَاهُمۡ وَمَمَاتُهُمۡۚ سَW<اۤ>ءَ مَا یَحۡكُمُJ<و>نَ A<۝>D<٢١>",
"وَخَلَقَ ٱللَّهُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ بِX<ٱ>لۡحَقِّ وَلِتُQ<جۡ>زَىٰ كُلُّ نَفۡسِN<ۭ> بِمَا كَسَبَتۡ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<٢٢>",
"أَفَR<رَ>ءَیۡتَ مَنِ ٱتَّخَذَ إِلَT<ـٰ>هَهُT<ۥ> هَوَX<ى>T<ٰ>هُ وَأَضَلَّهُ ٱللَّهُ عَلَىٰ عِلۡمX<ࣲ> N<وَ>خَتَمَ عَلَىٰ سَمۡعِهِT<ۦ> وَقَلۡبِهِT<ۦ> وَجَعَلَ عَلَىٰ بَصَرِهِT<ۦ> غِشَT<ـٰ>وَةN<ࣰ> فَمَX<ن> N<یَ>هۡدِیهِ مِX<ن>N<ۢ> بَعۡدِ ٱللَّهِۚ أَفَلَا تَذَكَّR<رُ>J<و>نَ A<۝>D<٢٣>",
"وَقَالُوX<ا۟> مَا هِیَ إِلَّا حَیَاتُنَا ٱX<ل>دُّنۡیَا نَمُوتُ وَنَحۡیَا وَمَا یُهۡلِكُنَW<اۤ> إِلَّا ٱX<ل>دَّهۡR<رُ>ۚ وَمَا لَهُN<م> بِذَT< ٰ>لِكَ مِنۡ عِلۡمٍۖ إِنۡ هُمۡ إِلَّا یَظُN<نُّ>J<و>نَ A<۝>D<٢٤>",
"وَإِذَا تُتۡلَىٰ عَلَیۡهِمۡ ءَایَT<ـٰ>تُنَا بَیِّنَT<ـٰ>تX<ࣲ> N<مَّ>ا كَانَ حُجَّتَهُمۡ إِلَّW<اۤ> أَN<ن> قَالُوX<ا۟> ٱئۡتُوX<ا۟> بِءَابَW<اۤ>ئِنَW<اۤ> إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٢٥>",
"قُلِ ٱللَّهُ یُحۡیِیكُمۡ ثُN<مَّ> یُمِیتُكُمۡ ثُN<مَّ> یَQ<جۡ>مَعُكُمۡ إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ لَا R<رَ>یۡبَ فِیهِ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ> ٱX<ل>N<نَّ>اسِ لَا یَعۡلَمُJ<و>نَ A<۝>D<٢٦>",
"وَلِلَّهِ مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَیَوۡمَ تَقُومُ ٱX<ل>سَّاعَةُ یَوۡمَئِذX<ࣲ> N<یَ>خۡسَR<رُ> ٱلۡمُQ<بۡ>طِلُJ<و>نَ A<۝>D<٢٧>",
"وَتَR<رَ>ىٰ كُلَّ أُN<مَّ>ةN<ࣲ> جَاثِیَةN<ࣰ>ۚ كُلُّ أُN<مَّ>ةN<ࣲ> تُQ<دۡ>عَىW<ٰۤ> إِلَىٰ كِتَT<ـٰ>بِهَا ٱلۡیَوۡمَ تُQ<جۡ>زَوۡنَ مَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٢٨>",
"هَT<ـٰ>ذَا كِتَT<ـٰ>بُنَا یَN<ن>طِقُ عَلَیۡكُN<م> بِX<ٱ>لۡحَقِّۚ إِN<نَّ>ا كُN<نَّ>ا نَسۡتَN<ن>سِخُ مَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٢٩>",
"فَأَN<مَّ>ا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ فَیُQ<دۡ>خِلُهُمۡ R<رَ>بُّهُمۡ فِی R<رَ>حۡمَتِهِT<ۦ>ۚ ذَT< ٰ>لِكَ هُوَ ٱلۡفَوۡزُ ٱلۡمُبِJ<ی>نُ A<۝>D<٣٠>",
"وَأَN<مَّ>ا ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَفَلَمۡ تَكُنۡ ءَایَT<ـٰ>تِی تُتۡلَىٰ عَلَیۡكُمۡ فَX<ٱ>سۡتَكۡبَR<رۡ>تُمۡ وَكُN<ن>تُمۡ قَوۡمX<ࣰ>ا N<مُّ>Q<جۡ>رِمِJ<ی>نَ A<۝>D<٣١>",
"وَإِذَا قِیلَ إِN<نَّ> وَعۡدَ ٱللَّهِ حَقّX<ࣱ> N<وَ>X<ٱ>X<ل>سَّاعَةُ لَا R<رَ>یۡبَ فِیهَا قُلۡتُم N<مَّ>ا نَQ<دۡ>رِی مَا ٱX<ل>سَّاعَةُ إِن N<نَّ>ظُN<نُّ> إِلَّا ظَN<نّ>X<ࣰ>ا N<وَ>مَا نَحۡنُ بِمُسۡتَیۡقِنِJ<ی>نَ A<۝>D<٣٢>",
"وَبَدَا لَهُمۡ سَیِّءَاتُ مَا عَمِلُوX<ا۟> وَحَاقَ بِهِم N<مَّ>ا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٣٣>",
"وَقِیلَ ٱلۡیَوۡمَ نَN<ن>سَX<ى>T<ٰ>كُمۡ كَمَا نَسِیتُمۡ لِقَW<اۤ>ءَ یَوۡمِكُمۡ هَT<ـٰ>ذَا وَمَأۡوَX<ى>T<ٰ>كُمُ ٱX<ل>N<نَّ>اR<رُ> وَمَا لَكُم N<مِّ>ن N<نَّ>T<ـٰ>صِرِJ<ی>نَ A<۝>D<٣٤>",
"ذَT< ٰ>لِكُN<م> بِأَN<نَّ>كُمُ ٱتَّخَذۡتُمۡ ءَایَT<ـٰ>تِ ٱللَّهِ هُزُوX<ࣰ>ا N<وَ>غَR<رَّ>تۡكُمُ ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَاۚ فَX<ٱ>لۡیَوۡمَ لَا یُخۡR<رَ>جُونَ مِنۡهَا وَلَا هُمۡ یُسۡتَعۡتَبُJ<و>نَ A<۝>D<٣٥>",
"فَلِلَّهِ ٱلۡحَمۡدُ R<رَ>بِّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَR<رَ>بِّ ٱلۡأَR<رۡ>ضِ R<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٣٦>",
"وَلَهُ ٱلۡكِQ<بۡ>رِیَW<اۤ>ءُ فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٣٧>",
"#۞ حL<مۤ> A<۝>D<١>",
"تَN<ن>زِیلُ ٱلۡكِتَT<ـٰ>بِ مِنَ ٱللَّهِ ٱلۡعَزِیزِ ٱلۡحَكِJ<ی>مِ A<۝>D<٢>",
"مَا خَلَQ<قۡ>نَا ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَمَا بَیۡنَهُمَW<اۤ> إِلَّا بِX<ٱ>لۡحَقِّ وَأَجَلX<ࣲ> N<مُّ>سَN<مّ>X<ࣰ>ىۚ N<وَ>X<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> عَN<مَّ>W<اۤ> أُN<ن>ذِR<رُ>وX<ا۟> مُعۡرِضُJ<و>نَ A<۝>D<٣>",
"قُلۡ أَR<رَ>ءَیۡتُم N<مَّ>ا تَQ<دۡ>عُونَ مِN<ن> دُونِ ٱللَّهِ أَR<رُ>ونِی مَاذَا خَلَقُوX<ا۟> مِنَ ٱلۡأَR<رۡ>ضِ أَمۡ لَهُمۡ شِرۡكN<ࣱ> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِۖ ٱئۡتُونِی بِكِتَT<ـٰ>بX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِ هَT<ـٰ>ذَW<اۤ> أَوۡ أَثَT<ـٰ>R<رَ>ةX<ࣲ> N<مِّ>نۡ عِلۡمٍ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٤>",
"وَمَنۡ أَضَلُّ مِN<مَّ>X<ن> N<یَ>Q<دۡ>عُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ مَX<ن> لَّا یَسۡتَجِیبُ لَهُW<ۥۤ> إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ وَهُمۡ عَN<ن> دُعَW<اۤ>ئِهِمۡ غَT<ـٰ>فِلُJ<و>نَ A<۝>D<٥>",
"وَإِذَا حُشِR<رَ> ٱX<ل>N<نَّ>اسُ كَانُوX<ا۟> لَهُمۡ أَعۡدَW<اۤ>ءX<ࣰ> N<وَ>كَانُوX<ا۟> بِعِبَادَتِهِمۡ كَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٦>",
"وَإِذَا تُتۡلَىٰ عَلَیۡهِمۡ ءَایَT<ـٰ>تُنَا بَیِّنَT<ـٰ>تN<ࣲ> قَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لِلۡحَقِّ لَN<مَّ>ا جَW<اۤ>ءَهُمۡ هَT<ـٰ>ذَا سِحۡR<ر>X<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<٧>",
"أَمۡ یَقُولُونَ ٱفۡتَR<رَ>X<ى>T<ٰ>هُۖ قُلۡ إِنِ ٱفۡتَR<رَ>یۡتُهُT<ۥ> فَلَا تَمۡلِكُونَ لِی مِنَ ٱللَّهِ شَیۡءًاۖ هُوَ أَعۡلَمُ بِمَا تُفِیضُونَ فِیهِۚ كَفَىٰ بِهِT<ۦ> شَهِیدَN<ۢ>ا بَیۡنِی وَبَیۡنَكُمۡۖ وَهُوَ ٱلۡغَفُوR<رُ> ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٨>",
"قُلۡ مَا كُN<ن>تُ بِQ<دۡ>عX<ࣰ>ا N<مِّ>نَ ٱX<ل>R<رُّ>سُلِ وَمَW<اۤ> أَQ<دۡ>رِی مَا یُفۡعَلُ بِی وَلَا بِكُمۡۖ إِنۡ أَتَّبِعُ إِلَّا مَا یُوحَىW<ٰۤ> إِلَیَّ وَمَW<اۤ> أَنَا۠ إِلَّا نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٩>",
"قُلۡ أَR<رَ>ءَیۡتُمۡ إِN<ن> كَانَ مِنۡ عِN<ن>دِ ٱللَّهِ وَكَفَR<رۡ>تُN<م> بِهِT<ۦ> وَشَهِدَ شَاهِدX<ࣱ> N<مِّ>X<ن>N<ۢ> بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ عَلَىٰ مِثۡلِهِT<ۦ> فَءَامَنَ وَX<ٱ>سۡتَكۡبَR<رۡ>تُمۡۚ إِN<نَّ> ٱللَّهَ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٠>",
"وَقَالَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لِلَّذِینَ ءَامَنُوX<ا۟> لَوۡ كَانَ خَیۡR<ر>X<ࣰ>ا N<مَّ>ا سَبَقُونَW<اۤ> إِلَیۡهِۚ وَإِذۡ لَمۡ یَهۡتَدُوX<ا۟> بِهِT<ۦ> فَسَیَقُولُونَ هَT<ـٰ>ذَW<اۤ> إِفۡكN<ࣱ> قَدِJ<ی>مࣱ A<۝>D<١١>",
"وَمِN<ن> قَQ<بۡ>لِهِT<ۦ> كِتَT<ـٰ>بُ مُوسَىW<ٰۤ> إِمَامX<ࣰ>ا N<وَ>R<رَ>حۡمَةX<ࣰ>ۚ N<وَ>هَT<ـٰ>ذَا كِتَT<ـٰ>بX<ࣱ> N<مُّ>صَدِّقX<ࣱ> لِّسَانًا عَR<رَ>بِیّX<ࣰ>ا لِّیُN<ن>ذِR<رَ> ٱلَّذِینَ ظَلَمُوX<ا۟> وَبُشۡR<رَ>ىٰ لِلۡمُحۡسِنِJ<ی>نَ A<۝>D<١٢>",
"إِN<نَّ> ٱلَّذِینَ قَالُوX<ا۟> R<رَ>بُّنَا ٱللَّهُ ثُN<مَّ> ٱسۡتَقَT<ـٰ>مُوX<ا۟> فَلَا خَوۡفٌ عَلَیۡهِمۡ وَلَا هُمۡ یَحۡزَنُJ<و>نَ A<۝>D<١٣>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡجَN<نَّ>ةِ خَT<ـٰ>لِدِینَ فِیهَا جَزَW<اۤ>ءَN<ۢ> بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٤>",
"وَوَصَّیۡنَا ٱلۡإِN<ن>سَT<ـٰ>نَ بِوَT< ٰ>لِدَیۡهِ إِحۡسَT<ـٰ>نًاۖ حَمَلَتۡهُ أُN<مُّ>هُT<ۥ> كُR<رۡ>هX<ࣰ>ا N<وَ>وَضَعَتۡهُ كُR<رۡ>هX<ࣰ>اۖ N<وَ>حَمۡلُهُT<ۥ> وَفِصَT<ـٰ>لُهُT<ۥ> ثَلَT<ـٰ>ثُونَ شَهۡR<ر>ًاۚ حَتَّىW<ٰۤ> إِذَا بَلَغَ أَشُدَّهُT<ۥ> وَبَلَغَ أَR<رۡ>بَعِینَ سَنَةN<ࣰ> قَالَ R<رَ>بِّ أَوۡزِعۡنِW<یۤ> أَنۡ أَشۡكُR<رَ> نِعۡمَتَكَ ٱلَّتِW<یۤ> أَنۡعَمۡتَ عَلَیَّ وَعَلَىٰ وَT< ٰ>لِدَیَّ وَأَنۡ أَعۡمَلَ صَT<ـٰ>لِحN<ࣰ>ا تَR<رۡ>ضَX<ى>T<ٰ>هُ وَأَصۡلِحۡ لِی فِی ذُرِّیَّتِW<یۤ>ۖ إِN<نِّ>ی تُQ<بۡ>تُ إِلَیۡكَ وَإِN<نِّ>ی مِنَ ٱلۡمُسۡلِمِJ<ی>نَ A<۝>D<١٥>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ نَتَقَبَّلُ عَنۡهُمۡ أَحۡسَنَ مَا عَمِلُوX<ا۟> وَنَتَجَاوَزُ عَN<ن> سَیِّءَاتِهِمۡ فِW<یۤ> أَصۡحَT<ـٰ>بِ ٱلۡجَN<نَّ>ةِۖ وَعۡدَ ٱX<ل>صِّQ<دۡ>قِ ٱلَّذِی كَانُوX<ا۟> یُوعَدُJ<و>نَ A<۝>D<١٦>",
"وَX<ٱ>لَّذِی قَالَ لِوَT< ٰ>لِدَیۡهِ أُفّX<ࣲ> لَّكُمَW<اۤ> أَتَعِدَانِنِW<یۤ> أَنۡ أُخۡR<رَ>جَ وَقَQ<دۡ> خَلَتِ ٱلۡقُR<رُ>ونُ مِN<ن> قَQ<بۡ>لِی وَهُمَا یَسۡتَغِیثَانِ ٱللَّهَ وَیۡلَكَ ءَامِنۡ إِN<نَّ> وَعۡدَ ٱللَّهِ حَقّN<ࣱ> فَیَقُولُ مَا هَT<ـٰ>ذَW<اۤ> إِلَّW<اۤ> أَسَT<ـٰ>طِیR<رُ> ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٧>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ حَقَّ عَلَیۡهِمُ ٱلۡقَوۡلُ فِW<یۤ> أُمَمN<ࣲ> قَQ<دۡ> خَلَتۡ مِN<ن> قَQ<بۡ>لِهِم N<مِّ>نَ ٱلۡجِN<نِّ> وَX<ٱ>لۡإِN<ن>سِۖ إِN<نَّ>هُمۡ كَانُوX<ا۟> خَT<ـٰ>سِرِJ<ی>نَ A<۝>D<١٨>",
"وَلِكُلّN<ࣲ> دَR<رَ>جَT<ـٰ>تX<ࣱ> N<مِّ>N<مَّ>ا عَمِلُوX<ا۟>ۖ وَلِیُوَفِّیَهُمۡ أَعۡمَT<ـٰ>لَهُمۡ وَهُمۡ لَا یُظۡلَمُJ<و>نَ A<۝>D<١٩>",
"وَیَوۡمَ یُعۡR<رَ>ضُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> عَلَى ٱX<ل>N<نَّ>ارِ أَذۡهَQ<بۡ>تُمۡ طَیِّبَT<ـٰ>تِكُمۡ فِی حَیَاتِكُمُ ٱX<ل>دُّنۡیَا وَX<ٱ>سۡتَمۡتَعۡتُN<م> بِهَا فَX<ٱ>لۡیَوۡمَ تُQ<جۡ>زَوۡنَ عَذَابَ ٱلۡهُونِ بِمَا كُN<ن>تُمۡ تَسۡتَكۡبِR<رُ>ونَ فِی ٱلۡأَR<رۡ>ضِ بِغَیۡرِ ٱلۡحَقِّ وَبِمَا كُN<ن>تُمۡ تَفۡسُقُJ<و>نَ A<۝>D<٢٠>",
"۞ وَX<ٱ>ذۡكُR<رۡ> أَخَا عَادٍ إِذۡ أَN<ن>ذَR<رَ> قَوۡمَهُT<ۥ> بِX<ٱ>لۡأَحۡقَافِ وَقَQ<دۡ> خَلَتِ ٱX<ل>N<نُّ>ذُR<رُ> مِX<ن>N<ۢ> بَیۡنِ یَدَیۡهِ وَمِنۡ خَلۡفِهِW<ۦۤ> أَلَّا تَعۡبُدُW<وۤ>X<ا۟> إِلَّا ٱللَّهَ إِN<نِّ>W<یۤ> أَخَافُ عَلَیۡكُمۡ عَذَابَ یَوۡمٍ عَظِJ<ی>مࣲ A<۝>D<٢١>",
"قَالُW<وۤ>X<ا۟> أَجِئۡتَنَا لِتَأۡفِكَنَا عَنۡ ءَالِهَتِنَا فَأۡتِنَا بِمَا تَعِدُنَW<اۤ> إِN<ن> كُN<ن>تَ مِنَ ٱX<ل>صَّT<ـٰ>دِقِJ<ی>نَ A<۝>D<٢٢>",
"قَالَ إِN<نَّ>مَا ٱلۡعِلۡمُ عِN<ن>دَ ٱللَّهِ وَأُبَلِّغُكُم N<مَّ>W<اۤ> أُR<رۡ>سِلۡتُ بِهِT<ۦ> وَلَT<ـٰ>كِN<نِّ>W<یۤ> أَR<رَ>X<ى>T<ٰ>كُمۡ قَوۡمN<ࣰ>ا تَQ<جۡ>هَلُJ<و>نَ A<۝>D<٢٣>",
"فَلَN<مَّ>ا R<رَ>أَوۡهُ عَارِضX<ࣰ>ا N<مُّ>سۡتَQ<قۡ>بِلَ أَوۡدِیَتِهِمۡ قَالُوX<ا۟> هَT<ـٰ>ذَا عَارِضX<ࣱ> N<مُّ>مۡطِR<رُ>نَاۚ بَلۡ هُوَ مَا ٱسۡتَعۡجَلۡتُN<م> بِهِT<ۦ>ۖ رِیحN<ࣱ> فِیهَا عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٢٤>",
"تُدَN<مِّ>R<رُ> كُلَّ شَیۡءِN<ۭ> بِأَمۡرِ R<رَ>بِّهَا فَأَصۡبَحُوX<ا۟> لَا یُR<رَ>ىW<ٰۤ> إِلَّا مَسَT<ـٰ>كِنُهُمۡۚ كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡقَوۡمَ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٢٥>",
"وَلَقَQ<دۡ> مَكَّN<نَّ>T<ـٰ>هُمۡ فِیمَW<اۤ> إِX<ن> N<مَّ>كَّN<نَّ>T<ـٰ>كُمۡ فِیهِ وَجَعَلۡنَا لَهُمۡ سَمۡعX<ࣰ>ا N<وَ>أَQ<بۡ>صَT<ـٰ>R<ر>X<ࣰ>ا N<وَ>أَفۡءِدَةN<ࣰ> فَمَW<اۤ> أَغۡنَىٰ عَنۡهُمۡ سَمۡعُهُمۡ وَلَW<اۤ> أَQ<بۡ>صَT<ـٰ>R<رُ>هُمۡ وَلَW<اۤ> أَفۡءِدَتُهُم N<مِّ>N<ن> شَیۡءٍ إِذۡ كَانُوX<ا۟> یَQ<جۡ>حَدُونَ بِءَایَT<ـٰ>تِ ٱللَّهِ وَحَاقَ بِهِم N<مَّ>ا كَانُوX<ا۟> بِهِT<ۦ> یَسۡتَهۡزِءُJ<و>نَ A<۝>D<٢٦>",
"وَلَقَQ<دۡ> أَهۡلَكۡنَا مَا حَوۡلَكُم N<مِّ>نَ ٱلۡقُR<رَ>ىٰ وَصَR<رَّ>فۡنَا ٱلۡءَایَT<ـٰ>تِ لَعَلَّهُمۡ یَR<رۡ>جِعُJ<و>نَ A<۝>D<٢٧>",
"فَلَوۡلَا نَصَR<رَ>هُمُ ٱلَّذِینَ ٱتَّخَذُوX<ا۟> مِN<ن> دُونِ ٱللَّهِ قُR<رۡ>بَانًا ءَالِهَةَN<ۢ>ۖ بَلۡ ضَلُّوX<ا۟> عَنۡهُمۡۚ وَذَT< ٰ>لِكَ إِفۡكُهُمۡ وَمَا كَانُوX<ا۟> یَفۡتَR<رُ>J<و>نَ A<۝>D<٢٨>",
"وَإِذۡ صَR<رَ>فۡنَW<اۤ> إِلَیۡكَ نَفَR<ر>X<ࣰ>ا N<مِّ>نَ ٱلۡجِN<نِّ> یَسۡتَمِعُونَ ٱلۡقُR<رۡ>ءَانَ فَلَN<مَّ>ا حَضَR<رُ>وهُ قَالُW<وۤ>X<ا۟> أَN<ن>صِتُوX<ا۟>ۖ فَلَN<مَّ>ا قُضِیَ وَلَّوۡX<ا۟> إِلَىٰ قَوۡمِهِم N<مُّ>N<ن>ذِرِJ<ی>نَ A<۝>D<٢٩>",
"قَالُوX<ا۟> یَT<ـٰ>قَوۡمَنَW<اۤ> إِN<نَّ>ا سَمِعۡنَا كِتَT<ـٰ>بًا أُN<ن>زِلَ مِX<ن>N<ۢ> بَعۡدِ مُوسَىٰ مُصَدِّقX<ࣰ>ا لِّمَا بَیۡنَ یَدَیۡهِ یَهۡدِW<یۤ> إِلَى ٱلۡحَقِّ وَإِلَىٰ طَرِیقX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٣٠>",
"یَT<ـٰ>قَوۡمَنَW<اۤ> أَجِیبُوX<ا۟> دَاعِیَ ٱللَّهِ وَءَامِنُوX<ا۟> بِهِT<ۦ> یَغۡفِرۡ لَكُم N<مِّ>N<ن> ذُنُوبِكُمۡ وَیُجِرۡكُم N<مِّ>نۡ عَذَابٍ أَلِJ<ی>مࣲ A<۝>D<٣١>",
"وَمَX<ن> لَّا یُجِQ<بۡ> دَاعِیَ ٱللَّهِ فَلَیۡسَ بِمُعۡجِزN<ࣲ> فِی ٱلۡأَR<رۡ>ضِ وَلَیۡسَ لَهُT<ۥ> مِN<ن> دُونِهِW<ۦۤ> أَوۡلِیَW<اۤ>ءُۚ أُX<و۟>لَW<ـٰۤ>ئِكَ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٣٢>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> أَN<نَّ> ٱللَّهَ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَلَمۡ یَعۡیَ بِخَلۡقِهِN<نَّ> بِقَT<ـٰ>دِرٍ عَلَىW<ٰۤ> أَX<ن> N<یُ>حۡT<ـۧ>ِیَ ٱلۡمَوۡتَىٰۚ بَلَىW<ٰۤ>ۚ إِN<نَّ>هُT<ۥ> عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٣٣>",
"وَیَوۡمَ یُعۡR<رَ>ضُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> عَلَى ٱX<ل>N<نَّ>ارِ أَلَیۡسَ هَT<ـٰ>ذَا بِX<ٱ>لۡحَقِّۖ قَالُوX<ا۟> بَلَىٰ وَR<رَ>بِّنَاۚ قَالَ فَذُوقُوX<ا۟> ٱلۡعَذَابَ بِمَا كُN<ن>تُمۡ تَكۡفُR<رُ>J<و>نَ A<۝>D<٣٤>",
"فَX<ٱ>صۡبِرۡ كَمَا صَبَR<رَ> أُX<و۟>لُوX<ا۟> ٱلۡعَزۡمِ مِنَ ٱX<ل>R<رُّ>سُلِ وَلَا تَسۡتَعۡجِل لَّهُمۡۚ كَأَN<نَّ>هُمۡ یَوۡمَ یَR<رَ>وۡنَ مَا یُوعَدُونَ لَمۡ یَلۡبَثُW<وۤ>X<ا۟> إِلَّا سَاعَةX<ࣰ> N<مِّ>ن N<نَّ>هَارِN<ۭ>ۚ بَلَT<ـٰ>غN<ࣱ>ۚ فَهَلۡ یُهۡلَكُ إِلَّا ٱلۡقَوۡمُ ٱلۡفَT<ـٰ>سِقُJ<و>نَ A<۝>D<٣٥>",
"#ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَصَدُّوX<ا۟> عَN<ن> سَبِیلِ ٱللَّهِ أَضَلَّ أَعۡمَT<ـٰ>لَهُمۡ A<۝>D<١>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَءَامَنُوX<ا۟> بِمَا نُزِّلَ عَلَىٰ مُحَN<مَّ>دX<ࣲ> N<وَ>هُوَ ٱلۡحَقُّ مِX<ن> R<رَّ>بِّهِمۡ كَفَّR<رَ> عَنۡهُمۡ سَیِّءَاتِهِمۡ وَأَصۡلَحَ بَالَهُمۡ A<۝>D<٢>",
"ذَT< ٰ>لِكَ بِأَN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> ٱتَّبَعُوX<ا۟> ٱلۡبَT<ـٰ>طِلَ وَأَN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> ٱتَّبَعُوX<ا۟> ٱلۡحَقَّ مِX<ن> R<رَّ>بِّهِمۡۚ كَذَT< ٰ>لِكَ یَضۡرِبُ ٱللَّهُ لِX<ل>N<نَّ>اسِ أَمۡثَT<ـٰ>لَهُمۡ A<۝>D<٣>",
"فَإِذَا لَقِیتُمُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فَضَR<رۡ>بَ ٱX<ل>رِّقَابِ حَتَّىW<ٰۤ> إِذَW<اۤ> أَثۡخَN<ن>تُمُوهُمۡ فَشُدُّوX<ا۟> ٱلۡوَثَاقَ فَإِN<مَّ>ا مَN<نَّ>N<ۢ>ا بَعۡدُ وَإِN<مَّ>ا فِدَW<اۤ>ءً حَتَّىٰ تَضَعَ ٱلۡحَR<رۡ>بُ أَوۡزَاR<رَ>هَاۚ ذَT< ٰ>لِكَۖ وَلَوۡ یَشَW<اۤ>ءُ ٱللَّهُ لَX<ٱ>N<ن>تَصَR<رَ> مِنۡهُمۡ وَلَT<ـٰ>كِX<ن> لِّیَQ<بۡ>لُوَX<ا۟> بَعۡضَكُN<م> بِبَعۡضX<ࣲ>ۗ N<وَ>X<ٱ>لَّذِینَ قُتِلُوX<ا۟> فِی سَبِیلِ ٱللَّهِ فَلَX<ن> N<یُ>ضِلَّ أَعۡمَT<ـٰ>لَهُمۡ A<۝>D<٤>",
"سَیَهۡدِیهِمۡ وَیُصۡلِحُ بَالَهُمۡ A<۝>D<٥>",
"وَیُQ<دۡ>خِلُهُمُ ٱلۡجَN<نَّ>ةَ عَR<رَّ>فَهَا لَهُمۡ A<۝>D<٦>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<ن> تَN<ن>صُR<رُ>وX<ا۟> ٱللَّهَ یَN<ن>صُR<رۡ>كُمۡ وَیُثَبِّتۡ أَQ<قۡ>دَامَكُمۡ A<۝>D<٧>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> فَتَعۡسX<ࣰ>ا لَّهُمۡ وَأَضَلَّ أَعۡمَT<ـٰ>لَهُمۡ A<۝>D<٨>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ كَرِهُوX<ا۟> مَW<اۤ> أَN<ن>زَلَ ٱللَّهُ فَأَحۡبَطَ أَعۡمَT<ـٰ>لَهُمۡ A<۝>D<٩>",
"۞ أَفَلَمۡ یَسِیR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ فَیَN<ن>ظُR<رُ>وX<ا۟> كَیۡفَ كَانَ عَT<ـٰ>قِبَةُ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۖ دَN<مَّ>R<رَ> ٱللَّهُ عَلَیۡهِمۡۖ وَلِلۡكَT<ـٰ>فِرِینَ أَمۡثَT<ـٰ>لُهَا A<۝>D<١٠>",
"ذَT< ٰ>لِكَ بِأَN<نَّ> ٱللَّهَ مَوۡلَى ٱلَّذِینَ ءَامَنُوX<ا۟> وَأَN<نَّ> ٱلۡكَT<ـٰ>فِرِینَ لَا مَوۡلَىٰ لَهُمۡ A<۝>D<١١>",
"إِN<نَّ> ٱللَّهَ یُQ<دۡ>خِلُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ>ۖ وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> یَتَمَتَّعُونَ وَیَأۡكُلُونَ كَمَا تَأۡكُلُ ٱلۡأَنۡعَT<ـٰ>مُ وَX<ٱ>X<ل>N<نَّ>اR<رُ> مَثۡوX<ࣰ>ى لَّهُمۡ A<۝>D<١٢>",
"وَكَأَیِّX<ن> N<مِّ>N<ن> قَR<رۡ>یَةٍ هِیَ أَشَدُّ قُوَّةX<ࣰ> N<مِّ>N<ن> قَR<رۡ>یَتِكَ ٱلَّتِW<یۤ> أَخۡR<رَ>جَتۡكَ أَهۡلَكۡنَT<ـٰ>هُمۡ فَلَا نَاصِR<رَ> لَهُمۡ A<۝>D<١٣>",
"أَفَمَN<ن> كَانَ عَلَىٰ بَیِّنَةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ> كَمَN<ن> زُیِّنَ لَهُT<ۥ> سُW<وۤ>ءُ عَمَلِهِT<ۦ> وَX<ٱ>تَّبَعُW<وۤ>X<ا۟> أَهۡوَW<اۤ>ءَهُم A<۝>D<١٤>",
"مَّثَلُ ٱلۡجَN<نَّ>ةِ ٱلَّتِی وُعِدَ ٱلۡمُتَّقُونَۖ فِیهَW<اۤ> أَنۡهَT<ـٰ>R<ر>X<ࣱ> N<مِّ>X<ن> N<مَّ>W<اۤ>ءٍ غَیۡرِ ءَاسِنX<ࣲ> N<وَ>أَنۡهَT<ـٰ>R<ر>X<ࣱ> N<مِّ>X<ن> لَّبَنX<ࣲ> لَّمۡ یَتَغَیَّR<رۡ> طَعۡمُهُT<ۥ> وَأَنۡهَT<ـٰ>R<ر>X<ࣱ> N<مِّ>نۡ خَمۡرX<ࣲ> لَّذَّةX<ࣲ> لِّX<ل>شَّT<ـٰ>رِبِینَ وَأَنۡهَT<ـٰ>R<ر>X<ࣱ> N<مِّ>نۡ عَسَلX<ࣲ> N<مُّ>صَفّX<ࣰ>ىۖ N<وَ>لَهُمۡ فِیهَا مِN<ن> كُلِّ ٱX<ل>ثَّمَR<رَ>T< ٰ>تِ وَمَغۡفِR<رَ>ةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِمۡۖ كَمَنۡ هُوَ خَT<ـٰ>لِدN<ࣱ> فِی ٱX<ل>N<نَّ>ارِ وَسُقُوX<ا۟> مَW<اۤ>ءً حَمِیمN<ࣰ>ا فَقَطَّعَ أَمۡعَW<اۤ>ءَهُمۡ A<۝>D<١٥>",
"وَمِنۡهُم N<مَّ>X<ن> N<یَ>سۡتَمِعُ إِلَیۡكَ حَتَّىW<ٰۤ> إِذَا خَR<رَ>جُوX<ا۟> مِنۡ عِN<ن>دِكَ قَالُوX<ا۟> لِلَّذِینَ أُوتُوX<ا۟> ٱلۡعِلۡمَ مَاذَا قَالَ ءَانِفًاۚ أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ طَبَعَ ٱللَّهُ عَلَىٰ قُلُوبِهِمۡ وَX<ٱ>تَّبَعُW<وۤ>X<ا۟> أَهۡوَW<اۤ>ءَهُمۡ A<۝>D<١٦>",
"وَX<ٱ>لَّذِینَ ٱهۡتَدَوۡX<ا۟> زَادَهُمۡ هُدX<ࣰ>ى N<وَ>ءَاتَX<ى>T<ٰ>هُمۡ تَQ<قۡ>وَX<ى>T<ٰ>هُمۡ A<۝>D<١٧>",
"فَهَلۡ یَN<ن>ظُR<رُ>ونَ إِلَّا ٱX<ل>سَّاعَةَ أَN<ن> تَأۡتِیَهُN<م> بَغۡتَةN<ࣰ>ۖ فَقَQ<دۡ> جَW<اۤ>ءَ أَشۡR<رَ>اطُهَاۚ فَأَN<نَّ>ىٰ لَهُمۡ إِذَا جَW<اۤ>ءَتۡهُمۡ ذِكۡR<رَ>X<ى>T<ٰ>هُمۡ A<۝>D<١٨>",
"فَX<ٱ>عۡلَمۡ أَN<نَّ>هُT<ۥ> لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا ٱللَّهُ وَX<ٱ>سۡتَغۡفِرۡ لِذَX<ن>N<ۢ>بِكَ وَلِلۡمُؤۡمِنِینَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تِۗ وَٱللَّهُ یَعۡلَمُ مُتَقَلَّبَكُمۡ وَمَثۡوَX<ى>T<ٰ>كُمۡ A<۝>D<١٩>",
"وَیَقُولُ ٱلَّذِینَ ءَامَنُوX<ا۟> لَوۡلَا نُزِّلَتۡ سُوR<رَ>ةN<ࣱ>ۖ فَإِذَW<اۤ> أُN<ن>زِلَتۡ سُوR<رَ>ةX<ࣱ> N<مُّ>حۡكَمَةX<ࣱ> N<وَ>ذُكِR<رَ> فِیهَا ٱلۡقِتَالُ R<رَ>أَیۡتَ ٱلَّذِینَ فِی قُلُوبِهِم N<مَّ>R<رَ>ضX<ࣱ> N<یَ>N<ن>ظُR<رُ>ونَ إِلَیۡكَ نَظَR<رَ> ٱلۡمَغۡشِیِّ عَلَیۡهِ مِنَ ٱلۡمَوۡتِۖ فَأَوۡلَىٰ لَهُمۡ A<۝>D<٢٠>",
"طَاعَةX<ࣱ> N<وَ>قَوۡلX<ࣱ> N<مَّ>عۡR<رُ>وفN<ࣱ>ۚ فَإِذَا عَزَمَ ٱلۡأَمۡR<رُ> فَلَوۡ صَدَقُوX<ا۟> ٱللَّهَ لَكَانَ خَیۡR<ر>X<ࣰ>ا لَّهُمۡ A<۝>D<٢١>",
"فَهَلۡ عَسَیۡتُمۡ إِN<ن> تَوَلَّیۡتُمۡ أَN<ن> تُفۡسِدُوX<ا۟> فِی ٱلۡأَR<رۡ>ضِ وَتُقَطِّعُW<وۤ>X<ا۟> أَR<رۡ>حَامَكُمۡ A<۝>D<٢٢>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ لَعَنَهُمُ ٱللَّهُ فَأَصَN<مَّ>هُمۡ وَأَعۡمَىW<ٰۤ> أَQ<بۡ>صَT<ـٰ>R<رَ>هُمۡ A<۝>D<٢٣>",
"أَفَلَا یَتَدَبَّR<رُ>ونَ ٱلۡقُR<رۡ>ءَانَ أَمۡ عَلَىٰ قُلُوبٍ أَQ<قۡ>فَالُهَاۤ A<۝>D<٢٤>",
"إِN<نَّ> ٱلَّذِینَ ٱR<رۡ>تَدُّوX<ا۟> عَلَىW<ٰۤ> أَQ<دۡ>بَT<ـٰ>رِهِم N<مِّ>X<ن>N<ۢ> بَعۡدِ مَا تَبَیَّنَ لَهُمُ ٱلۡهُدَى ٱX<ل>شَّیۡطَT<ـٰ>نُ سَوَّلَ لَهُمۡ وَأَمۡلَىٰ لَهُمۡ A<۝>D<٢٥>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ قَالُوX<ا۟> لِلَّذِینَ كَرِهُوX<ا۟> مَا نَزَّلَ ٱللَّهُ سَنُطِیعُكُمۡ فِی بَعۡضِ ٱلۡأَمۡرِۖ وَٱللَّهُ یَعۡلَمُ إِسۡR<رَ>اR<رَ>هُمۡ A<۝>D<٢٦>",
"فَكَیۡفَ إِذَا تَوَفَّتۡهُمُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ یَضۡرِبُونَ وُجُوهَهُمۡ وَأَQ<دۡ>بَT<ـٰ>R<رَ>هُمۡ A<۝>D<٢٧>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُمُ ٱتَّبَعُوX<ا۟> مَW<اۤ> أَسۡخَطَ ٱللَّهَ وَكَرِهُوX<ا۟> رِضۡوَT< ٰ>نَهُT<ۥ> فَأَحۡبَطَ أَعۡمَT<ـٰ>لَهُمۡ A<۝>D<٢٨>",
"أَمۡ حَسِبَ ٱلَّذِینَ فِی قُلُوبِهِم N<مَّ>R<رَ>ضٌ أَX<ن> لَّX<ن> N<یُ>خۡرِجَ ٱللَّهُ أَضۡغَT<ـٰ>نَهُمۡ A<۝>D<٢٩>",
"وَلَوۡ نَشَW<اۤ>ءُ لَأَR<رَ>یۡنَT<ـٰ>كَهُمۡ فَلَعَR<رَ>فۡتَهُN<م> بِسِیمَT<ـٰ>هُمۡۚ وَلَتَعۡرِفَN<نَّ>هُمۡ فِی لَحۡنِ ٱلۡقَوۡلِۚ وَٱللَّهُ یَعۡلَمُ أَعۡمَT<ـٰ>لَكُمۡ A<۝>D<٣٠>",
"وَلَنَQ<بۡ>لُوَN<نَّ>كُمۡ حَتَّىٰ نَعۡلَمَ ٱلۡمُجَT<ـٰ>هِدِینَ مِN<ن>كُمۡ وَX<ٱ>X<ل>صَّT<ـٰ>بِرِینَ وَنَQ<بۡ>لُوَX<ا۟> أَخۡبَاR<رَ>كُمۡ A<۝>D<٣١>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَصَدُّوX<ا۟> عَN<ن> سَبِیلِ ٱللَّهِ وَشَL<اۤ>قُّوX<ا۟> ٱX<ل>R<رَّ>سُولَ مِX<ن>N<ۢ> بَعۡدِ مَا تَبَیَّنَ لَهُمُ ٱلۡهُدَىٰ لَX<ن> N<یَ>ضُR<رُّ>وX<ا۟> ٱللَّهَ شَیۡءX<ࣰ>ا N<وَ>سَیُحۡبِطُ أَعۡمَT<ـٰ>لَهُمۡ A<۝>D<٣٢>",
"۞ یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَطِیعُوX<ا۟> ٱللَّهَ وَأَطِیعُوX<ا۟> ٱX<ل>R<رَّ>سُولَ وَلَا تُQ<بۡ>طِلُW<وۤ>X<ا۟> أَعۡمَT<ـٰ>لَكُمۡ A<۝>D<٣٣>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَصَدُّوX<ا۟> عَN<ن> سَبِیلِ ٱللَّهِ ثُN<مَّ> مَاتُوX<ا۟> وَهُمۡ كُفَّاR<ر>N<ࣱ> فَلَX<ن> N<یَ>غۡفِR<رَ> ٱللَّهُ لَهُمۡ A<۝>D<٣٤>",
"فَلَا تَهِنُوX<ا۟> وَتَQ<دۡ>عُW<وۤ>X<ا۟> إِلَى ٱX<ل>سَّلۡمِ وَأَN<ن>تُمُ ٱلۡأَعۡلَوۡنَ وَٱللَّهُ مَعَكُمۡ وَلَX<ن> N<یَ>تِR<رَ>كُمۡ أَعۡمَT<ـٰ>لَكُمۡ A<۝>D<٣٥>",
"إِN<نَّ>مَا ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَا لَعِبX<ࣱ> N<وَ>لَهۡوX<ࣱ>ۚ N<وَ>إِN<ن> تُؤۡمِنُوX<ا۟> وَتَتَّقُوX<ا۟> یُؤۡتِكُمۡ أُجُوR<رَ>كُمۡ وَلَا یَسۡءَلۡكُمۡ أَمۡوَT< ٰ>لَكُمۡ A<۝>D<٣٦>",
"إِX<ن> N<یَ>سۡءَلۡكُمُوهَا فَیُحۡفِكُمۡ تَQ<بۡ>خَلُوX<ا۟> وَیُخۡرِQ<جۡ> أَضۡغَT<ـٰ>نَكُمۡ A<۝>D<٣٧>",
"هَW<ـٰۤ>أَN<ن>تُمۡ هَW<ـٰۤ>ؤُلَW<اۤ>ءِ تُQ<دۡ>عَوۡنَ لِتُN<ن>فِقُوX<ا۟> فِی سَبِیلِ ٱللَّهِ فَمِN<ن>كُم N<مَّ>X<ن> N<یَ>Q<بۡ>خَلُۖ وَمَX<ن> N<یَ>Q<بۡ>خَلۡ فَإِN<نَّ>مَا یَQ<بۡ>خَلُ عَن N<نَّ>فۡسِهِT<ۦ>ۚ وَٱللَّهُ ٱلۡغَنِیُّ وَأَN<ن>تُمُ ٱلۡفُقَR<رَ>W<اۤ>ءُۚ وَإِN<ن> تَتَوَلَّوۡX<ا۟> یَسۡتَQ<بۡ>دِلۡ قَوۡمًا غَیۡR<رَ>كُمۡ ثُN<مَّ> لَا یَكُونُW<وۤ>X<ا۟> أَمۡثَT<ـٰ>لَكُم A<۝>D<٣٨>",
"#إِN<نَّ>ا فَتَحۡنَا لَكَ فَتۡحX<ࣰ>ا N<مُّ>بِینࣰا A<۝>D<١>",
"لِّیَغۡفِR<رَ> لَكَ ٱللَّهُ مَا تَقَدَّمَ مِN<ن> ذَX<ن>N<ۢ>بِكَ وَمَا تَأَخَّR<رَ> وَیُتِN<مَّ> نِعۡمَتَهُT<ۥ> عَلَیۡكَ وَیَهۡدِیَكَ صِR<رَ>T< ٰ>طX<ࣰ>ا N<مُّ>سۡتَقِیمࣰا A<۝>D<٢>",
"وَیَN<ن>صُR<رَ>كَ ٱللَّهُ نَصۡR<ر>ًا عَزِیزًا A<۝>D<٣>",
"هُوَ ٱلَّذِW<یۤ> أَN<ن>زَلَ ٱX<ل>سَّكِینَةَ فِی قُلُوبِ ٱلۡمُؤۡمِنِینَ لِیَزۡدَادُW<وۤ>X<ا۟> إِیمَT<ـٰ>نX<ࣰ>ا N<مَّ>عَ إِیمَT<ـٰ>نِهِمۡۗ وَلِلَّهِ جُنُودُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَكَانَ ٱللَّهُ عَلِیمًا حَكِیمࣰا A<۝>D<٤>",
"لِّیُQ<دۡ>خِلَ ٱلۡمُؤۡمِنِینَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تِ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَا وَیُكَفِّR<رَ> عَنۡهُمۡ سَیِّءَاتِهِمۡۚ وَكَانَ ذَT< ٰ>لِكَ عِN<ن>دَ ٱللَّهِ فَوۡزًا عَظِیمࣰا A<۝>D<٥>",
"وَیُعَذِّبَ ٱلۡمُنَT<ـٰ>فِقِینَ وَX<ٱ>لۡمُنَT<ـٰ>فِقَT<ـٰ>تِ وَX<ٱ>لۡمُشۡرِكِینَ وَX<ٱ>لۡمُشۡرِكَT<ـٰ>تِ ٱX<ل>ظَّL<اۤ>N<نِّ>ینَ بِٱللَّهِ ظَN<نَّ> ٱX<ل>سَّوۡءِۚ عَلَیۡهِمۡ دَW<اۤ>ئِR<رَ>ةُ ٱX<ل>سَّوۡءِۖ وَغَضِبَ ٱللَّهُ عَلَیۡهِمۡ وَلَعَنَهُمۡ وَأَعَدَّ لَهُمۡ جَهَN<نَّ>مَۖ وَسَW<اۤ>ءَتۡ مَصِیR<ر>ࣰا A<۝>D<٦>",
"وَلِلَّهِ جُنُودُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَكَانَ ٱللَّهُ عَزِیزًا حَكِیمًا A<۝>D<٧>",
"إِN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَT<ـٰ>كَ شَT<ـٰ>هِدX<ࣰ>ا N<وَ>مُبَشِّR<ر>X<ࣰ>ا N<وَ>نَذِیR<ر>ࣰا A<۝>D<٨>",
"لِّتُؤۡمِنُوX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ> وَتُعَزِّR<رُ>وهُ وَتُوَقِّR<رُ>وهُۚ وَتُسَبِّحُوهُ بُكۡR<رَ>ةX<ࣰ> N<وَ>أَصِیلًا A<۝>D<٩>",
"إِN<نَّ> ٱلَّذِینَ یُبَایِعُونَكَ إِN<نَّ>مَا یُبَایِعُونَ ٱللَّهَ یَدُ ٱللَّهِ فَوۡقَ أَیۡدِیهِمۡۚ فَمَن N<نَّ>كَثَ فَإِN<نَّ>مَا یَN<ن>كُثُ عَلَىٰ نَفۡسِهِT<ۦ>ۖ وَمَنۡ أَوۡفَىٰ بِمَا عَT<ـٰ>هَدَ عَلَیۡهُ ٱللَّهَ فَسَیُؤۡتِیهِ أَQ<جۡ>R<ر>ًا عَظِیمࣰا A<۝>D<١٠>",
"سَیَقُولُ لَكَ ٱلۡمُخَلَّفُونَ مِنَ ٱلۡأَعۡR<رَ>ابِ شَغَلَتۡنَW<اۤ> أَمۡوَT< ٰ>لُنَا وَأَهۡلُونَا فَX<ٱ>سۡتَغۡفِرۡ لَنَاۚ یَقُولُونَ بِأَلۡسِنَتِهِم N<مَّ>ا لَیۡسَ فِی قُلُوبِهِمۡۚ قُلۡ فَمَX<ن> N<یَ>مۡلِكُ لَكُم N<مِّ>نَ ٱللَّهِ شَیۡءًا إِنۡ أَR<رَ>ادَ بِكُمۡ ضَR<رّ>ًا أَوۡ أَR<رَ>ادَ بِكُمۡ نَفۡعَN<ۢ>اۚ بَلۡ كَانَ ٱللَّهُ بِمَا تَعۡمَلُونَ خَبِیR<رَ>ۢا A<۝>D<١١>",
"بَلۡ ظَنَN<ن>تُمۡ أَX<ن> لَّX<ن> N<یَ>N<ن>قَلِبَ ٱX<ل>R<رَّ>سُولُ وَX<ٱ>لۡمُؤۡمِنُونَ إِلَىW<ٰۤ> أَهۡلِیهِمۡ أَبَدX<ࣰ>ا N<وَ>زُیِّنَ ذَT< ٰ>لِكَ فِی قُلُوبِكُمۡ وَظَنَN<ن>تُمۡ ظَN<نَّ> ٱX<ل>سَّوۡءِ وَكُN<ن>تُمۡ قَوۡمَN<ۢ>ا بُوR<ر>ࣰا A<۝>D<١٢>",
"وَمَX<ن> لَّمۡ یُؤۡمِX<ن>N<ۢ> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ> فَإِN<نَّ>W<اۤ> أَعۡتَQ<دۡ>نَا لِلۡكَT<ـٰ>فِرِینَ سَعِیR<ر>ࣰا A<۝>D<١٣>",
"وَلِلَّهِ مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ یَغۡفِR<رُ> لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیُعَذِّبُ مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَكَانَ ٱللَّهُ غَفُوR<ر>X<ࣰ>ا R<رَّ>حِیمࣰا A<۝>D<١٤>",
"سَیَقُولُ ٱلۡمُخَلَّفُونَ إِذَا ٱN<ن>طَلَQ<قۡ>تُمۡ إِلَىٰ مَغَانِمَ لِتَأۡخُذُوهَا ذَR<رُ>ونَا نَتَّبِعۡكُمۡۖ یُرِیدُونَ أَX<ن> N<یُ>بَدِّلُوX<ا۟> كَلَT<ـٰ>مَ ٱللَّهِۚ قُل لَّN<ن> تَتَّبِعُونَا كَذَT< ٰ>لِكُمۡ قَالَ ٱللَّهُ مِN<ن> قَQ<بۡ>لُۖ فَسَیَقُولُونَ بَلۡ تَحۡسُدُونَنَاۚ بَلۡ كَانُوX<ا۟> لَا یَفۡقَهُونَ إِلَّا قَلِیلࣰا A<۝>D<١٥>",
"قُل لِّلۡمُخَلَّفِینَ مِنَ ٱلۡأَعۡR<رَ>ابِ سَتُQ<دۡ>عَوۡنَ إِلَىٰ قَوۡمٍ أُX<و۟>لِی بَأۡسN<ࣲ> شَدِیدN<ࣲ> تُقَT<ـٰ>تِلُونَهُمۡ أَوۡ یُسۡلِمُونَۖ فَإِN<ن> تُطِیعُوX<ا۟> یُؤۡتِكُمُ ٱللَّهُ أَQ<جۡ>R<ر>ًا حَسَنX<ࣰ>اۖ N<وَ>إِN<ن> تَتَوَلَّوۡX<ا۟> كَمَا تَوَلَّیۡتُم N<مِّ>N<ن> قَQ<بۡ>لُ یُعَذِّQ<بۡ>كُمۡ عَذَابًا أَلِیمࣰا A<۝>D<١٦>",
"لَّیۡسَ عَلَى ٱلۡأَعۡمَىٰ حَR<رَ>جX<ࣱ> N<وَ>لَا عَلَى ٱلۡأَعۡR<رَ>جِ حَR<رَ>جX<ࣱ> N<وَ>لَا عَلَى ٱلۡمَرِیضِ حَR<رَ>جX<ࣱ>ۗ N<وَ>مَX<ن> N<یُ>طِعِ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> یُQ<دۡ>خِلۡهُ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ>ۖ وَمَX<ن> N<یَ>تَوَلَّ یُعَذِّQ<بۡ>هُ عَذَابًا أَلِیمࣰا A<۝>D<١٧>",
"۞ لَّقَQ<دۡ> R<رَ>ضِیَ ٱللَّهُ عَنِ ٱلۡمُؤۡمِنِینَ إِذۡ یُبَایِعُونَكَ تَحۡتَ ٱX<ل>شَّجَR<رَ>ةِ فَعَلِمَ مَا فِی قُلُوبِهِمۡ فَأَN<ن>زَلَ ٱX<ل>سَّكِینَةَ عَلَیۡهِمۡ وَأَثَT<ـٰ>بَهُمۡ فَتۡحN<ࣰ>ا قَرِیبࣰا A<۝>D<١٨>",
"وَمَغَانِمَ كَثِیR<رَ>ةX<ࣰ> N<یَ>أۡخُذُونَهَاۗ وَكَانَ ٱللَّهُ عَزِیزًا حَكِیمࣰا A<۝>D<١٩>",
"وَعَدَكُمُ ٱللَّهُ مَغَانِمَ كَثِیR<رَ>ةN<ࣰ> تَأۡخُذُونَهَا فَعَجَّلَ لَكُمۡ هَT<ـٰ>ذِهِT<ۦ> وَكَفَّ أَیۡدِیَ ٱX<ل>N<نَّ>اسِ عَN<ن>كُمۡ وَلِتَكُونَ ءَایَةX<ࣰ> لِّلۡمُؤۡمِنِینَ وَیَهۡدِیَكُمۡ صِR<رَ>T< ٰ>طX<ࣰ>ا N<مُّ>سۡتَقِیمࣰا A<۝>D<٢٠>",
"وَأُخۡR<رَ>ىٰ لَمۡ تَQ<قۡ>دِR<رُ>وX<ا۟> عَلَیۡهَا قَQ<دۡ> أَحَاطَ ٱللَّهُ بِهَاۚ وَكَانَ ٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِیR<ر>ࣰا A<۝>D<٢١>",
"وَلَوۡ قَT<ـٰ>تَلَكُمُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَوَلَّوُX<ا۟> ٱلۡأَQ<دۡ>بَT<ـٰ>R<رَ> ثُN<مَّ> لَا یَجِدُونَ وَلِیّX<ࣰ>ا N<وَ>لَا نَصِیR<ر>ࣰا A<۝>D<٢٢>",
"سُN<نَّ>ةَ ٱللَّهِ ٱلَّتِی قَQ<دۡ> خَلَتۡ مِN<ن> قَQ<بۡ>لُۖ وَلَN<ن> تَجِدَ لِسُN<نَّ>ةِ ٱللَّهِ تَQ<بۡ>دِیلࣰا A<۝>D<٢٣>",
"وَهُوَ ٱلَّذِی كَفَّ أَیۡدِیَهُمۡ عَN<ن>كُمۡ وَأَیۡدِیَكُمۡ عَنۡهُN<م> بِبَQ<طۡ>نِ مَكَّةَ مِX<ن>N<ۢ> بَعۡدِ أَنۡ أَظۡفَR<رَ>كُمۡ عَلَیۡهِمۡۚ وَكَانَ ٱللَّهُ بِمَا تَعۡمَلُونَ بَصِیR<ر>ًا A<۝>D<٢٤>",
"هُمُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَصَدُّوكُمۡ عَنِ ٱلۡمَسۡجِدِ ٱلۡحَR<رَ>امِ وَX<ٱ>لۡهَQ<دۡ>یَ مَعۡكُوفًا أَX<ن> N<یَ>Q<بۡ>لُغَ مَحِلَّهُT<ۥ>ۚ وَلَوۡلَا رِجَالX<ࣱ> N<مُّ>ؤۡمِنُونَ وَنِسَW<اۤ>ءX<ࣱ> N<مُّ>ؤۡمِنَT<ـٰ>تX<ࣱ> لَّمۡ تَعۡلَمُوهُمۡ أَN<ن> تَطَءُوهُمۡ فَتُصِیبَكُم N<مِّ>نۡهُم N<مَّ>عَR<رَّ>ةُN<ۢ> بِغَیۡرِ عِلۡمX<ࣲ>ۖ لِّیُQ<دۡ>خِلَ ٱللَّهُ فِی R<رَ>حۡمَتِهِT<ۦ> مَX<ن> N<یَ>شَW<اۤ>ءُۚ لَوۡ تَزَیَّلُوX<ا۟> لَعَذَّQ<بۡ>نَا ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِنۡهُمۡ عَذَابًا أَلِیمًا A<۝>D<٢٥>",
"إِذۡ جَعَلَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فِی قُلُوبِهِمُ ٱلۡحَمِیَّةَ حَمِیَّةَ ٱلۡجَT<ـٰ>هِلِیَّةِ فَأَN<ن>زَلَ ٱللَّهُ سَكِینَتَهُT<ۥ> عَلَىٰ R<رَ>سُولِهِT<ۦ> وَعَلَى ٱلۡمُؤۡمِنِینَ وَأَلۡزَمَهُمۡ كَلِمَةَ ٱX<ل>تَّQ<قۡ>وَىٰ وَكَانُW<وۤ>X<ا۟> أَحَقَّ بِهَا وَأَهۡلَهَاۚ وَكَانَ ٱللَّهُ بِكُلِّ شَیۡءٍ عَلِیمࣰا A<۝>D<٢٦>",
"لَّقَQ<دۡ> صَدَقَ ٱللَّهُ R<رَ>سُولَهُ ٱX<ل>R<رُّ>ءۡیَا بِX<ٱ>لۡحَقِّۖ لَتَQ<دۡ>خُلُN<نَّ> ٱلۡمَسۡجِدَ ٱلۡحَR<رَ>امَ إِN<ن> شَW<اۤ>ءَ ٱللَّهُ ءَامِنِینَ مُحَلِّقِینَ R<رُ>ءُوسَكُمۡ وَمُقَصِّرِینَ لَا تَخَافُونَۖ فَعَلِمَ مَا لَمۡ تَعۡلَمُوX<ا۟> فَجَعَلَ مِN<ن> دُونِ ذَT< ٰ>لِكَ فَتۡحN<ࣰ>ا قَرِیبًا A<۝>D<٢٧>",
"هُوَ ٱلَّذِW<یۤ> أَR<رۡ>سَلَ R<رَ>سُولَهُT<ۥ> بِX<ٱ>لۡهُدَىٰ وَدِینِ ٱلۡحَقِّ لِیُظۡهِR<رَ>هُT<ۥ> عَلَى ٱX<ل>دِّینِ كُلِّهِT<ۦ>ۚ وَكَفَىٰ بِٱللَّهِ شَهِیدࣰا A<۝>D<٢٨>",
"مُّحَN<مَّ>دX<ࣱ> R<رَّ>سُولُ ٱللَّهِۚ وَX<ٱ>لَّذِینَ مَعَهُW<ۥۤ> أَشِدَّW<اۤ>ءُ عَلَى ٱلۡكُفَّارِ R<رُ>حَمَW<اۤ>ءُ بَیۡنَهُمۡۖ تَR<رَ>X<ى>T<ٰ>هُمۡ R<رُ>كَّعN<ࣰ>ا سُجَّدX<ࣰ>ا N<یَ>Q<بۡ>تَغُونَ فَضۡلX<ࣰ>ا N<مِّ>نَ ٱللَّهِ وَرِضۡوَT< ٰ>نN<ࣰ>اۖ سِیمَاهُمۡ فِی وُجُوهِهِم N<مِّ>نۡ أَثَرِ ٱX<ل>سُّجُودِۚ ذَT< ٰ>لِكَ مَثَلُهُمۡ فِی ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةِۚ وَمَثَلُهُمۡ فِی ٱلۡإِN<ن>جِیلِ كَزَR<رۡ>عٍ أَخۡR<رَ>جَ شَQ<طۡ>ءَهُT<ۥ> فَءَازَR<رَ>هُT<ۥ> فَX<ٱ>سۡتَغۡلَظَ فَX<ٱ>سۡتَوَىٰ عَلَىٰ سُوقِهِT<ۦ> یُعۡجِبُ ٱX<ل>زُّR<رَّ>اعَ لِیَغِیظَ بِهِمُ ٱلۡكُفَّاR<رَ>ۗ وَعَدَ ٱللَّهُ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ مِنۡهُم N<مَّ>غۡفِR<رَ>ةX<ࣰ> N<وَ>أَQ<جۡ>R<ر>ًا عَظِیمَۢا A<۝>D<٢٩>",
"#۞ یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تُقَدِّمُوX<ا۟> بَیۡنَ یَدَیِ ٱللَّهِ وَR<رَ>سُولِهِT<ۦ>ۖ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَۚ إِN<نَّ> ٱللَّهَ سَمِیعٌ عَلِJ<ی>مࣱ A<۝>D<١>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَR<رۡ>فَعُW<وۤ>X<ا۟> أَصۡوَT< ٰ>تَكُمۡ فَوۡقَ صَوۡتِ ٱX<ل>N<نَّ>بِیِّ وَلَا تَQ<جۡ>هَR<رُ>وX<ا۟> لَهُT<ۥ> بِX<ٱ>لۡقَوۡلِ كَجَهۡرِ بَعۡضِكُمۡ لِبَعۡضٍ أَN<ن> تَحۡبَطَ أَعۡمَT<ـٰ>لُكُمۡ وَأَN<ن>تُمۡ لَا تَشۡعُR<رُ>J<و>نَ A<۝>D<٢>",
"إِN<نَّ> ٱلَّذِینَ یَغُضُّونَ أَصۡوَT< ٰ>تَهُمۡ عِN<ن>دَ R<رَ>سُولِ ٱللَّهِ أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلَّذِینَ ٱمۡتَحَنَ ٱللَّهُ قُلُوبَهُمۡ لِX<ل>تَّQ<قۡ>وَىٰۚ لَهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<وَ>أَQ<جۡ>R<ر>ٌ عَظِJ<ی>مٌ A<۝>D<٣>",
"إِN<نَّ> ٱلَّذِینَ یُنَادُونَكَ مِX<ن> N<وَ>R<رَ>W<اۤ>ءِ ٱلۡحُجُR<رَ>T< ٰ>تِ أَكۡثَR<رُ>هُمۡ لَا یَعۡقِلُJ<و>نَ A<۝>D<٤>",
"وَلَوۡ أَN<نَّ>هُمۡ صَبَR<رُ>وX<ا۟> حَتَّىٰ تَخۡR<رُ>جَ إِلَیۡهِمۡ لَكَانَ خَیۡR<ر>X<ࣰ>ا لَّهُمۡۚ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٥>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<ن> جَW<اۤ>ءَكُمۡ فَاسِقُN<ۢ> بِنَبَإN<ࣲ> فَتَبَیَّنُW<وۤ>X<ا۟> أَN<ن> تُصِیبُوX<ا۟> قَوۡمَN<ۢ>ا بِجَهَT<ـٰ>لَةN<ࣲ> فَتُصۡبِحُوX<ا۟> عَلَىٰ مَا فَعَلۡتُمۡ نَT<ـٰ>دِمِJ<ی>نَ A<۝>D<٦>",
"وَX<ٱ>عۡلَمُW<وۤ>X<ا۟> أَN<نَّ> فِیكُمۡ R<رَ>سُولَ ٱللَّهِۚ لَوۡ یُطِیعُكُمۡ فِی كَثِیرX<ࣲ> N<مِّ>نَ ٱلۡأَمۡرِ لَعَنِتُّمۡ وَلَT<ـٰ>كِN<نَّ> ٱللَّهَ حَبَّبَ إِلَیۡكُمُ ٱلۡإِیمَT<ـٰ>نَ وَزَیَّنَهُT<ۥ> فِی قُلُوبِكُمۡ وَكَR<رَّ>هَ إِلَیۡكُمُ ٱلۡكُفۡR<رَ> وَX<ٱ>لۡفُسُوقَ وَX<ٱ>لۡعِصۡیَانَۚ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>R<رَّ>T< ٰ>شِدُJ<و>نَ A<۝>D<٧>",
"فَضۡلX<ࣰ>ا N<مِّ>نَ ٱللَّهِ وَنِعۡمَةX<ࣰ>ۚ N<وَ>ٱللَّهُ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<٨>",
"وَإِN<ن> طَW<اۤ>ئِفَتَانِ مِنَ ٱلۡمُؤۡمِنِینَ ٱQ<قۡ>تَتَلُوX<ا۟> فَأَصۡلِحُوX<ا۟> بَیۡنَهُمَاۖ فَإِX<ن>N<ۢ> بَغَتۡ إِحۡدَX<ى>T<ٰ>هُمَا عَلَى ٱلۡأُخۡR<رَ>ىٰ فَقَT<ـٰ>تِلُوX<ا۟> ٱلَّتِی تَQ<بۡ>غِی حَتَّىٰ تَفِW<یۤ>ءَ إِلَىW<ٰۤ> أَمۡرِ ٱللَّهِۚ فَإِN<ن> فَW<اۤ>ءَتۡ فَأَصۡلِحُوX<ا۟> بَیۡنَهُمَا بِX<ٱ>لۡعَQ<دۡ>لِ وَأَQ<قۡ>سِطُW<وۤ>X<ا۟>ۖ إِN<نَّ> ٱللَّهَ یُحِبُّ ٱلۡمُQ<قۡ>سِطِJ<ی>نَ A<۝>D<٩>",
"إِN<نَّ>مَا ٱلۡمُؤۡمِنُونَ إِخۡوَةN<ࣱ> فَأَصۡلِحُوX<ا۟> بَیۡنَ أَخَوَیۡكُمۡۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ لَعَلَّكُمۡ تُR<رۡ>حَمُJ<و>نَ A<۝>D<١٠>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا یَسۡخَR<رۡ> قَوۡمX<ࣱ> N<مِّ>N<ن> قَوۡمٍ عَسَىW<ٰۤ> أَX<ن> N<یَ>كُونُوX<ا۟> خَیۡR<ر>X<ࣰ>ا N<مِّ>نۡهُمۡ وَلَا نِسَW<اۤ>ءX<ࣱ> N<مِّ>ن N<نِّ>سَW<اۤ>ءٍ عَسَىW<ٰۤ> أَX<ن> N<یَ>كُN<نَّ> خَیۡR<ر>X<ࣰ>ا N<مِّ>نۡهُN<نَّ>ۖ وَلَا تَلۡمِزُW<وۤ>X<ا۟> أَN<ن>فُسَكُمۡ وَلَا تَنَابَزُوX<ا۟> بِX<ٱ>لۡأَلۡقَT<ـٰ>بِۖ بِئۡسَ ٱلِٱسۡمُ ٱلۡفُسُوقُ بَعۡدَ ٱلۡإِیمَT<ـٰ>نِۚ وَمَX<ن> لَّمۡ یَتُQ<بۡ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<١١>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱQ<جۡ>تَنِبُوX<ا۟> كَثِیR<ر>X<ࣰ>ا N<مِّ>نَ ٱX<ل>ظَّN<نِّ> إِN<نَّ> بَعۡضَ ٱX<ل>ظَّN<نِّ> إِثۡمX<ࣱ>ۖ N<وَ>لَا تَجَسَّسُوX<ا۟> وَلَا یَغۡتَب بَّعۡضُكُN<م> بَعۡضًاۚ أَیُحِبُّ أَحَدُكُمۡ أَX<ن> N<یَ>أۡكُلَ لَحۡمَ أَخِیهِ مَیۡتN<ࣰ>ا فَكَرِهۡتُمُوهُۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَۚ إِN<نَّ> ٱللَّهَ تَوَّابX<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٢>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>اسُ إِN<نَّ>ا خَلَQ<قۡ>نَT<ـٰ>كُم N<مِّ>N<ن> ذَكَرX<ࣲ> N<وَ>أُN<ن>ثَىٰ وَجَعَلۡنَT<ـٰ>كُمۡ شُعُوبX<ࣰ>ا N<وَ>قَبَW<اۤ>ئِلَ لِتَعَاR<رَ>فُW<وۤ>X<ا۟>ۚ إِN<نَّ> أَكۡR<رَ>مَكُمۡ عِN<ن>دَ ٱللَّهِ أَتۡقَX<ى>T<ٰ>كُمۡۚ إِN<نَّ> ٱللَّهَ عَلِیمٌ خَبِJ<ی>رࣱ A<۝>D<١٣>",
"۞ قَالَتِ ٱلۡأَعۡR<رَ>ابُ ءَامَN<نَّ>اۖ قُل لَّمۡ تُؤۡمِنُوX<ا۟> وَلَT<ـٰ>كِN<ن> قُولُW<وۤ>X<ا۟> أَسۡلَمۡنَا وَلَN<مَّ>ا یَQ<دۡ>خُلِ ٱلۡإِیمَT<ـٰ>نُ فِی قُلُوبِكُمۡۖ وَإِN<ن> تُطِیعُوX<ا۟> ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> لَا یَلِتۡكُم N<مِّ>نۡ أَعۡمَT<ـٰ>لِكُمۡ شَیۡءًاۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<١٤>",
"إِN<نَّ>مَا ٱلۡمُؤۡمِنُونَ ٱلَّذِینَ ءَامَنُوX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ> ثُN<مَّ> لَمۡ یَR<رۡ>تَابُوX<ا۟> وَجَT<ـٰ>هَدُوX<ا۟> بِأَمۡوَT< ٰ>لِهِمۡ وَأَN<ن>فُسِهِمۡ فِی سَبِیلِ ٱللَّهِۚ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>صَّT<ـٰ>دِقُJ<و>نَ A<۝>D<١٥>",
"قُلۡ أَتُعَلِّمُونَ ٱللَّهَ بِدِینِكُمۡ وَٱللَّهُ یَعۡلَمُ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۚ وَٱللَّهُ بِكُلِّ شَیۡءٍ عَلِJ<ی>مࣱ A<۝>D<١٦>",
"یَمُN<نُّ>ونَ عَلَیۡكَ أَنۡ أَسۡلَمُوX<ا۟>ۖ قُل لَّا تَمُN<نُّ>وX<ا۟> عَلَیَّ إِسۡلَT<ـٰ>مَكُN<م>ۖ بَلِ ٱللَّهُ یَمُN<نُّ> عَلَیۡكُمۡ أَنۡ هَدَX<ى>T<ٰ>كُمۡ لِلۡإِیمَT<ـٰ>نِ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<١٧>",
"إِN<نَّ> ٱللَّهَ یَعۡلَمُ غَیۡبَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَٱللَّهُ بَصِیR<رُ>N<ۢ> بِمَا تَعۡمَلُJ<و>نَ A<۝>D<١٨>",
"#L<قۤ>ۚ وَX<ٱ>لۡقُR<رۡ>ءَانِ ٱلۡمَجِJ<ی>Q<د>ِ A<۝>D<١>",
"بَلۡ عَجِبُW<وۤ>X<ا۟> أَN<ن> جَW<اۤ>ءَهُم N<مُّ>N<ن>ذِR<ر>X<ࣱ> N<مِّ>نۡهُمۡ فَقَالَ ٱلۡكَT<ـٰ>فِR<رُ>ونَ هَT<ـٰ>ذَا شَیۡءٌ عَجِJ<ی>Q<ب>ٌ A<۝>D<٢>",
"أَءِذَا مِتۡنَا وَكُN<نَّ>ا تُR<رَ>ابN<ࣰ>اۖ ذَT< ٰ>لِكَ R<رَ>Q<جۡ>عُN<ۢ> بَعِJ<ی>Q<د>ࣱ A<۝>D<٣>",
"قَQ<دۡ> عَلِمۡنَا مَا تَN<ن>قُصُ ٱلۡأَR<رۡ>ضُ مِنۡهُمۡۖ وَعِN<ن>دَنَا كِتَT<ـٰ>بٌ حَفِJ<ی>ظُۢ A<۝>D<٤>",
"بَلۡ كَذَّبُوX<ا۟> بِX<ٱ>لۡحَقِّ لَN<مَّ>ا جَW<اۤ>ءَهُمۡ فَهُمۡ فِW<یۤ> أَمۡرX<ࣲ> N<مَّ>رِJ<ی>Q<ج>ٍ A<۝>D<٥>",
"أَفَلَمۡ یَN<ن>ظُR<رُ>W<وۤ>X<ا۟> إِلَى ٱX<ل>سَّمَW<اۤ>ءِ فَوۡقَهُمۡ كَیۡفَ بَنَیۡنَT<ـٰ>هَا وَزَیَّN<نَّ>T<ـٰ>هَا وَمَا لَهَا مِN<ن> فُR<رُ>J<و>Q<ج>ࣲ A<۝>D<٦>",
"وَX<ٱ>لۡأَR<رۡ>ضَ مَدَQ<دۡ>نَT<ـٰ>هَا وَأَلۡقَیۡنَا فِیهَا R<رَ>وَT< ٰ>سِیَ وَأَX<ن>N<ۢ>بَتۡنَا فِیهَا مِN<ن> كُلِّ زَوۡجِN<ۭ> بَهِJ<ی>Q<ج>ࣲ A<۝>D<٧>",
"تَQ<بۡ>صِR<رَ>ةX<ࣰ> N<وَ>ذِكۡR<رَ>ىٰ لِكُلِّ عَQ<بۡ>دX<ࣲ> N<مُّ>نِJ<ی>Q<ب>ࣲ A<۝>D<٨>",
"وَنَزَّلۡنَا مِنَ ٱX<ل>سَّمَW<اۤ>ءِ مَW<اۤ>ءX<ࣰ> N<مُّ>بَT<ـٰ>R<رَ>كN<ࣰ>ا فَأَX<ن>N<ۢ>بَتۡنَا بِهِT<ۦ> جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>حَبَّ ٱلۡحَصِJ<ی>Q<د>ِ A<۝>D<٩>",
"وَX<ٱ>X<ل>N<نَّ>خۡلَ بَاسِقَT<ـٰ>تX<ࣲ> لَّهَا طَلۡعX<ࣱ> N<نَّ>ضِJ<ی>Q<د>ࣱ A<۝>D<١٠>",
"رِّزۡقX<ࣰ>ا لِّلۡعِبَادِۖ وَأَحۡیَیۡنَا بِهِT<ۦ> بَلۡدَةX<ࣰ> N<مَّ>یۡتN<ࣰ>اۚ كَذَT< ٰ>لِكَ ٱلۡخُR<رُ>J<و>Q<ج>ُ A<۝>D<١١>",
"كَذَّبَتۡ قَQ<بۡ>لَهُمۡ قَوۡمُ نُوحX<ࣲ> N<وَ>أَصۡحَT<ـٰ>بُ ٱX<ل>R<رَّ>سِّ وَثَمُJ<و>Q<د>ُ A<۝>D<١٢>",
"وَعَادX<ࣱ> N<وَ>فِرۡعَوۡنُ وَإِخۡوَT< ٰ>نُ لُJ<و>Q<ط>ࣲ A<۝>D<١٣>",
"وَأَصۡحَT<ـٰ>بُ ٱلۡأَیۡكَةِ وَقَوۡمُ تُبَّعN<ࣲ>ۚ كُلّN<ࣱ> كَذَّبَ ٱX<ل>R<رُّ>سُلَ فَحَقَّ وَعِJ<ی>Q<د>ِ A<۝>D<١٤>",
"أَفَعَیِینَا بِX<ٱ>لۡخَلۡقِ ٱلۡأَوَّلِۚ بَلۡ هُمۡ فِی لَQ<بۡ>سX<ࣲ> N<مِّ>نۡ خَلۡقN<ࣲ> جَدِJ<ی>Q<د>ࣲ A<۝>D<١٥>",
"وَلَقَQ<دۡ> خَلَQ<قۡ>نَا ٱلۡإِN<ن>سَT<ـٰ>نَ وَنَعۡلَمُ مَا تُوَسۡوِسُ بِهِT<ۦ> نَفۡسُهُT<ۥ>ۖ وَنَحۡنُ أَQ<قۡ>R<رَ>بُ إِلَیۡهِ مِنۡ حَQ<بۡ>لِ ٱلۡوَرِJ<ی>Q<د>ِ A<۝>D<١٦>",
"إِذۡ یَتَلَقَّى ٱلۡمُتَلَقِّیَانِ عَنِ ٱلۡیَمِینِ وَعَنِ ٱX<ل>شِّمَالِ قَعِJ<ی>Q<د>ࣱ A<۝>D<١٧>",
"مَّا یَلۡفِظُ مِN<ن> قَوۡلٍ إِلَّا لَدَیۡهِ R<رَ>قِیبٌ عَتِJ<ی>Q<د>ࣱ A<۝>D<١٨>",
"وَجَW<اۤ>ءَتۡ سَكۡR<رَ>ةُ ٱلۡمَوۡتِ بِX<ٱ>لۡحَقِّۖ ذَT< ٰ>لِكَ مَا كُN<ن>تَ مِنۡهُ تَحِJ<ی>Q<د>ُ A<۝>D<١٩>",
"وَنُفِخَ فِی ٱX<ل>صُّورِۚ ذَT< ٰ>لِكَ یَوۡمُ ٱلۡوَعِJ<ی>Q<د>ِ A<۝>D<٢٠>",
"وَجَW<اۤ>ءَتۡ كُلُّ نَفۡسX<ࣲ> N<مَّ>عَهَا سَW<اۤ>ئِقX<ࣱ> N<وَ>شَهِJ<ی>Q<د>ࣱ A<۝>D<٢١>",
"لَّقَQ<دۡ> كُN<ن>تَ فِی غَفۡلَةX<ࣲ> N<مِّ>نۡ هَT<ـٰ>ذَا فَكَشَفۡنَا عَN<ن>كَ غِطَW<اۤ>ءَكَ فَبَصَR<رُ>كَ ٱلۡیَوۡمَ حَدِJ<ی>Q<د>ࣱ A<۝>D<٢٢>",
"وَقَالَ قَرِینُهُT<ۥ> هَT<ـٰ>ذَا مَا لَدَیَّ عَتِJ<ی>Q<د>ٌ A<۝>D<٢٣>",
"أَلۡقِیَا فِی جَهَN<نَّ>مَ كُلَّ كَفَّارٍ عَنِJ<ی>Q<د>ࣲ A<۝>D<٢٤>",
"مَّN<نَّ>اعX<ࣲ> لِّلۡخَیۡرِ مُعۡتَدX<ࣲ> N<مُّ>رِJ<ی>Q<ب>ٍ A<۝>D<٢٥>",
"ٱلَّذِی جَعَلَ مَعَ ٱللَّهِ إِلَT<ـٰ>هًا ءَاخَR<رَ> فَأَلۡقِیَاهُ فِی ٱلۡعَذَابِ ٱX<ل>شَّدِJ<ی>Q<د>ِ A<۝>D<٢٦>",
"۞ قَالَ قَرِینُهُT<ۥ> R<رَ>بَّنَا مَW<اۤ> أَQ<طۡ>غَیۡتُهُT<ۥ> وَلَT<ـٰ>كِN<ن> كَانَ فِی ضَلَT<ـٰ>لِN<ۭ> بَعِJ<ی>Q<د>ࣲ A<۝>D<٢٧>",
"قَالَ لَا تَخۡتَصِمُوX<ا۟> لَدَیَّ وَقَQ<دۡ> قَدَّمۡتُ إِلَیۡكُN<م> بِX<ٱ>لۡوَعِJ<ی>Q<د>ِ A<۝>D<٢٨>",
"مَا یُبَدَّلُ ٱلۡقَوۡلُ لَدَیَّ وَمَW<اۤ> أَنَا۠ بِظَلَّT<ـٰ>مX<ࣲ> لِّلۡعَبِJ<ی>Q<د>ِ A<۝>D<٢٩>",
"یَوۡمَ نَقُولُ لِجَهَN<نَّ>مَ هَلِ ٱمۡتَلَأۡتِ وَتَقُولُ هَلۡ مِX<ن> N<مَّ>زِJ<ی>Q<د>ࣲ A<۝>D<٣٠>",
"وَأُزۡلِفَتِ ٱلۡجَN<نَّ>ةُ لِلۡمُتَّقِینَ غَیۡR<رَ> بَعِJ<ی>Q<د>ٍ A<۝>D<٣١>",
"هَT<ـٰ>ذَا مَا تُوعَدُونَ لِكُلِّ أَوَّابٍ حَفِJ<ی>ظࣲ A<۝>D<٣٢>",
"مَّنۡ خَشِیَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نَ بِX<ٱ>لۡغَیۡبِ وَجَW<اۤ>ءَ بِقَلۡبX<ࣲ> N<مُّ>نِJ<ی>Q<ب>ٍ A<۝>D<٣٣>",
"ٱQ<دۡ>خُلُوهَا بِسَلَT<ـٰ>مN<ࣲ>ۖ ذَT< ٰ>لِكَ یَوۡمُ ٱلۡخُلُJ<و>Q<د>ِ A<۝>D<٣٤>",
"لَهُم N<مَّ>ا یَشَW<اۤ>ءُونَ فِیهَا وَلَدَیۡنَا مَزِJ<ی>Q<د>ࣱ A<۝>D<٣٥>",
"وَكَمۡ أَهۡلَكۡنَا قَQ<بۡ>لَهُم N<مِّ>N<ن> قَR<رۡ>نٍ هُمۡ أَشَدُّ مِنۡهُN<م> بَQ<طۡ>شN<ࣰ>ا فَنَقَّبُوX<ا۟> فِی ٱلۡبِلَT<ـٰ>دِ هَلۡ مِX<ن> N<مَّ>حِJ<ی>صٍ A<۝>D<٣٦>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَذِكۡR<رَ>ىٰ لِمَN<ن> كَانَ لَهُT<ۥ> قَلۡبٌ أَوۡ أَلۡقَى ٱX<ل>سَّمۡعَ وَهُوَ شَهِJ<ی>Q<د>ࣱ A<۝>D<٣٧>",
"وَلَقَQ<دۡ> خَلَQ<قۡ>نَا ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ وَمَا بَیۡنَهُمَا فِی سِتَّةِ أَیَّامX<ࣲ> N<وَ>مَا مَسَّنَا مِX<ن> لُّغُJ<و>Q<ب>ࣲ A<۝>D<٣٨>",
"فَX<ٱ>صۡبِرۡ عَلَىٰ مَا یَقُولُونَ وَسَبِّحۡ بِحَمۡدِ R<رَ>بِّكَ قَQ<بۡ>لَ طُلُوعِ ٱX<ل>شَّمۡسِ وَقَQ<بۡ>لَ ٱلۡغُR<رُ>J<و>Q<ب>ِ A<۝>D<٣٩>",
"وَمِنَ ٱلَّیۡلِ فَسَبِّحۡهُ وَأَQ<دۡ>بَT<ـٰ>R<رَ> ٱX<ل>سُّجُJ<و>Q<د>ِ A<۝>D<٤٠>",
"وَX<ٱ>سۡتَمِعۡ یَوۡمَ یُنَادِ ٱلۡمُنَادِ مِX<ن> N<مَّ>كَانN<ࣲ> قَرِJ<ی>Q<ب>ࣲ A<۝>D<٤١>",
"یَوۡمَ یَسۡمَعُونَ ٱX<ل>صَّیۡحَةَ بِX<ٱ>لۡحَقِّۚ ذَT< ٰ>لِكَ یَوۡمُ ٱلۡخُR<رُ>J<و>Q<ج>ِ A<۝>D<٤٢>",
"إِN<نَّ>ا نَحۡنُ نُحۡیِT<ۦ> وَنُمِیتُ وَإِلَیۡنَا ٱلۡمَصِJ<ی>رُ A<۝>D<٤٣>",
"یَوۡمَ تَشَقَّقُ ٱلۡأَR<رۡ>ضُ عَنۡهُمۡ سِR<رَ>اعN<ࣰ>اۚ ذَT< ٰ>لِكَ حَشۡR<ر>ٌ عَلَیۡنَا یَسِJ<ی>رࣱ A<۝>D<٤٤>",
"نَّحۡنُ أَعۡلَمُ بِمَا یَقُولُونَۖ وَمَW<اۤ> أَN<ن>تَ عَلَیۡهِN<م> بِجَبَّارN<ࣲ>ۖ فَذَكِّرۡ بِX<ٱ>لۡقُR<رۡ>ءَانِ مَX<ن> N<یَ>خَافُ وَعِJ<ی>Q<د>ِ A<۝>D<٤٥>",
"#وَX<ٱ>X<ل>ذَّT< ٰ>رِیَT<ـٰ>تِ ذَR<رۡ>وࣰا A<۝>D<١>",
"فَX<ٱ>لۡحَT<ـٰ>مِلَT<ـٰ>تِ وِQ<قۡ>R<ر>ࣰا A<۝>D<٢>",
"فَX<ٱ>لۡجَT<ـٰ>رِیَT<ـٰ>تِ یُسۡR<ر>ࣰا A<۝>D<٣>",
"فَX<ٱ>لۡمُقَسِّمَT<ـٰ>تِ أَمۡR<ر>ًا A<۝>D<٤>",
"إِN<نَّ>مَا تُوعَدُونَ لَصَادِQ<ق>ࣱ A<۝>D<٥>",
"وَإِN<نَّ> ٱX<ل>دِّینَ لَوَT< ٰ>قِعࣱ A<۝>D<٦>",
"وَX<ٱ>X<ل>سَّمَW<اۤ>ءِ ذَاتِ ٱلۡحُبُكِ A<۝>D<٧>",
"إِN<نَّ>كُمۡ لَفِی قَوۡلX<ࣲ> N<مُّ>خۡتَلِفࣲ A<۝>D<٨>",
"یُؤۡفَكُ عَنۡهُ مَنۡ أُفِكَ A<۝>D<٩>",
"قُتِلَ ٱلۡخَR<رَّ>T< ٰ>صُJ<و>نَ A<۝>D<١٠>",
"ٱلَّذِینَ هُمۡ فِی غَمۡR<رَ>ةN<ࣲ> سَاهُJ<و>نَ A<۝>D<١١>",
"یَسۡءَلُونَ أَیَّانَ یَوۡمُ ٱX<ل>دِّJ<ی>نِ A<۝>D<١٢>",
"یَوۡمَ هُمۡ عَلَى ٱX<ل>N<نَّ>ارِ یُفۡتَنُJ<و>نَ A<۝>D<١٣>",
"ذُوقُوX<ا۟> فِتۡنَتَكُمۡ هَT<ـٰ>ذَا ٱلَّذِی كُN<ن>تُN<م> بِهِT<ۦ> تَسۡتَعۡجِلُJ<و>نَ A<۝>D<١٤>",
"إِN<نَّ> ٱلۡمُتَّقِینَ فِی جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>عُیُJ<و>نٍ A<۝>D<١٥>",
"ءَاخِذِینَ مَW<اۤ> ءَاتَX<ى>T<ٰ>هُمۡ R<رَ>بُّهُمۡۚ إِN<نَّ>هُمۡ كَانُوX<ا۟> قَQ<بۡ>لَ ذَT< ٰ>لِكَ مُحۡسِنِJ<ی>نَ A<۝>D<١٦>",
"كَانُوX<ا۟> قَلِیلX<ࣰ>ا N<مِّ>نَ ٱلَّیۡلِ مَا یَهۡجَعُJ<و>نَ A<۝>D<١٧>",
"وَبِX<ٱ>لۡأَسۡحَارِ هُمۡ یَسۡتَغۡفِR<رُ>J<و>نَ A<۝>D<١٨>",
"وَفِW<یۤ> أَمۡوَT< ٰ>لِهِمۡ حَقّX<ࣱ> لِّX<ل>سَّW<اۤ>ئِلِ وَX<ٱ>لۡمَحۡR<رُ>J<و>مِ A<۝>D<١٩>",
"وَفِی ٱلۡأَR<رۡ>ضِ ءَایَT<ـٰ>تX<ࣱ> لِّلۡمُوقِنِJ<ی>نَ A<۝>D<٢٠>",
"وَفِW<یۤ> أَN<ن>فُسِكُمۡۚ أَفَلَا تُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٢١>",
"وَفِی ٱX<ل>سَّمَW<اۤ>ءِ رِزۡقُكُمۡ وَمَا تُوعَدُJ<و>نَ A<۝>D<٢٢>",
"فَوَR<رَ>بِّ ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِ إِN<نَّ>هُT<ۥ> لَحَقّX<ࣱ> N<مِّ>ثۡلَ مَW<اۤ> أَN<نَّ>كُمۡ تَN<ن>طِقُJ<و>نَ A<۝>D<٢٣>",
"هَلۡ أَتَX<ى>T<ٰ>كَ حَدِیثُ ضَیۡفِ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ ٱلۡمُكۡR<رَ>مِJ<ی>نَ A<۝>D<٢٤>",
"إِذۡ دَخَلُوX<ا۟> عَلَیۡهِ فَقَالُوX<ا۟> سَلَT<ـٰ>مN<ࣰ>اۖ قَالَ سَلَT<ـٰ>مN<ࣱ> قَوۡمX<ࣱ> N<مُّ>N<ن>كَR<رُ>J<و>نَ A<۝>D<٢٥>",
"فَR<رَ>اغَ إِلَىW<ٰۤ> أَهۡلِهِT<ۦ> فَجَW<اۤ>ءَ بِعِQ<جۡ>لN<ࣲ> سَمِJ<ی>نࣲ A<۝>D<٢٦>",
"فَقَR<رَّ>بَهُW<ۥۤ> إِلَیۡهِمۡ قَالَ أَلَا تَأۡكُلُJ<و>نَ A<۝>D<٢٧>",
"فَأَوۡجَسَ مِنۡهُمۡ خِیفَةN<ࣰ>ۖ قَالُوX<ا۟> لَا تَخَفۡۖ وَبَشَّR<رُ>وهُ بِغُلَT<ـٰ>مٍ عَلِJ<ی>مࣲ A<۝>D<٢٨>",
"فَأَQ<قۡ>بَلَتِ ٱمۡR<رَ>أَتُهُT<ۥ> فِی صَR<رَّ>ةN<ࣲ> فَصَكَّتۡ وَQ<جۡ>هَهَا وَقَالَتۡ عَجُوزٌ عَقِJ<ی>مࣱ A<۝>D<٢٩>",
"قَالُوX<ا۟> كَذَT< ٰ>لِكِ قَالَ R<رَ>بُّكِۖ إِN<نَّ>هُT<ۥ> هُوَ ٱلۡحَكِیمُ ٱلۡعَلِJ<ی>مُ A<۝>D<٣٠>",
"۞ قَالَ فَمَا خَQ<طۡ>بُكُمۡ أَیُّهَا ٱلۡمُR<رۡ>سَلُJ<و>نَ A<۝>D<٣١>",
"قَالُW<وۤ>X<ا۟> إِN<نَّ>W<اۤ> أُR<رۡ>سِلۡنَW<اۤ> إِلَىٰ قَوۡمX<ࣲ> N<مُّ>Q<جۡ>رِمِJ<ی>نَ A<۝>D<٣٢>",
"لِنُR<رۡ>سِلَ عَلَیۡهِمۡ حِجَاR<رَ>ةX<ࣰ> N<مِّ>N<ن> طِJ<ی>نࣲ A<۝>D<٣٣>",
"مُّسَوَّمَةً عِN<ن>دَ R<رَ>بِّكَ لِلۡمُسۡرِفِJ<ی>نَ A<۝>D<٣٤>",
"فَأَخۡR<رَ>Q<جۡ>نَا مَN<ن> كَانَ فِیهَا مِنَ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٣٥>",
"فَمَا وَجَQ<دۡ>نَا فِیهَا غَیۡR<رَ> بَیۡتX<ࣲ> N<مِّ>نَ ٱلۡمُسۡلِمِJ<ی>نَ A<۝>D<٣٦>",
"وَتَR<رَ>كۡنَا فِیهَW<اۤ> ءَایَةX<ࣰ> لِّلَّذِینَ یَخَافُونَ ٱلۡعَذَابَ ٱلۡأَلِJ<ی>مَ A<۝>D<٣٧>",
"وَفِی مُوسَىW<ٰۤ> إِذۡ أَR<رۡ>سَلۡنَT<ـٰ>هُ إِلَىٰ فِرۡعَوۡنَ بِسُلۡطَT<ـٰ>نX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٣٨>",
"فَتَوَلَّىٰ بِR<رُ>كۡنِهِT<ۦ> وَقَالَ سَT<ـٰ>حِR<ر>ٌ أَوۡ مَQ<جۡ>نُJ<و>نࣱ A<۝>D<٣٩>",
"فَأَخَذۡنَT<ـٰ>هُ وَجُنُودَهُT<ۥ> فَنَبَذۡنَT<ـٰ>هُمۡ فِی ٱلۡیَN<مِّ> وَهُوَ مُلِJ<ی>مࣱ A<۝>D<٤٠>",
"وَفِی عَادٍ إِذۡ أَR<رۡ>سَلۡنَا عَلَیۡهِمُ ٱX<ل>رِّیحَ ٱلۡعَقِJ<ی>مَ A<۝>D<٤١>",
"مَا تَذَR<رُ> مِN<ن> شَیۡءٍ أَتَتۡ عَلَیۡهِ إِلَّا جَعَلَتۡهُ كَX<ٱ>X<ل>R<رَّ>مِJ<ی>مِ A<۝>D<٤٢>",
"وَفِی ثَمُودَ إِذۡ قِیلَ لَهُمۡ تَمَتَّعُوX<ا۟> حَتَّىٰ حِJ<ی>نࣲ A<۝>D<٤٣>",
"فَعَتَوۡX<ا۟> عَنۡ أَمۡرِ R<رَ>بِّهِمۡ فَأَخَذَتۡهُمُ ٱX<ل>صَّT<ـٰ>عِقَةُ وَهُمۡ یَN<ن>ظُR<رُ>J<و>نَ A<۝>D<٤٤>",
"فَمَا ٱسۡتَطَT<ـٰ>عُوX<ا۟> مِN<ن> قِیَامX<ࣲ> N<وَ>مَا كَانُوX<ا۟> مُN<ن>تَصِرِJ<ی>نَ A<۝>D<٤٥>",
"وَقَوۡمَ نُوحX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لُۖ إِN<نَّ>هُمۡ كَانُوX<ا۟> قَوۡمN<ࣰ>ا فَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٤٦>",
"وَX<ٱ>X<ل>سَّمَW<اۤ>ءَ بَنَیۡنَT<ـٰ>هَا بِأَیۡX<ی۟>دX<ࣲ> N<وَ>إِN<نَّ>ا لَمُوسِعُJ<و>نَ A<۝>D<٤٧>",
"وَX<ٱ>لۡأَR<رۡ>ضَ فَR<رَ>شۡنَT<ـٰ>هَا فَنِعۡمَ ٱلۡمَT<ـٰ>هِدُJ<و>نَ A<۝>D<٤٨>",
"وَمِN<ن> كُلِّ شَیۡءٍ خَلَQ<قۡ>نَا زَوۡجَیۡنِ لَعَلَّكُمۡ تَذَكَّR<رُ>J<و>نَ A<۝>D<٤٩>",
"فَفِR<رُّ>W<وۤ>X<ا۟> إِلَى ٱللَّهِۖ إِN<نِّ>ی لَكُم N<مِّ>نۡهُ نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٥٠>",
"وَلَا تَQ<جۡ>عَلُوX<ا۟> مَعَ ٱللَّهِ إِلَT<ـٰ>هًا ءَاخَR<رَ>ۖ إِN<نِّ>ی لَكُم N<مِّ>نۡهُ نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٥١>",
"كَذَT< ٰ>لِكَ مَW<اۤ> أَتَى ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِم N<مِّ>X<ن> R<رَّ>سُولٍ إِلَّا قَالُوX<ا۟> سَاحِR<ر>ٌ أَوۡ مَQ<جۡ>نُJ<و>نٌ A<۝>D<٥٢>",
"أَتَوَاصَوۡX<ا۟> بِهِT<ۦ>ۚ بَلۡ هُمۡ قَوۡمN<ࣱ> طَاغُJ<و>نَ A<۝>D<٥٣>",
"فَتَوَلَّ عَنۡهُمۡ فَمَW<اۤ> أَN<ن>تَ بِمَلُJ<و>مࣲ A<۝>D<٥٤>",
"وَذَكِّرۡ فَإِN<نَّ> ٱX<ل>ذِّكۡR<رَ>ىٰ تَN<ن>فَعُ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<٥٥>",
"وَمَا خَلَQ<قۡ>تُ ٱلۡجِN<نَّ> وَX<ٱ>لۡإِN<ن>سَ إِلَّا لِیَعۡبُدُJ<و>نِ A<۝>D<٥٦>",
"مَW<اۤ> أُرِیدُ مِنۡهُم N<مِّ>X<ن> رِّزۡقX<ࣲ> N<وَ>مَW<اۤ> أُرِیدُ أَX<ن> N<یُ>Q<طۡ>عِمُJ<و>نِ A<۝>D<٥٧>",
"إِN<نَّ> ٱللَّهَ هُوَ ٱX<ل>R<رَّ>زَّاقُ ذُو ٱلۡقُوَّةِ ٱلۡمَتِJ<ی>نُ A<۝>D<٥٨>",
"فَإِN<نَّ> لِلَّذِینَ ظَلَمُوX<ا۟> ذَنُوبX<ࣰ>ا N<مِّ>ثۡلَ ذَنُوبِ أَصۡحَT<ـٰ>بِهِمۡ فَلَا یَسۡتَعۡجِلُJ<و>نِ A<۝>D<٥٩>",
"فَوَیۡلX<ࣱ> لِّلَّذِینَ كَفَR<رُ>وX<ا۟> مِX<ن> N<یَ>وۡمِهِمُ ٱلَّذِی یُوعَدُJ<و>نَ A<۝>D<٦٠>",
"#وَX<ٱ>X<ل>طُّJ<و>R<ر>ِ A<۝>D<١>",
"وَكِتَT<ـٰ>بX<ࣲ> N<مَّ>سۡطُJ<و>R<ر>ࣲ A<۝>D<٢>",
"فِی R<رَ>قّX<ࣲ> N<مَّ>N<ن>شُJ<و>R<ر>ࣲ A<۝>D<٣>",
"وَX<ٱ>لۡبَیۡتِ ٱلۡمَعۡمُJ<و>R<ر>ِ A<۝>D<٤>",
"وَX<ٱ>X<ل>سَّQ<قۡ>فِ ٱلۡمَR<رۡ>فُJ<و>عِ A<۝>D<٥>",
"وَX<ٱ>لۡبَحۡرِ ٱلۡمَسۡجُJ<و>R<ر>ِ A<۝>D<٦>",
"إِN<نَّ> عَذَابَ R<رَ>بِّكَ لَوَT< ٰ>قِعࣱ A<۝>D<٧>",
"مَّا لَهُT<ۥ> مِN<ن> دَافِعࣲ A<۝>D<٨>",
"یَوۡمَ تَمُوR<رُ> ٱX<ل>سَّمَW<اۤ>ءُ مَوۡR<ر>ࣰا A<۝>D<٩>",
"وَتَسِیR<رُ> ٱلۡجِبَالُ سَیۡR<ر>ࣰا A<۝>D<١٠>",
"فَوَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<١١>",
"ٱلَّذِینَ هُمۡ فِی خَوۡضX<ࣲ> N<یَ>لۡعَبُJ<و>نَ A<۝>D<١٢>",
"یَوۡمَ یُدَعُّونَ إِلَىٰ نَارِ جَهَN<نَّ>مَ دَعًّا A<۝>D<١٣>",
"هَT<ـٰ>ذِهِ ٱX<ل>N<نَّ>اR<رُ> ٱلَّتِی كُN<ن>تُN<م> بِهَا تُكَذِّبُJ<و>نَ A<۝>D<١٤>",
"أَفَسِحۡR<ر>ٌ هَT<ـٰ>ذَW<اۤ> أَمۡ أَN<ن>تُمۡ لَا تُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<١٥>",
"ٱصۡلَوۡهَا فَX<ٱ>صۡبِR<رُ>W<وۤ>X<ا۟> أَوۡ لَا تَصۡبِR<رُ>وX<ا۟> سَوَW<اۤ>ءٌ عَلَیۡكُمۡۖ إِN<نَّ>مَا تُQ<جۡ>زَوۡنَ مَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<١٦>",
"إِN<نَّ> ٱلۡمُتَّقِینَ فِی جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>نَعِJ<ی>مࣲ A<۝>D<١٧>",
"فَT<ـٰ>كِهِینَ بِمَW<اۤ> ءَاتَX<ى>T<ٰ>هُمۡ R<رَ>بُّهُمۡ وَوَقَX<ى>T<ٰ>هُمۡ R<رَ>بُّهُمۡ عَذَابَ ٱلۡجَحِJ<ی>مِ A<۝>D<١٨>",
"كُلُوX<ا۟> وَX<ٱ>شۡR<رَ>بُوX<ا۟> هَنِW<یۤ>ءَN<ۢ>ا بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<١٩>",
"مُتَّكِءِینَ عَلَىٰ سُR<رُ>رX<ࣲ> N<مَّ>صۡفُوفَةX<ࣲ>ۖ N<وَ>زَوَّQ<جۡ>نَT<ـٰ>هُN<م> بِحُورٍ عِJ<ی>نࣲ A<۝>D<٢٠>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> وَX<ٱ>تَّبَعَتۡهُمۡ ذُرِّیَّتُهُN<م> بِإِیمَT<ـٰ>نٍ أَلۡحَQ<قۡ>نَا بِهِمۡ ذُرِّیَّتَهُمۡ وَمَW<اۤ> أَلَتۡنَT<ـٰ>هُم N<مِّ>نۡ عَمَلِهِم N<مِّ>N<ن> شَیۡءN<ࣲ>ۚ كُلُّ ٱمۡرِئِN<ۭ> بِمَا كَسَبَ R<رَ>هِJ<ی>نࣱ A<۝>D<٢١>",
"وَأَمۡدَQ<دۡ>نَT<ـٰ>هُN<م> بِفَT<ـٰ>كِهَةX<ࣲ> N<وَ>لَحۡمX<ࣲ> N<مِّ>N<مَّ>ا یَشۡتَهُJ<و>نَ A<۝>D<٢٢>",
"یَتَنَT<ـٰ>زَعُونَ فِیهَا كَأۡسX<ࣰ>ا لَّا لَغۡوN<ࣱ> فِیهَا وَلَا تَأۡثِJ<ی>مࣱ A<۝>D<٢٣>",
"۞ وَیَطُوفُ عَلَیۡهِمۡ غِلۡمَانX<ࣱ> لَّهُمۡ كَأَN<نَّ>هُمۡ لُؤۡلُؤX<ࣱ> N<مَّ>كۡنُJ<و>نࣱ A<۝>D<٢٤>",
"وَأَQ<قۡ>بَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضX<ࣲ> N<یَ>تَسَW<اۤ>ءَلُJ<و>نَ A<۝>D<٢٥>",
"قَالُW<وۤ>X<ا۟> إِN<نَّ>ا كُN<نَّ>ا قَQ<بۡ>لُ فِW<یۤ> أَهۡلِنَا مُشۡفِقِJ<ی>نَ A<۝>D<٢٦>",
"فَمَN<نَّ> ٱللَّهُ عَلَیۡنَا وَوَقَX<ى>T<ٰ>نَا عَذَابَ ٱX<ل>سَّمُJ<و>مِ A<۝>D<٢٧>",
"إِN<نَّ>ا كُN<نَّ>ا مِN<ن> قَQ<بۡ>لُ نَQ<دۡ>عُوهُۖ إِN<نَّ>هُT<ۥ> هُوَ ٱلۡبَR<رُّ> ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٢٨>",
"فَذَكِّرۡ فَمَW<اۤ> أَN<ن>تَ بِنِعۡمَتِ R<رَ>بِّكَ بِكَاهِنX<ࣲ> N<وَ>لَا مَQ<جۡ>نُJ<و>نٍ A<۝>D<٢٩>",
"أَمۡ یَقُولُونَ شَاعِR<ر>X<ࣱ> N<نَّ>تَR<رَ>بَّصُ بِهِT<ۦ> R<رَ>یۡبَ ٱلۡمَنُJ<و>نِ A<۝>D<٣٠>",
"قُلۡ تَR<رَ>بَّصُوX<ا۟> فَإِN<نِّ>ی مَعَكُم N<مِّ>نَ ٱلۡمُتَR<رَ>بِّصِJ<ی>نَ A<۝>D<٣١>",
"أَمۡ تَأۡمُR<رُ>هُمۡ أَحۡلَT<ـٰ>مُهُN<م> بِهَT<ـٰ>ذَW<اۤ>ۚ أَمۡ هُمۡ قَوۡمN<ࣱ> طَاغُJ<و>نَ A<۝>D<٣٢>",
"أَمۡ یَقُولُونَ تَقَوَّلَهُT<ۥ>ۚ بَل لَّا یُؤۡمِنُJ<و>نَ A<۝>D<٣٣>",
"فَلۡیَأۡتُوX<ا۟> بِحَدِیثX<ࣲ> N<مِّ>ثۡلِهِW<ۦۤ> إِN<ن> كَانُوX<ا۟> صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٣٤>",
"أَمۡ خُلِقُوX<ا۟> مِنۡ غَیۡرِ شَیۡءٍ أَمۡ هُمُ ٱلۡخَT<ـٰ>لِقُJ<و>نَ A<۝>D<٣٥>",
"أَمۡ خَلَقُوX<ا۟> ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَۚ بَل لَّا یُوقِنُJ<و>نَ A<۝>D<٣٦>",
"أَمۡ عِN<ن>دَهُمۡ خَزَW<اۤ>ئِنُ R<رَ>بِّكَ أَمۡ هُمُ ٱلۡمُصَۣیۡطِR<رُ>J<و>نَ A<۝>D<٣٧>",
"أَمۡ لَهُمۡ سُلَّمX<ࣱ> N<یَ>سۡتَمِعُونَ فِیهِۖ فَلۡیَأۡتِ مُسۡتَمِعُهُN<م> بِسُلۡطَT<ـٰ>نX<ࣲ> N<مُّ>بِJ<ی>نٍ A<۝>D<٣٨>",
"أَمۡ لَهُ ٱلۡبَنَT<ـٰ>تُ وَلَكُمُ ٱلۡبَنُJ<و>نَ A<۝>D<٣٩>",
"أَمۡ تَسۡءَلُهُمۡ أَQ<جۡ>R<ر>N<ࣰ>ا فَهُم N<مِّ>X<ن> N<مَّ>غۡR<رَ>مX<ࣲ> N<مُّ>ثۡقَلُJ<و>نَ A<۝>D<٤٠>",
"أَمۡ عِN<ن>دَهُمُ ٱلۡغَیۡبُ فَهُمۡ یَكۡتُبُJ<و>نَ A<۝>D<٤١>",
"أَمۡ یُرِیدُونَ كَیۡدN<ࣰ>اۖ فَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> هُمُ ٱلۡمَكِیدُJ<و>نَ A<۝>D<٤٢>",
"أَمۡ لَهُمۡ إِلَT<ـٰ>هٌ غَیۡR<رُ> ٱللَّهِۚ سُQ<بۡ>حَT<ـٰ>نَ ٱللَّهِ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٤٣>",
"وَإِX<ن> N<یَ>R<رَ>وۡX<ا۟> كِسۡفX<ࣰ>ا N<مِّ>نَ ٱX<ل>سَّمَW<اۤ>ءِ سَاقِطX<ࣰ>ا N<یَ>قُولُوX<ا۟> سَحَابX<ࣱ> N<مَّ>R<رۡ>كُJ<و>مࣱ A<۝>D<٤٤>",
"فَذَR<رۡ>هُمۡ حَتَّىٰ یُلَT<ـٰ>قُوX<ا۟> یَوۡمَهُمُ ٱلَّذِی فِیهِ یُصۡعَقُJ<و>نَ A<۝>D<٤٥>",
"یَوۡمَ لَا یُغۡنِی عَنۡهُمۡ كَیۡدُهُمۡ شَیۡءX<ࣰ>ا N<وَ>لَا هُمۡ یُN<ن>صَR<رُ>J<و>نَ A<۝>D<٤٦>",
"وَإِN<نَّ> لِلَّذِینَ ظَلَمُوX<ا۟> عَذَابN<ࣰ>ا دُونَ ذَT< ٰ>لِكَ وَلَT<ـٰ>كِN<نَّ> أَكۡثَR<رَ>هُمۡ لَا یَعۡلَمُJ<و>نَ A<۝>D<٤٧>",
"وَX<ٱ>صۡبِرۡ لِحُكۡمِ R<رَ>بِّكَ فَإِN<نَّ>كَ بِأَعۡیُنِنَاۖ وَسَبِّحۡ بِحَمۡدِ R<رَ>بِّكَ حِینَ تَقُJ<و>مُ A<۝>D<٤٨>",
"وَمِنَ ٱلَّیۡلِ فَسَبِّحۡهُ وَإِQ<دۡ>بَT<ـٰ>R<رَ> ٱX<ل>N<نُّ>جُJ<و>مِ A<۝>D<٤٩>",
"#وَX<ٱ>X<ل>N<نَّ>Q<جۡ>مِ إِذَا هَوَىٰ A<۝>D<١>",
"مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ A<۝>D<٢>",
"وَمَا یَN<ن>طِقُ عَنِ ٱلۡهَوَىٰۤ A<۝>D<٣>",
"إِنۡ هُوَ إِلَّا وَحۡیX<ࣱ> N<یُ>وحَىٰ A<۝>D<٤>",
"عَلَّمَهُT<ۥ> شَدِیدُ ٱلۡقُوَىٰ A<۝>D<٥>",
"ذُو مِR<رَّ>ةN<ࣲ> فَX<ٱ>سۡتَوَىٰ A<۝>D<٦>",
"وَهُوَ بِX<ٱ>لۡأُفُقِ ٱلۡأَعۡلَىٰ A<۝>D<٧>",
"ثُN<مَّ> دَنَا فَتَدَلَّىٰ A<۝>D<٨>",
"فَكَانَ قَابَ قَوۡسَیۡنِ أَوۡ أَQ<دۡ>نَىٰ A<۝>D<٩>",
"فَأَوۡحَىW<ٰۤ> إِلَىٰ عَQ<بۡ>دِهِT<ۦ> مَW<اۤ> أَوۡحَىٰ A<۝>D<١٠>",
"مَا كَذَبَ ٱلۡفُؤَادُ مَا R<رَ>أَىٰۤ A<۝>D<١١>",
"أَفَتُمَT<ـٰ>R<رُ>ونَهُT<ۥ> عَلَىٰ مَا یَR<رَ>ىٰ A<۝>D<١٢>",
"وَلَقَQ<دۡ> R<رَ>ءَاهُ نَزۡلَةً أُخۡR<رَ>ىٰ A<۝>D<١٣>",
"عِN<ن>دَ سِQ<دۡ>R<رَ>ةِ ٱلۡمُN<ن>تَهَىٰ A<۝>D<١٤>",
"عِN<ن>دَهَا جَN<نَّ>ةُ ٱلۡمَأۡوَىٰۤ A<۝>D<١٥>",
"إِذۡ یَغۡشَى ٱX<ل>سِّQ<دۡ>R<رَ>ةَ مَا یَغۡشَىٰ A<۝>D<١٦>",
"مَا زَاغَ ٱلۡبَصَR<رُ> وَمَا طَغَىٰ A<۝>D<١٧>",
"لَقَQ<دۡ> R<رَ>أَىٰ مِنۡ ءَایَT<ـٰ>تِ R<رَ>بِّهِ ٱلۡكُQ<بۡ>R<رَ>ىٰۤ A<۝>D<١٨>",
"أَفَR<رَ>ءَیۡتُمُ ٱX<ل>لَّT<ـٰ>تَ وَX<ٱ>لۡعُزَّىٰ A<۝>D<١٩>",
"وَمَنَX<و>T<ٰ>ةَ ٱX<ل>ثَّالِثَةَ ٱلۡأُخۡR<رَ>ىٰۤ A<۝>D<٢٠>",
"أَلَكُمُ ٱX<ل>ذَّكَR<رُ> وَلَهُ ٱلۡأُN<ن>ثَىٰ A<۝>D<٢١>",
"تِلۡكَ إِذN<ࣰ>ا قِسۡمَةN<ࣱ> ضِیزَىٰۤ A<۝>D<٢٢>",
"إِنۡ هِیَ إِلَّW<اۤ> أَسۡمَW<اۤ>ءN<ࣱ> سَN<مَّ>یۡتُمُوهَW<اۤ> أَN<ن>تُمۡ وَءَابَW<اۤ>ؤُكُم N<مَّ>W<اۤ> أَN<ن>زَلَ ٱللَّهُ بِهَا مِN<ن> سُلۡطَT<ـٰ>نٍۚ إِX<ن> N<یَ>تَّبِعُونَ إِلَّا ٱX<ل>ظَّN<نَّ> وَمَا تَهۡوَى ٱلۡأَN<ن>فُسُۖ وَلَقَQ<دۡ> جَW<اۤ>ءَهُم N<مِّ>X<ن> R<رَّ>بِّهِمُ ٱلۡهُدَىٰۤ A<۝>D<٢٣>",
"أَمۡ لِلۡإِN<ن>سَT<ـٰ>نِ مَا تَمَN<نَّ>ىٰ A<۝>D<٢٤>",
"فَلِلَّهِ ٱلۡءَاخِR<رَ>ةُ وَX<ٱ>لۡأُولَىٰ A<۝>D<٢٥>",
"۞ وَكَم N<مِّ>X<ن> N<مَّ>لَكN<ࣲ> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ لَا تُغۡنِی شَفَT<ـٰ>عَتُهُمۡ شَیۡءًا إِلَّا مِX<ن>N<ۢ> بَعۡدِ أَX<ن> N<یَ>أۡذَنَ ٱللَّهُ لِمَX<ن> N<یَ>شَW<اۤ>ءُ وَیَR<رۡ>ضَىٰۤ A<۝>D<٢٦>",
"إِN<نَّ> ٱلَّذِینَ لَا یُؤۡمِنُونَ بِX<ٱ>لۡءَاخِR<رَ>ةِ لَیُسَN<مُّ>ونَ ٱلۡمَلَW<ـٰۤ>ئِكَةَ تَسۡمِیَةَ ٱلۡأُN<ن>ثَىٰ A<۝>D<٢٧>",
"وَمَا لَهُN<م> بِهِT<ۦ> مِنۡ عِلۡمٍۖ إِX<ن> N<یَ>تَّبِعُونَ إِلَّا ٱX<ل>ظَّN<نَّ>ۖ وَإِN<نَّ> ٱX<ل>ظَّN<نَّ> لَا یُغۡنِی مِنَ ٱلۡحَقِّ شَیۡءࣰا A<۝>D<٢٨>",
"فَأَعۡرِضۡ عَX<ن> N<مَّ>N<ن> تَوَلَّىٰ عَN<ن> ذِكۡرِنَا وَلَمۡ یُرِQ<دۡ> إِلَّا ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا A<۝>D<٢٩>",
"ذَT< ٰ>لِكَ مَQ<بۡ>لَغُهُم N<مِّ>نَ ٱلۡعِلۡمِۚ إِN<نَّ> R<رَ>بَّكَ هُوَ أَعۡلَمُ بِمَN<ن> ضَلَّ عَN<ن> سَبِیلِهِT<ۦ> وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ A<۝>D<٣٠>",
"وَلِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِ لِیَQ<جۡ>زِیَ ٱلَّذِینَ أَسَW<ـٰۤ>ءُوX<ا۟> بِمَا عَمِلُوX<ا۟> وَیَQ<جۡ>زِیَ ٱلَّذِینَ أَحۡسَنُوX<ا۟> بِX<ٱ>لۡحُسۡنَى A<۝>D<٣١>",
"ٱلَّذِینَ یَQ<جۡ>تَنِبُونَ كَبَW<ـٰۤ>ئِR<رَ> ٱلۡإِثۡمِ وَX<ٱ>لۡفَوَT< ٰ>حِشَ إِلَّا ٱX<ل>لَّمَمَۚ إِN<نَّ> R<رَ>بَّكَ وَT< ٰ>سِعُ ٱلۡمَغۡفِR<رَ>ةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَN<ن>شَأَكُم N<مِّ>نَ ٱلۡأَR<رۡ>ضِ وَإِذۡ أَN<ن>تُمۡ أَجِN<نَّ>ةN<ࣱ> فِی بُطُونِ أُN<مَّ>هَT<ـٰ>تِكُمۡۖ فَلَا تُزَكُّW<وۤ>X<ا۟> أَN<ن>فُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰۤ A<۝>D<٣٢>",
"أَفَR<رَ>ءَیۡتَ ٱلَّذِی تَوَلَّىٰ A<۝>D<٣٣>",
"وَأَعۡطَىٰ قَلِیلX<ࣰ>ا N<وَ>أَكۡدَىٰۤ A<۝>D<٣٤>",
"أَعِN<ن>دَهُT<ۥ> عِلۡمُ ٱلۡغَیۡبِ فَهُوَ یَR<رَ>ىٰۤ A<۝>D<٣٥>",
"أَمۡ لَمۡ یُنَبَّأۡ بِمَا فِی صُحُفِ مُوسَىٰ A<۝>D<٣٦>",
"وَإِQ<بۡ>R<رَ>T< ٰ>هِیمَ ٱلَّذِی وَفَّىٰۤ A<۝>D<٣٧>",
"أَلَّا تَزِR<رُ> وَازِR<رَ>ةX<ࣱ> N<وِ>زۡR<رَ> أُخۡR<رَ>ىٰ A<۝>D<٣٨>",
"وَأَX<ن> لَّیۡسَ لِلۡإِN<ن>سَT<ـٰ>نِ إِلَّا مَا سَعَىٰ A<۝>D<٣٩>",
"وَأَN<نَّ> سَعۡیَهُT<ۥ> سَوۡفَ یُR<رَ>ىٰ A<۝>D<٤٠>",
"ثُN<مَّ> یُQ<جۡ>زَX<ى>T<ٰ>هُ ٱلۡجَزَW<اۤ>ءَ ٱلۡأَوۡفَىٰ A<۝>D<٤١>",
"وَأَN<نَّ> إِلَىٰ R<رَ>بِّكَ ٱلۡمُN<ن>تَهَىٰ A<۝>D<٤٢>",
"وَأَN<نَّ>هُT<ۥ> هُوَ أَضۡحَكَ وَأَQ<بۡ>كَىٰ A<۝>D<٤٣>",
"وَأَN<نَّ>هُT<ۥ> هُوَ أَمَاتَ وَأَحۡیَا A<۝>D<٤٤>",
"وَأَN<نَّ>هُT<ۥ> خَلَقَ ٱX<ل>زَّوۡجَیۡنِ ٱX<ل>ذَّكَR<رَ> وَX<ٱ>لۡأُN<ن>ثَىٰ A<۝>D<٤٥>",
"مِن N<نُّ>Q<طۡ>فَةٍ إِذَا تُمۡنَىٰ A<۝>D<٤٦>",
"وَأَN<نَّ> عَلَیۡهِ ٱX<ل>N<نَّ>شۡأَةَ ٱلۡأُخۡR<رَ>ىٰ A<۝>D<٤٧>",
"وَأَN<نَّ>هُT<ۥ> هُوَ أَغۡنَىٰ وَأَQ<قۡ>نَىٰ A<۝>D<٤٨>",
"وَأَN<نَّ>هُT<ۥ> هُوَ R<رَ>بُّ ٱX<ل>شِّعۡR<رَ>ىٰ A<۝>D<٤٩>",
"وَأَN<نَّ>هُW<ۥۤ> أَهۡلَكَ عَادًا ٱلۡأُولَىٰ A<۝>D<٥٠>",
"وَثَمُودَX<ا۟> فَمَW<اۤ> أَQ<بۡ>قَىٰ A<۝>D<٥١>",
"وَقَوۡمَ نُوحX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لُۖ إِN<نَّ>هُمۡ كَانُوX<ا۟> هُمۡ أَظۡلَمَ وَأَQ<طۡ>غَىٰ A<۝>D<٥٢>",
"وَX<ٱ>لۡمُؤۡتَفِكَةَ أَهۡوَىٰ A<۝>D<٥٣>",
"فَغَشَّX<ى>T<ٰ>هَا مَا غَشَّىٰ A<۝>D<٥٤>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكَ تَتَمَاR<رَ>ىٰ A<۝>D<٥٥>",
"هَT<ـٰ>ذَا نَذِیR<ر>X<ࣱ> N<مِّ>نَ ٱX<ل>N<نُّ>ذُرِ ٱلۡأُولَىٰۤ A<۝>D<٥٦>",
"أَزِفَتِ ٱلۡءَازِفَةُ A<۝>D<٥٧>",
"لَیۡسَ لَهَا مِN<ن> دُونِ ٱللَّهِ كَاشِفَةٌ A<۝>D<٥٨>",
"أَفَمِنۡ هَT<ـٰ>ذَا ٱلۡحَدِیثِ تَعۡجَبُJ<و>نَ A<۝>D<٥٩>",
"وَتَضۡحَكُونَ وَلَا تَQ<بۡ>كُJ<و>نَ A<۝>D<٦٠>",
"وَأَN<ن>تُمۡ سَT<ـٰ>مِدُJ<و>نَ A<۝>D<٦١>",
"فَ̅X<ٱ>̅سۡ̅جُ̅دُ̅و̅X<ا۟>̅ لِلَّهِ وَX<ٱ>عۡبُدُوX<ا۟> A<۝>D<٦٢> ۩",
"#ٱQ<قۡ>تَR<رَ>بَتِ ٱX<ل>سَّاعَةُ وَX<ٱ>N<ن>شَقَّ ٱلۡقَمَR<ر>ُ A<۝>D<١>",
"وَإِX<ن> N<یَ>R<رَ>وۡX<ا۟> ءَایَةX<ࣰ> N<یُ>عۡرِضُوX<ا۟> وَیَقُولُوX<ا۟> سِحۡR<ر>X<ࣱ> N<مُّ>سۡتَمِرࣱّ A<۝>D<٢>",
"وَكَذَّبُوX<ا۟> وَX<ٱ>تَّبَعُW<وۤ>X<ا۟> أَهۡوَW<اۤ>ءَهُمۡۚ وَكُلُّ أَمۡرX<ࣲ> N<مُّ>سۡتَقِرࣱّ A<۝>D<٣>",
"وَلَقَQ<دۡ> جَW<اۤ>ءَهُم N<مِّ>نَ ٱلۡأَX<ن>N<ۢ>بَW<اۤ>ءِ مَا فِیهِ مُزۡدَجَR<ر>ٌ A<۝>D<٤>",
"حِكۡمَةُN<ۢ> بَT<ـٰ>لِغَةN<ࣱ>ۖ فَمَا تُغۡنِ ٱX<ل>N<نُّ>ذُR<ر>ُ A<۝>D<٥>",
"فَتَوَلَّ عَنۡهُمۡۘ یَوۡمَ یَQ<دۡ>عُ ٱX<ل>دَّاعِ إِلَىٰ شَیۡءX<ࣲ> N<نُّ>كُR<ر>ٍ A<۝>D<٦>",
"خُشَّعًا أَQ<بۡ>صَT<ـٰ>R<رُ>هُمۡ یَخۡR<رُ>جُونَ مِنَ ٱلۡأَQ<جۡ>دَاثِ كَأَN<نَّ>هُمۡ جَR<رَ>ادX<ࣱ> N<مُّ>N<ن>تَشِرࣱ A<۝>D<٧>",
"مُّهۡطِعِینَ إِلَى ٱX<ل>دَّاعِۖ یَقُولُ ٱلۡكَT<ـٰ>فِR<رُ>ونَ هَT<ـٰ>ذَا یَوۡمٌ عَسِرࣱ A<۝>D<٨>",
"۞ كَذَّبَتۡ قَQ<بۡ>لَهُمۡ قَوۡمُ نُوحN<ࣲ> فَكَذَّبُوX<ا۟> عَQ<بۡ>دَنَا وَقَالُوX<ا۟> مَQ<جۡ>نُونX<ࣱ> N<وَ>X<ٱ>زۡدُجِرَ A<۝>D<٩>",
"فَدَعَا R<رَ>بَّهُW<ۥۤ> أَN<نِّ>ی مَغۡلُوبN<ࣱ> فَX<ٱ>N<ن>تَصِرۡ A<۝>D<١٠>",
"فَفَتَحۡنَW<اۤ> أَQ<بۡ>وَT< ٰ>بَ ٱX<ل>سَّمَW<اۤ>ءِ بِمَW<اۤ>ءX<ࣲ> N<مُّ>نۡهَمِرࣲ A<۝>D<١١>",
"وَفَجَّR<رۡ>نَا ٱلۡأَR<رۡ>ضَ عُیُونN<ࣰ>ا فَX<ٱ>لۡتَقَى ٱلۡمَW<اۤ>ءُ عَلَىW<ٰۤ> أَمۡرN<ࣲ> قَQ<دۡ> قُدِرَ A<۝>D<١٢>",
"وَحَمَلۡنَT<ـٰ>هُ عَلَىٰ ذَاتِ أَلۡوَT< ٰ>حX<ࣲ> N<وَ>دُسُR<ر>ࣲ A<۝>D<١٣>",
"تَQ<جۡ>رِی بِأَعۡیُنِنَا جَزَW<اۤ>ءX<ࣰ> لِّمَN<ن> كَانَ كُفِرَ A<۝>D<١٤>",
"وَلَقَد تَّR<رَ>كۡنَT<ـٰ>هَW<اۤ> ءَایَةN<ࣰ> فَهَلۡ مِX<ن> N<مُّ>دَّكِرࣲ A<۝>D<١٥>",
"فَكَیۡفَ كَانَ عَذَابِی وَنُذُR<ر>ِ A<۝>D<١٦>",
"وَلَقَQ<دۡ> یَسَّR<رۡ>نَا ٱلۡقُR<رۡ>ءَانَ لِX<ل>ذِّكۡرِ فَهَلۡ مِX<ن> N<مُّ>دَّكِرࣲ A<۝>D<١٧>",
"كَذَّبَتۡ عَادN<ࣱ> فَكَیۡفَ كَانَ عَذَابِی وَنُذُR<ر>ِ A<۝>D<١٨>",
"إِN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَا عَلَیۡهِمۡ رِیحN<ࣰ>ا صَR<رۡ>صَR<ر>N<ࣰ>ا فِی یَوۡمِ نَحۡسX<ࣲ> N<مُّ>سۡتَمِرࣲّ A<۝>D<١٩>",
"تَN<ن>زِعُ ٱX<ل>N<نَّ>اسَ كَأَN<نَّ>هُمۡ أَعۡجَازُ نَخۡلX<ࣲ> N<مُّ>N<ن>قَعِرࣲ A<۝>D<٢٠>",
"فَكَیۡفَ كَانَ عَذَابِی وَنُذُR<ر>ِ A<۝>D<٢١>",
"وَلَقَQ<دۡ> یَسَّR<رۡ>نَا ٱلۡقُR<رۡ>ءَانَ لِX<ل>ذِّكۡرِ فَهَلۡ مِX<ن> N<مُّ>دَّكِرࣲ A<۝>D<٢٢>",
"كَذَّبَتۡ ثَمُودُ بِX<ٱ>X<ل>N<نُّ>ذُR<ر>ِ A<۝>D<٢٣>",
"فَقَالُW<وۤ>X<ا۟> أَبَشَR<ر>X<ࣰ>ا N<مِّ>N<نَّ>ا وَT< ٰ>حِدX<ࣰ>ا N<نَّ>تَّبِعُهُW<ۥۤ> إِN<نَّ>W<اۤ> إِذX<ࣰ>ا لَّفِی ضَلَT<ـٰ>لX<ࣲ> N<وَ>سُعُR<ر>ٍ A<۝>D<٢٤>",
"أَءُلۡقِیَ ٱX<ل>ذِّكۡR<رُ> عَلَیۡهِ مِX<ن>N<ۢ> بَیۡنِنَا بَلۡ هُوَ كَذَّابٌ أَشِرࣱ A<۝>D<٢٥>",
"سَیَعۡلَمُونَ غَدX<ࣰ>ا N<مَّ>نِ ٱلۡكَذَّابُ ٱلۡأَشِرُ A<۝>D<٢٦>",
"إِN<نَّ>ا مُR<رۡ>سِلُوX<ا۟> ٱX<ل>N<نَّ>اقَةِ فِتۡنَةX<ࣰ> لَّهُمۡ فَX<ٱ>R<رۡ>تَقِQ<بۡ>هُمۡ وَX<ٱ>صۡطَبِرۡ A<۝>D<٢٧>",
"وَنَبِّئۡهُمۡ أَN<نَّ> ٱلۡمَW<اۤ>ءَ قِسۡمَةُN<ۢ> بَیۡنَهُمۡۖ كُلُّ شِرۡبX<ࣲ> N<مُّ>حۡتَضَR<ر>ࣱ A<۝>D<٢٨>",
"فَنَادَوۡX<ا۟> صَاحِبَهُمۡ فَتَعَاطَىٰ فَعَقَR<ر>َ A<۝>D<٢٩>",
"فَكَیۡفَ كَانَ عَذَابِی وَنُذُR<ر>ِ A<۝>D<٣٠>",
"إِN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَا عَلَیۡهِمۡ صَیۡحَةX<ࣰ> N<وَ>T< ٰ>حِدَةN<ࣰ> فَكَانُوX<ا۟> كَهَشِیمِ ٱلۡمُحۡتَظِرِ A<۝>D<٣١>",
"وَلَقَQ<دۡ> یَسَّR<رۡ>نَا ٱلۡقُR<رۡ>ءَانَ لِX<ل>ذِّكۡرِ فَهَلۡ مِX<ن> N<مُّ>دَّكِرࣲ A<۝>D<٣٢>",
"كَذَّبَتۡ قَوۡمُ لُوطِN<ۭ> بِX<ٱ>X<ل>N<نُّ>ذُR<ر>ِ A<۝>D<٣٣>",
"إِN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَا عَلَیۡهِمۡ حَاصِبًا إِلَّW<اۤ> ءَالَ لُوطX<ࣲ>ۖ N<نَّ>جَّیۡنَT<ـٰ>هُN<م> بِسَحَR<ر>ࣲ A<۝>D<٣٤>",
"نِّعۡمَةX<ࣰ> N<مِّ>نۡ عِN<ن>دِنَاۚ كَذَT< ٰ>لِكَ نَQ<جۡ>زِی مَN<ن> شَكَR<ر>َ A<۝>D<٣٥>",
"وَلَقَQ<دۡ> أَN<ن>ذَR<رَ>هُN<م> بَQ<طۡ>شَتَنَا فَتَمَاR<رَ>وۡX<ا۟> بِX<ٱ>X<ل>N<نُّ>ذُR<ر>ِ A<۝>D<٣٦>",
"وَلَقَQ<دۡ> R<رَ>T< ٰ>وَدُوهُ عَN<ن> ضَیۡفِهِT<ۦ> فَطَمَسۡنَW<اۤ> أَعۡیُنَهُمۡ فَذُوقُوX<ا۟> عَذَابِی وَنُذُR<ر>ِ A<۝>D<٣٧>",
"وَلَقَQ<دۡ> صَبَّحَهُN<م> بُكۡR<رَ>ةً عَذَابX<ࣱ> N<مُّ>سۡتَقِرࣱّ A<۝>D<٣٨>",
"فَذُوقُوX<ا۟> عَذَابِی وَنُذُR<ر>ِ A<۝>D<٣٩>",
"وَلَقَQ<دۡ> یَسَّR<رۡ>نَا ٱلۡقُR<رۡ>ءَانَ لِX<ل>ذِّكۡرِ فَهَلۡ مِX<ن> N<مُّ>دَّكِرࣲ A<۝>D<٤٠>",
"وَلَقَQ<دۡ> جَW<اۤ>ءَ ءَالَ فِرۡعَوۡنَ ٱX<ل>N<نُّ>ذُR<ر>ُ A<۝>D<٤١>",
"كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا كُلِّهَا فَأَخَذۡنَT<ـٰ>هُمۡ أَخۡذَ عَزِیزX<ࣲ> N<مُّ>Q<قۡ>تَدِرٍ A<۝>D<٤٢>",
"أَكُفَّاR<رُ>كُمۡ خَیۡR<ر>X<ࣱ> N<مِّ>نۡ أُX<و۟>لَW<ـٰۤ>ئِكُمۡ أَمۡ لَكُN<م> بَR<رَ>W<اۤ>ءَةN<ࣱ> فِی ٱX<ل>زُّبُR<ر>ِ A<۝>D<٤٣>",
"أَمۡ یَقُولُونَ نَحۡنُ جَمِیعX<ࣱ> N<مُّ>N<ن>تَصِرࣱ A<۝>D<٤٤>",
"سَیُهۡزَمُ ٱلۡجَمۡعُ وَیُوَلُّونَ ٱX<ل>دُّبُR<ر>َ A<۝>D<٤٥>",
"بَلِ ٱX<ل>سَّاعَةُ مَوۡعِدُهُمۡ وَX<ٱ>X<ل>سَّاعَةُ أَQ<دۡ>هَىٰ وَأَمَR<رّ>ُ A<۝>D<٤٦>",
"إِN<نَّ> ٱلۡمُQ<جۡ>رِمِینَ فِی ضَلَT<ـٰ>لX<ࣲ> N<وَ>سُعُR<ر>ࣲ A<۝>D<٤٧>",
"یَوۡمَ یُسۡحَبُونَ فِی ٱX<ل>N<نَّ>ارِ عَلَىٰ وُجُوهِهِمۡ ذُوقُوX<ا۟> مَسَّ سَقَR<ر>َ A<۝>D<٤٨>",
"إِN<نَّ>ا كُلَّ شَیۡءٍ خَلَQ<قۡ>نَT<ـٰ>هُ بِقَدَR<ر>ࣲ A<۝>D<٤٩>",
"وَمَW<اۤ> أَمۡR<رُ>نَW<اۤ> إِلَّا وَT< ٰ>حِدَةN<ࣱ> كَلَمۡحِN<ۭ> بِX<ٱ>لۡبَصَR<ر>ِ A<۝>D<٥٠>",
"وَلَقَQ<دۡ> أَهۡلَكۡنَW<اۤ> أَشۡیَاعَكُمۡ فَهَلۡ مِX<ن> N<مُّ>دَّكِرࣲ A<۝>D<٥١>",
"وَكُلُّ شَیۡءN<ࣲ> فَعَلُوهُ فِی ٱX<ل>زُّبُR<ر>ِ A<۝>D<٥٢>",
"وَكُلُّ صَغِیرX<ࣲ> N<وَ>كَبِیرX<ࣲ> N<مُّ>سۡتَطَR<ر>ٌ A<۝>D<٥٣>",
"إِN<نَّ> ٱلۡمُتَّقِینَ فِی جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>نَهَR<ر>ࣲ A<۝>D<٥٤>",
"فِی مَQ<قۡ>عَدِ صِQ<دۡ>قٍ عِN<ن>دَ مَلِیكX<ࣲ> N<مُّ>Q<قۡ>تَدِرِۭ A<۝>D<٥٥>",
"#۞ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ A<۝>D<١>",
"عَلَّمَ ٱلۡقُR<رۡ>ءَJ<ا>نَ A<۝>D<٢>",
"خَلَقَ ٱلۡإِN<ن>سَT<ـٰ>نَ A<۝>D<٣>",
"عَلَّمَهُ ٱلۡبَیَJ<ا>نَ A<۝>D<٤>",
"ٱX<ل>شَّمۡسُ وَX<ٱ>لۡقَمَR<رُ> بِحُسۡبَJ<ا>نࣲ A<۝>D<٥>",
"وَX<ٱ>X<ل>N<نَّ>Q<جۡ>مُ وَX<ٱ>X<ل>شَّجَR<رُ> یَسۡجُدَJ<ا>نِ A<۝>D<٦>",
"وَX<ٱ>X<ل>سَّمَW<اۤ>ءَ R<رَ>فَعَهَا وَوَضَعَ ٱلۡمِیزَJ<ا>نَ A<۝>D<٧>",
"أَلَّا تَQ<طۡ>غَوۡX<ا۟> فِی ٱلۡمِیزَJ<ا>نِ A<۝>D<٨>",
"وَأَقِیمُوX<ا۟> ٱلۡوَزۡنَ بِX<ٱ>لۡقِسۡطِ وَلَا تُخۡسِR<رُ>وX<ا۟> ٱلۡمِیزَJ<ا>نَ A<۝>D<٩>",
"وَX<ٱ>لۡأَR<رۡ>ضَ وَضَعَهَا لِلۡأَنَJ<ا>مِ A<۝>D<١٠>",
"فِیهَا فَT<ـٰ>كِهَةX<ࣱ> N<وَ>X<ٱ>X<ل>N<نَّ>خۡلُ ذَاتُ ٱلۡأَكۡمَJ<ا>مِ A<۝>D<١١>",
"وَX<ٱ>لۡحَبُّ ذُو ٱلۡعَصۡفِ وَX<ٱ>X<ل>R<رَّ>یۡحَJ<ا>نُ A<۝>D<١٢>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<١٣>",
"خَلَقَ ٱلۡإِN<ن>سَT<ـٰ>نَ مِN<ن> صَلۡصَT<ـٰ>لN<ࣲ> كَX<ٱ>لۡفَخَّJ<ا>R<ر>ِ A<۝>D<١٤>",
"وَخَلَقَ ٱلۡجَL<اۤ>N<نَّ> مِX<ن> N<مَّ>ارِجX<ࣲ> N<مِّ>ن N<نَّ>J<ا>R<ر>ࣲ A<۝>D<١٥>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<١٦>",
"R<رَ>بُّ ٱلۡمَشۡرِقَیۡنِ وَR<رَ>بُّ ٱلۡمَغۡرِبَJ<یۡ>نِ A<۝>D<١٧>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<١٨>",
"مَR<رَ>جَ ٱلۡبَحۡR<رَ>یۡنِ یَلۡتَقِیَJ<ا>نِ A<۝>D<١٩>",
"بَیۡنَهُمَا بَR<رۡ>زَخX<ࣱ> لَّا یَQ<بۡ>غِیَJ<ا>نِ A<۝>D<٢٠>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٢١>",
"یَخۡR<رُ>جُ مِنۡهُمَا ٱX<ل>لُّؤۡلُؤُ وَX<ٱ>لۡمَR<رۡ>جَJ<ا>نُ A<۝>D<٢٢>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٢٣>",
"وَلَهُ ٱلۡجَوَارِ ٱلۡمُN<ن>شَءَاتُ فِی ٱلۡبَحۡرِ كَX<ٱ>لۡأَعۡلَT<ـٰ>مِ A<۝>D<٢٤>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٢٥>",
"كُلُّ مَنۡ عَلَیۡهَا فَJ<ا>نࣲ A<۝>D<٢٦>",
"وَیَQ<بۡ>قَىٰ وَQ<جۡ>هُ R<رَ>بِّكَ ذُو ٱلۡجَلَT<ـٰ>لِ وَX<ٱ>لۡإِكۡR<رَ>J<ا>مِ A<۝>D<٢٧>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٢٨>",
"یَسۡءَلُهُT<ۥ> مَN<ن> فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ كُلَّ یَوۡمٍ هُوَ فِی شَأۡنࣲ A<۝>D<٢٩>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٣٠>",
"سَنَفۡR<رُ>غُ لَكُمۡ أَیُّهَ ٱX<ل>ثَّقَلَJ<ا>نِ A<۝>D<٣١>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٣٢>",
"یَT<ـٰ>مَعۡشَR<رَ> ٱلۡجِN<نِّ> وَX<ٱ>لۡإِN<ن>سِ إِنِ ٱسۡتَطَعۡتُمۡ أَN<ن> تَN<ن>فُذُوX<ا۟> مِنۡ أَQ<قۡ>طَارِ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ فَX<ٱ>N<ن>فُذُوX<ا۟>ۚ لَا تَN<ن>فُذُونَ إِلَّا بِسُلۡطَT<ـٰ>نࣲ A<۝>D<٣٣>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٣٤>",
"یُR<رۡ>سَلُ عَلَیۡكُمَا شُوَاظX<ࣱ> N<مِّ>ن N<نَّ>ارX<ࣲ> N<وَ>نُحَاسN<ࣱ> فَلَا تَN<ن>تَصِR<رَ>J<ا>نِ A<۝>D<٣٥>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٣٦>",
"فَإِذَا ٱN<ن>شَقَّتِ ٱX<ل>سَّمَW<اۤ>ءُ فَكَانَتۡ وَR<رۡ>دَةN<ࣰ> كَX<ٱ>X<ل>دِّهَJ<ا>نِ A<۝>D<٣٧>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٣٨>",
"فَیَوۡمَئِذX<ࣲ> لَّا یُسۡءَلُ عَN<ن> ذَX<ن>N<ۢ>بِهِW<ۦۤ> إِN<ن>سX<ࣱ> N<وَ>لَا جَL<اۤ>N<نّ>ࣱ A<۝>D<٣٩>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٤٠>",
"یُعۡR<رَ>فُ ٱلۡمُQ<جۡ>رِمُونَ بِسِیمَT<ـٰ>هُمۡ فَیُؤۡخَذُ بِX<ٱ>X<ل>N<نَّ>وَT< ٰ>صِی وَX<ٱ>لۡأَQ<قۡ>دَJ<ا>مِ A<۝>D<٤١>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٤٢>",
"هَT<ـٰ>ذِهِT<ۦ> جَهَN<نَّ>مُ ٱلَّتِی یُكَذِّبُ بِهَا ٱلۡمُQ<جۡ>رِمُJ<و>نَ A<۝>D<٤٣>",
"یَطُوفُونَ بَیۡنَهَا وَبَیۡنَ حَمِیمٍ ءَJ<ا>نࣲ A<۝>D<٤٤>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٤٥>",
"وَلِمَنۡ خَافَ مَقَامَ R<رَ>بِّهِT<ۦ> جَN<نَّ>تَJ<ا>نِ A<۝>D<٤٦>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٤٧>",
"ذَوَاتَW<اۤ> أَفۡنَJ<ا>نࣲ A<۝>D<٤٨>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٤٩>",
"فِیهِمَا عَیۡنَانِ تَQ<جۡ>رِیَJ<ا>نِ A<۝>D<٥٠>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٥١>",
"فِیهِمَا مِN<ن> كُلِّ فَT<ـٰ>كِهَةN<ࣲ> زَوۡجَJ<ا>نِ A<۝>D<٥٢>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٥٣>",
"مُتَّكِءِینَ عَلَىٰ فُR<رُ>شِN<ۭ> بَطَW<اۤ>ئِنُهَا مِنۡ إِسۡتَQ<بۡ>R<رَ>قX<ࣲ>ۚ N<وَ>جَنَى ٱلۡجَN<نَّ>تَیۡنِ دَJ<ا>نࣲ A<۝>D<٥٤>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٥٥>",
"فِیهِN<نَّ> قَT<ـٰ>صِR<رَ>T< ٰ>تُ ٱX<ل>طَّR<رۡ>فِ لَمۡ یَQ<طۡ>مِثۡهُN<نَّ> إِN<ن>سN<ࣱ> قَQ<بۡ>لَهُمۡ وَلَا جَL<اۤ>N<نّ>ࣱ A<۝>D<٥٦>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٥٧>",
"كَأَN<نَّ>هُN<نَّ> ٱلۡیَاقُوتُ وَX<ٱ>لۡمَR<رۡ>جَJ<ا>نُ A<۝>D<٥٨>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٥٩>",
"هَلۡ جَزَW<اۤ>ءُ ٱلۡإِحۡسَT<ـٰ>نِ إِلَّا ٱلۡإِحۡسَT<ـٰ>نُ A<۝>D<٦٠>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٦١>",
"وَمِN<ن> دُونِهِمَا جَN<نَّ>تَJ<ا>نِ A<۝>D<٦٢>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٦٣>",
"مُQ<دۡ>هَL<اۤ>N<مَّ>تَJ<ا>نِ A<۝>D<٦٤>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٦٥>",
"فِیهِمَا عَیۡنَانِ نَضَّاخَتَJ<ا>نِ A<۝>D<٦٦>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٦٧>",
"فِیهِمَا فَT<ـٰ>كِهَةX<ࣱ> N<وَ>نَخۡلX<ࣱ> N<وَ>R<رُ>N<مَّ>J<ا>نࣱ A<۝>D<٦٨>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٦٩>",
"فِیهِN<نَّ> خَیۡR<رَ>T< ٰ>تٌ حِسَJ<ا>نࣱ A<۝>D<٧٠>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٧١>",
"حُوR<ر>X<ࣱ> N<مَّ>Q<قۡ>صُوR<رَ>T< ٰ>تN<ࣱ> فِی ٱلۡخِیَJ<ا>مِ A<۝>D<٧٢>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٧٣>",
"لَمۡ یَQ<طۡ>مِثۡهُN<نَّ> إِN<ن>سN<ࣱ> قَQ<بۡ>لَهُمۡ وَلَا جَL<اۤ>N<نّ>ࣱ A<۝>D<٧٤>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٧٥>",
"مُتَّكِءِینَ عَلَىٰ R<رَ>فۡR<رَ>فٍ خُضۡرX<ࣲ> N<وَ>عَQ<بۡ>قَرِیٍّ حِسَJ<ا>نࣲ A<۝>D<٧٦>",
"فَبِأَیِّ ءَالَW<اۤ>ءِ R<رَ>بِّكُمَا تُكَذِّبَJ<ا>نِ A<۝>D<٧٧>",
"تَبَT<ـٰ>R<رَ>كَ ٱسۡمُ R<رَ>بِّكَ ذِی ٱلۡجَلَT<ـٰ>لِ وَX<ٱ>لۡإِكۡR<رَ>J<ا>مِ A<۝>D<٧٨>",
"#۞ إِذَا وَقَعَتِ ٱلۡوَاقِعَةُ A<۝>D<١>",
"لَیۡسَ لِوَQ<قۡ>عَتِهَا كَاذِبَةٌ A<۝>D<٢>",
"خَافِضَةX<ࣱ> R<رَّ>افِعَةٌ A<۝>D<٣>",
"إِذَا R<رُ>جَّتِ ٱلۡأَR<رۡ>ضُ R<رَ>جࣰّا A<۝>D<٤>",
"وَبُسَّتِ ٱلۡجِبَالُ بَسࣰّا A<۝>D<٥>",
"فَكَانَتۡ هَبَW<اۤ>ءX<ࣰ> N<مُّ>X<ن>N<ۢ>بَثࣰّا A<۝>D<٦>",
"وَكُN<ن>تُمۡ أَزۡوَT< ٰ>جN<ࣰ>ا ثَلَT<ـٰ>ثَةࣰ A<۝>D<٧>",
"فَأَصۡحَT<ـٰ>بُ ٱلۡمَیۡمَنَةِ مَW<اۤ> أَصۡحَT<ـٰ>بُ ٱلۡمَیۡمَنَةِ A<۝>D<٨>",
"وَأَصۡحَT<ـٰ>بُ ٱلۡمَشۡءَمَةِ مَW<اۤ> أَصۡحَT<ـٰ>بُ ٱلۡمَشۡءَمَةِ A<۝>D<٩>",
"وَX<ٱ>X<ل>سَّT<ـٰ>بِقُونَ ٱX<ل>سَّT<ـٰ>بِقُJ<و>نَ A<۝>D<١٠>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ ٱلۡمُقَR<رَّ>بُJ<و>نَ A<۝>D<١١>",
"فِی جَN<نَّ>T<ـٰ>تِ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<١٢>",
"ثُلَّةX<ࣱ> N<مِّ>نَ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٣>",
"وَقَلِیلX<ࣱ> N<مِّ>نَ ٱلۡءَاخِرِJ<ی>نَ A<۝>D<١٤>",
"عَلَىٰ سُR<رُ>رX<ࣲ> N<مَّ>وۡضُونَةࣲ A<۝>D<١٥>",
"مُّتَّكِءِینَ عَلَیۡهَا مُتَقَT<ـٰ>بِلِJ<ی>نَ A<۝>D<١٦>",
"یَطُوفُ عَلَیۡهِمۡ وِلۡدَT< ٰ>نX<ࣱ> N<مُّ>خَلَّدُJ<و>نَ A<۝>D<١٧>",
"بِأَكۡوَابX<ࣲ> N<وَ>أَبَارِیقَ وَكَأۡسX<ࣲ> N<مِّ>X<ن> N<مَّ>عِJ<ی>نࣲ A<۝>D<١٨>",
"لَّا یُصَدَّعُونَ عَنۡهَا وَلَا یُN<ن>زِفُJ<و>نَ A<۝>D<١٩>",
"وَفَT<ـٰ>كِهَةX<ࣲ> N<مِّ>N<مَّ>ا یَتَخَیَّR<رُ>J<و>نَ A<۝>D<٢٠>",
"وَلَحۡمِ طَیۡرX<ࣲ> N<مِّ>N<مَّ>ا یَشۡتَهُJ<و>نَ A<۝>D<٢١>",
"وَحُوR<ر>ٌ عِJ<ی>نࣱ A<۝>D<٢٢>",
"كَأَمۡثَT<ـٰ>لِ ٱX<ل>لُّؤۡلُؤِ ٱلۡمَكۡنُJ<و>نِ A<۝>D<٢٣>",
"جَزَW<اۤ>ءَN<ۢ> بِمَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٢٤>",
"لَا یَسۡمَعُونَ فِیهَا لَغۡوX<ࣰ>ا N<وَ>لَا تَأۡثِیمًا A<۝>D<٢٥>",
"إِلَّا قِیلN<ࣰ>ا سَلَT<ـٰ>مN<ࣰ>ا سَلَT<ـٰ>مࣰا A<۝>D<٢٦>",
"وَأَصۡحَT<ـٰ>بُ ٱلۡیَمِینِ مَW<اۤ> أَصۡحَT<ـٰ>بُ ٱلۡیَمِJ<ی>نِ A<۝>D<٢٧>",
"فِی سِQ<دۡ>رX<ࣲ> N<مَّ>خۡضُJ<و>Q<د>ࣲ A<۝>D<٢٨>",
"وَطَلۡحX<ࣲ> N<مَّ>N<ن>ضُJ<و>Q<د>ࣲ A<۝>D<٢٩>",
"وَظِلّX<ࣲ> N<مَّ>مۡدُJ<و>Q<د>ࣲ A<۝>D<٣٠>",
"وَمَW<اۤ>ءX<ࣲ> N<مَّ>سۡكُJ<و>Q<ب>ࣲ A<۝>D<٣١>",
"وَفَT<ـٰ>كِهَةN<ࣲ> كَثِیR<رَ>ةࣲ A<۝>D<٣٢>",
"لَّا مَQ<قۡ>طُوعَةX<ࣲ> N<وَ>لَا مَمۡنُوعَةࣲ A<۝>D<٣٣>",
"وَفُR<رُ>شX<ࣲ> N<مَّ>R<رۡ>فُوعَةٍ A<۝>D<٣٤>",
"إِN<نَّ>W<اۤ> أَN<ن>شَأۡنَT<ـٰ>هُN<نَّ> إِN<ن>شَW<اۤ>ءࣰ A<۝>D<٣٥>",
"فَجَعَلۡنَT<ـٰ>هُN<نَّ> أَQ<بۡ>كَاR<ر>ًا A<۝>D<٣٦>",
"عُR<رُ>بًا أَتۡR<رَ>ابࣰا A<۝>D<٣٧>",
"لِّأَصۡحَT<ـٰ>بِ ٱلۡیَمِJ<ی>نِ A<۝>D<٣٨>",
"ثُلَّةX<ࣱ> N<مِّ>نَ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<٣٩>",
"وَثُلَّةX<ࣱ> N<مِّ>نَ ٱلۡءَاخِرِJ<ی>نَ A<۝>D<٤٠>",
"وَأَصۡحَT<ـٰ>بُ ٱX<ل>شِّمَالِ مَW<اۤ> أَصۡحَT<ـٰ>بُ ٱX<ل>شِّمَJ<ا>لِ A<۝>D<٤١>",
"فِی سَمُومX<ࣲ> N<وَ>حَمِJ<ی>مࣲ A<۝>D<٤٢>",
"وَظِلّX<ࣲ> N<مِّ>X<ن> N<یَ>حۡمُJ<و>مࣲ A<۝>D<٤٣>",
"لَّا بَارِدX<ࣲ> N<وَ>لَا كَرِJ<ی>مٍ A<۝>D<٤٤>",
"إِN<نَّ>هُمۡ كَانُوX<ا۟> قَQ<بۡ>لَ ذَT< ٰ>لِكَ مُتۡR<رَ>فِJ<ی>نَ A<۝>D<٤٥>",
"وَكَانُوX<ا۟> یُصِR<رُّ>ونَ عَلَى ٱلۡحِN<ن>ثِ ٱلۡعَظِJ<ی>مِ A<۝>D<٤٦>",
"وَكَانُوX<ا۟> یَقُولُونَ أَئِذَا مِتۡنَا وَكُN<نَّ>ا تُR<رَ>ابX<ࣰ>ا N<وَ>عِظَT<ـٰ>مًا أَءِN<نَّ>ا لَمَQ<بۡ>عُوثُJ<و>نَ A<۝>D<٤٧>",
"أَوَ ءَابَW<اۤ>ؤُنَا ٱلۡأَوَّلُJ<و>نَ A<۝>D<٤٨>",
"قُلۡ إِN<نَّ> ٱلۡأَوَّلِینَ وَX<ٱ>لۡءَاخِرِJ<ی>نَ A<۝>D<٤٩>",
"لَمَQ<جۡ>مُوعُونَ إِلَىٰ مِیقَT<ـٰ>تِ یَوۡمX<ࣲ> N<مَّ>عۡلُJ<و>مࣲ A<۝>D<٥٠>",
"ثُN<مَّ> إِN<نَّ>كُمۡ أَیُّهَا ٱX<ل>ضَّL<اۤ>لُّونَ ٱلۡمُكَذِّبُJ<و>نَ A<۝>D<٥١>",
"لَءَاكِلُونَ مِN<ن> شَجَرX<ࣲ> N<مِّ>N<ن> زَقُّJ<و>مࣲ A<۝>D<٥٢>",
"فَمَالِءُونَ مِنۡهَا ٱلۡبُطُJ<و>نَ A<۝>D<٥٣>",
"فَشَT<ـٰ>رِبُونَ عَلَیۡهِ مِنَ ٱلۡحَمِJ<ی>مِ A<۝>D<٥٤>",
"فَشَT<ـٰ>رِبُونَ شُR<رۡ>بَ ٱلۡهِJ<ی>مِ A<۝>D<٥٥>",
"هَT<ـٰ>ذَا نُزُلُهُمۡ یَوۡمَ ٱX<ل>دِّJ<ی>نِ A<۝>D<٥٦>",
"نَحۡنُ خَلَQ<قۡ>نَT<ـٰ>كُمۡ فَلَوۡلَا تُصَدِّقُJ<و>نَ A<۝>D<٥٧>",
"أَفَR<رَ>ءَیۡتُم N<مَّ>ا تُمۡنُJ<و>نَ A<۝>D<٥٨>",
"ءَأَN<ن>تُمۡ تَخۡلُقُونَهُW<ۥۤ> أَمۡ نَحۡنُ ٱلۡخَT<ـٰ>لِقُJ<و>نَ A<۝>D<٥٩>",
"نَحۡنُ قَدَّR<رۡ>نَا بَیۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِJ<ی>نَ A<۝>D<٦٠>",
"عَلَىW<ٰۤ> أَن N<نُّ>بَدِّلَ أَمۡثَT<ـٰ>لَكُمۡ وَنُN<ن>شِئَكُمۡ فِی مَا لَا تَعۡلَمُJ<و>نَ A<۝>D<٦١>",
"وَلَقَQ<دۡ> عَلِمۡتُمُ ٱX<ل>N<نَّ>شۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَكَّR<رُ>J<و>نَ A<۝>D<٦٢>",
"أَفَR<رَ>ءَیۡتُم N<مَّ>ا تَحۡR<رُ>ثُJ<و>نَ A<۝>D<٦٣>",
"ءَأَN<ن>تُمۡ تَزۡR<رَ>عُونَهُW<ۥۤ> أَمۡ نَحۡنُ ٱX<ل>زَّT< ٰ>رِعُJ<و>نَ A<۝>D<٦٤>",
"لَوۡ نَشَW<اۤ>ءُ لَجَعَلۡنَT<ـٰ>هُ حُطَT<ـٰ>مN<ࣰ>ا فَظَلۡتُمۡ تَفَكَّهُJ<و>نَ A<۝>D<٦٥>",
"إِN<نَّ>ا لَمُغۡR<رَ>مُJ<و>نَ A<۝>D<٦٦>",
"بَلۡ نَحۡنُ مَحۡR<رُ>ومُJ<و>نَ A<۝>D<٦٧>",
"أَفَR<رَ>ءَیۡتُمُ ٱلۡمَW<اۤ>ءَ ٱلَّذِی تَشۡR<رَ>بُJ<و>نَ A<۝>D<٦٨>",
"ءَأَN<ن>تُمۡ أَN<ن>زَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُN<ن>زِلُJ<و>نَ A<۝>D<٦٩>",
"لَوۡ نَشَW<اۤ>ءُ جَعَلۡنَT<ـٰ>هُ أُجَاجN<ࣰ>ا فَلَوۡلَا تَشۡكُR<رُ>J<و>نَ A<۝>D<٧٠>",
"أَفَR<رَ>ءَیۡتُمُ ٱX<ل>N<نَّ>اR<رَ> ٱلَّتِی تُوR<رُ>J<و>نَ A<۝>D<٧١>",
"ءَأَN<ن>تُمۡ أَN<ن>شَأۡتُمۡ شَجَR<رَ>تَهَW<اۤ> أَمۡ نَحۡنُ ٱلۡمُN<ن>شِءُJ<و>نَ A<۝>D<٧٢>",
"نَحۡنُ جَعَلۡنَT<ـٰ>هَا تَذۡكِR<رَ>ةX<ࣰ> N<وَ>مَتَT<ـٰ>عX<ࣰ>ا لِّلۡمُQ<قۡ>وِJ<ی>نَ A<۝>D<٧٣>",
"فَسَبِّحۡ بِX<ٱ>سۡمِ R<رَ>بِّكَ ٱلۡعَظِJ<ی>مِ A<۝>D<٧٤>",
"۞ فَلَW<اۤ> أُQ<قۡ>سِمُ بِمَوَT< ٰ>قِعِ ٱX<ل>N<نُّ>جُJ<و>مِ A<۝>D<٧٥>",
"وَإِN<نَّ>هُT<ۥ> لَقَسَمX<ࣱ> لَّوۡ تَعۡلَمُونَ عَظِJ<ی>مٌ A<۝>D<٧٦>",
"إِN<نَّ>هُT<ۥ> لَقُR<رۡ>ءَانN<ࣱ> كَرِJ<ی>مࣱ A<۝>D<٧٧>",
"فِی كِتَT<ـٰ>بX<ࣲ> N<مَّ>كۡنُJ<و>نࣲ A<۝>D<٧٨>",
"لَّا یَمَسُّهُW<ۥۤ> إِلَّا ٱلۡمُطَهَّR<رُ>J<و>نَ A<۝>D<٧٩>",
"تَN<ن>زِیلX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٨٠>",
"أَفَبِهَT<ـٰ>ذَا ٱلۡحَدِیثِ أَN<ن>تُم N<مُّ>Q<دۡ>هِنُJ<و>نَ A<۝>D<٨١>",
"وَتَQ<جۡ>عَلُونَ رِزۡقَكُمۡ أَN<نَّ>كُمۡ تُكَذِّبُJ<و>نَ A<۝>D<٨٢>",
"فَلَوۡلَW<اۤ> إِذَا بَلَغَتِ ٱلۡحُلۡقُJ<و>مَ A<۝>D<٨٣>",
"وَأَN<ن>تُمۡ حِینَئِذN<ࣲ> تَN<ن>ظُR<رُ>J<و>نَ A<۝>D<٨٤>",
"وَنَحۡنُ أَQ<قۡ>R<رَ>بُ إِلَیۡهِ مِN<ن>كُمۡ وَلَT<ـٰ>كِX<ن> لَّا تُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٨٥>",
"فَلَوۡلَW<اۤ> إِN<ن> كُN<ن>تُمۡ غَیۡR<رَ> مَدِینِJ<ی>نَ A<۝>D<٨٦>",
"تَR<رۡ>جِعُونَهَW<اۤ> إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٨٧>",
"فَأَN<مَّ>W<اۤ> إِN<ن> كَانَ مِنَ ٱلۡمُقَR<رَّ>بِJ<ی>نَ A<۝>D<٨٨>",
"فَR<رَ>وۡحX<ࣱ> N<وَ>R<رَ>یۡحَانX<ࣱ> N<وَ>جَN<نَّ>تُ نَعِJ<ی>مࣲ A<۝>D<٨٩>",
"وَأَN<مَّ>W<اۤ> إِN<ن> كَانَ مِنۡ أَصۡحَT<ـٰ>بِ ٱلۡیَمِJ<ی>نِ A<۝>D<٩٠>",
"فَسَلَT<ـٰ>مX<ࣱ> لَّكَ مِنۡ أَصۡحَT<ـٰ>بِ ٱلۡیَمِJ<ی>نِ A<۝>D<٩١>",
"وَأَN<مَّ>W<اۤ> إِN<ن> كَانَ مِنَ ٱلۡمُكَذِّبِینَ ٱX<ل>ضَّL<اۤ>لِّJ<ی>نَ A<۝>D<٩٢>",
"فَنُزُلX<ࣱ> N<مِّ>نۡ حَمِJ<ی>مࣲ A<۝>D<٩٣>",
"وَتَصۡلِیَةُ جَحِJ<ی>مٍ A<۝>D<٩٤>",
"إِN<نَّ> هَT<ـٰ>ذَا لَهُوَ حَقُّ ٱلۡیَقِJ<ی>نِ A<۝>D<٩٥>",
"فَسَبِّحۡ بِX<ٱ>سۡمِ R<رَ>بِّكَ ٱلۡعَظِJ<ی>مِ A<۝>D<٩٦>",
"#سَبَّحَ لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<١>",
"لَهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ یُحۡیِT<ۦ> وَیُمِیتُۖ وَهُوَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<٢>",
"هُوَ ٱلۡأَوَّلُ وَX<ٱ>لۡءَاخِR<رُ> وَX<ٱ>X<ل>ظَّT<ـٰ>هِR<رُ> وَX<ٱ>لۡبَاطِنُۖ وَهُوَ بِكُلِّ شَیۡءٍ عَلِJ<ی>مٌ A<۝>D<٣>",
"هُوَ ٱلَّذِی خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ فِی سِتَّةِ أَیَّامN<ࣲ> ثُN<مَّ> ٱسۡتَوَىٰ عَلَى ٱلۡعَR<رۡ>شِۖ یَعۡلَمُ مَا یَلِجُ فِی ٱلۡأَR<رۡ>ضِ وَمَا یَخۡR<رُ>جُ مِنۡهَا وَمَا یَN<ن>زِلُ مِنَ ٱX<ل>سَّمَW<اۤ>ءِ وَمَا یَعۡR<رُ>جُ فِیهَاۖ وَهُوَ مَعَكُمۡ أَیۡنَ مَا كُN<ن>تُمۡۚ وَٱللَّهُ بِمَا تَعۡمَلُونَ بَصِJ<ی>رࣱ A<۝>D<٤>",
"لَّهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَإِلَى ٱللَّهِ تُR<رۡ>جَعُ ٱلۡأُمُJ<و>R<ر>ُ A<۝>D<٥>",
"یُولِجُ ٱلَّیۡلَ فِی ٱX<ل>N<نَّ>هَارِ وَیُولِجُ ٱX<ل>N<نَّ>هَاR<رَ> فِی ٱلَّیۡلِۚ وَهُوَ عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٦>",
"ءَامِنُوX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ> وَأَN<ن>فِقُوX<ا۟> مِN<مَّ>ا جَعَلَكُم N<مُّ>سۡتَخۡلَفِینَ فِیهِۖ فَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مِN<ن>كُمۡ وأَN<ن>فَقُوX<ا۟> لَهُمۡ أَQ<جۡ>R<ر>N<ࣱ> كَبِJ<ی>رࣱ A<۝>D<٧>",
"وَمَا لَكُمۡ لَا تُؤۡمِنُونَ بِٱللَّهِ وَX<ٱ>X<ل>R<رَّ>سُولُ یَQ<دۡ>عُوكُمۡ لِتُؤۡمِنُوX<ا۟> بِR<رَ>بِّكُمۡ وَقَQ<دۡ> أَخَذَ مِیثَT<ـٰ>قَكُمۡ إِN<ن> كُN<ن>تُم N<مُّ>ؤۡمِنِJ<ی>نَ A<۝>D<٨>",
"هُوَ ٱلَّذِی یُنَزِّلُ عَلَىٰ عَQ<بۡ>دِهِW<ۦۤ> ءَایَT<ـٰ>تِN<ۭ> بَیِّنَT<ـٰ>تX<ࣲ> لِّیُخۡرِجَكُم N<مِّ>نَ ٱX<ل>ظُّلُمَT<ـٰ>تِ إِلَى ٱX<ل>N<نُّ>ورِۚ وَإِN<نَّ> ٱللَّهَ بِكُمۡ لَR<رَ>ءُوفX<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٩>",
"وَمَا لَكُمۡ أَلَّا تُN<ن>فِقُوX<ا۟> فِی سَبِیلِ ٱللَّهِ وَلِلَّهِ مِیR<رَ>T< ٰ>ثُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ لَا یَسۡتَوِی مِN<ن>كُم N<مَّ>نۡ أَN<ن>فَقَ مِN<ن> قَQ<بۡ>لِ ٱلۡفَتۡحِ وَقَT<ـٰ>تَلَۚ أُX<و۟>لَW<ـٰۤ>ئِكَ أَعۡظَمُ دَR<رَ>جَةX<ࣰ> N<مِّ>نَ ٱلَّذِینَ أَN<ن>فَقُوX<ا۟> مِX<ن>N<ۢ> بَعۡدُ وَقَT<ـٰ>تَلُوX<ا۟>ۚ وَكُلّX<ࣰ>ا N<وَ>عَدَ ٱللَّهُ ٱلۡحُسۡنَىٰۚ وَٱللَّهُ بِمَا تَعۡمَلُونَ خَبِJ<ی>رࣱ A<۝>D<١٠>",
"مَّN<ن> ذَا ٱلَّذِی یُQ<قۡ>رِضُ ٱللَّهَ قَR<رۡ>ضًا حَسَنN<ࣰ>ا فَیُضَT<ـٰ>عِفَهُT<ۥ> لَهُT<ۥ> وَلَهُW<ۥۤ> أَQ<جۡ>R<ر>N<ࣱ> كَرِJ<ی>مࣱ A<۝>D<١١>",
"یَوۡمَ تَR<رَ>ى ٱلۡمُؤۡمِنِینَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تِ یَسۡعَىٰ نُوR<رُ>هُN<م> بَیۡنَ أَیۡدِیهِمۡ وَبِأَیۡمَT<ـٰ>نِهِN<م>ۖ بُشۡR<رَ>X<ى>T<ٰ>كُمُ ٱلۡیَوۡمَ جَN<نَّ>T<ـٰ>تN<ࣱ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَاۚ ذَT< ٰ>لِكَ هُوَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<١٢>",
"یَوۡمَ یَقُولُ ٱلۡمُنَT<ـٰ>فِقُونَ وَX<ٱ>لۡمُنَT<ـٰ>فِقَT<ـٰ>تُ لِلَّذِینَ ءَامَنُوX<ا۟> ٱN<ن>ظُR<رُ>ونَا نَQ<قۡ>تَبِسۡ مِن N<نُّ>ورِكُمۡ قِیلَ ٱR<رۡ>جِعُوX<ا۟> وَR<رَ>W<اۤ>ءَكُمۡ فَX<ٱ>لۡتَمِسُوX<ا۟> نُوR<ر>N<ࣰ>اۖ فَضُرِبَ بَیۡنَهُN<م> بِسُورX<ࣲ> لَّهُT<ۥ> بَابُN<ۢ> بَاطِنُهُT<ۥ> فِیهِ ٱX<ل>R<رَّ>حۡمَةُ وَظَT<ـٰ>هِR<رُ>هُT<ۥ> مِN<ن> قِبَلِهِ ٱلۡعَذَJ<ا>Q<ب>ُ A<۝>D<١٣>",
"یُنَادُونَهُمۡ أَلَمۡ نَكُX<ن> N<مَّ>عَكُمۡۖ قَالُوX<ا۟> بَلَىٰ وَلَT<ـٰ>كِN<نَّ>كُمۡ فَتَN<ن>تُمۡ أَN<ن>فُسَكُمۡ وَتَR<رَ>بَّصۡتُمۡ وَX<ٱ>R<رۡ>تَQ<بۡ>تُمۡ وَغَR<رَّ>تۡكُمُ ٱلۡأَمَانِیُّ حَتَّىٰ جَW<اۤ>ءَ أَمۡR<رُ> ٱللَّهِ وَغَR<رَّ>كُN<م> بِٱللَّهِ ٱلۡغَR<رُ>J<و>R<ر>ُ A<۝>D<١٤>",
"فَX<ٱ>لۡیَوۡمَ لَا یُؤۡخَذُ مِN<ن>كُمۡ فِQ<دۡ>یَةX<ࣱ> N<وَ>لَا مِنَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟>ۚ مَأۡوَX<ى>T<ٰ>كُمُ ٱX<ل>N<نَّ>اR<رُ>ۖ هِیَ مَوۡلَX<ى>T<ٰ>كُمۡۖ وَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<١٥>",
"۞ أَلَمۡ یَأۡنِ لِلَّذِینَ ءَامَنُW<وۤ>X<ا۟> أَN<ن> تَخۡشَعَ قُلُوبُهُمۡ لِذِكۡرِ ٱللَّهِ وَمَا نَزَلَ مِنَ ٱلۡحَقِّ وَلَا یَكُونُوX<ا۟> كَX<ٱ>لَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ مِN<ن> قَQ<بۡ>لُ فَطَالَ عَلَیۡهِمُ ٱلۡأَمَدُ فَقَسَتۡ قُلُوبُهُمۡۖ وَكَثِیR<ر>X<ࣱ> N<مِّ>نۡهُمۡ فَT<ـٰ>سِقُJ<و>نَ A<۝>D<١٦>",
"ٱعۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ یُحۡیِ ٱلۡأَR<رۡ>ضَ بَعۡدَ مَوۡتِهَاۚ قَQ<دۡ> بَیَّN<نَّ>ا لَكُمُ ٱلۡءَایَT<ـٰ>تِ لَعَلَّكُمۡ تَعۡقِلُJ<و>نَ A<۝>D<١٧>",
"إِN<نَّ> ٱلۡمُصَّدِّقِینَ وَX<ٱ>لۡمُصَّدِّقَT<ـٰ>تِ وَأَQ<قۡ>R<رَ>ضُوX<ا۟> ٱللَّهَ قَR<رۡ>ضًا حَسَنX<ࣰ>ا N<یُ>ضَT<ـٰ>عَفُ لَهُمۡ وَلَهُمۡ أَQ<جۡ>R<ر>N<ࣱ> كَرِJ<ی>مࣱ A<۝>D<١٨>",
"وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> بِٱللَّهِ وَR<رُ>سُلِهِW<ۦۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>صِّدِّیقُونَۖ وَX<ٱ>X<ل>شُّهَدَW<اۤ>ءُ عِN<ن>دَ R<رَ>بِّهِمۡ لَهُمۡ أَQ<جۡ>R<رُ>هُمۡ وَنُوR<رُ>هُمۡۖ وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> وَكَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَW<اۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡجَحِJ<ی>مِ A<۝>D<١٩>",
"ٱعۡلَمُW<وۤ>X<ا۟> أَN<نَّ>مَا ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَا لَعِبX<ࣱ> N<وَ>لَهۡوX<ࣱ> N<وَ>زِینَةX<ࣱ> N<وَ>تَفَاخُR<رُ>N<ۢ> بَیۡنَكُمۡ وَتَكَاثُR<ر>N<ࣱ> فِی ٱلۡأَمۡوَT< ٰ>لِ وَX<ٱ>لۡأَوۡلَT<ـٰ>دِۖ كَمَثَلِ غَیۡثٍ أَعۡجَبَ ٱلۡكُفَّاR<رَ> نَبَاتُهُT<ۥ> ثُN<مَّ> یَهِیجُ فَتَR<رَ>X<ى>T<ٰ>هُ مُصۡفَR<رّ>N<ࣰ>ا ثُN<مَّ> یَكُونُ حُطَT<ـٰ>مX<ࣰ>اۖ N<وَ>فِی ٱلۡءَاخِR<رَ>ةِ عَذَابN<ࣱ> شَدِیدX<ࣱ> N<وَ>مَغۡفِR<رَ>ةX<ࣱ> N<مِّ>نَ ٱللَّهِ وَرِضۡوَT< ٰ>نX<ࣱ>ۚ N<وَ>مَا ٱلۡحَیَX<و>T<ٰ>ةُ ٱX<ل>دُّنۡیَW<اۤ> إِلَّا مَتَT<ـٰ>عُ ٱلۡغُR<رُ>J<و>R<ر>ِ A<۝>D<٢٠>",
"سَابِقُW<وۤ>X<ا۟> إِلَىٰ مَغۡفِR<رَ>ةX<ࣲ> N<مِّ>X<ن> R<رَّ>بِّكُمۡ وَجَN<نَّ>ةٍ عَR<رۡ>ضُهَا كَعَR<رۡ>ضِ ٱX<ل>سَّمَW<اۤ>ءِ وَX<ٱ>لۡأَR<رۡ>ضِ أُعِدَّتۡ لِلَّذِینَ ءَامَنُوX<ا۟> بِٱللَّهِ وَR<رُ>سُلِهِT<ۦ>ۚ ذَT< ٰ>لِكَ فَضۡلُ ٱللَّهِ یُؤۡتِیهِ مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَٱللَّهُ ذُو ٱلۡفَضۡلِ ٱلۡعَظِJ<ی>مِ A<۝>D<٢١>",
"مَW<اۤ> أَصَابَ مِX<ن> N<مُّ>صِیبَةN<ࣲ> فِی ٱلۡأَR<رۡ>ضِ وَلَا فِW<یۤ> أَN<ن>فُسِكُمۡ إِلَّا فِی كِتَT<ـٰ>بX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِ أَن N<نَّ>Q<بۡ>R<رَ>أَهَW<اۤ>ۚ إِN<نَّ> ذَT< ٰ>لِكَ عَلَى ٱللَّهِ یَسِJ<ی>رࣱ A<۝>D<٢٢>",
"لِّكَیۡلَا تَأۡسَوۡX<ا۟> عَلَىٰ مَا فَاتَكُمۡ وَلَا تَفۡR<رَ>حُوX<ا۟> بِمَW<اۤ> ءَاتَX<ى>T<ٰ>كُمۡۗ وَٱللَّهُ لَا یُحِبُّ كُلَّ مُخۡتَالN<ࣲ> فَخُJ<و>R<ر>ٍ A<۝>D<٢٣>",
"ٱلَّذِینَ یَQ<بۡ>خَلُونَ وَیَأۡمُR<رُ>ونَ ٱX<ل>N<نَّ>اسَ بِX<ٱ>لۡبُخۡلِۗ وَمَX<ن> N<یَ>تَوَلَّ فَإِN<نَّ> ٱللَّهَ هُوَ ٱلۡغَنِیُّ ٱلۡحَمِJ<ی>Q<د>ُ A<۝>D<٢٤>",
"لَقَQ<دۡ> أَR<رۡ>سَلۡنَا R<رُ>سُلَنَا بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ وَأَN<ن>زَلۡنَا مَعَهُمُ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡمِیزَانَ لِیَقُومَ ٱX<ل>N<نَّ>اسُ بِX<ٱ>لۡقِسۡطِۖ وَأَN<ن>زَلۡنَا ٱلۡحَدِیدَ فِیهِ بَأۡسN<ࣱ> شَدِیدX<ࣱ> N<وَ>مَنَT<ـٰ>فِعُ لِX<ل>N<نَّ>اسِ وَلِیَعۡلَمَ ٱللَّهُ مَX<ن> N<یَ>N<ن>صُR<رُ>هُT<ۥ> وَR<رُ>سُلَهُT<ۥ> بِX<ٱ>لۡغَیۡبِۚ إِN<نَّ> ٱللَّهَ قَوِیٌّ عَزِJ<ی>زࣱ A<۝>D<٢٥>",
"وَلَقَQ<دۡ> أَR<رۡ>سَلۡنَا نُوحX<ࣰ>ا N<وَ>إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَجَعَلۡنَا فِی ذُرِّیَّتِهِمَا ٱX<ل>N<نُّ>بُوَّةَ وَX<ٱ>لۡكِتَT<ـٰ>بَۖ فَمِنۡهُم N<مُّ>هۡتَدX<ࣲ>ۖ N<وَ>كَثِیR<ر>X<ࣱ> N<مِّ>نۡهُمۡ فَT<ـٰ>سِقُJ<و>نَ A<۝>D<٢٦>",
"ثُN<مَّ> قَفَّیۡنَا عَلَىW<ٰۤ> ءَاثَT<ـٰ>رِهِN<م> بِR<رُ>سُلِنَا وَقَفَّیۡنَا بِعِیسَى ٱQ<بۡ>نِ مَR<رۡ>یَمَ وَءَاتَیۡنَT<ـٰ>هُ ٱلۡإِN<ن>جِیلَۖ وَجَعَلۡنَا فِی قُلُوبِ ٱلَّذِینَ ٱتَّبَعُوهُ R<رَ>أۡفَةX<ࣰ> N<وَ>R<رَ>حۡمَةX<ࣰ>ۚ N<وَ>R<رَ>هۡبَانِیَّةً ٱQ<بۡ>تَدَعُوهَا مَا كَتَQ<بۡ>نَT<ـٰ>هَا عَلَیۡهِمۡ إِلَّا ٱQ<بۡ>تِغَW<اۤ>ءَ رِضۡوَT< ٰ>نِ ٱللَّهِ فَمَا R<رَ>عَوۡهَا حَقَّ رِعَایَتِهَاۖ فَءَاتَیۡنَا ٱلَّذِینَ ءَامَنُوX<ا۟> مِنۡهُمۡ أَQ<جۡ>R<رَ>هُمۡۖ وَكَثِیR<ر>X<ࣱ> N<مِّ>نۡهُمۡ فَT<ـٰ>سِقُJ<و>نَ A<۝>D<٢٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱتَّقُوX<ا۟> ٱللَّهَ وَءَامِنُوX<ا۟> بِR<رَ>سُولِهِT<ۦ> یُؤۡتِكُمۡ كِفۡلَیۡنِ مِX<ن> R<رَّ>حۡمَتِهِT<ۦ> وَیَQ<جۡ>عَل لَّكُمۡ نُوR<ر>N<ࣰ>ا تَمۡشُونَ بِهِT<ۦ> وَیَغۡفِرۡ لَكُمۡۚ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٢٨>",
"لِّئَلَّا یَعۡلَمَ أَهۡلُ ٱلۡكِتَT<ـٰ>بِ أَلَّا یَQ<قۡ>دِR<رُ>ونَ عَلَىٰ شَیۡءX<ࣲ> N<مِّ>N<ن> فَضۡلِ ٱللَّهِ وَأَN<نَّ> ٱلۡفَضۡلَ بِیَدِ ٱللَّهِ یُؤۡتِیهِ مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَٱللَّهُ ذُو ٱلۡفَضۡلِ ٱلۡعَظِJ<ی>مِ A<۝>D<٢٩>",
"#۞ قَQ<دۡ> سَمِعَ ٱللَّهُ قَوۡلَ ٱلَّتِی تُجَT<ـٰ>دِلُكَ فِی زَوۡجِهَا وَتَشۡتَكِW<یۤ> إِلَى ٱللَّهِ وَٱللَّهُ یَسۡمَعُ تَحَاوُR<رَ>كُمَW<اۤ>ۚ إِN<نَّ> ٱللَّهَ سَمِیعُN<ۢ> بَصِJ<ی>رٌ A<۝>D<١>",
"ٱلَّذِینَ یُظَT<ـٰ>هِR<رُ>ونَ مِN<ن>كُم N<مِّ>ن N<نِّ>سَW<اۤ>ئِهِم N<مَّ>ا هُN<نَّ> أُN<مَّ>هَT<ـٰ>تِهِمۡۖ إِنۡ أُN<مَّ>هَT<ـٰ>تُهُمۡ إِلَّا ٱلَّW<ـٰۤ>ءِی وَلَQ<دۡ>نَهُمۡۚ وَإِN<نَّ>هُمۡ لَیَقُولُونَ مُN<ن>كَR<ر>X<ࣰ>ا N<مِّ>نَ ٱلۡقَوۡلِ وَزُوR<ر>X<ࣰ>اۚ N<وَ>إِN<نَّ> ٱللَّهَ لَعَفُوٌّ غَفُJ<و>R<ر>ࣱ A<۝>D<٢>",
"وَX<ٱ>لَّذِینَ یُظَT<ـٰ>هِR<رُ>ونَ مِن N<نِّ>سَW<اۤ>ئِهِمۡ ثُN<مَّ> یَعُودُونَ لِمَا قَالُوX<ا۟> فَتَحۡرِیR<رُ> R<رَ>قَبَةX<ࣲ> N<مِّ>N<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>تَمَL<اۤ>سَّاۚ ذَT< ٰ>لِكُمۡ تُوعَظُونَ بِهِT<ۦ>ۚ وَٱللَّهُ بِمَا تَعۡمَلُونَ خَبِJ<ی>رࣱ A<۝>D<٣>",
"فَمَX<ن> لَّمۡ یَجِQ<دۡ> فَصِیَامُ شَهۡR<رَ>یۡنِ مُتَتَابِعَیۡنِ مِN<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>تَمَL<اۤ>سَّاۖ فَمَX<ن> لَّمۡ یَسۡتَطِعۡ فَإِQ<طۡ>عَامُ سِتِّینَ مِسۡكِینN<ࣰ>اۚ ذَT< ٰ>لِكَ لِتُؤۡمِنُوX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ>ۚ وَتِلۡكَ حُدُودُ ٱللَّهِۗ وَلِلۡكَT<ـٰ>فِرِینَ عَذَابٌ أَلِJ<ی>مٌ A<۝>D<٤>",
"إِN<نَّ> ٱلَّذِینَ یُحَL<اۤ>دُّونَ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> كُبِتُوX<ا۟> كَمَا كُبِتَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡۚ وَقَQ<دۡ> أَN<ن>زَلۡنَW<اۤ> ءَایَT<ـٰ>تِN<ۭ> بَیِّنَT<ـٰ>تX<ࣲ>ۚ N<وَ>لِلۡكَT<ـٰ>فِرِینَ عَذَابX<ࣱ> N<مُّ>هِJ<ی>نࣱ A<۝>D<٥>",
"یَوۡمَ یَQ<بۡ>عَثُهُمُ ٱللَّهُ جَمِیعN<ࣰ>ا فَیُنَبِّئُهُN<م> بِمَا عَمِلُW<وۤ>X<ا۟>ۚ أَحۡصَX<ى>T<ٰ>هُ ٱللَّهُ وَنَسُوهُۚ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> شَهِJ<ی>Q<د>ٌ A<۝>D<٦>",
"أَلَمۡ تَR<رَ> أَN<نَّ> ٱللَّهَ یَعۡلَمُ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۖ مَا یَكُونُ مِن N<نَّ>Q<جۡ>وَىٰ ثَلَT<ـٰ>ثَةٍ إِلَّا هُوَ R<رَ>ابِعُهُمۡ وَلَا خَمۡسَةٍ إِلَّا هُوَ سَادِسُهُمۡ وَلَW<اۤ> أَQ<دۡ>نَىٰ مِN<ن> ذَT< ٰ>لِكَ وَلَW<اۤ> أَكۡثَR<رَ> إِلَّا هُوَ مَعَهُمۡ أَیۡنَ مَا كَانُوX<ا۟>ۖ ثُN<مَّ> یُنَبِّئُهُN<م> بِمَا عَمِلُوX<ا۟> یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِۚ إِN<نَّ> ٱللَّهَ بِكُلِّ شَیۡءٍ عَلِJ<ی>مٌ A<۝>D<٧>",
"أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ نُهُوX<ا۟> عَنِ ٱX<ل>N<نَّ>Q<جۡ>وَىٰ ثُN<مَّ> یَعُودُونَ لِمَا نُهُوX<ا۟> عَنۡهُ وَیَتَنَT<ـٰ>جَوۡنَ بِX<ٱ>لۡإِثۡمِ وَX<ٱ>لۡعُQ<دۡ>وَT< ٰ>نِ وَمَعۡصِیَتِ ٱX<ل>R<رَّ>سُولِۖ وَإِذَا جَW<اۤ>ءُوكَ حَیَّوۡكَ بِمَا لَمۡ یُحَیِّكَ بِهِ ٱللَّهُ وَیَقُولُونَ فِW<یۤ> أَN<ن>فُسِهِمۡ لَوۡلَا یُعَذِّبُنَا ٱللَّهُ بِمَا نَقُولُۚ حَسۡبُهُمۡ جَهَN<نَّ>مُ یَصۡلَوۡنَهَاۖ فَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا تَنَT<ـٰ>جَیۡتُمۡ فَلَا تَتَنَT<ـٰ>جَوۡX<ا۟> بِX<ٱ>لۡإِثۡمِ وَX<ٱ>لۡعُQ<دۡ>وَT< ٰ>نِ وَمَعۡصِیَتِ ٱX<ل>R<رَّ>سُولِ وَتَنَT<ـٰ>جَوۡX<ا۟> بِX<ٱ>لۡبِرِّ وَX<ٱ>X<ل>تَّQ<قۡ>وَىٰۖ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ ٱلَّذِW<یۤ> إِلَیۡهِ تُحۡشَR<رُ>J<و>نَ A<۝>D<٩>",
"إِN<نَّ>مَا ٱX<ل>N<نَّ>Q<جۡ>وَىٰ مِنَ ٱX<ل>شَّیۡطَT<ـٰ>نِ لِیَحۡزُنَ ٱلَّذِینَ ءَامَنُوX<ا۟> وَلَیۡسَ بِضَL<اۤ>رِّهِمۡ شَیۡءًا إِلَّا بِإِذۡنِ ٱللَّهِۚ وَعَلَى ٱللَّهِ فَلۡیَتَوَكَّلِ ٱلۡمُؤۡمِنُJ<و>نَ A<۝>D<١٠>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا قِیلَ لَكُمۡ تَفَسَّحُوX<ا۟> فِی ٱلۡمَجَT<ـٰ>لِسِ فَX<ٱ>فۡسَحُوX<ا۟> یَفۡسَحِ ٱللَّهُ لَكُمۡۖ وَإِذَا قِیلَ ٱN<ن>شُزُوX<ا۟> فَX<ٱ>N<ن>شُزُوX<ا۟> یَR<رۡ>فَعِ ٱللَّهُ ٱلَّذِینَ ءَامَنُوX<ا۟> مِN<ن>كُمۡ وَX<ٱ>لَّذِینَ أُوتُوX<ا۟> ٱلۡعِلۡمَ دَR<رَ>جَT<ـٰ>تX<ࣲ>ۚ N<وَ>ٱللَّهُ بِمَا تَعۡمَلُونَ خَبِJ<ی>رࣱ A<۝>D<١١>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا نَT<ـٰ>جَیۡتُمُ ٱX<ل>R<رَّ>سُولَ فَقَدِّمُوX<ا۟> بَیۡنَ یَدَیۡ نَQ<جۡ>وَX<ى>T<ٰ>كُمۡ صَدَقَةN<ࣰ>ۚ ذَT< ٰ>لِكَ خَیۡR<ر>X<ࣱ> لَّكُمۡ وَأَQ<طۡ>هَR<رُ>ۚ فَإِX<ن> لَّمۡ تَجِدُوX<ا۟> فَإِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<١٢>",
"ءَأَشۡفَQ<قۡ>تُمۡ أَN<ن> تُقَدِّمُوX<ا۟> بَیۡنَ یَدَیۡ نَQ<جۡ>وَX<ى>T<ٰ>كُمۡ صَدَقَT<ـٰ>تN<ࣲ>ۚ فَإِذۡ لَمۡ تَفۡعَلُوX<ا۟> وَتَابَ ٱللَّهُ عَلَیۡكُمۡ فَأَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتُوX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَأَطِیعُوX<ا۟> ٱللَّهَ وَR<رَ>سُولَهُT<ۥ>ۚ وَٱللَّهُ خَبِیR<رُ>N<ۢ> بِمَا تَعۡمَلُJ<و>نَ A<۝>D<١٣>",
"۞ أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ تَوَلَّوۡX<ا۟> قَوۡمًا غَضِبَ ٱللَّهُ عَلَیۡهِم N<مَّ>ا هُم N<مِّ>N<ن>كُمۡ وَلَا مِنۡهُمۡ وَیَحۡلِفُونَ عَلَى ٱلۡكَذِبِ وَهُمۡ یَعۡلَمُJ<و>نَ A<۝>D<١٤>",
"أَعَدَّ ٱللَّهُ لَهُمۡ عَذَابN<ࣰ>ا شَدِیدًاۖ إِN<نَّ>هُمۡ سَW<اۤ>ءَ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<١٥>",
"ٱتَّخَذُW<وۤ>X<ا۟> أَیۡمَT<ـٰ>نَهُمۡ جُN<نَّ>ةN<ࣰ> فَصَدُّوX<ا۟> عَN<ن> سَبِیلِ ٱللَّهِ فَلَهُمۡ عَذَابX<ࣱ> N<مُّ>هِJ<ی>نࣱ A<۝>D<١٦>",
"لَّN<ن> تُغۡنِیَ عَنۡهُمۡ أَمۡوَT< ٰ>لُهُمۡ وَلَW<اۤ> أَوۡلَT<ـٰ>دُهُم N<مِّ>نَ ٱللَّهِ شَیۡءًاۚ أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِۖ هُمۡ فِیهَا خَT<ـٰ>لِدُJ<و>نَ A<۝>D<١٧>",
"یَوۡمَ یَQ<بۡ>عَثُهُمُ ٱللَّهُ جَمِیعN<ࣰ>ا فَیَحۡلِفُونَ لَهُT<ۥ> كَمَا یَحۡلِفُونَ لَكُمۡ وَیَحۡسَبُونَ أَN<نَّ>هُمۡ عَلَىٰ شَیۡءٍۚ أَلَW<اۤ> إِN<نَّ>هُمۡ هُمُ ٱلۡكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<١٨>",
"ٱسۡتَحۡوَذَ عَلَیۡهِمُ ٱX<ل>شَّیۡطَT<ـٰ>نُ فَأَN<ن>سَX<ى>T<ٰ>هُمۡ ذِكۡR<رَ> ٱللَّهِۚ أُX<و۟>لَW<ـٰۤ>ئِكَ حِزۡبُ ٱX<ل>شَّیۡطَT<ـٰ>نِۚ أَلَW<اۤ> إِN<نَّ> حِزۡبَ ٱX<ل>شَّیۡطَT<ـٰ>نِ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<١٩>",
"إِN<نَّ> ٱلَّذِینَ یُحَL<اۤ>دُّونَ ٱللَّهَ وَR<رَ>سُولَهُW<ۥۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ فِی ٱلۡأَذَلِّJ<ی>نَ A<۝>D<٢٠>",
"كَتَبَ ٱللَّهُ لَأَغۡلِبَN<نَّ> أَنَا۠ وَR<رُ>سُلِW<یۤ>ۚ إِN<نَّ> ٱللَّهَ قَوِیٌّ عَزِJ<ی>زࣱ A<۝>D<٢١>",
"لَّا تَجِدُ قَوۡمX<ࣰ>ا N<یُ>ؤۡمِنُونَ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِ یُوَL<اۤ>دُّونَ مَنۡ حَL<اۤ>دَّ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> وَلَوۡ كَانُW<وۤ>X<ا۟> ءَابَW<اۤ>ءَهُمۡ أَوۡ أَQ<بۡ>نَW<اۤ>ءَهُمۡ أَوۡ إِخۡوَT< ٰ>نَهُمۡ أَوۡ عَشِیR<رَ>تَهُمۡۚ أُX<و۟>لَW<ـٰۤ>ئِكَ كَتَبَ فِی قُلُوبِهِمُ ٱلۡإِیمَT<ـٰ>نَ وَأَیَّدَهُN<م> بِR<رُ>وحX<ࣲ> N<مِّ>نۡهُۖ وَیُQ<دۡ>خِلُهُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَاۚ R<رَ>ضِیَ ٱللَّهُ عَنۡهُمۡ وَR<رَ>ضُوX<ا۟> عَنۡهُۚ أُX<و۟>لَW<ـٰۤ>ئِكَ حِزۡبُ ٱللَّهِۚ أَلَW<اۤ> إِN<نَّ> حِزۡبَ ٱللَّهِ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<٢٢>",
"#سَبَّحَ لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۖ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<١>",
"هُوَ ٱلَّذِW<یۤ> أَخۡR<رَ>جَ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِنۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ مِN<ن> دِیَT<ـٰ>رِهِمۡ لِأَوَّلِ ٱلۡحَشۡرِۚ مَا ظَنَN<ن>تُمۡ أَX<ن> N<یَ>خۡR<رُ>جُوX<ا۟>ۖ وَظَN<نُّ>W<وۤ>X<ا۟> أَN<نَّ>هُم N<مَّ>انِعَتُهُمۡ حُصُونُهُم N<مِّ>نَ ٱللَّهِ فَأَتَX<ى>T<ٰ>هُمُ ٱللَّهُ مِنۡ حَیۡثُ لَمۡ یَحۡتَسِبُوX<ا۟>ۖ وَقَذَفَ فِی قُلُوبِهِمُ ٱX<ل>R<رُّ>عۡبَۚ یُخۡرِبُونَ بُیُوتَهُN<م> بِأَیۡدِیهِمۡ وَأَیۡدِی ٱلۡمُؤۡمِنِینَ فَX<ٱ>عۡتَبِR<رُ>وX<ا۟> یَW<ـٰۤ>أُX<و۟>لِی ٱلۡأَQ<بۡ>صَT<ـٰ>R<ر>ِ A<۝>D<٢>",
"وَلَوۡلَW<اۤ> أَN<ن> كَتَبَ ٱللَّهُ عَلَیۡهِمُ ٱلۡجَلَW<اۤ>ءَ لَعَذَّبَهُمۡ فِی ٱX<ل>دُّنۡیَاۖ وَلَهُمۡ فِی ٱلۡءَاخِR<رَ>ةِ عَذَابُ ٱX<ل>N<نَّ>J<ا>R<ر>ِ A<۝>D<٣>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ شَL<اۤ>قُّوX<ا۟> ٱللَّهَ وَR<رَ>سُولَهُT<ۥ>ۖ وَمَX<ن> N<یُ>شَL<اۤ>قِّ ٱللَّهَ فَإِN<نَّ> ٱللَّهَ شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<٤>",
"مَا قَطَعۡتُم N<مِّ>X<ن> لِّینَةٍ أَوۡ تَR<رَ>كۡتُمُوهَا قَW<اۤ>ئِمَةً عَلَىW<ٰۤ> أُصُولِهَا فَبِإِذۡنِ ٱللَّهِ وَلِیُخۡزِیَ ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٥>",
"وَمَW<اۤ> أَفَW<اۤ>ءَ ٱللَّهُ عَلَىٰ R<رَ>سُولِهِT<ۦ> مِنۡهُمۡ فَمَW<اۤ> أَوۡجَفۡتُمۡ عَلَیۡهِ مِنۡ خَیۡلX<ࣲ> N<وَ>لَا رِكَابX<ࣲ> N<وَ>لَT<ـٰ>كِN<نَّ> ٱللَّهَ یُسَلِّطُ R<رُ>سُلَهُT<ۥ> عَلَىٰ مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٦>",
"مَّW<اۤ> أَفَW<اۤ>ءَ ٱللَّهُ عَلَىٰ R<رَ>سُولِهِT<ۦ> مِنۡ أَهۡلِ ٱلۡقُR<رَ>ىٰ فَلِلَّهِ وَلِX<ل>R<رَّ>سُولِ وَلِذِی ٱلۡقُR<رۡ>بَىٰ وَX<ٱ>لۡیَتَT<ـٰ>مَىٰ وَX<ٱ>لۡمَسَT<ـٰ>كِینِ وَX<ٱ>Q<بۡ>نِ ٱX<ل>سَّبِیلِ كَیۡ لَا یَكُونَ دُولَةَN<ۢ> بَیۡنَ ٱلۡأَغۡنِیَW<اۤ>ءِ مِN<ن>كُمۡۚ وَمَW<اۤ> ءَاتَX<ى>T<ٰ>كُمُ ٱX<ل>R<رَّ>سُولُ فَخُذُوهُ وَمَا نَهَX<ى>T<ٰ>كُمۡ عَنۡهُ فَX<ٱ>N<ن>تَهُوX<ا۟>ۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَۖ إِN<نَّ> ٱللَّهَ شَدِیدُ ٱلۡعِقَJ<ا>Q<ب>ِ A<۝>D<٧>",
"لِلۡفُقَR<رَ>W<اۤ>ءِ ٱلۡمُهَT<ـٰ>جِرِینَ ٱلَّذِینَ أُخۡرِجُوX<ا۟> مِN<ن> دِیَT<ـٰ>رِهِمۡ وَأَمۡوَT< ٰ>لِهِمۡ یَQ<بۡ>تَغُونَ فَضۡلX<ࣰ>ا N<مِّ>نَ ٱللَّهِ وَرِضۡوَT< ٰ>نX<ࣰ>ا N<وَ>یَN<ن>صُR<رُ>ونَ ٱللَّهَ وَR<رَ>سُولَهُW<ۥۤ>ۚ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>صَّT<ـٰ>دِقُJ<و>نَ A<۝>D<٨>",
"وَX<ٱ>لَّذِینَ تَبَوَّءُو ٱX<ل>دَّاR<رَ> وَX<ٱ>لۡإِیمَT<ـٰ>نَ مِN<ن> قَQ<بۡ>لِهِمۡ یُحِبُّونَ مَنۡ هَاجَR<رَ> إِلَیۡهِمۡ وَلَا یَجِدُونَ فِی صُدُورِهِمۡ حَاجَةX<ࣰ> N<مِّ>N<مَّ>W<اۤ> أُوتُوX<ا۟> وَیُؤۡثِR<رُ>ونَ عَلَىW<ٰۤ> أَN<ن>فُسِهِمۡ وَلَوۡ كَانَ بِهِمۡ خَصَاصَةX<ࣱ>ۚ N<وَ>مَX<ن> N<یُ>وقَ شُحَّ نَفۡسِهِT<ۦ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<٩>",
"وَX<ٱ>لَّذِینَ جَW<اۤ>ءُو مِX<ن>N<ۢ> بَعۡدِهِمۡ یَقُولُونَ R<رَ>بَّنَا ٱغۡفِرۡ لَنَا وَلِإِخۡوَT< ٰ>نِنَا ٱلَّذِینَ سَبَقُونَا بِX<ٱ>لۡإِیمَT<ـٰ>نِ وَلَا تَQ<جۡ>عَلۡ فِی قُلُوبِنَا غِلّX<ࣰ>ا لِّلَّذِینَ ءَامَنُوX<ا۟> R<رَ>بَّنَW<اۤ> إِN<نَّ>كَ R<رَ>ءُوفX<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<١٠>",
"۞ أَلَمۡ تَR<رَ> إِلَى ٱلَّذِینَ نَافَقُوX<ا۟> یَقُولُونَ لِإِخۡوَT< ٰ>نِهِمُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِنۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ لَئِنۡ أُخۡرِQ<جۡ>تُمۡ لَنَخۡR<رُ>جَN<نَّ> مَعَكُمۡ وَلَا نُطِیعُ فِیكُمۡ أَحَدًا أَبَدX<ࣰ>ا N<وَ>إِN<ن> قُوتِلۡتُمۡ لَنَN<ن>صُR<رَ>N<نَّ>كُمۡ وَٱللَّهُ یَشۡهَدُ إِN<نَّ>هُمۡ لَكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<١١>",
"لَئِنۡ أُخۡرِجُوX<ا۟> لَا یَخۡR<رُ>جُونَ مَعَهُمۡ وَلَئِN<ن> قُوتِلُوX<ا۟> لَا یَN<ن>صُR<رُ>ونَهُمۡ وَلَئِن N<نَّ>صَR<رُ>وهُمۡ لَیُوَلُّN<نَّ> ٱلۡأَQ<دۡ>بَT<ـٰ>R<رَ> ثُN<مَّ> لَا یُN<ن>صَR<رُ>J<و>نَ A<۝>D<١٢>",
"لَأَN<ن>تُمۡ أَشَدُّ R<رَ>هۡبَةN<ࣰ> فِی صُدُورِهِم N<مِّ>نَ ٱللَّهِۚ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ قَوۡمX<ࣱ> لَّا یَفۡقَهُJ<و>نَ A<۝>D<١٣>",
"لَا یُقَT<ـٰ>تِلُونَكُمۡ جَمِیعًا إِلَّا فِی قُR<ر>X<ࣰ>ى N<مُّ>حَصَّنَةٍ أَوۡ مِX<ن> N<وَ>R<رَ>W<اۤ>ءِ جُدُرِN<ۭ>ۚ بَأۡسُهُN<م> بَیۡنَهُمۡ شَدِیدN<ࣱ>ۚ تَحۡسَبُهُمۡ جَمِیعX<ࣰ>ا N<وَ>قُلُوبُهُمۡ شَتَّىٰۚ ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ قَوۡمX<ࣱ> لَّا یَعۡقِلُJ<و>نَ A<۝>D<١٤>",
"كَمَثَلِ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ قَرِیبN<ࣰ>اۖ ذَاقُوX<ا۟> وَبَالَ أَمۡرِهِمۡ وَلَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<١٥>",
"كَمَثَلِ ٱX<ل>شَّیۡطَT<ـٰ>نِ إِذۡ قَالَ لِلۡإِN<ن>سَT<ـٰ>نِ ٱكۡفُR<رۡ> فَلَN<مَّ>ا كَفَR<رَ> قَالَ إِN<نِّ>ی بَرِW<یۤ>ءX<ࣱ> N<مِّ>N<ن>كَ إِN<نِّ>W<یۤ> أَخَافُ ٱللَّهَ R<رَ>بَّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<١٦>",
"فَكَانَ عَT<ـٰ>قِبَتَهُمَW<اۤ> أَN<نَّ>هُمَا فِی ٱX<ل>N<نَّ>ارِ خَT<ـٰ>لِدَیۡنِ فِیهَاۚ وَذَT< ٰ>لِكَ جَزَW< ٰۤ>ؤُX<ا۟> ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> ٱتَّقُوX<ا۟> ٱللَّهَ وَلۡتَN<ن>ظُR<رۡ> نَفۡسX<ࣱ> N<مَّ>ا قَدَّمَتۡ لِغَدX<ࣲ>ۖ N<وَ>X<ٱ>تَّقُوX<ا۟> ٱللَّهَۚ إِN<نَّ> ٱللَّهَ خَبِیR<رُ>N<ۢ> بِمَا تَعۡمَلُJ<و>نَ A<۝>D<١٨>",
"وَلَا تَكُونُوX<ا۟> كَX<ٱ>لَّذِینَ نَسُوX<ا۟> ٱللَّهَ فَأَN<ن>سَX<ى>T<ٰ>هُمۡ أَN<ن>فُسَهُمۡۚ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡفَT<ـٰ>سِقُJ<و>نَ A<۝>D<١٩>",
"لَا یَسۡتَوِW<یۤ> أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِ وَأَصۡحَT<ـٰ>بُ ٱلۡجَN<نَّ>ةِۚ أَصۡحَT<ـٰ>بُ ٱلۡجَN<نَّ>ةِ هُمُ ٱلۡفَW<اۤ>ئِزُJ<و>نَ A<۝>D<٢٠>",
"لَوۡ أَN<ن>زَلۡنَا هَT<ـٰ>ذَا ٱلۡقُR<رۡ>ءَانَ عَلَىٰ جَبَلX<ࣲ> لَّR<رَ>أَیۡتَهُT<ۥ> خَT<ـٰ>شِعX<ࣰ>ا N<مُّ>تَصَدِّعX<ࣰ>ا N<مِّ>نۡ خَشۡیَةِ ٱللَّهِۚ وَتِلۡكَ ٱلۡأَمۡثَT<ـٰ>لُ نَضۡرِبُهَا لِX<ل>N<نَّ>اسِ لَعَلَّهُمۡ یَتَفَكَّR<رُ>J<و>نَ A<۝>D<٢١>",
"هُوَ ٱللَّهُ ٱلَّذِی لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۖ عَT<ـٰ>لِمُ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِۖ هُوَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ ٱX<ل>R<رَّ>حِJ<ی>مُ A<۝>D<٢٢>",
"هُوَ ٱللَّهُ ٱلَّذِی لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ ٱلۡمَلِكُ ٱلۡقُدُّوسُ ٱX<ل>سَّلَT<ـٰ>مُ ٱلۡمُؤۡمِنُ ٱلۡمُهَیۡمِنُ ٱلۡعَزِیزُ ٱلۡجَبَّاR<رُ> ٱلۡمُتَكَبِّR<رُ>ۚ سُQ<بۡ>حَT<ـٰ>نَ ٱللَّهِ عَN<مَّ>ا یُشۡرِكُJ<و>نَ A<۝>D<٢٣>",
"هُوَ ٱللَّهُ ٱلۡخَT<ـٰ>لِقُ ٱلۡبَارِئُ ٱلۡمُصَوِّR<رُ>ۖ لَهُ ٱلۡأَسۡمَW<اۤ>ءُ ٱلۡحُسۡنَىٰۚ یُسَبِّحُ لَهُT<ۥ> مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۖ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٢٤>",
"#یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَتَّخِذُوX<ا۟> عَدُوِّی وَعَدُوَّكُمۡ أَوۡلِیَW<اۤ>ءَ تُلۡقُونَ إِلَیۡهِN<م> بِX<ٱ>لۡمَوَدَّةِ وَقَQ<دۡ> كَفَR<رُ>وX<ا۟> بِمَا جَW<اۤ>ءَكُم N<مِّ>نَ ٱلۡحَقِّ یُخۡرِجُونَ ٱX<ل>R<رَّ>سُولَ وَإِیَّاكُمۡ أَN<ن> تُؤۡمِنُوX<ا۟> بِٱللَّهِ R<رَ>بِّكُمۡ إِN<ن> كُN<ن>تُمۡ خَR<رَ>Q<جۡ>تُمۡ جِهَT<ـٰ>دN<ࣰ>ا فِی سَبِیلِی وَX<ٱ>Q<بۡ>تِغَW<اۤ>ءَ مَR<رۡ>ضَاتِیۚ تُسِR<رُّ>ونَ إِلَیۡهِN<م> بِX<ٱ>لۡمَوَدَّةِ وَأَنَا۠ أَعۡلَمُ بِمَW<اۤ> أَخۡفَیۡتُمۡ وَمَW<اۤ> أَعۡلَN<ن>تُمۡۚ وَمَX<ن> N<یَ>فۡعَلۡهُ مِN<ن>كُمۡ فَقَQ<دۡ> ضَلَّ سَوَW<اۤ>ءَ ٱX<ل>سَّبِJ<ی>لِ A<۝>D<١>",
"إِX<ن> N<یَ>ثۡقَفُوكُمۡ یَكُونُوX<ا۟> لَكُمۡ أَعۡدَW<اۤ>ءX<ࣰ> N<وَ>یَQ<بۡ>سُطُW<وۤ>X<ا۟> إِلَیۡكُمۡ أَیۡدِیَهُمۡ وَأَلۡسِنَتَهُN<م> بِX<ٱ>X<ل>سُّW<وۤ>ءِ وَوَدُّوX<ا۟> لَوۡ تَكۡفُR<رُ>J<و>نَ A<۝>D<٢>",
"لَN<ن> تَN<ن>فَعَكُمۡ أَR<رۡ>حَامُكُمۡ وَلَW<اۤ> أَوۡلَT<ـٰ>دُكُمۡۚ یَوۡمَ ٱلۡقِیَT<ـٰ>مَةِ یَفۡصِلُ بَیۡنَكُمۡۚ وَٱللَّهُ بِمَا تَعۡمَلُونَ بَصِJ<ی>رࣱ A<۝>D<٣>",
"قَQ<دۡ> كَانَتۡ لَكُمۡ أُسۡوَةٌ حَسَنَةN<ࣱ> فِW<یۤ> إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَX<ٱ>لَّذِینَ مَعَهُW<ۥۤ> إِذۡ قَالُوX<ا۟> لِقَوۡمِهِمۡ إِN<نَّ>ا بُR<رَ>ءَW< ٰۤ>ؤُX<ا۟> مِN<ن>كُمۡ وَمِN<مَّ>ا تَعۡبُدُونَ مِN<ن> دُونِ ٱللَّهِ كَفَR<رۡ>نَا بِكُمۡ وَبَدَا بَیۡنَنَا وَبَیۡنَكُمُ ٱلۡعَدَT< ٰ>وَةُ وَX<ٱ>لۡبَغۡضَW<اۤ>ءُ أَبَدًا حَتَّىٰ تُؤۡمِنُوX<ا۟> بِٱللَّهِ وَحۡدَهُW<ۥۤ> إِلَّا قَوۡلَ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ لِأَبِیهِ لَأَسۡتَغۡفِR<رَ>N<نَّ> لَكَ وَمَW<اۤ> أَمۡلِكُ لَكَ مِنَ ٱللَّهِ مِN<ن> شَیۡءX<ࣲ>ۖ R<رَّ>بَّنَا عَلَیۡكَ تَوَكَّلۡنَا وَإِلَیۡكَ أَنَQ<بۡ>نَا وَإِلَیۡكَ ٱلۡمَصِJ<ی>رُ A<۝>D<٤>",
"R<رَ>بَّنَا لَا تَQ<جۡ>عَلۡنَا فِتۡنَةX<ࣰ> لِّلَّذِینَ كَفَR<رُ>وX<ا۟> وَX<ٱ>غۡفِرۡ لَنَا R<رَ>بَّنَW<اۤ>ۖ إِN<نَّ>كَ أَN<ن>تَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٥>",
"لَقَQ<دۡ> كَانَ لَكُمۡ فِیهِمۡ أُسۡوَةٌ حَسَنَةX<ࣱ> لِّمَN<ن> كَانَ یَR<رۡ>جُوX<ا۟> ٱللَّهَ وَX<ٱ>لۡیَوۡمَ ٱلۡءَاخِR<رَ>ۚ وَمَX<ن> N<یَ>تَوَلَّ فَإِN<نَّ> ٱللَّهَ هُوَ ٱلۡغَنِیُّ ٱلۡحَمِJ<ی>Q<د>ُ A<۝>D<٦>",
"۞ عَسَى ٱللَّهُ أَX<ن> N<یَ>Q<جۡ>عَلَ بَیۡنَكُمۡ وَبَیۡنَ ٱلَّذِینَ عَادَیۡتُم N<مِّ>نۡهُم N<مَّ>وَدَّةX<ࣰ>ۚ N<وَ>ٱللَّهُ قَدِیR<ر>X<ࣱ>ۚ N<وَ>ٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<٧>",
"لَّا یَنۡهَX<ى>T<ٰ>كُمُ ٱللَّهُ عَنِ ٱلَّذِینَ لَمۡ یُقَT<ـٰ>تِلُوكُمۡ فِی ٱX<ل>دِّینِ وَلَمۡ یُخۡرِجُوكُم N<مِّ>N<ن> دِیَT<ـٰ>رِكُمۡ أَN<ن> تَبَR<رُّ>وهُمۡ وَتُQ<قۡ>سِطُW<وۤ>X<ا۟> إِلَیۡهِمۡۚ إِN<نَّ> ٱللَّهَ یُحِبُّ ٱلۡمُQ<قۡ>سِطِJ<ی>نَ A<۝>D<٨>",
"إِN<نَّ>مَا یَنۡهَX<ى>T<ٰ>كُمُ ٱللَّهُ عَنِ ٱلَّذِینَ قَT<ـٰ>تَلُوكُمۡ فِی ٱX<ل>دِّینِ وَأَخۡR<رَ>جُوكُم N<مِّ>N<ن> دِیَT<ـٰ>رِكُمۡ وَظَT<ـٰ>هَR<رُ>وX<ا۟> عَلَىW<ٰۤ> إِخۡR<رَ>اجِكُمۡ أَN<ن> تَوَلَّوۡهُمۡۚ وَمَX<ن> N<یَ>تَوَلَّهُمۡ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱX<ل>ظَّT<ـٰ>لِمُJ<و>نَ A<۝>D<٩>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا جَW<اۤ>ءَكُمُ ٱلۡمُؤۡمِنَT<ـٰ>تُ مُهَT<ـٰ>جِR<رَ>T< ٰ>تN<ࣲ> فَX<ٱ>مۡتَحِنُوهُN<نَّ>ۖ ٱللَّهُ أَعۡلَمُ بِإِیمَT<ـٰ>نِهِN<نَّ>ۖ فَإِنۡ عَلِمۡتُمُوهُN<نَّ> مُؤۡمِنَT<ـٰ>تN<ࣲ> فَلَا تَR<رۡ>جِعُوهُN<نَّ> إِلَى ٱلۡكُفَّارِۖ لَا هُN<نَّ> حِلّX<ࣱ> لَّهُمۡ وَلَا هُمۡ یَحِلُّونَ لَهُN<نَّ>ۖ وَءَاتُوهُم N<مَّ>W<اۤ> أَN<ن>فَقُوX<ا۟>ۚ وَلَا جُنَاحَ عَلَیۡكُمۡ أَN<ن> تَN<ن>كِحُوهُN<نَّ> إِذَW<اۤ> ءَاتَیۡتُمُوهُN<نَّ> أُجُوR<رَ>هُN<نَّ>ۚ وَلَا تُمۡسِكُوX<ا۟> بِعِصَمِ ٱلۡكَوَافِرِ وَسۡءَلُوX<ا۟> مَW<اۤ> أَN<ن>فَQ<قۡ>تُمۡ وَلۡیَسۡءَلُوX<ا۟> مَW<اۤ> أَN<ن>فَقُوX<ا۟>ۚ ذَT< ٰ>لِكُمۡ حُكۡمُ ٱللَّهِ یَحۡكُمُ بَیۡنَكُمۡۖ وَٱللَّهُ عَلِیمٌ حَكِJ<ی>مࣱ A<۝>D<١٠>",
"وَإِN<ن> فَاتَكُمۡ شَیۡءX<ࣱ> N<مِّ>نۡ أَزۡوَT< ٰ>جِكُمۡ إِلَى ٱلۡكُفَّارِ فَعَاقَQ<بۡ>تُمۡ فَءَاتُوX<ا۟> ٱلَّذِینَ ذَهَبَتۡ أَزۡوَT< ٰ>جُهُم N<مِّ>ثۡلَ مَW<اۤ> أَN<ن>فَقُوX<ا۟>ۚ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ ٱلَّذِW<یۤ> أَN<ن>تُN<م> بِهِT<ۦ> مُؤۡمِنُJ<و>نَ A<۝>D<١١>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ إِذَا جَW<اۤ>ءَكَ ٱلۡمُؤۡمِنَT<ـٰ>تُ یُبَایِعۡنَكَ عَلَىW<ٰۤ> أَX<ن> لَّا یُشۡرِكۡنَ بِٱللَّهِ شَیۡءX<ࣰ>ا N<وَ>لَا یَسۡرِQ<قۡ>نَ وَلَا یَزۡنِینَ وَلَا یَQ<قۡ>تُلۡنَ أَوۡلَT<ـٰ>دَهُN<نَّ> وَلَا یَأۡتِینَ بِبُهۡتَT<ـٰ>نX<ࣲ> N<یَ>فۡتَرِینَهُT<ۥ> بَیۡنَ أَیۡدِیهِN<نَّ> وَأَR<رۡ>جُلِهِN<نَّ> وَلَا یَعۡصِینَكَ فِی مَعۡR<رُ>وفN<ࣲ> فَبَایِعۡهُN<نَّ> وَX<ٱ>سۡتَغۡفِرۡ لَهُN<نَّ> ٱللَّهَۚ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١٢>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تَتَوَلَّوۡX<ا۟> قَوۡمًا غَضِبَ ٱللَّهُ عَلَیۡهِمۡ قَQ<دۡ> یَئِسُوX<ا۟> مِنَ ٱلۡءَاخِR<رَ>ةِ كَمَا یَئِسَ ٱلۡكُفَّاR<رُ> مِنۡ أَصۡحَT<ـٰ>بِ ٱلۡقُبُJ<و>R<ر>ِ A<۝>D<١٣>",
"#سَبَّحَ لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۖ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<١>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لِمَ تَقُولُونَ مَا لَا تَفۡعَلُJ<و>نَ A<۝>D<٢>",
"كَبُR<رَ> مَQ<قۡ>تًا عِN<ن>دَ ٱللَّهِ أَN<ن> تَقُولُوX<ا۟> مَا لَا تَفۡعَلُJ<و>نَ A<۝>D<٣>",
"إِN<نَّ> ٱللَّهَ یُحِبُّ ٱلَّذِینَ یُقَT<ـٰ>تِلُونَ فِی سَبِیلِهِT<ۦ> صَفّN<ࣰ>ا كَأَN<نَّ>هُN<م> بُنۡیَT<ـٰ>نX<ࣱ> N<مَّ>R<رۡ>صُJ<و>صࣱ A<۝>D<٤>",
"وَإِذۡ قَالَ مُوسَىٰ لِقَوۡمِهِT<ۦ> یَT<ـٰ>قَوۡمِ لِمَ تُؤۡذُونَنِی وَقَد تَّعۡلَمُونَ أَN<نِّ>ی R<رَ>سُولُ ٱللَّهِ إِلَیۡكُمۡۖ فَلَN<مَّ>ا زَاغُW<وۤ>X<ا۟> أَزَاغَ ٱللَّهُ قُلُوبَهُمۡۚ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٥>",
"وَإِذۡ قَالَ عِیسَى ٱQ<بۡ>نُ مَR<رۡ>یَمَ یَT<ـٰ>بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ إِN<نِّ>ی R<رَ>سُولُ ٱللَّهِ إِلَیۡكُم N<مُّ>صَدِّقX<ࣰ>ا لِّمَا بَیۡنَ یَدَیَّ مِنَ ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةِ وَمُبَشِّR<رَ>N<ۢ>ا بِR<رَ>سُولX<ࣲ> N<یَ>أۡتِی مِX<ن>N<ۢ> بَعۡدِی ٱسۡمُهُW<ۥۤ> أَحۡمَدُۖ فَلَN<مَّ>ا جَW<اۤ>ءَهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ قَالُوX<ا۟> هَT<ـٰ>ذَا سِحۡR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٦>",
"وَمَنۡ أَظۡلَمُ مِN<مَّ>نِ ٱفۡتَR<رَ>ىٰ عَلَى ٱللَّهِ ٱلۡكَذِبَ وَهُوَ یُQ<دۡ>عَىW<ٰۤ> إِلَى ٱلۡإِسۡلَT<ـٰ>مِۚ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٧>",
"یُرِیدُونَ لِیُQ<طۡ>فِءُوX<ا۟> نُوR<رَ> ٱللَّهِ بِأَفۡوَT< ٰ>هِهِمۡ وَٱللَّهُ مُتِN<مُّ> نُورِهِT<ۦ> وَلَوۡ كَرِهَ ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<٨>",
"هُوَ ٱلَّذِW<یۤ> أَR<رۡ>سَلَ R<رَ>سُولَهُT<ۥ> بِX<ٱ>لۡهُدَىٰ وَدِینِ ٱلۡحَقِّ لِیُظۡهِR<رَ>هُT<ۥ> عَلَى ٱX<ل>دِّینِ كُلِّهِT<ۦ> وَلَوۡ كَرِهَ ٱلۡمُشۡرِكُJ<و>نَ A<۝>D<٩>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> هَلۡ أَدُلُّكُمۡ عَلَىٰ تِجَT<ـٰ>R<رَ>ةN<ࣲ> تُN<ن>جِیكُم N<مِّ>نۡ عَذَابٍ أَلِJ<ی>مࣲ A<۝>D<١٠>",
"تُؤۡمِنُونَ بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ> وَتُجَT<ـٰ>هِدُونَ فِی سَبِیلِ ٱللَّهِ بِأَمۡوَT< ٰ>لِكُمۡ وَأَN<ن>فُسِكُمۡۚ ذَT< ٰ>لِكُمۡ خَیۡR<ر>X<ࣱ> لَّكُمۡ إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<١١>",
"یَغۡفِرۡ لَكُمۡ ذُنُوبَكُمۡ وَیُQ<دۡ>خِلۡكُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> وَمَسَT<ـٰ>كِنَ طَیِّبَةN<ࣰ> فِی جَN<نَّ>T<ـٰ>تِ عَQ<دۡ>نN<ࣲ>ۚ ذَT< ٰ>لِكَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<١٢>",
"وَأُخۡR<رَ>ىٰ تُحِبُّونَهَاۖ نَصۡR<ر>X<ࣱ> N<مِّ>نَ ٱللَّهِ وَفَتۡحN<ࣱ> قَرِیبX<ࣱ>ۗ N<وَ>بَشِّرِ ٱلۡمُؤۡمِنِJ<ی>نَ A<۝>D<١٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> كُونُW<وۤ>X<ا۟> أَN<ن>صَاR<رَ> ٱللَّهِ كَمَا قَالَ عِیسَى ٱQ<بۡ>نُ مَR<رۡ>یَمَ لِلۡحَوَارِیِّT<ـۧ>نَ مَنۡ أَN<ن>صَارِW<یۤ> إِلَى ٱللَّهِۖ قَالَ ٱلۡحَوَارِیُّونَ نَحۡنُ أَN<ن>صَاR<رُ> ٱللَّهِۖ فَءَامَنَت طَّW<اۤ>ئِفَةX<ࣱ> N<مِّ>X<ن>N<ۢ> بَنِW<یۤ> إِسۡR<رَ>W< ٰۤ>ءِیلَ وَكَفَR<رَ>ت طَّW<اۤ>ئِفَةN<ࣱ>ۖ فَأَیَّQ<دۡ>نَا ٱلَّذِینَ ءَامَنُوX<ا۟> عَلَىٰ عَدُوِّهِمۡ فَأَصۡبَحُوX<ا۟> ظَT<ـٰ>هِرِJ<ی>نَ A<۝>D<١٤>",
"#۞ یُسَبِّحُ لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِ ٱلۡمَلِكِ ٱلۡقُدُّوسِ ٱلۡعَزِیزِ ٱلۡحَكِJ<ی>مِ A<۝>D<١>",
"هُوَ ٱلَّذِی بَعَثَ فِی ٱلۡأُN<مِّ>یِّT<ـۧ>نَ R<رَ>سُولX<ࣰ>ا N<مِّ>نۡهُمۡ یَتۡلُوX<ا۟> عَلَیۡهِمۡ ءَایَT<ـٰ>تِهِT<ۦ> وَیُزَكِّیهِمۡ وَیُعَلِّمُهُمُ ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡحِكۡمَةَ وَإِN<ن> كَانُوX<ا۟> مِN<ن> قَQ<بۡ>لُ لَفِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٢>",
"وَءَاخَرِینَ مِنۡهُمۡ لَN<مَّ>ا یَلۡحَقُوX<ا۟> بِهِمۡۚ وَهُوَ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<٣>",
"ذَT< ٰ>لِكَ فَضۡلُ ٱللَّهِ یُؤۡتِیهِ مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَٱللَّهُ ذُو ٱلۡفَضۡلِ ٱلۡعَظِJ<ی>مِ A<۝>D<٤>",
"مَثَلُ ٱلَّذِینَ حُN<مِّ>لُوX<ا۟> ٱX<ل>تَّوۡR<رَ>X<ى>T<ٰ>ةَ ثُN<مَّ> لَمۡ یَحۡمِلُوهَا كَمَثَلِ ٱلۡحِمَارِ یَحۡمِلُ أَسۡفَاR<رَ>N<ۢ>اۚ بِئۡسَ مَثَلُ ٱلۡقَوۡمِ ٱلَّذِینَ كَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِ ٱللَّهِۚ وَٱللَّهُ لَا یَهۡدِی ٱلۡقَوۡمَ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٥>",
"قُلۡ یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ هَادُW<وۤ>X<ا۟> إِN<ن> زَعَمۡتُمۡ أَN<نَّ>كُمۡ أَوۡلِیَW<اۤ>ءُ لِلَّهِ مِN<ن> دُونِ ٱX<ل>N<نَّ>اسِ فَتَمَN<نَّ>وُX<ا۟> ٱلۡمَوۡتَ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٦>",
"وَلَا یَتَمَN<نَّ>وۡنَهُW<ۥۤ> أَبَدَN<ۢ>ا بِمَا قَدَّمَتۡ أَیۡدِیهِمۡۚ وَٱللَّهُ عَلِیمُN<ۢ> بِX<ٱ>X<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<٧>",
"قُلۡ إِN<نَّ> ٱلۡمَوۡتَ ٱلَّذِی تَفِR<رُّ>ونَ مِنۡهُ فَإِN<نَّ>هُT<ۥ> مُلَT<ـٰ>قِیكُمۡۖ ثُN<مَّ> تُR<رَ>دُّونَ إِلَىٰ عَT<ـٰ>لِمِ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِ فَیُنَبِّئُكُN<م> بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِذَا نُودِیَ لِX<ل>صَّلَX<و>T<ٰ>ةِ مِX<ن> N<یَ>وۡمِ ٱلۡجُمُعَةِ فَX<ٱ>سۡعَوۡX<ا۟> إِلَىٰ ذِكۡرِ ٱللَّهِ وَذَR<رُ>وX<ا۟> ٱلۡبَیۡعَۚ ذَT< ٰ>لِكُمۡ خَیۡR<ر>X<ࣱ> لَّكُمۡ إِN<ن> كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٩>",
"فَإِذَا قُضِیَتِ ٱX<ل>صَّلَX<و>T<ٰ>ةُ فَX<ٱ>N<ن>تَشِR<رُ>وX<ا۟> فِی ٱلۡأَR<رۡ>ضِ وَX<ٱ>Q<بۡ>تَغُوX<ا۟> مِN<ن> فَضۡلِ ٱللَّهِ وَX<ٱ>ذۡكُR<رُ>وX<ا۟> ٱللَّهَ كَثِیR<ر>X<ࣰ>ا لَّعَلَّكُمۡ تُفۡلِحُJ<و>نَ A<۝>D<١٠>",
"وَإِذَا R<رَ>أَوۡX<ا۟> تِجَT<ـٰ>R<رَ>ةً أَوۡ لَهۡوًا ٱN<ن>فَضُّW<وۤ>X<ا۟> إِلَیۡهَا وَتَR<رَ>كُوكَ قَW<اۤ>ئِمN<ࣰ>اۚ قُلۡ مَا عِN<ن>دَ ٱللَّهِ خَیۡR<ر>X<ࣱ> N<مِّ>نَ ٱللَّهۡوِ وَمِنَ ٱX<ل>تِّجَT<ـٰ>R<رَ>ةِۚ وَٱللَّهُ خَیۡR<رُ> ٱX<ل>R<رَّ>T< ٰ>زِقِJ<ی>نَ A<۝>D<١١>",
"#إِذَا جَW<اۤ>ءَكَ ٱلۡمُنَT<ـٰ>فِقُونَ قَالُوX<ا۟> نَشۡهَدُ إِN<نَّ>كَ لَR<رَ>سُولُ ٱللَّهِۗ وَٱللَّهُ یَعۡلَمُ إِN<نَّ>كَ لَR<رَ>سُولُهُT<ۥ> وَٱللَّهُ یَشۡهَدُ إِN<نَّ> ٱلۡمُنَT<ـٰ>فِقِینَ لَكَT<ـٰ>ذِبُJ<و>نَ A<۝>D<١>",
"ٱتَّخَذُW<وۤ>X<ا۟> أَیۡمَT<ـٰ>نَهُمۡ جُN<نَّ>ةN<ࣰ> فَصَدُّوX<ا۟> عَN<ن> سَبِیلِ ٱللَّهِۚ إِN<نَّ>هُمۡ سَW<اۤ>ءَ مَا كَانُوX<ا۟> یَعۡمَلُJ<و>نَ A<۝>D<٢>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُمۡ ءَامَنُوX<ا۟> ثُN<مَّ> كَفَR<رُ>وX<ا۟> فَطُبِعَ عَلَىٰ قُلُوبِهِمۡ فَهُمۡ لَا یَفۡقَهُJ<و>نَ A<۝>D<٣>",
"۞ وَإِذَا R<رَ>أَیۡتَهُمۡ تُعۡجِبُكَ أَQ<جۡ>سَامُهُمۡۖ وَإِX<ن> N<یَ>قُولُوX<ا۟> تَسۡمَعۡ لِقَوۡلِهِمۡۖ كَأَN<نَّ>هُمۡ خُشُبX<ࣱ> N<مُّ>سَN<نَّ>دَةX<ࣱ>ۖ N<یَ>حۡسَبُونَ كُلَّ صَیۡحَةٍ عَلَیۡهِمۡۚ هُمُ ٱلۡعَدُوُّ فَX<ٱ>حۡذَR<رۡ>هُمۡۚ قَT<ـٰ>تَلَهُمُ ٱللَّهُۖ أَN<نَّ>ىٰ یُؤۡفَكُJ<و>نَ A<۝>D<٤>",
"وَإِذَا قِیلَ لَهُمۡ تَعَالَوۡX<ا۟> یَسۡتَغۡفِرۡ لَكُمۡ R<رَ>سُولُ ٱللَّهِ لَوَّوۡX<ا۟> R<رُ>ءُوسَهُمۡ وَR<رَ>أَیۡتَهُمۡ یَصُدُّونَ وَهُم N<مُّ>سۡتَكۡبِR<رُ>J<و>نَ A<۝>D<٥>",
"سَوَW<اۤ>ءٌ عَلَیۡهِمۡ أَسۡتَغۡفَR<رۡ>تَ لَهُمۡ أَمۡ لَمۡ تَسۡتَغۡفِرۡ لَهُمۡ لَX<ن> N<یَ>غۡفِR<رَ> ٱللَّهُ لَهُمۡۚ إِN<نَّ> ٱللَّهَ لَا یَهۡدِی ٱلۡقَوۡمَ ٱلۡفَT<ـٰ>سِقِJ<ی>نَ A<۝>D<٦>",
"هُمُ ٱلَّذِینَ یَقُولُونَ لَا تُN<ن>فِقُوX<ا۟> عَلَىٰ مَنۡ عِN<ن>دَ R<رَ>سُولِ ٱللَّهِ حَتَّىٰ یَN<ن>فَضُّوX<ا۟>ۗ وَلِلَّهِ خَزَW<اۤ>ئِنُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَلَT<ـٰ>كِN<نَّ> ٱلۡمُنَT<ـٰ>فِقِینَ لَا یَفۡقَهُJ<و>نَ A<۝>D<٧>",
"یَقُولُونَ لَئِX<ن> R<رَّ>جَعۡنَW<اۤ> إِلَى ٱلۡمَدِینَةِ لَیُخۡرِجَN<نَّ> ٱلۡأَعَزُّ مِنۡهَا ٱلۡأَذَلَّۚ وَلِلَّهِ ٱلۡعِزَّةُ وَلِR<رَ>سُولِهِT<ۦ> وَلِلۡمُؤۡمِنِینَ وَلَT<ـٰ>كِN<نَّ> ٱلۡمُنَT<ـٰ>فِقِینَ لَا یَعۡلَمُJ<و>نَ A<۝>D<٨>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> لَا تُلۡهِكُمۡ أَمۡوَT< ٰ>لُكُمۡ وَلَW<اۤ> أَوۡلَT<ـٰ>دُكُمۡ عَN<ن> ذِكۡرِ ٱللَّهِۚ وَمَX<ن> N<یَ>فۡعَلۡ ذَT< ٰ>لِكَ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡخَT<ـٰ>سِR<رُ>J<و>نَ A<۝>D<٩>",
"وَأَN<ن>فِقُوX<ا۟> مِX<ن> N<مَّ>ا R<رَ>زَQ<قۡ>نَT<ـٰ>كُم N<مِّ>N<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>أۡتِیَ أَحَدَكُمُ ٱلۡمَوۡتُ فَیَقُولَ R<رَ>بِّ لَوۡلَW<اۤ> أَخَّR<رۡ>تَنِW<یۤ> إِلَىW<ٰۤ> أَجَلN<ࣲ> قَرِیبN<ࣲ> فَأَصَّدَّقَ وَأَكُX<ن> N<مِّ>نَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<١٠>",
"وَلَX<ن> N<یُ>ؤَخِّR<رَ> ٱللَّهُ نَفۡسًا إِذَا جَW<اۤ>ءَ أَجَلُهَاۚ وَٱللَّهُ خَبِیR<رُ>N<ۢ> بِمَا تَعۡمَلُJ<و>نَ A<۝>D<١١>",
"#یُسَبِّحُ لِلَّهِ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَمَا فِی ٱلۡأَR<رۡ>ضِۖ لَهُ ٱلۡمُلۡكُ وَلَهُ ٱلۡحَمۡدُۖ وَهُوَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<١>",
"هُوَ ٱلَّذِی خَلَقَكُمۡ فَمِN<ن>كُمۡ كَافِR<ر>X<ࣱ> N<وَ>مِN<ن>كُم N<مُّ>ؤۡمِنX<ࣱ>ۚ N<وَ>ٱللَّهُ بِمَا تَعۡمَلُونَ بَصِJ<ی>رٌ A<۝>D<٢>",
"خَلَقَ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضَ بِX<ٱ>لۡحَقِّ وَصَوَّR<رَ>كُمۡ فَأَحۡسَنَ صُوَR<رَ>كُمۡۖ وَإِلَیۡهِ ٱلۡمَصِJ<ی>رُ A<۝>D<٣>",
"یَعۡلَمُ مَا فِی ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَیَعۡلَمُ مَا تُسِR<رُّ>ونَ وَمَا تُعۡلِنُونَۚ وَٱللَّهُ عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<٤>",
"أَلَمۡ یَأۡتِكُمۡ نَبَؤُX<ا۟> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِN<ن> قَQ<بۡ>لُ فَذَاقُوX<ا۟> وَبَالَ أَمۡرِهِمۡ وَلَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<٥>",
"ذَT< ٰ>لِكَ بِأَN<نَّ>هُT<ۥ> كَانَت تَّأۡتِیهِمۡ R<رُ>سُلُهُN<م> بِX<ٱ>لۡبَیِّنَT<ـٰ>تِ فَقَالُW<وۤ>X<ا۟> أَبَشَR<ر>X<ࣱ> N<یَ>هۡدُونَنَا فَكَفَR<رُ>وX<ا۟> وَتَوَلَّوX<ا۟>ۖ وَّX<ٱ>سۡتَغۡنَى ٱللَّهُۚ وَٱللَّهُ غَنِیٌّ حَمِJ<ی>Q<د>ࣱ A<۝>D<٦>",
"زَعَمَ ٱلَّذِینَ كَفَR<رُ>W<وۤ>X<ا۟> أَX<ن> لَّX<ن> N<یُ>Q<بۡ>عَثُوX<ا۟>ۚ قُلۡ بَلَىٰ وَR<رَ>بِّی لَتُQ<بۡ>عَثُN<نَّ> ثُN<مَّ> لَتُنَبَّؤُN<نَّ> بِمَا عَمِلۡتُمۡۚ وَذَT< ٰ>لِكَ عَلَى ٱللَّهِ یَسِJ<ی>رࣱ A<۝>D<٧>",
"فَءَامِنُوX<ا۟> بِٱللَّهِ وَR<رَ>سُولِهِT<ۦ> وَX<ٱ>X<ل>N<نُّ>ورِ ٱلَّذِW<یۤ> أَN<ن>زَلۡنَاۚ وَٱللَّهُ بِمَا تَعۡمَلُونَ خَبِJ<ی>رࣱ A<۝>D<٨>",
"یَوۡمَ یَQ<جۡ>مَعُكُمۡ لِیَوۡمِ ٱلۡجَمۡعِۖ ذَT< ٰ>لِكَ یَوۡمُ ٱX<ل>تَّغَابُنِۗ وَمَX<ن> N<یُ>ؤۡمِX<ن>N<ۢ> بِٱللَّهِ وَیَعۡمَلۡ صَT<ـٰ>لِحX<ࣰ>ا N<یُ>كَفِّرۡ عَنۡهُ سَیِّءَاتِهِT<ۦ> وَیُQ<دۡ>خِلۡهُ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدN<ࣰ>اۚ ذَT< ٰ>لِكَ ٱلۡفَوۡزُ ٱلۡعَظِJ<ی>مُ A<۝>D<٩>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> وَكَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَW<اۤ> أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱX<ل>N<نَّ>ارِ خَT<ـٰ>لِدِینَ فِیهَاۖ وَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<١٠>",
"مَW<اۤ> أَصَابَ مِX<ن> N<مُّ>صِیبَةٍ إِلَّا بِإِذۡنِ ٱللَّهِۗ وَمَX<ن> N<یُ>ؤۡمِX<ن>N<ۢ> بِٱللَّهِ یَهۡدِ قَلۡبَهُT<ۥ>ۚ وَٱللَّهُ بِكُلِّ شَیۡءٍ عَلِJ<ی>مࣱ A<۝>D<١١>",
"وَأَطِیعُوX<ا۟> ٱللَّهَ وَأَطِیعُوX<ا۟> ٱX<ل>R<رَّ>سُولَۚ فَإِN<ن> تَوَلَّیۡتُمۡ فَإِN<نَّ>مَا عَلَىٰ R<رَ>سُولِنَا ٱلۡبَلَT<ـٰ>غُ ٱلۡمُبِJ<ی>نُ A<۝>D<١٢>",
"ٱللَّهُ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَۚ وَعَلَى ٱللَّهِ فَلۡیَتَوَكَّلِ ٱلۡمُؤۡمِنُJ<و>نَ A<۝>D<١٣>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِN<نَّ> مِنۡ أَزۡوَT< ٰ>جِكُمۡ وَأَوۡلَT<ـٰ>دِكُمۡ عَدُوّX<ࣰ>ا لَّكُمۡ فَX<ٱ>حۡذَR<رُ>وهُمۡۚ وَإِN<ن> تَعۡفُوX<ا۟> وَتَصۡفَحُوX<ا۟> وَتَغۡفِR<رُ>وX<ا۟> فَإِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مٌ A<۝>D<١٤>",
"إِN<نَّ>مَW<اۤ> أَمۡوَT< ٰ>لُكُمۡ وَأَوۡلَT<ـٰ>دُكُمۡ فِتۡنَةX<ࣱ>ۚ N<وَ>ٱللَّهُ عِN<ن>دَهُW<ۥۤ> أَQ<جۡ>R<ر>ٌ عَظِJ<ی>مࣱ A<۝>D<١٥>",
"فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ مَا ٱسۡتَطَعۡتُمۡ وَX<ٱ>سۡمَعُوX<ا۟> وَأَطِیعُوX<ا۟> وَأَN<ن>فِقُوX<ا۟> خَیۡR<ر>X<ࣰ>ا لِّأَN<ن>فُسِكُمۡۗ وَمَX<ن> N<یُ>وقَ شُحَّ نَفۡسِهِT<ۦ> فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡمُفۡلِحُJ<و>نَ A<۝>D<١٦>",
"إِN<ن> تُQ<قۡ>رِضُوX<ا۟> ٱللَّهَ قَR<رۡ>ضًا حَسَنX<ࣰ>ا N<یُ>ضَT<ـٰ>عِفۡهُ لَكُمۡ وَیَغۡفِرۡ لَكُمۡۚ وَٱللَّهُ شَكُوR<ر>ٌ حَلِJ<ی>مٌ A<۝>D<١٧>",
"عَT<ـٰ>لِمُ ٱلۡغَیۡبِ وَX<ٱ>X<ل>شَّهَT<ـٰ>دَةِ ٱلۡعَزِیزُ ٱلۡحَكِJ<ی>مُ A<۝>D<١٨>",
"#۞ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ إِذَا طَلَّQ<قۡ>تُمُ ٱX<ل>N<نِّ>سَW<اۤ>ءَ فَطَلِّقُوهُN<نَّ> لِعِدَّتِهِN<نَّ> وَأَحۡصُوX<ا۟> ٱلۡعِدَّةَۖ وَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ R<رَ>بَّكُمۡۖ لَا تُخۡرِجُوهُN<نَّ> مِX<ن>N<ۢ> بُیُوتِهِN<نَّ> وَلَا یَخۡR<رُ>Q<جۡ>نَ إِلَّW<اۤ> أَX<ن> N<یَ>أۡتِینَ بِفَT<ـٰ>حِشَةX<ࣲ> N<مُّ>بَیِّنَةX<ࣲ>ۚ N<وَ>تِلۡكَ حُدُودُ ٱللَّهِۚ وَمَX<ن> N<یَ>تَعَدَّ حُدُودَ ٱللَّهِ فَقَQ<دۡ> ظَلَمَ نَفۡسَهُT<ۥ>ۚ لَا تَQ<دۡ>رِی لَعَلَّ ٱللَّهَ یُحۡدِثُ بَعۡدَ ذَT< ٰ>لِكَ أَمۡR<ر>ࣰا A<۝>D<١>",
"فَإِذَا بَلَغۡنَ أَجَلَهُN<نَّ> فَأَمۡسِكُوهُN<نَّ> بِمَعۡR<رُ>وفٍ أَوۡ فَارِقُوهُN<نَّ> بِمَعۡR<رُ>وفX<ࣲ> N<وَ>أَشۡهِدُوX<ا۟> ذَوَیۡ عَQ<دۡ>لX<ࣲ> N<مِّ>N<ن>كُمۡ وَأَقِیمُوX<ا۟> ٱX<ل>شَّهَT<ـٰ>دَةَ لِلَّهِۚ ذَT< ٰ>لِكُمۡ یُوعَظُ بِهِT<ۦ> مَN<ن> كَانَ یُؤۡمِنُ بِٱللَّهِ وَX<ٱ>لۡیَوۡمِ ٱلۡءَاخِرِۚ وَمَX<ن> N<یَ>تَّقِ ٱللَّهَ یَQ<جۡ>عَل لَّهُT<ۥ> مَخۡR<رَ>جࣰا A<۝>D<٢>",
"وَیَR<رۡ>زُQ<قۡ>هُ مِنۡ حَیۡثُ لَا یَحۡتَسِبُۚ وَمَX<ن> N<یَ>تَوَكَّلۡ عَلَى ٱللَّهِ فَهُوَ حَسۡبُهُW<ۥۤ>ۚ إِN<نَّ> ٱللَّهَ بَT<ـٰ>لِغُ أَمۡرِهِT<ۦ>ۚ قَQ<دۡ> جَعَلَ ٱللَّهُ لِكُلِّ شَیۡءN<ࣲ> قَQ<دۡ>R<ر>ࣰا A<۝>D<٣>",
"وَX<ٱ>لَّW<ـٰۤ>ءِی یَئِسۡنَ مِنَ ٱلۡمَحِیضِ مِن N<نِّ>سَW<اۤ>ئِكُمۡ إِنِ ٱR<رۡ>تَQ<بۡ>تُمۡ فَعِدَّتُهُN<نَّ> ثَلَT<ـٰ>ثَةُ أَشۡهُرX<ࣲ> N<وَ>X<ٱ>لَّW<ـٰۤ>ءِی لَمۡ یَحِضۡنَۚ وَأُX<و۟>لَT<ـٰ>تُ ٱلۡأَحۡمَالِ أَجَلُهُN<نَّ> أَX<ن> N<یَ>ضَعۡنَ حَمۡلَهُN<نَّ>ۚ وَمَX<ن> N<یَ>تَّقِ ٱللَّهَ یَQ<جۡ>عَل لَّهُT<ۥ> مِنۡ أَمۡرِهِT<ۦ> یُسۡR<ر>ࣰا A<۝>D<٤>",
"ذَT< ٰ>لِكَ أَمۡR<رُ> ٱللَّهِ أَN<ن>زَلَهُW<ۥۤ> إِلَیۡكُمۡۚ وَمَX<ن> N<یَ>تَّقِ ٱللَّهَ یُكَفِّرۡ عَنۡهُ سَیِّءَاتِهِT<ۦ> وَیُعۡظِمۡ لَهُW<ۥۤ> أَQ<جۡ>R<ر>ًا A<۝>D<٥>",
"أَسۡكِنُوهُN<نَّ> مِنۡ حَیۡثُ سَكَN<ن>تُم N<مِّ>X<ن> N<وُ>Q<جۡ>دِكُمۡ وَلَا تُضَL<اۤ>R<رُّ>وهُN<نَّ> لِتُضَیِّقُوX<ا۟> عَلَیۡهِN<نَّ>ۚ وَإِN<ن> كُN<نَّ> أُX<و۟>لَT<ـٰ>تِ حَمۡلN<ࣲ> فَأَN<ن>فِقُوX<ا۟> عَلَیۡهِN<نَّ> حَتَّىٰ یَضَعۡنَ حَمۡلَهُN<نَّ>ۚ فَإِنۡ أَR<رۡ>ضَعۡنَ لَكُمۡ فَءَاتُوهُN<نَّ> أُجُوR<رَ>هُN<نَّ> وَأۡتَمِR<رُ>وX<ا۟> بَیۡنَكُN<م> بِمَعۡR<رُ>وفX<ࣲ>ۖ N<وَ>إِN<ن> تَعَاسَR<رۡ>تُمۡ فَسَتُR<رۡ>ضِعُ لَهُW<ۥۤ> أُخۡR<رَ>ىٰ A<۝>D<٦>",
"لِیُN<ن>فِQ<قۡ> ذُو سَعَةX<ࣲ> N<مِّ>N<ن> سَعَتِهِT<ۦ>ۖ وَمَN<ن> قُدِR<رَ> عَلَیۡهِ رِزۡقُهُT<ۥ> فَلۡیُN<ن>فِQ<قۡ> مِN<مَّ>W<اۤ> ءَاتَX<ى>T<ٰ>هُ ٱللَّهُۚ لَا یُكَلِّفُ ٱللَّهُ نَفۡسًا إِلَّا مَW<اۤ> ءَاتَX<ى>T<ٰ>هَاۚ سَیَQ<جۡ>عَلُ ٱللَّهُ بَعۡدَ عُسۡرX<ࣲ> N<یُ>سۡR<ر>ࣰا A<۝>D<٧>",
"وَكَأَیِّX<ن> N<مِّ>N<ن> قَR<رۡ>یَةٍ عَتَتۡ عَنۡ أَمۡرِ R<رَ>بِّهَا وَR<رُ>سُلِهِT<ۦ> فَحَاسَQ<بۡ>نَT<ـٰ>هَا حِسَابN<ࣰ>ا شَدِیدX<ࣰ>ا N<وَ>عَذَّQ<بۡ>نَT<ـٰ>هَا عَذَابX<ࣰ>ا N<نُّ>كۡR<ر>ࣰا A<۝>D<٨>",
"فَذَاقَتۡ وَبَالَ أَمۡرِهَا وَكَانَ عَT<ـٰ>قِبَةُ أَمۡرِهَا خُسۡR<ر>ًا A<۝>D<٩>",
"أَعَدَّ ٱللَّهُ لَهُمۡ عَذَابN<ࣰ>ا شَدِیدN<ࣰ>اۖ فَX<ٱ>تَّقُوX<ا۟> ٱللَّهَ یَW<ـٰۤ>أُX<و۟>لِی ٱلۡأَلۡبَT<ـٰ>بِ ٱلَّذِینَ ءَامَنُوX<ا۟>ۚ قَQ<دۡ> أَN<ن>زَلَ ٱللَّهُ إِلَیۡكُمۡ ذِكۡR<ر>ࣰا A<۝>D<١٠>",
"R<رَّ>سُولX<ࣰ>ا N<یَ>تۡلُوX<ا۟> عَلَیۡكُمۡ ءَایَT<ـٰ>تِ ٱللَّهِ مُبَیِّنَT<ـٰ>تX<ࣲ> لِّیُخۡرِجَ ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ مِنَ ٱX<ل>ظُّلُمَT<ـٰ>تِ إِلَى ٱX<ل>N<نُّ>ورِۚ وَمَX<ن> N<یُ>ؤۡمِX<ن>N<ۢ> بِٱللَّهِ وَیَعۡمَلۡ صَT<ـٰ>لِحX<ࣰ>ا N<یُ>Q<دۡ>خِلۡهُ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدN<ࣰ>اۖ قَQ<دۡ> أَحۡسَنَ ٱللَّهُ لَهُT<ۥ> رِزۡقًا A<۝>D<١١>",
"ٱللَّهُ ٱلَّذِی خَلَقَ سَQ<بۡ>عَ سَمَT<ـٰ>وَT< ٰ>تX<ࣲ> N<وَ>مِنَ ٱلۡأَR<رۡ>ضِ مِثۡلَهُN<نَّ>ۖ یَتَنَزَّلُ ٱلۡأَمۡR<رُ> بَیۡنَهُN<نَّ> لِتَعۡلَمُW<وۤ>X<ا۟> أَN<نَّ> ٱللَّهَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِیR<ر>X<ࣱ> N<وَ>أَN<نَّ> ٱللَّهَ قَQ<دۡ> أَحَاطَ بِكُلِّ شَیۡءٍ عِلۡمَۢا A<۝>D<١٢>",
"#۞ یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ لِمَ تُحَرِّمُ مَW<اۤ> أَحَلَّ ٱللَّهُ لَكَۖ تَQ<بۡ>تَغِی مَR<رۡ>ضَاتَ أَزۡوَT< ٰ>جِكَۚ وَٱللَّهُ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مࣱ A<۝>D<١>",
"قَQ<دۡ> فَR<رَ>ضَ ٱللَّهُ لَكُمۡ تَحِلَّةَ أَیۡمَT<ـٰ>نِكُمۡۚ وَٱللَّهُ مَوۡلَX<ى>T<ٰ>كُمۡۖ وَهُوَ ٱلۡعَلِیمُ ٱلۡحَكِJ<ی>مُ A<۝>D<٢>",
"وَإِذۡ أَسَR<رَّ> ٱX<ل>N<نَّ>بِیُّ إِلَىٰ بَعۡضِ أَزۡوَT< ٰ>جِهِT<ۦ> حَدِیثN<ࣰ>ا فَلَN<مَّ>ا نَبَّأَتۡ بِهِT<ۦ> وَأَظۡهَR<رَ>هُ ٱللَّهُ عَلَیۡهِ عَR<رَّ>فَ بَعۡضَهُT<ۥ> وَأَعۡR<رَ>ضَ عَX<ن>N<ۢ> بَعۡضN<ࣲ>ۖ فَلَN<مَّ>ا نَبَّأَهَا بِهِT<ۦ> قَالَتۡ مَنۡ أَX<ن>N<ۢ>بَأَكَ هَT<ـٰ>ذَاۖ قَالَ نَبَّأَنِیَ ٱلۡعَلِیمُ ٱلۡخَبِJ<ی>رُ A<۝>D<٣>",
"إِN<ن> تَتُوبَW<اۤ> إِلَى ٱللَّهِ فَقَQ<دۡ> صَغَتۡ قُلُوبُكُمَاۖ وَإِN<ن> تَظَT<ـٰ>هَR<رَ>ا عَلَیۡهِ فَإِN<نَّ> ٱللَّهَ هُوَ مَوۡلَX<ى>T<ٰ>هُ وَجِQ<بۡ>رِیلُ وَصَT<ـٰ>لِحُ ٱلۡمُؤۡمِنِینَۖ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ بَعۡدَ ذَT< ٰ>لِكَ ظَهِJ<ی>رٌ A<۝>D<٤>",
"عَسَىٰ R<رَ>بُّهُW<ۥۤ> إِN<ن> طَلَّقَكُN<نَّ> أَX<ن> N<یُ>Q<بۡ>دِلَهُW<ۥۤ> أَزۡوَT< ٰ>جًا خَیۡR<ر>X<ࣰ>ا N<مِّ>N<ن>كُN<نَّ> مُسۡلِمَT<ـٰ>تX<ࣲ> N<مُّ>ؤۡمِنَT<ـٰ>تN<ࣲ> قَT<ـٰ>نِتَT<ـٰ>تN<ࣲ> تَW<ـٰۤ>ئِبَT<ـٰ>تٍ عَT<ـٰ>بِدَT< ٰ>تN<ࣲ> سَW<ـٰۤ>ئِحَT<ـٰ>تN<ࣲ> ثَیِّبَT<ـٰ>تX<ࣲ> N<وَ>أَQ<بۡ>كَاR<ر>ࣰا A<۝>D<٥>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> قُW<وۤ>X<ا۟> أَN<ن>فُسَكُمۡ وَأَهۡلِیكُمۡ نَاR<ر>X<ࣰ>ا N<وَ>قُودُهَا ٱX<ل>N<نَّ>اسُ وَX<ٱ>لۡحِجَاR<رَ>ةُ عَلَیۡهَا مَلَW<ـٰۤ>ئِكَةٌ غِلَاظN<ࣱ> شِدَادX<ࣱ> لَّا یَعۡصُونَ ٱللَّهَ مَW<اۤ> أَمَR<رَ>هُمۡ وَیَفۡعَلُونَ مَا یُؤۡمَR<رُ>J<و>نَ A<۝>D<٦>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَا تَعۡتَذِR<رُ>وX<ا۟> ٱلۡیَوۡمَۖ إِN<نَّ>مَا تُQ<جۡ>زَوۡنَ مَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٧>",
"یَW<ـٰۤ>أَیُّهَا ٱلَّذِینَ ءَامَنُوX<ا۟> تُوبُW<وۤ>X<ا۟> إِلَى ٱللَّهِ تَوۡبَةX<ࣰ> N<نَّ>صُوحًا عَسَىٰ R<رَ>بُّكُمۡ أَX<ن> N<یُ>كَفِّR<رَ> عَN<ن>كُمۡ سَیِّءَاتِكُمۡ وَیُQ<دۡ>خِلَكُمۡ جَN<نَّ>T<ـٰ>تN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> یَوۡمَ لَا یُخۡزِی ٱللَّهُ ٱX<ل>N<نَّ>بِیَّ وَX<ٱ>لَّذِینَ ءَامَنُوX<ا۟> مَعَهُT<ۥ>ۖ نُوR<رُ>هُمۡ یَسۡعَىٰ بَیۡنَ أَیۡدِیهِمۡ وَبِأَیۡمَT<ـٰ>نِهِمۡ یَقُولُونَ R<رَ>بَّنَW<اۤ> أَتۡمِمۡ لَنَا نُوR<رَ>نَا وَX<ٱ>غۡفِرۡ لَنَW<اۤ>ۖ إِN<نَّ>كَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رࣱ A<۝>D<٨>",
"یَW<ـٰۤ>أَیُّهَا ٱX<ل>N<نَّ>بِیُّ جَT<ـٰ>هِدِ ٱلۡكُفَّاR<رَ> وَX<ٱ>لۡمُنَT<ـٰ>فِقِینَ وَX<ٱ>غۡلُظۡ عَلَیۡهِمۡۚ وَمَأۡوَX<ى>T<ٰ>هُمۡ جَهَN<نَّ>مُۖ وَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<٩>",
"ضَR<رَ>بَ ٱللَّهُ مَثَلX<ࣰ>ا لِّلَّذِینَ كَفَR<رُ>وX<ا۟> ٱمۡR<رَ>أَتَ نُوحX<ࣲ> N<وَ>X<ٱ>مۡR<رَ>أَتَ لُوطN<ࣲ>ۖ كَانَتَا تَحۡتَ عَQ<بۡ>دَیۡنِ مِنۡ عِبَادِنَا صَT<ـٰ>لِحَیۡنِ فَخَانَتَاهُمَا فَلَمۡ یُغۡنِیَا عَنۡهُمَا مِنَ ٱللَّهِ شَیۡءX<ࣰ>ا N<وَ>قِیلَ ٱQ<دۡ>خُلَا ٱX<ل>N<نَّ>اR<رَ> مَعَ ٱX<ل>دَّT< ٰ>خِلِJ<ی>نَ A<۝>D<١٠>",
"وَضَR<رَ>بَ ٱللَّهُ مَثَلX<ࣰ>ا لِّلَّذِینَ ءَامَنُوX<ا۟> ٱمۡR<رَ>أَتَ فِرۡعَوۡنَ إِذۡ قَالَتۡ R<رَ>بِّ ٱQ<بۡ>نِ لِی عِN<ن>دَكَ بَیۡتN<ࣰ>ا فِی ٱلۡجَN<نَّ>ةِ وَنَجِّنِی مِN<ن> فِرۡعَوۡنَ وَعَمَلِهِT<ۦ> وَنَجِّنِی مِنَ ٱلۡقَوۡمِ ٱX<ل>ظَّT<ـٰ>لِمِJ<ی>نَ A<۝>D<١١>",
"وَمَR<رۡ>یَمَ ٱQ<بۡ>نَتَ عِمۡR<رَ>T< ٰ>نَ ٱلَّتِW<یۤ> أَحۡصَنَتۡ فَR<رۡ>جَهَا فَنَفَخۡنَا فِیهِ مِX<ن> R<رُّ>وحِنَا وَصَدَّقَتۡ بِكَلِمَT<ـٰ>تِ R<رَ>بِّهَا وَكُتُبِهِT<ۦ> وَكَانَتۡ مِنَ ٱلۡقَT<ـٰ>نِتِJ<ی>نَ A<۝>D<١٢>",
"#۞ تَبَT<ـٰ>R<رَ>كَ ٱلَّذِی بِیَدِهِ ٱلۡمُلۡكُ وَهُوَ عَلَىٰ كُلِّ شَیۡءN<ࣲ> قَدِJ<ی>رٌ A<۝>D<١>",
"ٱلَّذِی خَلَقَ ٱلۡمَوۡتَ وَX<ٱ>لۡحَیَX<و>T<ٰ>ةَ لِیَQ<بۡ>لُوَكُمۡ أَیُّكُمۡ أَحۡسَنُ عَمَلX<ࣰ>اۚ N<وَ>هُوَ ٱلۡعَزِیزُ ٱلۡغَفُJ<و>R<ر>ُ A<۝>D<٢>",
"ٱلَّذِی خَلَقَ سَQ<بۡ>عَ سَمَT<ـٰ>وَT< ٰ>تN<ࣲ> طِبَاقX<ࣰ>اۖ N<مَّ>ا تَR<رَ>ىٰ فِی خَلۡقِ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِ مِN<ن> تَفَT<ـٰ>وُتN<ࣲ>ۖ فَX<ٱ>R<رۡ>جِعِ ٱلۡبَصَR<رَ> هَلۡ تَR<رَ>ىٰ مِN<ن> فُطُJ<و>R<ر>ࣲ A<۝>D<٣>",
"ثُN<مَّ> ٱR<رۡ>جِعِ ٱلۡبَصَR<رَ> كَR<رَّ>تَیۡنِ یَN<ن>قَلِQ<بۡ> إِلَیۡكَ ٱلۡبَصَR<رُ> خَاسِئX<ࣰ>ا N<وَ>هُوَ حَسِJ<ی>رࣱ A<۝>D<٤>",
"وَلَقَQ<دۡ> زَیَّN<نَّ>ا ٱX<ل>سَّمَW<اۤ>ءَ ٱX<ل>دُّنۡیَا بِمَصَT<ـٰ>بِیحَ وَجَعَلۡنَT<ـٰ>هَا R<رُ>جُومX<ࣰ>ا لِّX<ل>شَّیَT<ـٰ>طِینِۖ وَأَعۡتَQ<دۡ>نَا لَهُمۡ عَذَابَ ٱX<ل>سَّعِJ<ی>رِ A<۝>D<٥>",
"وَلِلَّذِینَ كَفَR<رُ>وX<ا۟> بِR<رَ>بِّهِمۡ عَذَابُ جَهَN<نَّ>مَۖ وَبِئۡسَ ٱلۡمَصِJ<ی>رُ A<۝>D<٦>",
"إِذَW<اۤ> أُلۡقُوX<ا۟> فِیهَا سَمِعُوX<ا۟> لَهَا شَهِیقX<ࣰ>ا N<وَ>هِیَ تَفُJ<و>R<ر>ُ A<۝>D<٧>",
"تَكَادُ تَمَیَّزُ مِنَ ٱلۡغَیۡظِۖ كُلَّمَW<اۤ> أُلۡقِیَ فِیهَا فَوۡجN<ࣱ> سَأَلَهُمۡ خَزَنَتُهَW<اۤ> أَلَمۡ یَأۡتِكُمۡ نَذِJ<ی>رࣱ A<۝>D<٨>",
"قَالُوX<ا۟> بَلَىٰ قَQ<دۡ> جَW<اۤ>ءَنَا نَذِیR<ر>N<ࣱ> فَكَذَّQ<بۡ>نَا وَقُلۡنَا مَا نَزَّلَ ٱللَّهُ مِN<ن> شَیۡءٍ إِنۡ أَN<ن>تُمۡ إِلَّا فِی ضَلَT<ـٰ>لN<ࣲ> كَبِJ<ی>رࣲ A<۝>D<٩>",
"وَقَالُوX<ا۟> لَوۡ كُN<نَّ>ا نَسۡمَعُ أَوۡ نَعۡقِلُ مَا كُN<نَّ>ا فِW<یۤ> أَصۡحَT<ـٰ>بِ ٱX<ل>سَّعِJ<ی>رِ A<۝>D<١٠>",
"فَX<ٱ>عۡتَR<رَ>فُوX<ا۟> بِذَX<ن>N<ۢ>بِهِمۡ فَسُحۡقX<ࣰ>ا لِّأَصۡحَT<ـٰ>بِ ٱX<ل>سَّعِJ<ی>رِ A<۝>D<١١>",
"إِN<نَّ> ٱلَّذِینَ یَخۡشَوۡنَ R<رَ>بَّهُN<م> بِX<ٱ>لۡغَیۡبِ لَهُم N<مَّ>غۡفِR<رَ>ةX<ࣱ> N<وَ>أَQ<جۡ>R<ر>N<ࣱ> كَبِJ<ی>رࣱ A<۝>D<١٢>",
"وَأَسِR<رُّ>وX<ا۟> قَوۡلَكُمۡ أَوِ ٱQ<جۡ>هَR<رُ>وX<ا۟> بِهِW<ۦۤ>ۖ إِN<نَّ>هُT<ۥ> عَلِیمُN<ۢ> بِذَاتِ ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<١٣>",
"أَلَا یَعۡلَمُ مَنۡ خَلَقَ وَهُوَ ٱX<ل>لَّطِیفُ ٱلۡخَبِJ<ی>رُ A<۝>D<١٤>",
"هُوَ ٱلَّذِی جَعَلَ لَكُمُ ٱلۡأَR<رۡ>ضَ ذَلُولN<ࣰ>ا فَX<ٱ>مۡشُوX<ا۟> فِی مَنَاكِبِهَا وَكُلُوX<ا۟> مِX<ن> رِّزۡقِهِT<ۦ>ۖ وَإِلَیۡهِ ٱX<ل>N<نُّ>شُJ<و>R<ر>ُ A<۝>D<١٥>",
"ءَأَمِN<ن>تُم N<مَّ>N<ن> فِی ٱX<ل>سَّمَW<اۤ>ءِ أَX<ن> N<یَ>خۡسِفَ بِكُمُ ٱلۡأَR<رۡ>ضَ فَإِذَا هِیَ تَمُJ<و>R<ر>ُ A<۝>D<١٦>",
"أَمۡ أَمِN<ن>تُم N<مَّ>N<ن> فِی ٱX<ل>سَّمَW<اۤ>ءِ أَX<ن> N<یُ>R<رۡ>سِلَ عَلَیۡكُمۡ حَاصِبN<ࣰ>اۖ فَسَتَعۡلَمُونَ كَیۡفَ نَذِJ<ی>رِ A<۝>D<١٧>",
"وَلَقَQ<دۡ> كَذَّبَ ٱلَّذِینَ مِN<ن> قَQ<بۡ>لِهِمۡ فَكَیۡفَ كَانَ نَكِJ<ی>رِ A<۝>D<١٨>",
"أَوَ لَمۡ یَR<رَ>وۡX<ا۟> إِلَى ٱX<ل>طَّیۡرِ فَوۡقَهُمۡ صَL<ـٰۤ>فَّT<ـٰ>تX<ࣲ> N<وَ>یَQ<قۡ>بِضۡنَۚ مَا یُمۡسِكُهُN<نَّ> إِلَّا ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُۚ إِN<نَّ>هُT<ۥ> بِكُلِّ شَیۡءِN<ۭ> بَصِJ<ی>رٌ A<۝>D<١٩>",
"أَN<مَّ>نۡ هَT<ـٰ>ذَا ٱلَّذِی هُوَ جُN<ن>دX<ࣱ> لَّكُمۡ یَN<ن>صُR<رُ>كُم N<مِّ>N<ن> دُونِ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِۚ إِنِ ٱلۡكَT<ـٰ>فِR<رُ>ونَ إِلَّا فِی غُR<رُ>J<و>R<ر>ٍ A<۝>D<٢٠>",
"أَN<مَّ>نۡ هَT<ـٰ>ذَا ٱلَّذِی یَR<رۡ>زُقُكُمۡ إِنۡ أَمۡسَكَ رِزۡقَهُT<ۥ>ۚ بَل لَّجُّوX<ا۟> فِی عُتُوّX<ࣲ> N<وَ>نُفُJ<و>R<ر>ٍ A<۝>D<٢١>",
"أَفَمَX<ن> N<یَ>مۡشِی مُكِبًّا عَلَىٰ وَQ<جۡ>هِهِW<ۦۤ> أَهۡدَىW<ٰۤ> أَN<مَّ>X<ن> N<یَ>مۡشِی سَوِیًّا عَلَىٰ صِR<رَ>T< ٰ>طX<ࣲ> N<مُّ>سۡتَقِJ<ی>مࣲ A<۝>D<٢٢>",
"قُلۡ هُوَ ٱلَّذِW<یۤ> أَN<ن>شَأَكُمۡ وَجَعَلَ لَكُمُ ٱX<ل>سَّمۡعَ وَX<ٱ>لۡأَQ<بۡ>صَT<ـٰ>R<رَ> وَX<ٱ>لۡأَفۡءِدَةَۚ قَلِیلX<ࣰ>ا N<مَّ>ا تَشۡكُR<رُ>J<و>نَ A<۝>D<٢٣>",
"قُلۡ هُوَ ٱلَّذِی ذَR<رَ>أَكُمۡ فِی ٱلۡأَR<رۡ>ضِ وَإِلَیۡهِ تُحۡشَR<رُ>J<و>نَ A<۝>D<٢٤>",
"وَیَقُولُونَ مَتَىٰ هَT<ـٰ>ذَا ٱلۡوَعۡدُ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٢٥>",
"قُلۡ إِN<نَّ>مَا ٱلۡعِلۡمُ عِN<ن>دَ ٱللَّهِ وَإِN<نَّ>مَW<اۤ> أَنَا۠ نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نࣱ A<۝>D<٢٦>",
"فَلَN<مَّ>ا R<رَ>أَوۡهُ زُلۡفَةN<ࣰ> سِW<یۤ>ءَتۡ وُجُوهُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> وَقِیلَ هَT<ـٰ>ذَا ٱلَّذِی كُN<ن>تُN<م> بِهِT<ۦ> تَدَّعُJ<و>نَ A<۝>D<٢٧>",
"قُلۡ أَR<رَ>ءَیۡتُمۡ إِنۡ أَهۡلَكَنِیَ ٱللَّهُ وَمَX<ن> N<مَّ>عِیَ أَوۡ R<رَ>حِمَنَا فَمَX<ن> N<یُ>جِیR<رُ> ٱلۡكَT<ـٰ>فِرِینَ مِنۡ عَذَابٍ أَلِJ<ی>مࣲ A<۝>D<٢٨>",
"قُلۡ هُوَ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ ءَامَN<نَّ>ا بِهِT<ۦ> وَعَلَیۡهِ تَوَكَّلۡنَاۖ فَسَتَعۡلَمُونَ مَنۡ هُوَ فِی ضَلَT<ـٰ>لX<ࣲ> N<مُّ>بِJ<ی>نࣲ A<۝>D<٢٩>",
"قُلۡ أَR<رَ>ءَیۡتُمۡ إِنۡ أَصۡبَحَ مَW<اۤ>ؤُكُمۡ غَوۡR<ر>N<ࣰ>ا فَمَX<ن> N<یَ>أۡتِیكُN<م> بِمَW<اۤ>ءX<ࣲ> N<مَّ>عِJ<ی>نِۭ A<۝>D<٣٠>",
"#۞ L<نۤ>ۚ وَX<ٱ>لۡقَلَمِ وَمَا یَسۡطُR<رُ>J<و>نَ A<۝>D<١>",
"مَW<اۤ> أَN<ن>تَ بِنِعۡمَةِ R<رَ>بِّكَ بِمَQ<جۡ>نُJ<و>نࣲ A<۝>D<٢>",
"وَإِN<نَّ> لَكَ لَأَQ<جۡ>R<ر>ًا غَیۡR<رَ> مَمۡنُJ<و>نࣲ A<۝>D<٣>",
"وَإِN<نَّ>كَ لَعَلَىٰ خُلُقٍ عَظِJ<ی>مࣲ A<۝>D<٤>",
"فَسَتُQ<بۡ>صِR<رُ> وَیُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٥>",
"بِأَییِّكُمُ ٱلۡمَفۡتُJ<و>نُ A<۝>D<٦>",
"إِN<نَّ> R<رَ>بَّكَ هُوَ أَعۡلَمُ بِمَN<ن> ضَلَّ عَN<ن> سَبِیلِهِT<ۦ> وَهُوَ أَعۡلَمُ بِX<ٱ>لۡمُهۡتَدِJ<ی>نَ A<۝>D<٧>",
"فَلَا تُطِعِ ٱلۡمُكَذِّبِJ<ی>نَ A<۝>D<٨>",
"وَدُّوX<ا۟> لَوۡ تُQ<دۡ>هِنُ فَیُQ<دۡ>هِنُJ<و>نَ A<۝>D<٩>",
"وَلَا تُطِعۡ كُلَّ حَلَّافX<ࣲ> N<مَّ>هِJ<ی>نٍ A<۝>D<١٠>",
"هَN<مَّ>ازX<ࣲ> N<مَّ>شَّW<اۤ>ءِN<ۭ> بِنَمِJ<ی>مࣲ A<۝>D<١١>",
"مَّN<نَّ>اعX<ࣲ> لِّلۡخَیۡرِ مُعۡتَدٍ أَثِJ<ی>مٍ A<۝>D<١٢>",
"عُتُلِّN<ۭ> بَعۡدَ ذَT< ٰ>لِكَ زَنِJ<ی>مٍ A<۝>D<١٣>",
"أَN<ن> كَانَ ذَا مَالX<ࣲ> N<وَ>بَنِJ<ی>نَ A<۝>D<١٤>",
"إِذَا تُتۡلَىٰ عَلَیۡهِ ءَایَT<ـٰ>تُنَا قَالَ أَسَT<ـٰ>طِیR<رُ> ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٥>",
"سَنَسِمُهُT<ۥ> عَلَى ٱلۡخُR<رۡ>طُJ<و>مِ A<۝>D<١٦>",
"إِN<نَّ>ا بَلَوۡنَT<ـٰ>هُمۡ كَمَا بَلَوۡنَW<اۤ> أَصۡحَT<ـٰ>بَ ٱلۡجَN<نَّ>ةِ إِذۡ أَQ<قۡ>سَمُوX<ا۟> لَیَصۡرِمُN<نَّ>هَا مُصۡبِحِJ<ی>نَ A<۝>D<١٧>",
"وَلَا یَسۡتَثۡنُJ<و>نَ A<۝>D<١٨>",
"فَطَافَ عَلَیۡهَا طَW<اۤ>ئِفX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّكَ وَهُمۡ نَW<اۤ>ئِمُJ<و>نَ A<۝>D<١٩>",
"فَأَصۡبَحَتۡ كَX<ٱ>X<ل>صَّرِJ<ی>مِ A<۝>D<٢٠>",
"فَتَنَادَوۡX<ا۟> مُصۡبِحِJ<ی>نَ A<۝>D<٢١>",
"أَنِ ٱغۡدُوX<ا۟> عَلَىٰ حَR<رۡ>ثِكُمۡ إِN<ن> كُN<ن>تُمۡ صَT<ـٰ>رِمِJ<ی>نَ A<۝>D<٢٢>",
"فَX<ٱ>N<ن>طَلَقُوX<ا۟> وَهُمۡ یَتَخَT<ـٰ>فَتُJ<و>نَ A<۝>D<٢٣>",
"أَX<ن> لَّا یَQ<دۡ>خُلَN<نَّ>هَا ٱلۡیَوۡمَ عَلَیۡكُم N<مِّ>سۡكِJ<ی>نࣱ A<۝>D<٢٤>",
"وَغَدَوۡX<ا۟> عَلَىٰ حَR<رۡ>دN<ࣲ> قَT<ـٰ>دِرِJ<ی>نَ A<۝>D<٢٥>",
"فَلَN<مَّ>ا R<رَ>أَوۡهَا قَالُW<وۤ>X<ا۟> إِN<نَّ>ا لَضَL<اۤ>لُّJ<و>نَ A<۝>D<٢٦>",
"بَلۡ نَحۡنُ مَحۡR<رُ>ومُJ<و>نَ A<۝>D<٢٧>",
"قَالَ أَوۡسَطُهُمۡ أَلَمۡ أَقُل لَّكُمۡ لَوۡلَا تُسَبِّحُJ<و>نَ A<۝>D<٢٨>",
"قَالُوX<ا۟> سُQ<بۡ>حَT<ـٰ>نَ R<رَ>بِّنَW<اۤ> إِN<نَّ>ا كُN<نَّ>ا ظَT<ـٰ>لِمِJ<ی>نَ A<۝>D<٢٩>",
"فَأَQ<قۡ>بَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضX<ࣲ> N<یَ>تَلَT<ـٰ>وَمُJ<و>نَ A<۝>D<٣٠>",
"قَالُوX<ا۟> یَT<ـٰ>وَیۡلَنَW<اۤ> إِN<نَّ>ا كُN<نَّ>ا طَT<ـٰ>غِJ<ی>نَ A<۝>D<٣١>",
"عَسَىٰ R<رَ>بُّنَW<اۤ> أَX<ن> N<یُ>Q<بۡ>دِلَنَا خَیۡR<ر>X<ࣰ>ا N<مِّ>نۡهَW<اۤ> إِN<نَّ>W<اۤ> إِلَىٰ R<رَ>بِّنَا R<رَ>T< ٰ>غِبُJ<و>نَ A<۝>D<٣٢>",
"كَذَT< ٰ>لِكَ ٱلۡعَذَابُۖ وَلَعَذَابُ ٱلۡءَاخِR<رَ>ةِ أَكۡبَR<رُ>ۚ لَوۡ كَانُوX<ا۟> یَعۡلَمُJ<و>نَ A<۝>D<٣٣>",
"إِN<نَّ> لِلۡمُتَّقِینَ عِN<ن>دَ R<رَ>بِّهِمۡ جَN<نَّ>T<ـٰ>تِ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<٣٤>",
"أَفَنَQ<جۡ>عَلُ ٱلۡمُسۡلِمِینَ كَX<ٱ>لۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٣٥>",
"مَا لَكُمۡ كَیۡفَ تَحۡكُمُJ<و>نَ A<۝>D<٣٦>",
"أَمۡ لَكُمۡ كِتَT<ـٰ>بN<ࣱ> فِیهِ تَQ<دۡ>R<رُ>سُJ<و>نَ A<۝>D<٣٧>",
"إِN<نَّ> لَكُمۡ فِیهِ لَمَا تَخَیَّR<رُ>J<و>نَ A<۝>D<٣٨>",
"أَمۡ لَكُمۡ أَیۡمَT<ـٰ>نٌ عَلَیۡنَا بَT<ـٰ>لِغَةٌ إِلَىٰ یَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ إِN<نَّ> لَكُمۡ لَمَا تَحۡكُمُJ<و>نَ A<۝>D<٣٩>",
"سَلۡهُمۡ أَیُّهُN<م> بِذَT< ٰ>لِكَ زَعِJ<ی>مٌ A<۝>D<٤٠>",
"أَمۡ لَهُمۡ شُR<رَ>كَW<اۤ>ءُ فَلۡیَأۡتُوX<ا۟> بِشُR<رَ>كَW<اۤ>ئِهِمۡ إِN<ن> كَانُوX<ا۟> صَT<ـٰ>دِقِJ<ی>نَ A<۝>D<٤١>",
"یَوۡمَ یُكۡشَفُ عَN<ن> سَاقX<ࣲ> N<وَ>یُQ<دۡ>عَوۡنَ إِلَى ٱX<ل>سُّجُودِ فَلَا یَسۡتَطِیعُJ<و>نَ A<۝>D<٤٢>",
"خَT<ـٰ>شِعَةً أَQ<بۡ>صَT<ـٰ>R<رُ>هُمۡ تَR<رۡ>هَقُهُمۡ ذِلَّةX<ࣱ>ۖ N<وَ>قَQ<دۡ> كَانُوX<ا۟> یُQ<دۡ>عَوۡنَ إِلَى ٱX<ل>سُّجُودِ وَهُمۡ سَT<ـٰ>لِمُJ<و>نَ A<۝>D<٤٣>",
"فَذَR<رۡ>نِی وَمَX<ن> N<یُ>كَذِّبُ بِهَT<ـٰ>ذَا ٱلۡحَدِیثِۖ سَنَسۡتَQ<دۡ>رِجُهُم N<مِّ>نۡ حَیۡثُ لَا یَعۡلَمُJ<و>نَ A<۝>D<٤٤>",
"وَأُمۡلِی لَهُمۡۚ إِN<نَّ> كَیۡدِی مَتِJ<ی>نٌ A<۝>D<٤٥>",
"أَمۡ تَسۡءَلُهُمۡ أَQ<جۡ>R<ر>N<ࣰ>ا فَهُم N<مِّ>X<ن> N<مَّ>غۡR<رَ>مX<ࣲ> N<مُّ>ثۡقَلُJ<و>نَ A<۝>D<٤٦>",
"أَمۡ عِN<ن>دَهُمُ ٱلۡغَیۡبُ فَهُمۡ یَكۡتُبُJ<و>نَ A<۝>D<٤٧>",
"فَX<ٱ>صۡبِرۡ لِحُكۡمِ R<رَ>بِّكَ وَلَا تَكُN<ن> كَصَاحِبِ ٱلۡحُوتِ إِذۡ نَادَىٰ وَهُوَ مَكۡظُJ<و>مࣱ A<۝>D<٤٨>",
"لَّوۡلَW<اۤ> أَN<ن> تَدَT< ٰ>R<رَ>كَهُT<ۥ> نِعۡمَةX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّهِT<ۦ> لَنُبِذَ بِX<ٱ>لۡعَR<رَ>W<اۤ>ءِ وَهُوَ مَذۡمُJ<و>مࣱ A<۝>D<٤٩>",
"فَX<ٱ>Q<جۡ>تَبَT<ـٰ>هُ R<رَ>بُّهُT<ۥ> فَجَعَلَهُT<ۥ> مِنَ ٱX<ل>صَّT<ـٰ>لِحِJ<ی>نَ A<۝>D<٥٠>",
"وَإِX<ن> N<یَ>كَادُ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> لَیُزۡلِقُونَكَ بِأَQ<بۡ>صَT<ـٰ>رِهِمۡ لَN<مَّ>ا سَمِعُوX<ا۟> ٱX<ل>ذِّكۡR<رَ> وَیَقُولُونَ إِN<نَّ>هُT<ۥ> لَمَQ<جۡ>نُJ<و>نࣱ A<۝>D<٥١>",
"وَمَا هُوَ إِلَّا ذِكۡR<ر>X<ࣱ> لِّلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٥٢>",
"#۞ ٱلۡحَL<اۤ>قَّةُ A<۝>D<١>",
"مَا ٱلۡحَL<اۤ>قَّةُ A<۝>D<٢>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا ٱلۡحَL<اۤ>قَّةُ A<۝>D<٣>",
"كَذَّبَتۡ ثَمُودُ وَعَادُN<ۢ> بِX<ٱ>لۡقَارِعَةِ A<۝>D<٤>",
"فَأَN<مَّ>ا ثَمُودُ فَأُهۡلِكُوX<ا۟> بِX<ٱ>X<ل>طَّاغِیَةِ A<۝>D<٥>",
"وَأَN<مَّ>ا عَادN<ࣱ> فَأُهۡلِكُوX<ا۟> بِرِیحN<ࣲ> صَR<رۡ>صَرٍ عَاتِیَةࣲ A<۝>D<٦>",
"سَخَّR<رَ>هَا عَلَیۡهِمۡ سَQ<بۡ>عَ لَیَالX<ࣲ> N<وَ>ثَمَT<ـٰ>نِیَةَ أَیَّامٍ حُسُومN<ࣰ>اۖ فَتَR<رَ>ى ٱلۡقَوۡمَ فِیهَا صَR<رۡ>عَىٰ كَأَN<نَّ>هُمۡ أَعۡجَازُ نَخۡلٍ خَاوِیَةࣲ A<۝>D<٧>",
"فَهَلۡ تَR<رَ>ىٰ لَهُم N<مِّ>X<ن>N<ۢ> بَاقِیَةࣲ A<۝>D<٨>",
"وَجَW<اۤ>ءَ فِرۡعَوۡنُ وَمَN<ن> قَQ<بۡ>لَهُT<ۥ> وَX<ٱ>لۡمُؤۡتَفِكَT<ـٰ>تُ بِX<ٱ>لۡخَاطِئَةِ A<۝>D<٩>",
"فَعَصَوۡX<ا۟> R<رَ>سُولَ R<رَ>بِّهِمۡ فَأَخَذَهُمۡ أَخۡذَةX<ࣰ> R<رَّ>ابِیَةً A<۝>D<١٠>",
"إِN<نَّ>ا لَN<مَّ>ا طَغَا ٱلۡمَW<اۤ>ءُ حَمَلۡنَT<ـٰ>كُمۡ فِی ٱلۡجَارِیَةِ A<۝>D<١١>",
"لِنَQ<جۡ>عَلَهَا لَكُمۡ تَذۡكِR<رَ>ةX<ࣰ> N<وَ>تَعِیَهَW<اۤ> أُذُنX<ࣱ> N<وَ>T< ٰ>عِیَةࣱ A<۝>D<١٢>",
"فَإِذَا نُفِخَ فِی ٱX<ل>صُّورِ نَفۡخَةX<ࣱ> N<وَ>T< ٰ>حِدَةࣱ A<۝>D<١٣>",
"وَحُمِلَتِ ٱلۡأَR<رۡ>ضُ وَX<ٱ>لۡجِبَالُ فَدُكَّتَا دَكَّةX<ࣰ> N<وَ>T< ٰ>حِدَةࣰ A<۝>D<١٤>",
"فَیَوۡمَئِذX<ࣲ> N<وَ>قَعَتِ ٱلۡوَاقِعَةُ A<۝>D<١٥>",
"وَX<ٱ>N<ن>شَقَّتِ ٱX<ل>سَّمَW<اۤ>ءُ فَهِیَ یَوۡمَئِذX<ࣲ> N<وَ>اهِیَةࣱ A<۝>D<١٦>",
"وَX<ٱ>لۡمَلَكُ عَلَىW<ٰۤ> أَR<رۡ>جَW<اۤ>ئِهَاۚ وَیَحۡمِلُ عَR<رۡ>شَ R<رَ>بِّكَ فَوۡقَهُمۡ یَوۡمَئِذN<ࣲ> ثَمَT<ـٰ>نِیَةࣱ A<۝>D<١٧>",
"یَوۡمَئِذN<ࣲ> تُعۡR<رَ>ضُونَ لَا تَخۡفَىٰ مِN<ن>كُمۡ خَافِیَةࣱ A<۝>D<١٨>",
"فَأَN<مَّ>ا مَنۡ أُوتِیَ كِتَT<ـٰ>بَهُT<ۥ> بِیَمِینِهِT<ۦ> فَیَقُولُ هَW<اۤ>ؤُمُ ٱQ<قۡ>R<رَ>ءُوX<ا۟> كِتَT<ـٰ>بِیَهۡ A<۝>D<١٩>",
"إِN<نِّ>ی ظَنَN<ن>تُ أَN<نِّ>ی مُلَT<ـٰ>قٍ حِسَابِیَهۡ A<۝>D<٢٠>",
"فَهُوَ فِی عِیشَةX<ࣲ> R<رَّ>اضِیَةࣲ A<۝>D<٢١>",
"فِی جَN<نَّ>ةٍ عَالِیَةࣲ A<۝>D<٢٢>",
"قُطُوفُهَا دَانِیَةࣱ A<۝>D<٢٣>",
"كُلُوX<ا۟> وَX<ٱ>شۡR<رَ>بُوX<ا۟> هَنِW<یۤ>ءَN<ۢ>ا بِمَW<اۤ> أَسۡلَفۡتُمۡ فِی ٱلۡأَیَّامِ ٱلۡخَالِیَةِ A<۝>D<٢٤>",
"وَأَN<مَّ>ا مَنۡ أُوتِیَ كِتَT<ـٰ>بَهُT<ۥ> بِشِمَالِهِT<ۦ> فَیَقُولُ یَT<ـٰ>لَیۡتَنِی لَمۡ أُوتَ كِتَT<ـٰ>بِیَهۡ A<۝>D<٢٥>",
"وَلَمۡ أَQ<دۡ>رِ مَا حِسَابِیَهۡ A<۝>D<٢٦>",
"یَT<ـٰ>لَیۡتَهَا كَانَتِ ٱلۡقَاضِیَةَ A<۝>D<٢٧>",
"مَW<اۤ> أَغۡنَىٰ عَN<نِّ>ی مَالِیَهۡۜ A<۝>D<٢٨>",
"هَلَكَ عَN<نِّ>ی سُلۡطَT<ـٰ>نِیَهۡ A<۝>D<٢٩>",
"خُذُوهُ فَغُلُّJ<و>هُ A<۝>D<٣٠>",
"ثُN<مَّ> ٱلۡجَحِیمَ صَلُّJ<و>هُ A<۝>D<٣١>",
"ثُN<مَّ> فِی سِلۡسِلَةN<ࣲ> ذَR<رۡ>عُهَا سَQ<بۡ>عُونَ ذِR<رَ>اعN<ࣰ>ا فَX<ٱ>سۡلُكُJ<و>هُ A<۝>D<٣٢>",
"إِN<نَّ>هُT<ۥ> كَانَ لَا یُؤۡمِنُ بِٱللَّهِ ٱلۡعَظِJ<ی>مِ A<۝>D<٣٣>",
"وَلَا یَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِJ<ی>نِ A<۝>D<٣٤>",
"فَلَیۡسَ لَهُ ٱلۡیَوۡمَ هَT<ـٰ>هُنَا حَمِJ<ی>مࣱ A<۝>D<٣٥>",
"وَلَا طَعَامٌ إِلَّا مِنۡ غِسۡلِJ<ی>نࣲ A<۝>D<٣٦>",
"لَّا یَأۡكُلُهُW<ۥۤ> إِلَّا ٱلۡخَT<ـٰ>طِءُJ<و>نَ A<۝>D<٣٧>",
"فَلَW<اۤ> أُQ<قۡ>سِمُ بِمَا تُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٣٨>",
"وَمَا لَا تُQ<بۡ>صِR<رُ>J<و>نَ A<۝>D<٣٩>",
"إِN<نَّ>هُT<ۥ> لَقَوۡلُ R<رَ>سُولN<ࣲ> كَرِJ<ی>مࣲ A<۝>D<٤٠>",
"وَمَا هُوَ بِقَوۡلِ شَاعِرN<ࣲ>ۚ قَلِیلX<ࣰ>ا N<مَّ>ا تُؤۡمِنُJ<و>نَ A<۝>D<٤١>",
"وَلَا بِقَوۡلِ كَاهِنN<ࣲ>ۚ قَلِیلX<ࣰ>ا N<مَّ>ا تَذَكَّR<رُ>J<و>نَ A<۝>D<٤٢>",
"تَN<ن>زِیلX<ࣱ> N<مِّ>X<ن> R<رَّ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٤٣>",
"وَلَوۡ تَقَوَّلَ عَلَیۡنَا بَعۡضَ ٱلۡأَقَاوِJ<ی>لِ A<۝>D<٤٤>",
"لَأَخَذۡنَا مِنۡهُ بِX<ٱ>لۡیَمِJ<ی>نِ A<۝>D<٤٥>",
"ثُN<مَّ> لَقَطَعۡنَا مِنۡهُ ٱلۡوَتِJ<ی>نَ A<۝>D<٤٦>",
"فَمَا مِN<ن>كُم N<مِّ>نۡ أَحَدٍ عَنۡهُ حَT<ـٰ>جِزِJ<ی>نَ A<۝>D<٤٧>",
"وَإِN<نَّ>هُT<ۥ> لَتَذۡكِR<رَ>ةX<ࣱ> لِّلۡمُتَّقِJ<ی>نَ A<۝>D<٤٨>",
"وَإِN<نَّ>ا لَنَعۡلَمُ أَN<نَّ> مِN<ن>كُم N<مُّ>كَذِّبِJ<ی>نَ A<۝>D<٤٩>",
"وَإِN<نَّ>هُT<ۥ> لَحَسۡR<رَ>ةٌ عَلَى ٱلۡكَT<ـٰ>فِرِJ<ی>نَ A<۝>D<٥٠>",
"وَإِN<نَّ>هُT<ۥ> لَحَقُّ ٱلۡیَقِJ<ی>نِ A<۝>D<٥١>",
"فَسَبِّحۡ بِX<ٱ>سۡمِ R<رَ>بِّكَ ٱلۡعَظِJ<ی>مِ A<۝>D<٥٢>",
"#سَأَلَ سَW<اۤ>ئِلُN<ۢ> بِعَذَابX<ࣲ> N<وَ>اقِعࣲ A<۝>D<١>",
"لِّلۡكَT<ـٰ>فِرِینَ لَیۡسَ لَهُT<ۥ> دَافِعࣱ A<۝>D<٢>",
"مِّنَ ٱللَّهِ ذِی ٱلۡمَعَارِQ<ج>ِ A<۝>D<٣>",
"تَعۡR<رُ>جُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ وَX<ٱ>X<ل>R<رُّ>وحُ إِلَیۡهِ فِی یَوۡمN<ࣲ> كَانَ مِQ<قۡ>دَاR<رُ>هُT<ۥ> خَمۡسِینَ أَلۡفَ سَنَةࣲ A<۝>D<٤>",
"فَX<ٱ>صۡبِرۡ صَQ<بۡ>R<ر>N<ࣰ>ا جَمِیلًا A<۝>D<٥>",
"إِN<نَّ>هُمۡ یَR<رَ>وۡنَهُT<ۥ> بَعِیدࣰا A<۝>D<٦>",
"وَنَR<رَ>X<ى>T<ٰ>هُ قَرِیبࣰا A<۝>D<٧>",
"یَوۡمَ تَكُونُ ٱX<ل>سَّمَW<اۤ>ءُ كَX<ٱ>لۡمُهۡلِ A<۝>D<٨>",
"وَتَكُونُ ٱلۡجِبَالُ كَX<ٱ>لۡعِهۡنِ A<۝>D<٩>",
"وَلَا یَسۡءَلُ حَمِیمٌ حَمِیمࣰا A<۝>D<١٠>",
"یُبَصَّR<رُ>ونَهُمۡۚ یَوَدُّ ٱلۡمُQ<جۡ>رِمُ لَوۡ یَفۡتَدِی مِنۡ عَذَابِ یَوۡمِئِذِN<ۭ> بِبَنِJ<ی>هِ A<۝>D<١١>",
"وَصَT<ـٰ>حِبَتِهِT<ۦ> وَأَخِJ<ی>هِ A<۝>D<١٢>",
"وَفَصِیلَتِهِ ٱلَّتِی تُءۡوِJ<ی>هِ A<۝>D<١٣>",
"وَمَN<ن> فِی ٱلۡأَR<رۡ>ضِ جَمِیعN<ࣰ>ا ثُN<مَّ> یُN<ن>جِJ<ی>هِ A<۝>D<١٤>",
"كَلَّW<اۤ>ۖ إِN<نَّ>هَا لَظَىٰ A<۝>D<١٥>",
"نَزَّاعَةX<ࣰ> لِّX<ل>شَّوَىٰ A<۝>D<١٦>",
"تَQ<دۡ>عُوX<ا۟> مَنۡ أَQ<دۡ>بَR<رَ> وَتَوَلَّىٰ A<۝>D<١٧>",
"وَجَمَعَ فَأَوۡعَىٰۤ A<۝>D<١٨>",
"۞ إِN<نَّ> ٱلۡإِN<ن>سَT<ـٰ>نَ خُلِقَ هَلُوعًا A<۝>D<١٩>",
"إِذَا مَسَّهُ ٱX<ل>شَّR<رُّ> جَزُوعࣰا A<۝>D<٢٠>",
"وَإِذَا مَسَّهُ ٱلۡخَیۡR<رُ> مَنُوعًا A<۝>D<٢١>",
"إِلَّا ٱلۡمُصَلِّJ<ی>نَ A<۝>D<٢٢>",
"ٱلَّذِینَ هُمۡ عَلَىٰ صَلَاتِهِمۡ دَW<اۤ>ئِمُJ<و>نَ A<۝>D<٢٣>",
"وَX<ٱ>لَّذِینَ فِW<یۤ> أَمۡوَT< ٰ>لِهِمۡ حَقّX<ࣱ> N<مَّ>عۡلُJ<و>مࣱ A<۝>D<٢٤>",
"لِّX<ل>سَّW<اۤ>ئِلِ وَX<ٱ>لۡمَحۡR<رُ>J<و>مِ A<۝>D<٢٥>",
"وَX<ٱ>لَّذِینَ یُصَدِّقُونَ بِیَوۡمِ ٱX<ل>دِّJ<ی>نِ A<۝>D<٢٦>",
"وَX<ٱ>لَّذِینَ هُم N<مِّ>نۡ عَذَابِ R<رَ>بِّهِم N<مُّ>شۡفِقُJ<و>نَ A<۝>D<٢٧>",
"إِN<نَّ> عَذَابَ R<رَ>بِّهِمۡ غَیۡR<رُ> مَأۡمُJ<و>نࣲ A<۝>D<٢٨>",
"وَX<ٱ>لَّذِینَ هُمۡ لِفُR<رُ>وجِهِمۡ حَT<ـٰ>فِظُJ<و>نَ A<۝>D<٢٩>",
"إِلَّا عَلَىW<ٰۤ> أَزۡوَT< ٰ>جِهِمۡ أَوۡ مَا مَلَكَتۡ أَیۡمَT<ـٰ>نُهُمۡ فَإِN<نَّ>هُمۡ غَیۡR<رُ> مَلُومِJ<ی>نَ A<۝>D<٣٠>",
"فَمَنِ ٱQ<بۡ>تَغَىٰ وَR<رَ>W<اۤ>ءَ ذَT< ٰ>لِكَ فَأُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡعَادُJ<و>نَ A<۝>D<٣١>",
"وَX<ٱ>لَّذِینَ هُمۡ لِأَمَT<ـٰ>نَT<ـٰ>تِهِمۡ وَعَهۡدِهِمۡ R<رَ>T< ٰ>عُJ<و>نَ A<۝>D<٣٢>",
"وَX<ٱ>لَّذِینَ هُN<م> بِشَهَT<ـٰ>دَT< ٰ>تِهِمۡ قَW<اۤ>ئِمُJ<و>نَ A<۝>D<٣٣>",
"وَX<ٱ>لَّذِینَ هُمۡ عَلَىٰ صَلَاتِهِمۡ یُحَافِظُJ<و>نَ A<۝>D<٣٤>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ فِی جَN<نَّ>T<ـٰ>تX<ࣲ> N<مُّ>كۡR<رَ>مُJ<و>نَ A<۝>D<٣٥>",
"فَمَالِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> قِبَلَكَ مُهۡطِعِJ<ی>نَ A<۝>D<٣٦>",
"عَنِ ٱلۡیَمِینِ وَعَنِ ٱX<ل>شِّمَالِ عِزِJ<ی>نَ A<۝>D<٣٧>",
"أَیَQ<طۡ>مَعُ كُلُّ ٱمۡرِئX<ࣲ> N<مِّ>نۡهُمۡ أَX<ن> N<یُ>Q<دۡ>خَلَ جَN<نَّ>ةَ نَعِJ<ی>مࣲ A<۝>D<٣٨>",
"كَلَّW<اۤ>ۖ إِN<نَّ>ا خَلَQ<قۡ>نَT<ـٰ>هُم N<مِّ>N<مَّ>ا یَعۡلَمُJ<و>نَ A<۝>D<٣٩>",
"فَلَW<اۤ> أُQ<قۡ>سِمُ بِR<رَ>بِّ ٱلۡمَشَT<ـٰ>رِقِ وَX<ٱ>لۡمَغَT<ـٰ>رِبِ إِN<نَّ>ا لَقَT<ـٰ>دِR<رُ>J<و>نَ A<۝>D<٤٠>",
"عَلَىW<ٰۤ> أَن N<نُّ>بَدِّلَ خَیۡR<ر>X<ࣰ>ا N<مِّ>نۡهُمۡ وَمَا نَحۡنُ بِمَسۡبُوقِJ<ی>نَ A<۝>D<٤١>",
"فَذَR<رۡ>هُمۡ یَخُوضُوX<ا۟> وَیَلۡعَبُوX<ا۟> حَتَّىٰ یُلَT<ـٰ>قُوX<ا۟> یَوۡمَهُمُ ٱلَّذِی یُوعَدُJ<و>نَ A<۝>D<٤٢>",
"یَوۡمَ یَخۡR<رُ>جُونَ مِنَ ٱلۡأَQ<جۡ>دَاثِ سِR<رَ>اعN<ࣰ>ا كَأَN<نَّ>هُمۡ إِلَىٰ نُصُبX<ࣲ> N<یُ>وفِضُJ<و>نَ A<۝>D<٤٣>",
"خَT<ـٰ>شِعَةً أَQ<بۡ>صَT<ـٰ>R<رُ>هُمۡ تَR<رۡ>هَقُهُمۡ ذِلَّةN<ࣱ>ۚ ذَT< ٰ>لِكَ ٱلۡیَوۡمُ ٱلَّذِی كَانُوX<ا۟> یُوعَدُJ<و>نَ A<۝>D<٤٤>",
"#إِN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَا نُوحًا إِلَىٰ قَوۡمِهِW<ۦۤ> أَنۡ أَN<ن>ذِرۡ قَوۡمَكَ مِN<ن> قَQ<بۡ>لِ أَX<ن> N<یَ>أۡتِیَهُمۡ عَذَابٌ أَلِJ<ی>مࣱ A<۝>D<١>",
"قَالَ یَT<ـٰ>قَوۡمِ إِN<نِّ>ی لَكُمۡ نَذِیR<ر>X<ࣱ> N<مُّ>بِJ<ی>نٌ A<۝>D<٢>",
"أَنِ ٱعۡبُدُوX<ا۟> ٱللَّهَ وَX<ٱ>تَّقُوهُ وَأَطِیعُJ<و>نِ A<۝>D<٣>",
"یَغۡفِرۡ لَكُم N<مِّ>N<ن> ذُنُوبِكُمۡ وَیُؤَخِّرۡكُمۡ إِلَىW<ٰۤ> أَجَلX<ࣲ> N<مُّ>سَN<مّ>ًىۚ إِN<نَّ> أَجَلَ ٱللَّهِ إِذَا جَW<اۤ>ءَ لَا یُؤَخَّR<رُ>ۚ لَوۡ كُN<ن>تُمۡ تَعۡلَمُJ<و>نَ A<۝>D<٤>",
"قَالَ R<رَ>بِّ إِN<نِّ>ی دَعَوۡتُ قَوۡمِی لَیۡلX<ࣰ>ا N<وَ>نَهَاR<ر>ࣰا A<۝>D<٥>",
"فَلَمۡ یَزِQ<دۡ>هُمۡ دُعَW<اۤ>ءِW<یۤ> إِلَّا فِR<رَ>اR<ر>ࣰا A<۝>D<٦>",
"وَإِN<نِّ>ی كُلَّمَا دَعَوۡتُهُمۡ لِتَغۡفِR<رَ> لَهُمۡ جَعَلُW<وۤ>X<ا۟> أَصَT<ـٰ>بِعَهُمۡ فِW<یۤ> ءَاذَانِهِمۡ وَX<ٱ>سۡتَغۡشَوۡX<ا۟> ثِیَابَهُمۡ وَأَصَR<رُّ>وX<ا۟> وَX<ٱ>سۡتَكۡبَR<رُ>وX<ا۟> ٱسۡتِكۡبَاR<ر>ࣰا A<۝>D<٧>",
"ثُN<مَّ> إِN<نِّ>ی دَعَوۡتُهُمۡ جِهَاR<ر>ࣰا A<۝>D<٨>",
"ثُN<مَّ> إِN<نِّ>W<یۤ> أَعۡلَN<ن>تُ لَهُمۡ وَأَسۡR<رَ>R<رۡ>تُ لَهُمۡ إِسۡR<رَ>اR<ر>ࣰا A<۝>D<٩>",
"فَقُلۡتُ ٱسۡتَغۡفِR<رُ>وX<ا۟> R<رَ>بَّكُمۡ إِN<نَّ>هُT<ۥ> كَانَ غَفَّاR<ر>ࣰا A<۝>D<١٠>",
"یُR<رۡ>سِلِ ٱX<ل>سَّمَW<اۤ>ءَ عَلَیۡكُم N<مِّ>Q<دۡ>R<رَ>اR<ر>ࣰا A<۝>D<١١>",
"وَیُمۡدِQ<دۡ>كُN<م> بِأَمۡوَT< ٰ>لX<ࣲ> N<وَ>بَنِینَ وَیَQ<جۡ>عَل لَّكُمۡ جَN<نَّ>T<ـٰ>تX<ࣲ> N<وَ>یَQ<جۡ>عَل لَّكُمۡ أَنۡهَT<ـٰ>R<ر>ࣰا A<۝>D<١٢>",
"مَّا لَكُمۡ لَا تَR<رۡ>جُونَ لِلَّهِ وَقَاR<ر>ࣰا A<۝>D<١٣>",
"وَقَQ<دۡ> خَلَقَكُمۡ أَQ<طۡ>وَاR<ر>ًا A<۝>D<١٤>",
"أَلَمۡ تَR<رَ>وۡX<ا۟> كَیۡفَ خَلَقَ ٱللَّهُ سَQ<بۡ>عَ سَمَT<ـٰ>وَT< ٰ>تN<ࣲ> طِبَاقࣰا A<۝>D<١٥>",
"وَجَعَلَ ٱلۡقَمَR<رَ> فِیهِN<نَّ> نُوR<ر>X<ࣰ>ا N<وَ>جَعَلَ ٱX<ل>شَّمۡسَ سِR<رَ>اجࣰا A<۝>D<١٦>",
"وَٱللَّهُ أَX<ن>N<ۢ>بَتَكُم N<مِّ>نَ ٱلۡأَR<رۡ>ضِ نَبَاتࣰا A<۝>D<١٧>",
"ثُN<مَّ> یُعِیدُكُمۡ فِیهَا وَیُخۡرِجُكُمۡ إِخۡR<رَ>اجࣰا A<۝>D<١٨>",
"وَٱللَّهُ جَعَلَ لَكُمُ ٱلۡأَR<رۡ>ضَ بِسَاطࣰا A<۝>D<١٩>",
"لِّتَسۡلُكُوX<ا۟> مِنۡهَا سُبُلN<ࣰ>ا فِجَاجࣰا A<۝>D<٢٠>",
"قَالَ نُوحX<ࣱ> R<رَّ>بِّ إِN<نَّ>هُمۡ عَصَوۡنِی وَX<ٱ>تَّبَعُوX<ا۟> مَX<ن> لَّمۡ یَزِQ<دۡ>هُ مَالُهُT<ۥ> وَوَلَدُهُW<ۥۤ> إِلَّا خَسَاR<ر>ࣰا A<۝>D<٢١>",
"وَمَكَR<رُ>وX<ا۟> مَكۡR<ر>N<ࣰ>ا كُبَّاR<ر>ࣰا A<۝>D<٢٢>",
"وَقَالُوX<ا۟> لَا تَذَR<رُ>N<نَّ> ءَالِهَتَكُمۡ وَلَا تَذَR<رُ>N<نَّ> وَدّX<ࣰ>ا N<وَ>لَا سُوَاعX<ࣰ>ا N<وَ>لَا یَغُوثَ وَیَعُوقَ وَنَسۡR<ر>ࣰا A<۝>D<٢٣>",
"وَقَQ<دۡ> أَضَلُّوX<ا۟> كَثِیR<ر>X<ࣰ>اۖ N<وَ>لَا تَزِدِ ٱX<ل>ظَّT<ـٰ>لِمِینَ إِلَّا ضَلَT<ـٰ>لࣰا A<۝>D<٢٤>",
"مِّN<مَّ>ا خَطِW<یۤ>ءَT<ـٰ>تِهِمۡ أُغۡرِقُوX<ا۟> فَأُQ<دۡ>خِلُوX<ا۟> نَاR<ر>N<ࣰ>ا فَلَمۡ یَجِدُوX<ا۟> لَهُم N<مِّ>N<ن> دُونِ ٱللَّهِ أَN<ن>صَاR<ر>ࣰا A<۝>D<٢٥>",
"وَقَالَ نُوحX<ࣱ> R<رَّ>بِّ لَا تَذَR<رۡ> عَلَى ٱلۡأَR<رۡ>ضِ مِنَ ٱلۡكَT<ـٰ>فِرِینَ دَیَّاR<ر>ًا A<۝>D<٢٦>",
"إِN<نَّ>كَ إِN<ن> تَذَR<رۡ>هُمۡ یُضِلُّوX<ا۟> عِبَادَكَ وَلَا یَلِدُW<وۤ>X<ا۟> إِلَّا فَاجِR<ر>N<ࣰ>ا كَفَّاR<ر>ࣰا A<۝>D<٢٧>",
"R<رَّ>بِّ ٱغۡفِرۡ لِی وَلِوَT< ٰ>لِدَیَّ وَلِمَN<ن> دَخَلَ بَیۡتِیَ مُؤۡمِنX<ࣰ>ا N<وَ>لِلۡمُؤۡمِنِینَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تِۖ وَلَا تَزِدِ ٱX<ل>ظَّT<ـٰ>لِمِینَ إِلَّا تَبَاR<رَ>ۢا A<۝>D<٢٨>",
"#۞ قُلۡ أُوحِیَ إِلَیَّ أَN<نَّ>هُ ٱسۡتَمَعَ نَفَR<ر>X<ࣱ> N<مِّ>نَ ٱلۡجِN<نِّ> فَقَالُW<وۤ>X<ا۟> إِN<نَّ>ا سَمِعۡنَا قُR<رۡ>ءَانًا عَجَبࣰا A<۝>D<١>",
"یَهۡدِW<یۤ> إِلَى ٱX<ل>R<رُّ>شۡدِ فَءَامَN<نَّ>ا بِهِT<ۦ>ۖ وَلَن N<نُّ>شۡرِكَ بِR<رَ>بِّنَW<اۤ> أَحَدࣰا A<۝>D<٢>",
"وَأَN<نَّ>هُT<ۥ> تَعَT<ـٰ>لَىٰ جَدُّ R<رَ>بِّنَا مَا ٱتَّخَذَ صَT<ـٰ>حِبَةX<ࣰ> N<وَ>لَا وَلَدࣰا A<۝>D<٣>",
"وَأَN<نَّ>هُT<ۥ> كَانَ یَقُولُ سَفِیهُنَا عَلَى ٱللَّهِ شَطَطࣰا A<۝>D<٤>",
"وَأَN<نَّ>ا ظَنَN<نَّ>W<اۤ> أَX<ن> لَّN<ن> تَقُولَ ٱلۡإِN<ن>سُ وَX<ٱ>لۡجِN<نُّ> عَلَى ٱللَّهِ كَذِبࣰا A<۝>D<٥>",
"وَأَN<نَّ>هُT<ۥ> كَانَ رِجَالX<ࣱ> N<مِّ>نَ ٱلۡإِN<ن>سِ یَعُوذُونَ بِرِجَالX<ࣲ> N<مِّ>نَ ٱلۡجِN<نِّ> فَزَادُوهُمۡ R<رَ>هَقࣰا A<۝>D<٦>",
"وَأَN<نَّ>هُمۡ ظَN<نُّ>وX<ا۟> كَمَا ظَنَN<ن>تُمۡ أَX<ن> لَّX<ن> N<یَ>Q<بۡ>عَثَ ٱللَّهُ أَحَدࣰا A<۝>D<٧>",
"وَأَN<نَّ>ا لَمَسۡنَا ٱX<ل>سَّمَW<اۤ>ءَ فَوَجَQ<دۡ>نَT<ـٰ>هَا مُلِئَتۡ حَR<رَ>سN<ࣰ>ا شَدِیدX<ࣰ>ا N<وَ>شُهُبࣰا A<۝>D<٨>",
"وَأَN<نَّ>ا كُN<نَّ>ا نَQ<قۡ>عُدُ مِنۡهَا مَقَT<ـٰ>عِدَ لِX<ل>سَّمۡعِۖ فَمَX<ن> N<یَ>سۡتَمِعِ ٱلۡءَانَ یَجِQ<دۡ> لَهُT<ۥ> شِهَابX<ࣰ>ا R<رَّ>صَدࣰا A<۝>D<٩>",
"وَأَN<نَّ>ا لَا نَQ<دۡ>رِW<یۤ> أَشَR<رّ>ٌ أُرِیدَ بِمَN<ن> فِی ٱلۡأَR<رۡ>ضِ أَمۡ أَR<رَ>ادَ بِهِمۡ R<رَ>بُّهُمۡ R<رَ>شَدࣰا A<۝>D<١٠>",
"وَأَN<نَّ>ا مِN<نَّ>ا ٱX<ل>صَّT<ـٰ>لِحُونَ وَمِN<نَّ>ا دُونَ ذَT< ٰ>لِكَۖ كُN<نَّ>ا طَR<رَ>W<اۤ>ئِقَ قِدَدࣰا A<۝>D<١١>",
"وَأَN<نَّ>ا ظَنَN<نَّ>W<اۤ> أَX<ن> لَّن N<نُّ>عۡجِزَ ٱللَّهَ فِی ٱلۡأَR<رۡ>ضِ وَلَن N<نُّ>عۡجِزَهُT<ۥ> هَR<رَ>بࣰا A<۝>D<١٢>",
"وَأَN<نَّ>ا لَN<مَّ>ا سَمِعۡنَا ٱلۡهُدَىW<ٰۤ> ءَامَN<نَّ>ا بِهِT<ۦ>ۖ فَمَX<ن> N<یُ>ؤۡمِX<ن>N<ۢ> بِR<رَ>بِّهِT<ۦ> فَلَا یَخَافُ بَخۡسX<ࣰ>ا N<وَ>لَا R<رَ>هَقࣰا A<۝>D<١٣>",
"وَأَN<نَّ>ا مِN<نَّ>ا ٱلۡمُسۡلِمُونَ وَمِN<نَّ>ا ٱلۡقَT<ـٰ>سِطُونَۖ فَمَنۡ أَسۡلَمَ فَأُX<و۟>لَW<ـٰۤ>ئِكَ تَحَR<رَّ>وۡX<ا۟> R<رَ>شَدࣰا A<۝>D<١٤>",
"وَأَN<مَّ>ا ٱلۡقَT<ـٰ>سِطُونَ فَكَانُوX<ا۟> لِجَهَN<نَّ>مَ حَطَبࣰا A<۝>D<١٥>",
"وَأَلَّوِ ٱسۡتَقَT<ـٰ>مُوX<ا۟> عَلَى ٱX<ل>طَّرِیقَةِ لَأَسۡقَیۡنَT<ـٰ>هُم N<مَّ>W<اۤ>ءً غَدَقࣰا A<۝>D<١٦>",
"لِّنَفۡتِنَهُمۡ فِیهِۚ وَمَX<ن> N<یُ>عۡرِضۡ عَN<ن> ذِكۡرِ R<رَ>بِّهِT<ۦ> یَسۡلُكۡهُ عَذَابN<ࣰ>ا صَعَدࣰا A<۝>D<١٧>",
"وَأَN<نَّ> ٱلۡمَسَT<ـٰ>جِدَ لِلَّهِ فَلَا تَQ<دۡ>عُوX<ا۟> مَعَ ٱللَّهِ أَحَدࣰا A<۝>D<١٨>",
"وَأَN<نَّ>هُT<ۥ> لَN<مَّ>ا قَامَ عَQ<بۡ>دُ ٱللَّهِ یَQ<دۡ>عُوهُ كَادُوX<ا۟> یَكُونُونَ عَلَیۡهِ لِبَدࣰا A<۝>D<١٩>",
"قُلۡ إِN<نَّ>مَW<اۤ> أَQ<دۡ>عُوX<ا۟> R<رَ>بِّی وَلَW<اۤ> أُشۡرِكُ بِهِW<ۦۤ> أَحَدࣰا A<۝>D<٢٠>",
"قُلۡ إِN<نِّ>ی لَW<اۤ> أَمۡلِكُ لَكُمۡ ضَR<رّ>X<ࣰ>ا N<وَ>لَا R<رَ>شَدࣰا A<۝>D<٢١>",
"قُلۡ إِN<نِّ>ی لَX<ن> N<یُ>جِیR<رَ>نِی مِنَ ٱللَّهِ أَحَدX<ࣱ> N<وَ>لَنۡ أَجِدَ مِN<ن> دُونِهِT<ۦ> مُلۡتَحَدًا A<۝>D<٢٢>",
"إِلَّا بَلَT<ـٰ>غX<ࣰ>ا N<مِّ>نَ ٱللَّهِ وَرِسَT<ـٰ>لَT<ـٰ>تِهِT<ۦ>ۚ وَمَX<ن> N<یَ>عۡصِ ٱللَّهَ وَR<رَ>سُولَهُT<ۥ> فَإِN<نَّ> لَهُT<ۥ> نَاR<رَ> جَهَN<نَّ>مَ خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدًا A<۝>D<٢٣>",
"حَتَّىW<ٰۤ> إِذَا R<رَ>أَوۡX<ا۟> مَا یُوعَدُونَ فَسَیَعۡلَمُونَ مَنۡ أَضۡعَفُ نَاصِR<ر>X<ࣰ>ا N<وَ>أَقَلُّ عَدَدࣰا A<۝>D<٢٤>",
"قُلۡ إِنۡ أَQ<دۡ>رِW<یۤ> أَقَرِیبX<ࣱ> N<مَّ>ا تُوعَدُونَ أَمۡ یَQ<جۡ>عَلُ لَهُT<ۥ> R<رَ>بِّW<یۤ> أَمَدًا A<۝>D<٢٥>",
"عَT<ـٰ>لِمُ ٱلۡغَیۡبِ فَلَا یُظۡهِR<رُ> عَلَىٰ غَیۡبِهِW<ۦۤ> أَحَدًا A<۝>D<٢٦>",
"إِلَّا مَنِ ٱR<رۡ>تَضَىٰ مِX<ن> R<رَّ>سُولN<ࣲ> فَإِN<نَّ>هُT<ۥ> یَسۡلُكُ مِX<ن>N<ۢ> بَیۡنِ یَدَیۡهِ وَمِنۡ خَلۡفِهِT<ۦ> R<رَ>صَدࣰا A<۝>D<٢٧>",
"لِّیَعۡلَمَ أَN<ن> قَQ<دۡ> أَQ<بۡ>لَغُوX<ا۟> رِسَT<ـٰ>لَT<ـٰ>تِ R<رَ>بِّهِمۡ وَأَحَاطَ بِمَا لَدَیۡهِمۡ وَأَحۡصَىٰ كُلَّ شَیۡءٍ عَدَدَۢا A<۝>D<٢٨>",
"#یَW<ـٰۤ>أَیُّهَا ٱلۡمُزَّN<مِّ>لُ A<۝>D<١>",
"قُمِ ٱلَّیۡلَ إِلَّا قَلِیلࣰا A<۝>D<٢>",
"نِّصۡفَهُW<ۥۤ> أَوِ ٱN<ن>قُصۡ مِنۡهُ قَلِیلًا A<۝>D<٣>",
"أَوۡ زِQ<دۡ> عَلَیۡهِ وَR<رَ>تِّلِ ٱلۡقُR<رۡ>ءَانَ تَR<رۡ>تِیلًا A<۝>D<٤>",
"إِN<نَّ>ا سَنُلۡقِی عَلَیۡكَ قَوۡلN<ࣰ>ا ثَقِیلًا A<۝>D<٥>",
"إِN<نَّ> نَاشِئَةَ ٱلَّیۡلِ هِیَ أَشَدُّ وَQ<طۡ>ءX<ࣰ>ا N<وَ>أَQ<قۡ>وَمُ قِیلًا A<۝>D<٦>",
"إِN<نَّ> لَكَ فِی ٱX<ل>N<نَّ>هَارِ سَQ<بۡ>حN<ࣰ>ا طَوِیلࣰا A<۝>D<٧>",
"وَX<ٱ>ذۡكُرِ ٱسۡمَ R<رَ>بِّكَ وَتَبَتَّلۡ إِلَیۡهِ تَQ<بۡ>تِیلࣰا A<۝>D<٨>",
"R<رَّ>بُّ ٱلۡمَشۡرِقِ وَX<ٱ>لۡمَغۡرِبِ لَW<اۤ> إِلَT<ـٰ>هَ إِلَّا هُوَ فَX<ٱ>تَّخِذۡهُ وَكِیلࣰا A<۝>D<٩>",
"وَX<ٱ>صۡبِرۡ عَلَىٰ مَا یَقُولُونَ وَX<ٱ>هۡجُR<رۡ>هُمۡ هَQ<جۡ>R<ر>N<ࣰ>ا جَمِیلࣰا A<۝>D<١٠>",
"وَذَR<رۡ>نِی وَX<ٱ>لۡمُكَذِّبِینَ أُX<و۟>لِی ٱX<ل>N<نَّ>عۡمَةِ وَمَهِّلۡهُمۡ قَلِیلًا A<۝>D<١١>",
"إِN<نَّ> لَدَیۡنَW<اۤ> أَN<ن>كَالX<ࣰ>ا N<وَ>جَحِیمࣰا A<۝>D<١٢>",
"وَطَعَامN<ࣰ>ا ذَا غُصَّةX<ࣲ> N<وَ>عَذَابًا أَلِیمࣰا A<۝>D<١٣>",
"یَوۡمَ تَR<رۡ>جُفُ ٱلۡأَR<رۡ>ضُ وَX<ٱ>لۡجِبَالُ وَكَانَتِ ٱلۡجِبَالُ كَثِیبX<ࣰ>ا N<مَّ>هِیلًا A<۝>D<١٤>",
"إِN<نَّ>W<اۤ> أَR<رۡ>سَلۡنَW<اۤ> إِلَیۡكُمۡ R<رَ>سُولN<ࣰ>ا شَT<ـٰ>هِدًا عَلَیۡكُمۡ كَمَW<اۤ> أَR<رۡ>سَلۡنَW<اۤ> إِلَىٰ فِرۡعَوۡنَ R<رَ>سُولࣰا A<۝>D<١٥>",
"فَعَصَىٰ فِرۡعَوۡنُ ٱX<ل>R<رَّ>سُولَ فَأَخَذۡنَT<ـٰ>هُ أَخۡذX<ࣰ>ا N<وَ>بِیلࣰا A<۝>D<١٦>",
"فَكَیۡفَ تَتَّقُونَ إِN<ن> كَفَR<رۡ>تُمۡ یَوۡمX<ࣰ>ا N<یَ>Q<جۡ>عَلُ ٱلۡوِلۡدَT< ٰ>نَ شِیبًا A<۝>D<١٧>",
"ٱX<ل>سَّمَW<اۤ>ءُ مُN<ن>فَطِR<رُ>N<ۢ> بِهِT<ۦ>ۚ كَانَ وَعۡدُهُT<ۥ> مَفۡعُولًا A<۝>D<١٨>",
"إِN<نَّ> هَT<ـٰ>ذِهِT<ۦ> تَذۡكِR<رَ>ةN<ࣱ>ۖ فَمَN<ن> شَW<اۤ>ءَ ٱتَّخَذَ إِلَىٰ R<رَ>بِّهِT<ۦ> سَبِیلًا A<۝>D<١٩>",
"۞ إِN<نَّ> R<رَ>بَّكَ یَعۡلَمُ أَN<نَّ>كَ تَقُومُ أَQ<دۡ>نَىٰ مِN<ن> ثُلُثَیِ ٱلَّیۡلِ وَنِصۡفَهُT<ۥ> وَثُلُثَهُT<ۥ> وَطَW<اۤ>ئِفَةX<ࣱ> N<مِّ>نَ ٱلَّذِینَ مَعَكَۚ وَٱللَّهُ یُقَدِّR<رُ> ٱلَّیۡلَ وَX<ٱ>X<ل>N<نَّ>هَاR<رَ>ۚ عَلِمَ أَX<ن> لَّN<ن> تُحۡصُوهُ فَتَابَ عَلَیۡكُمۡۖ فَX<ٱ>Q<قۡ>R<رَ>ءُوX<ا۟> مَا تَیَسَّR<رَ> مِنَ ٱلۡقُR<رۡ>ءَانِۚ عَلِمَ أَN<ن> سَیَكُونُ مِN<ن>كُم N<مَّ>R<رۡ>ضَىٰ وَءَاخَR<رُ>ونَ یَضۡرِبُونَ فِی ٱلۡأَR<رۡ>ضِ یَQ<بۡ>تَغُونَ مِN<ن> فَضۡلِ ٱللَّهِ وَءَاخَR<رُ>ونَ یُقَT<ـٰ>تِلُونَ فِی سَبِیلِ ٱللَّهِۖ فَX<ٱ>Q<قۡ>R<رَ>ءُوX<ا۟> مَا تَیَسَّR<رَ> مِنۡهُۚ وَأَقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَءَاتُوX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَ وَأَQ<قۡ>رِضُوX<ا۟> ٱللَّهَ قَR<رۡ>ضًا حَسَنX<ࣰ>اۚ N<وَ>مَا تُقَدِّمُوX<ا۟> لِأَN<ن>فُسِكُم N<مِّ>نۡ خَیۡرN<ࣲ> تَجِدُوهُ عِN<ن>دَ ٱللَّهِ هُوَ خَیۡR<ر>X<ࣰ>ا N<وَ>أَعۡظَمَ أَQ<جۡ>R<ر>X<ࣰ>اۚ N<وَ>X<ٱ>سۡتَغۡفِR<رُ>وX<ا۟> ٱللَّهَۖ إِN<نَّ> ٱللَّهَ غَفُوR<ر>X<ࣱ> R<رَّ>حِJ<ی>مُۢ A<۝>D<٢٠>",
"#یَW<ـٰۤ>أَیُّهَا ٱلۡمُدَّثِّرُ A<۝>D<١>",
"قُمۡ فَأَN<ن>ذِرۡ A<۝>D<٢>",
"وَR<رَ>بَّكَ فَكَبِّرۡ A<۝>D<٣>",
"وَثِیَابَكَ فَطَهِّرۡ A<۝>D<٤>",
"وَX<ٱ>X<ل>R<رُّ>Q<جۡ>زَ فَX<ٱ>هۡجُR<رۡ> A<۝>D<٥>",
"وَلَا تَمۡنُN<ن> تَسۡتَكۡثِرُ A<۝>D<٦>",
"وَلِR<رَ>بِّكَ فَX<ٱ>صۡبِرۡ A<۝>D<٧>",
"فَإِذَا نُقِR<رَ> فِی ٱX<ل>N<نَّ>اقُJ<و>R<ر>ِ A<۝>D<٨>",
"فَذَT< ٰ>لِكَ یَوۡمَئِذX<ࣲ> N<یَ>وۡمٌ عَسِJ<ی>رٌ A<۝>D<٩>",
"عَلَى ٱلۡكَT<ـٰ>فِرِینَ غَیۡR<رُ> یَسِJ<ی>رࣲ A<۝>D<١٠>",
"ذَR<رۡ>نِی وَمَنۡ خَلَQ<قۡ>تُ وَحِیدࣰا A<۝>D<١١>",
"وَجَعَلۡتُ لَهُT<ۥ> مَالX<ࣰ>ا N<مَّ>مۡدُودࣰا A<۝>D<١٢>",
"وَبَنِینَ شُهُودࣰا A<۝>D<١٣>",
"وَمَهَّX<د>تُّ لَهُT<ۥ> تَمۡهِیدࣰا A<۝>D<١٤>",
"ثُN<مَّ> یَQ<طۡ>مَعُ أَنۡ أَزِJ<ی>Q<د>َ A<۝>D<١٥>",
"كَلَّW<اۤ>ۖ إِN<نَّ>هُT<ۥ> كَانَ لِءَایَT<ـٰ>تِنَا عَنِیدࣰا A<۝>D<١٦>",
"سَأُR<رۡ>هِقُهُT<ۥ> صَعُودًا A<۝>D<١٧>",
"إِN<نَّ>هُT<ۥ> فَكَّR<رَ> وَقَدَّR<ر>َ A<۝>D<١٨>",
"فَقُتِلَ كَیۡفَ قَدَّR<ر>َ A<۝>D<١٩>",
"ثُN<مَّ> قُتِلَ كَیۡفَ قَدَّR<ر>َ A<۝>D<٢٠>",
"ثُN<مَّ> نَظَR<ر>َ A<۝>D<٢١>",
"ثُN<مَّ> عَبَسَ وَبَسَR<ر>َ A<۝>D<٢٢>",
"ثُN<مَّ> أَQ<دۡ>بَR<رَ> وَX<ٱ>سۡتَكۡبَR<ر>َ A<۝>D<٢٣>",
"فَقَالَ إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا سِحۡR<ر>X<ࣱ> N<یُ>ؤۡثَR<ر>ُ A<۝>D<٢٤>",
"إِنۡ هَT<ـٰ>ذَW<اۤ> إِلَّا قَوۡلُ ٱلۡبَشَR<ر>ِ A<۝>D<٢٥>",
"سَأُصۡلِیهِ سَقَR<ر>َ A<۝>D<٢٦>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا سَقَR<ر>ُ A<۝>D<٢٧>",
"لَا تُQ<بۡ>قِی وَلَا تَذَR<ر>ُ A<۝>D<٢٨>",
"لَوَّاحَةX<ࣱ> لِّلۡبَشَR<ر>ِ A<۝>D<٢٩>",
"عَلَیۡهَا تِسۡعَةَ عَشَR<ر>َ A<۝>D<٣٠>",
"وَمَا جَعَلۡنَW<اۤ> أَصۡحَT<ـٰ>بَ ٱX<ل>N<نَّ>ارِ إِلَّا مَلَW<ـٰۤ>ئِكَةX<ࣰ>ۖ N<وَ>مَا جَعَلۡنَا عِدَّتَهُمۡ إِلَّا فِتۡنَةX<ࣰ> لِّلَّذِینَ كَفَR<رُ>وX<ا۟> لِیَسۡتَیۡقِنَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ وَیَزۡدَادَ ٱلَّذِینَ ءَامَنُW<وۤ>X<ا۟> إِیمَT<ـٰ>نX<ࣰ>ا N<وَ>لَا یَR<رۡ>تَابَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ وَX<ٱ>لۡمُؤۡمِنُونَ وَلِیَقُولَ ٱلَّذِینَ فِی قُلُوبِهِم N<مَّ>R<رَ>ضX<ࣱ> N<وَ>X<ٱ>لۡكَT<ـٰ>فِR<رُ>ونَ مَاذَW<اۤ> أَR<رَ>ادَ ٱللَّهُ بِهَT<ـٰ>ذَا مَثَلN<ࣰ>اۚ كَذَT< ٰ>لِكَ یُضِلُّ ٱللَّهُ مَX<ن> N<یَ>شَW<اۤ>ءُ وَیَهۡدِی مَX<ن> N<یَ>شَW<اۤ>ءُۚ وَمَا یَعۡلَمُ جُنُودَ R<رَ>بِّكَ إِلَّا هُوَۚ وَمَا هِیَ إِلَّا ذِكۡR<رَ>ىٰ لِلۡبَشَR<ر>ِ A<۝>D<٣١>",
"كَلَّا وَX<ٱ>لۡقَمَR<ر>ِ A<۝>D<٣٢>",
"وَX<ٱ>لَّیۡلِ إِذۡ أَQ<دۡ>بَR<ر>َ A<۝>D<٣٣>",
"وَX<ٱ>X<ل>صُّQ<بۡ>حِ إِذَW<اۤ> أَسۡفَR<ر>َ A<۝>D<٣٤>",
"إِN<نَّ>هَا لَإِحۡدَى ٱلۡكُبَR<ر>ِ A<۝>D<٣٥>",
"نَذِیR<ر>X<ࣰ>ا لِّلۡبَشَR<ر>ِ A<۝>D<٣٦>",
"لِمَN<ن> شَW<اۤ>ءَ مِN<ن>كُمۡ أَX<ن> N<یَ>تَقَدَّمَ أَوۡ یَتَأَخَّR<ر>َ A<۝>D<٣٧>",
"كُلُّ نَفۡسِN<ۭ> بِمَا كَسَبَتۡ R<رَ>هِینَةٌ A<۝>D<٣٨>",
"إِلَّW<اۤ> أَصۡحَT<ـٰ>بَ ٱلۡیَمِJ<ی>نِ A<۝>D<٣٩>",
"فِی جَN<نَّ>T<ـٰ>تX<ࣲ> N<یَ>تَسَW<اۤ>ءَلُJ<و>نَ A<۝>D<٤٠>",
"عَنِ ٱلۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<٤١>",
"مَا سَلَكَكُمۡ فِی سَقَR<ر>َ A<۝>D<٤٢>",
"قَالُوX<ا۟> لَمۡ نَكُ مِنَ ٱلۡمُصَلِّJ<ی>نَ A<۝>D<٤٣>",
"وَلَمۡ نَكُ نُQ<طۡ>عِمُ ٱلۡمِسۡكِJ<ی>نَ A<۝>D<٤٤>",
"وَكُN<نَّ>ا نَخُوضُ مَعَ ٱلۡخَW<اۤ>ئِضِJ<ی>نَ A<۝>D<٤٥>",
"وَكُN<نَّ>ا نُكَذِّبُ بِیَوۡمِ ٱX<ل>دِّJ<ی>نِ A<۝>D<٤٦>",
"حَتَّىW<ٰۤ> أَتَX<ى>T<ٰ>نَا ٱلۡیَقِJ<ی>نُ A<۝>D<٤٧>",
"فَمَا تَN<ن>فَعُهُمۡ شَفَT<ـٰ>عَةُ ٱX<ل>شَّT<ـٰ>فِعِJ<ی>نَ A<۝>D<٤٨>",
"فَمَا لَهُمۡ عَنِ ٱX<ل>تَّذۡكِR<رَ>ةِ مُعۡرِضِJ<ی>نَ A<۝>D<٤٩>",
"كَأَN<نَّ>هُمۡ حُمُR<ر>X<ࣱ> N<مُّ>سۡتَN<ن>فِR<رَ>ةࣱ A<۝>D<٥٠>",
"فَR<رَّ>تۡ مِN<ن> قَسۡوَR<رَ>ةِۭ A<۝>D<٥١>",
"بَلۡ یُرِیدُ كُلُّ ٱمۡرِئX<ࣲ> N<مِّ>نۡهُمۡ أَX<ن> N<یُ>ؤۡتَىٰ صُحُفX<ࣰ>ا N<مُّ>نَشَّR<رَ>ةࣰ A<۝>D<٥٢>",
"كَلَّاۖ بَل لَّا یَخَافُونَ ٱلۡءَاخِR<رَ>ةَ A<۝>D<٥٣>",
"كَلَّW<اۤ> إِN<نَّ>هُT<ۥ> تَذۡكِR<رَ>ةࣱ A<۝>D<٥٤>",
"فَمَN<ن> شَW<اۤ>ءَ ذَكَR<رَ>هُۥ A<۝>D<٥٥>",
"وَمَا یَذۡكُR<رُ>ونَ إِلَّW<اۤ> أَX<ن> N<یَ>شَW<اۤ>ءَ ٱللَّهُۚ هُوَ أَهۡلُ ٱX<ل>تَّQ<قۡ>وَىٰ وَأَهۡلُ ٱلۡمَغۡفِR<رَ>ةِ A<۝>D<٥٦>",
"#۞ لَW<اۤ> أُQ<قۡ>سِمُ بِیَوۡمِ ٱلۡقِیَT<ـٰ>مَةِ A<۝>D<١>",
"وَلَW<اۤ> أُQ<قۡ>سِمُ بِX<ٱ>X<ل>N<نَّ>فۡسِ ٱX<ل>لَّوَّامَةِ A<۝>D<٢>",
"أَیَحۡسَبُ ٱلۡإِN<ن>سَT<ـٰ>نُ أَلَّن N<نَّ>Q<جۡ>مَعَ عِظَامَهُۥ A<۝>D<٣>",
"بَلَىٰ قَT<ـٰ>دِرِینَ عَلَىW<ٰۤ> أَن N<نُّ>سَوِّیَ بَنَانَهُۥ A<۝>D<٤>",
"بَلۡ یُرِیدُ ٱلۡإِN<ن>سَT<ـٰ>نُ لِیَفۡجُR<رَ> أَمَامَهُۥ A<۝>D<٥>",
"یَسۡءَلُ أَیَّانَ یَوۡمُ ٱلۡقِیَT<ـٰ>مَةِ A<۝>D<٦>",
"فَإِذَا بَرِقَ ٱلۡبَصَR<ر>ُ A<۝>D<٧>",
"وَخَسَفَ ٱلۡقَمَR<ر>ُ A<۝>D<٨>",
"وَجُمِعَ ٱX<ل>شَّمۡسُ وَX<ٱ>لۡقَمَR<ر>ُ A<۝>D<٩>",
"یَقُولُ ٱلۡإِN<ن>سَT<ـٰ>نُ یَوۡمَئِذٍ أَیۡنَ ٱلۡمَفَR<رّ>ُ A<۝>D<١٠>",
"كَلَّا لَا وَزَR<ر>َ A<۝>D<١١>",
"إِلَىٰ R<رَ>بِّكَ یَوۡمَئِذٍ ٱلۡمُسۡتَقَR<رّ>ُ A<۝>D<١٢>",
"یُنَبَّؤُX<ا۟> ٱلۡإِN<ن>سَT<ـٰ>نُ یَوۡمَئِذِN<ۭ> بِمَا قَدَّمَ وَأَخَّR<ر>َ A<۝>D<١٣>",
"بَلِ ٱلۡإِN<ن>سَT<ـٰ>نُ عَلَىٰ نَفۡسِهِT<ۦ> بَصِیR<رَ>ةࣱ A<۝>D<١٤>",
"وَلَوۡ أَلۡقَىٰ مَعَاذِیR<رَ>هُۥ A<۝>D<١٥>",
"لَا تُحَرِّكۡ بِهِT<ۦ> لِسَانَكَ لِتَعۡجَلَ بِهِۦۤ A<۝>D<١٦>",
"إِN<نَّ> عَلَیۡنَا جَمۡعَهُT<ۥ> وَقُR<رۡ>ءَانَهُۥ A<۝>D<١٧>",
"فَإِذَا قَR<رَ>أۡنَT<ـٰ>هُ فَX<ٱ>تَّبِعۡ قُR<رۡ>ءَانَهُۥ A<۝>D<١٨>",
"ثُN<مَّ> إِN<نَّ> عَلَیۡنَا بَیَانَهُۥ A<۝>D<١٩>",
"كَلَّا بَلۡ تُحِبُّونَ ٱلۡعَاجِلَةَ A<۝>D<٢٠>",
"وَتَذَR<رُ>ونَ ٱلۡءَاخِR<رَ>ةَ A<۝>D<٢١>",
"وُجُوهX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> N<نَّ>اضِR<رَ>ةٌ A<۝>D<٢٢>",
"إِلَىٰ R<رَ>بِّهَا نَاظِR<رَ>ةࣱ A<۝>D<٢٣>",
"وَوُجُوهX<ࣱ> N<یَ>وۡمَئِذِN<ۭ> بَاسِR<رَ>ةࣱ A<۝>D<٢٤>",
"تَظُN<نُّ> أَX<ن> N<یُ>فۡعَلَ بِهَا فَاقِR<رَ>ةࣱ A<۝>D<٢٥>",
"كَلَّW<اۤ> إِذَا بَلَغَتِ ٱX<ل>تَّR<رَ>اقِیَ A<۝>D<٢٦>",
"وَقِیلَ مَنۡۜ R<رَ>J<ا>Q<ق>ࣲ A<۝>D<٢٧>",
"وَظَN<نَّ> أَN<نَّ>هُ ٱلۡفِR<رَ>J<ا>Q<ق>ُ A<۝>D<٢٨>",
"وَX<ٱ>لۡتَفَّتِ ٱX<ل>سَّاقُ بِX<ٱ>X<ل>سَّJ<ا>Q<ق>ِ A<۝>D<٢٩>",
"إِلَىٰ R<رَ>بِّكَ یَوۡمَئِذٍ ٱلۡمَسَJ<ا>Q<ق>ُ A<۝>D<٣٠>",
"فَلَا صَدَّقَ وَلَا صَلَّىٰ A<۝>D<٣١>",
"وَلَT<ـٰ>كِN<ن> كَذَّبَ وَتَوَلَّىٰ A<۝>D<٣٢>",
"ثُN<مَّ> ذَهَبَ إِلَىW<ٰۤ> أَهۡلِهِT<ۦ> یَتَمَطَّىٰۤ A<۝>D<٣٣>",
"أَوۡلَىٰ لَكَ فَأَوۡلَىٰ A<۝>D<٣٤>",
"ثُN<مَّ> أَوۡلَىٰ لَكَ فَأَوۡلَىٰۤ A<۝>D<٣٥>",
"أَیَحۡسَبُ ٱلۡإِN<ن>سَT<ـٰ>نُ أَX<ن> N<یُ>تۡR<رَ>كَ سُدًى A<۝>D<٣٦>",
"أَلَمۡ یَكُ نُQ<طۡ>فَةX<ࣰ> N<مِّ>X<ن> N<مَّ>نِیّX<ࣲ> N<یُ>مۡنَىٰ A<۝>D<٣٧>",
"ثُN<مَّ> كَانَ عَلَقَةN<ࣰ> فَخَلَقَ فَسَوَّىٰ A<۝>D<٣٨>",
"فَجَعَلَ مِنۡهُ ٱX<ل>زَّوۡجَیۡنِ ٱX<ل>ذَّكَR<رَ> وَX<ٱ>لۡأُN<ن>ثَىٰۤ A<۝>D<٣٩>",
"أَلَیۡسَ ذَT< ٰ>لِكَ بِقَT<ـٰ>دِرٍ عَلَىW<ٰۤ> أَX<ن> N<یُ>حۡT<ـۧ>ِیَ ٱلۡمَوۡتَىٰ A<۝>D<٤٠>",
"#هَلۡ أَتَىٰ عَلَى ٱلۡإِN<ن>سَT<ـٰ>نِ حِینX<ࣱ> N<مِّ>نَ ٱX<ل>دَّهۡرِ لَمۡ یَكُN<ن> شَیۡءX<ࣰ>ا N<مَّ>ذۡكُوR<ر>ًا A<۝>D<١>",
"إِN<نَّ>ا خَلَQ<قۡ>نَا ٱلۡإِN<ن>سَT<ـٰ>نَ مِن N<نُّ>Q<طۡ>فَةٍ أَمۡشَاجX<ࣲ> N<نَّ>Q<بۡ>تَلِیهِ فَجَعَلۡنَT<ـٰ>هُ سَمِیعَN<ۢ>ا بَصِیR<ر>ًا A<۝>D<٢>",
"إِN<نَّ>ا هَدَیۡنَT<ـٰ>هُ ٱX<ل>سَّبِیلَ إِN<مَّ>ا شَاكِR<ر>X<ࣰ>ا N<وَ>إِN<مَّ>ا كَفُوR<ر>ًا A<۝>D<٣>",
"إِN<نَّ>W<اۤ> أَعۡتَQ<دۡ>نَا لِلۡكَT<ـٰ>فِرِینَ سَلَT<ـٰ>سِلَX<ا۟> وَأَغۡلَT<ـٰ>لX<ࣰ>ا N<وَ>سَعِیR<ر>ًا A<۝>D<٤>",
"إِN<نَّ> ٱلۡأَQ<بۡ>R<رَ>اR<رَ> یَشۡR<رَ>بُونَ مِN<ن> كَأۡسN<ࣲ> كَانَ مِزَاجُهَا كَافُوR<ر>ًا A<۝>D<٥>",
"عَیۡنX<ࣰ>ا N<یَ>شۡR<رَ>بُ بِهَا عِبَادُ ٱللَّهِ یُفَجِّR<رُ>ونَهَا تَفۡجِیR<ر>ࣰا A<۝>D<٦>",
"یُوفُونَ بِX<ٱ>X<ل>N<نَّ>ذۡرِ وَیَخَافُونَ یَوۡمN<ࣰ>ا كَانَ شَR<رُّ>هُT<ۥ> مُسۡتَطِیR<ر>ࣰا A<۝>D<٧>",
"وَیُQ<طۡ>عِمُونَ ٱX<ل>طَّعَامَ عَلَىٰ حُبِّهِT<ۦ> مِسۡكِینX<ࣰ>ا N<وَ>یَتِیمX<ࣰ>ا N<وَ>أَسِیR<ر>ًا A<۝>D<٨>",
"إِN<نَّ>مَا نُQ<طۡ>عِمُكُمۡ لِوَQ<جۡ>هِ ٱللَّهِ لَا نُرِیدُ مِN<ن>كُمۡ جَزَW<اۤ>ءX<ࣰ> N<وَ>لَا شُكُوR<ر>ًا A<۝>D<٩>",
"إِN<نَّ>ا نَخَافُ مِX<ن> R<رَّ>بِّنَا یَوۡمًا عَبُوسN<ࣰ>ا قَمۡطَرِیR<ر>ࣰا A<۝>D<١٠>",
"فَوَقَX<ى>T<ٰ>هُمُ ٱللَّهُ شَR<رَّ> ذَT< ٰ>لِكَ ٱلۡیَوۡمِ وَلَقَّX<ى>T<ٰ>هُمۡ نَضۡR<رَ>ةX<ࣰ> N<وَ>سُR<رُ>وR<ر>ࣰا A<۝>D<١١>",
"وَجَزَX<ى>T<ٰ>هُN<م> بِمَا صَبَR<رُ>وX<ا۟> جَN<نَّ>ةX<ࣰ> N<وَ>حَرِیR<ر>ࣰا A<۝>D<١٢>",
"مُّتَّكِءِینَ فِیهَا عَلَى ٱلۡأَR<رَ>W<اۤ>ئِكِۖ لَا یَR<رَ>وۡنَ فِیهَا شَمۡسX<ࣰ>ا N<وَ>لَا زَمۡهَرِیR<ر>ࣰا A<۝>D<١٣>",
"وَدَانِیَةً عَلَیۡهِمۡ ظِلَT<ـٰ>لُهَا وَذُلِّلَتۡ قُطُوفُهَا تَذۡلِیلࣰا A<۝>D<١٤>",
"وَیُطَافُ عَلَیۡهِN<م> بِءَانِیَةX<ࣲ> N<مِّ>N<ن> فِضَّةX<ࣲ> N<وَ>أَكۡوَابN<ࣲ> كَانَتۡ قَوَارِیR<رَ>ا۠ A<۝>D<١٥>",
"قَوَارِیR<رَ>X<ا۟> مِN<ن> فِضَّةN<ࣲ> قَدَّR<رُ>وهَا تَQ<قۡ>دِیR<ر>ࣰا A<۝>D<١٦>",
"وَیُسۡقَوۡنَ فِیهَا كَأۡسN<ࣰ>ا كَانَ مِزَاجُهَا زَN<ن>جَبِیلًا A<۝>D<١٧>",
"عَیۡنN<ࣰ>ا فِیهَا تُسَN<مَّ>ىٰ سَلۡسَبِیلࣰا A<۝>D<١٨>",
"۞ وَیَطُوفُ عَلَیۡهِمۡ وِلۡدَT< ٰ>نX<ࣱ> N<مُّ>خَلَّدُونَ إِذَا R<رَ>أَیۡتَهُمۡ حَسِQ<بۡ>تَهُمۡ لُؤۡلُؤX<ࣰ>ا N<مَّ>N<ن>ثُوR<ر>ࣰا A<۝>D<١٩>",
"وَإِذَا R<رَ>أَیۡتَ ثَN<مَّ> R<رَ>أَیۡتَ نَعِیمX<ࣰ>ا N<وَ>مُلۡكN<ࣰ>ا كَبِیR<ر>ًا A<۝>D<٢٠>",
"عَT<ـٰ>لِیَهُمۡ ثِیَابُ سُN<ن>دُسٍ خُضۡR<ر>X<ࣱ> N<وَ>إِسۡتَQ<بۡ>R<رَ>قX<ࣱ>ۖ N<وَ>حُلُّW<وۤ>X<ا۟> أَسَاوِR<رَ> مِN<ن> فِضَّةX<ࣲ> N<وَ>سَقَX<ى>T<ٰ>هُمۡ R<رَ>بُّهُمۡ شَR<رَ>ابN<ࣰ>ا طَهُوR<ر>ًا A<۝>D<٢١>",
"إِN<نَّ> هَT<ـٰ>ذَا كَانَ لَكُمۡ جَزَW<اۤ>ءX<ࣰ> N<وَ>كَانَ سَعۡیُكُم N<مَّ>شۡكُوR<ر>ًا A<۝>D<٢٢>",
"إِN<نَّ>ا نَحۡنُ نَزَّلۡنَا عَلَیۡكَ ٱلۡقُR<رۡ>ءَانَ تَN<ن>زِیلࣰا A<۝>D<٢٣>",
"فَX<ٱ>صۡبِرۡ لِحُكۡمِ R<رَ>بِّكَ وَلَا تُطِعۡ مِنۡهُمۡ ءَاثِمًا أَوۡ كَفُوR<ر>ࣰا A<۝>D<٢٤>",
"وَX<ٱ>ذۡكُرِ ٱسۡمَ R<رَ>بِّكَ بُكۡR<رَ>ةX<ࣰ> N<وَ>أَصِیلࣰا A<۝>D<٢٥>",
"وَمِنَ ٱلَّیۡلِ فَX<ٱ>سۡجُQ<دۡ> لَهُT<ۥ> وَسَبِّحۡهُ لَیۡلN<ࣰ>ا طَوِیلًا A<۝>D<٢٦>",
"إِN<نَّ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ یُحِبُّونَ ٱلۡعَاجِلَةَ وَیَذَR<رُ>ونَ وَR<رَ>W<اۤ>ءَهُمۡ یَوۡمN<ࣰ>ا ثَقِیلࣰا A<۝>D<٢٧>",
"نَّحۡنُ خَلَQ<قۡ>نَT<ـٰ>هُمۡ وَشَدَQ<دۡ>نَW<اۤ> أَسۡR<رَ>هُمۡۖ وَإِذَا شِئۡنَا بَدَّلۡنَW<اۤ> أَمۡثَT<ـٰ>لَهُمۡ تَQ<بۡ>دِیلًا A<۝>D<٢٨>",
"إِN<نَّ> هَT<ـٰ>ذِهِT<ۦ> تَذۡكِR<رَ>ةN<ࣱ>ۖ فَمَN<ن> شَW<اۤ>ءَ ٱتَّخَذَ إِلَىٰ R<رَ>بِّهِT<ۦ> سَبِیلࣰا A<۝>D<٢٩>",
"وَمَا تَشَW<اۤ>ءُونَ إِلَّW<اۤ> أَX<ن> N<یَ>شَW<اۤ>ءَ ٱللَّهُۚ إِN<نَّ> ٱللَّهَ كَانَ عَلِیمًا حَكِیمࣰا A<۝>D<٣٠>",
"یُQ<دۡ>خِلُ مَX<ن> N<یَ>شَW<اۤ>ءُ فِی R<رَ>حۡمَتِهِT<ۦ>ۚ وَX<ٱ>X<ل>ظَّT<ـٰ>لِمِینَ أَعَدَّ لَهُمۡ عَذَابًا أَلِیمَۢا A<۝>D<٣١>",
"#وَX<ٱ>لۡمُR<رۡ>سَلَT<ـٰ>تِ عُR<رۡ>فࣰا A<۝>D<١>",
"فَX<ٱ>لۡعَT<ـٰ>صِفَT<ـٰ>تِ عَصۡفࣰا A<۝>D<٢>",
"وَX<ٱ>X<ل>N<نَّ>T<ـٰ>شِR<رَ>T< ٰ>تِ نَشۡR<ر>ࣰا A<۝>D<٣>",
"فَX<ٱ>لۡفَT<ـٰ>رِقَT<ـٰ>تِ فَR<رۡ>قࣰا A<۝>D<٤>",
"فَX<ٱ>لۡمُلۡقِیَT<ـٰ>تِ ذِكۡR<ر>ًا A<۝>D<٥>",
"عُذۡR<ر>ًا أَوۡ نُذۡR<ر>ًا A<۝>D<٦>",
"إِN<نَّ>مَا تُوعَدُونَ لَوَT< ٰ>قِعࣱ A<۝>D<٧>",
"فَإِذَا ٱX<ل>N<نُّ>جُومُ طُمِسَتۡ A<۝>D<٨>",
"وَإِذَا ٱX<ل>سَّمَW<اۤ>ءُ فُرِجَتۡ A<۝>D<٩>",
"وَإِذَا ٱلۡجِبَالُ نُسِفَتۡ A<۝>D<١٠>",
"وَإِذَا ٱX<ل>R<رُّ>سُلُ أُقِّتَتۡ A<۝>D<١١>",
"لِأَیِّ یَوۡمٍ أُجِّلَتۡ A<۝>D<١٢>",
"لِیَوۡمِ ٱلۡفَصۡلِ A<۝>D<١٣>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا یَوۡمُ ٱلۡفَصۡلِ A<۝>D<١٤>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<١٥>",
"أَلَمۡ نُهۡلِكِ ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٦>",
"ثُN<مَّ> نُتۡبِعُهُمُ ٱلۡءَاخِرِJ<ی>نَ A<۝>D<١٧>",
"كَذَT< ٰ>لِكَ نَفۡعَلُ بِX<ٱ>لۡمُQ<جۡ>رِمِJ<ی>نَ A<۝>D<١٨>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<١٩>",
"أَلَمۡ نَخۡلُX<ق>كُّم N<مِّ>X<ن> N<مَّ>W<اۤ>ءX<ࣲ> N<مَّ>هِJ<ی>نࣲ A<۝>D<٢٠>",
"فَجَعَلۡنَT<ـٰ>هُ فِی قَR<رَ>ارX<ࣲ> N<مَّ>كِJ<ی>نٍ A<۝>D<٢١>",
"إِلَىٰ قَدَرX<ࣲ> N<مَّ>عۡلُJ<و>مࣲ A<۝>D<٢٢>",
"فَقَدَR<رۡ>نَا فَنِعۡمَ ٱلۡقَT<ـٰ>دِR<رُ>J<و>نَ A<۝>D<٢٣>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<٢٤>",
"أَلَمۡ نَQ<جۡ>عَلِ ٱلۡأَR<رۡ>ضَ كِفَاتًا A<۝>D<٢٥>",
"أَحۡیَW<اۤ>ءX<ࣰ> N<وَ>أَمۡوَT< ٰ>تࣰا A<۝>D<٢٦>",
"وَجَعَلۡنَا فِیهَا R<رَ>وَT< ٰ>سِیَ شَT<ـٰ>مِخَT<ـٰ>تX<ࣲ> N<وَ>أَسۡقَیۡنَT<ـٰ>كُم N<مَّ>W<اۤ>ءN<ࣰ> فُR<رَ>اتࣰا A<۝>D<٢٧>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<٢٨>",
"ٱN<ن>طَلِقُW<وۤ>X<ا۟> إِلَىٰ مَا كُN<ن>تُN<م> بِهِT<ۦ> تُكَذِّبُJ<و>نَ A<۝>D<٢٩>",
"ٱN<ن>طَلِقُW<وۤ>X<ا۟> إِلَىٰ ظِلّN<ࣲ> ذِی ثَلَT<ـٰ>ثِ شُعَQ<ب>ࣲ A<۝>D<٣٠>",
"لَّا ظَلِیلX<ࣲ> N<وَ>لَا یُغۡنِی مِنَ ٱللَّهَQ<ب>ِ A<۝>D<٣١>",
"إِN<نَّ>هَا تَR<رۡ>مِی بِشَR<رَ>رN<ࣲ> كَX<ٱ>لۡقَصۡR<ر>ِ A<۝>D<٣٢>",
"كَأَN<نَّ>هُT<ۥ> جِمَT<ـٰ>لَتN<ࣱ> صُفۡR<ر>ࣱ A<۝>D<٣٣>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<٣٤>",
"هَT<ـٰ>ذَا یَوۡمُ لَا یَN<ن>طِقُJ<و>نَ A<۝>D<٣٥>",
"وَلَا یُؤۡذَنُ لَهُمۡ فَیَعۡتَذِR<رُ>J<و>نَ A<۝>D<٣٦>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<٣٧>",
"هَT<ـٰ>ذَا یَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَT<ـٰ>كُمۡ وَX<ٱ>لۡأَوَّلِJ<ی>نَ A<۝>D<٣٨>",
"فَإِN<ن> كَانَ لَكُمۡ كَیۡدN<ࣱ> فَكِیدُJ<و>نِ A<۝>D<٣٩>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<٤٠>",
"إِN<نَّ> ٱلۡمُتَّقِینَ فِی ظِلَT<ـٰ>لX<ࣲ> N<وَ>عُیُJ<و>نࣲ A<۝>D<٤١>",
"وَفَوَT< ٰ>كِهَ مِN<مَّ>ا یَشۡتَهُJ<و>نَ A<۝>D<٤٢>",
"كُلُوX<ا۟> وَX<ٱ>شۡR<رَ>بُوX<ا۟> هَنِW<یۤ>ءَN<ۢ>ا بِمَا كُN<ن>تُمۡ تَعۡمَلُJ<و>نَ A<۝>D<٤٣>",
"إِN<نَّ>ا كَذَT< ٰ>لِكَ نَQ<جۡ>زِی ٱلۡمُحۡسِنِJ<ی>نَ A<۝>D<٤٤>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<٤٥>",
"كُلُوX<ا۟> وَتَمَتَّعُوX<ا۟> قَلِیلًا إِN<نَّ>كُم N<مُّ>Q<جۡ>رِمُJ<و>نَ A<۝>D<٤٦>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<٤٧>",
"وَإِذَا قِیلَ لَهُمُ ٱR<رۡ>كَعُوX<ا۟> لَا یَR<رۡ>كَعُJ<و>نَ A<۝>D<٤٨>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<٤٩>",
"فَبِأَیِّ حَدِیثِN<ۭ> بَعۡدَهُT<ۥ> یُؤۡمِنُJ<و>نَ A<۝>D<٥٠>",
"#۞ عَN<مَّ> یَتَسَW<اۤ>ءَلُJ<و>نَ A<۝>D<١>",
"عَنِ ٱX<ل>N<نَّ>بَإِ ٱلۡعَظِJ<ی>مِ A<۝>D<٢>",
"ٱلَّذِی هُمۡ فِیهِ مُخۡتَلِفُJ<و>نَ A<۝>D<٣>",
"كَلَّا سَیَعۡلَمُJ<و>نَ A<۝>D<٤>",
"ثُN<مَّ> كَلَّا سَیَعۡلَمُJ<و>نَ A<۝>D<٥>",
"أَلَمۡ نَQ<جۡ>عَلِ ٱلۡأَR<رۡ>ضَ مِهَT<ـٰ>دࣰا A<۝>D<٦>",
"وَX<ٱ>لۡجِبَالَ أَوۡتَادࣰا A<۝>D<٧>",
"وَخَلَQ<قۡ>نَT<ـٰ>كُمۡ أَزۡوَT< ٰ>جࣰا A<۝>D<٨>",
"وَجَعَلۡنَا نَوۡمَكُمۡ سُبَاتࣰا A<۝>D<٩>",
"وَجَعَلۡنَا ٱلَّیۡلَ لِبَاسࣰا A<۝>D<١٠>",
"وَجَعَلۡنَا ٱX<ل>N<نَّ>هَاR<رَ> مَعَاشࣰا A<۝>D<١١>",
"وَبَنَیۡنَا فَوۡقَكُمۡ سَQ<بۡ>عN<ࣰ>ا شِدَادࣰا A<۝>D<١٢>",
"وَجَعَلۡنَا سِR<رَ>اجX<ࣰ>ا N<وَ>هَّاجࣰا A<۝>D<١٣>",
"وَأَN<ن>زَلۡنَا مِنَ ٱلۡمُعۡصِR<رَ>T< ٰ>تِ مَW<اۤ>ءN<ࣰ> ثَجَّاجࣰا A<۝>D<١٤>",
"لِّنُخۡرِجَ بِهِT<ۦ> حَبّX<ࣰ>ا N<وَ>نَبَاتࣰا A<۝>D<١٥>",
"وَجَN<نَّ>T<ـٰ>تٍ أَلۡفَافًا A<۝>D<١٦>",
"إِN<نَّ> یَوۡمَ ٱلۡفَصۡلِ كَانَ مِیقَT<ـٰ>تࣰا A<۝>D<١٧>",
"یَوۡمَ یُN<ن>فَخُ فِی ٱX<ل>صُّورِ فَتَأۡتُونَ أَفۡوَاجࣰا A<۝>D<١٨>",
"وَفُتِحَتِ ٱX<ل>سَّمَW<اۤ>ءُ فَكَانَتۡ أَQ<بۡ>وَT< ٰ>بࣰا A<۝>D<١٩>",
"وَسُیِّR<رَ>تِ ٱلۡجِبَالُ فَكَانَتۡ سَR<رَ>ابًا A<۝>D<٢٠>",
"إِN<نَّ> جَهَN<نَّ>مَ كَانَتۡ مِرۡصَادࣰا A<۝>D<٢١>",
"لِّX<ل>طَّT<ـٰ>غِینَ مَءَابࣰا A<۝>D<٢٢>",
"لَّT<ـٰ>بِثِینَ فِیهَW<اۤ> أَحۡقَابࣰا A<۝>D<٢٣>",
"لَّا یَذُوقُونَ فِیهَا بَR<رۡ>دX<ࣰ>ا N<وَ>لَا شَR<رَ>ابًا A<۝>D<٢٤>",
"إِلَّا حَمِیمX<ࣰ>ا N<وَ>غَسَّاقࣰا A<۝>D<٢٥>",
"جَزَW<اۤ>ءX<ࣰ> N<وِ>فَاقًا A<۝>D<٢٦>",
"إِN<نَّ>هُمۡ كَانُوX<ا۟> لَا یَR<رۡ>جُونَ حِسَابࣰا A<۝>D<٢٧>",
"وَكَذَّبُوX<ا۟> بِءَایَT<ـٰ>تِنَا كِذَّابࣰا A<۝>D<٢٨>",
"وَكُلَّ شَیۡءٍ أَحۡصَیۡنَT<ـٰ>هُ كِتَT<ـٰ>بࣰا A<۝>D<٢٩>",
"فَذُوقُوX<ا۟> فَلَن N<نَّ>زِیدَكُمۡ إِلَّا عَذَابًا A<۝>D<٣٠>",
"إِN<نَّ> لِلۡمُتَّقِینَ مَفَازًا A<۝>D<٣١>",
"حَدَW<اۤ>ئِقَ وَأَعۡنَT<ـٰ>بࣰا A<۝>D<٣٢>",
"وَكَوَاعِبَ أَتۡR<رَ>ابࣰا A<۝>D<٣٣>",
"وَكَأۡسN<ࣰ>ا دِهَاقࣰا A<۝>D<٣٤>",
"لَّا یَسۡمَعُونَ فِیهَا لَغۡوX<ࣰ>ا N<وَ>لَا كِذَّT< ٰ>بࣰا A<۝>D<٣٥>",
"جَزَW<اۤ>ءX<ࣰ> N<مِّ>X<ن> R<رَّ>بِّكَ عَطَW<اۤ>ءً حِسَابࣰا A<۝>D<٣٦>",
"R<رَّ>بِّ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا بَیۡنَهُمَا ٱX<ل>R<رَّ>حۡمَT<ـٰ>نِۖ لَا یَمۡلِكُونَ مِنۡهُ خِطَابࣰا A<۝>D<٣٧>",
"یَوۡمَ یَقُومُ ٱX<ل>R<رُّ>وحُ وَX<ٱ>لۡمَلَW<ـٰۤ>ئِكَةُ صَفّX<ࣰ>اۖ لَّا یَتَكَلَّمُونَ إِلَّا مَنۡ أَذِنَ لَهُ ٱX<ل>R<رَّ>حۡمَT<ـٰ>نُ وَقَالَ صَوَابࣰا A<۝>D<٣٨>",
"ذَT< ٰ>لِكَ ٱلۡیَوۡمُ ٱلۡحَقُّۖ فَمَN<ن> شَW<اۤ>ءَ ٱتَّخَذَ إِلَىٰ R<رَ>بِّهِT<ۦ> مَءَابًا A<۝>D<٣٩>",
"إِN<نَّ>W<اۤ> أَN<ن>ذَR<رۡ>نَT<ـٰ>كُمۡ عَذَابN<ࣰ>ا قَرِیبX<ࣰ>ا N<یَ>وۡمَ یَN<ن>ظُR<رُ> ٱلۡمَR<رۡ>ءُ مَا قَدَّمَتۡ یَدَاهُ وَیَقُولُ ٱلۡكَافِR<رُ> یَT<ـٰ>لَیۡتَنِی كُN<ن>تُ تُR<رَ>T< ٰ>بَۢا A<۝>D<٤٠>",
"#وَX<ٱ>X<ل>N<نَّ>T<ـٰ>زِعَT<ـٰ>تِ غَR<رۡ>قࣰا A<۝>D<١>",
"وَX<ٱ>X<ل>N<نَّ>T<ـٰ>شِطَT<ـٰ>تِ نَشۡطࣰا A<۝>D<٢>",
"وَX<ٱ>X<ل>سَّT<ـٰ>بِحَT<ـٰ>تِ سَQ<بۡ>حࣰا A<۝>D<٣>",
"فَX<ٱ>X<ل>سَّT<ـٰ>بِقَT<ـٰ>تِ سَQ<بۡ>قࣰا A<۝>D<٤>",
"فَX<ٱ>لۡمُدَبِّR<رَ>T< ٰ>تِ أَمۡR<ر>ࣰا A<۝>D<٥>",
"یَوۡمَ تَR<رۡ>جُفُ ٱX<ل>R<رَّ>اجِفَةُ A<۝>D<٦>",
"تَتۡبَعُهَا ٱX<ل>R<رَّ>ادِفَةُ A<۝>D<٧>",
"قُلُوبX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> N<وَ>اجِفَةٌ A<۝>D<٨>",
"أَQ<بۡ>صَT<ـٰ>R<رُ>هَا خَT<ـٰ>شِعَةࣱ A<۝>D<٩>",
"یَقُولُونَ أَءِN<نَّ>ا لَمَR<رۡ>دُودُونَ فِی ٱلۡحَافِR<رَ>ةِ A<۝>D<١٠>",
"أَءِذَا كُN<نَّ>ا عِظَT<ـٰ>مX<ࣰ>ا N<نَّ>خِR<رَ>ةࣰ A<۝>D<١١>",
"قَالُوX<ا۟> تِلۡكَ إِذN<ࣰ>ا كَR<رَّ>ةٌ خَاسِR<رَ>ةࣱ A<۝>D<١٢>",
"فَإِN<نَّ>مَا هِیَ زَQ<جۡ>R<رَ>ةX<ࣱ> N<وَ>T< ٰ>حِدَةࣱ A<۝>D<١٣>",
"فَإِذَا هُN<م> بِX<ٱ>X<ل>سَّاهِR<رَ>ةِ A<۝>D<١٤>",
"هَلۡ أَتَX<ى>T<ٰ>كَ حَدِیثُ مُوسَىٰۤ A<۝>D<١٥>",
"إِذۡ نَادَX<ى>T<ٰ>هُ R<رَ>بُّهُT<ۥ> بِX<ٱ>لۡوَادِ ٱلۡمُقَدَّسِ طُوًى A<۝>D<١٦>",
"ٱذۡهَQ<بۡ> إِلَىٰ فِرۡعَوۡنَ إِN<نَّ>هُT<ۥ> طَغَىٰ A<۝>D<١٧>",
"فَقُلۡ هَل لَّكَ إِلَىW<ٰۤ> أَN<ن> تَزَكَّىٰ A<۝>D<١٨>",
"وَأَهۡدِیَكَ إِلَىٰ R<رَ>بِّكَ فَتَخۡشَىٰ A<۝>D<١٩>",
"فَأَR<رَ>X<ى>T<ٰ>هُ ٱلۡءَایَةَ ٱلۡكُQ<بۡ>R<رَ>ىٰ A<۝>D<٢٠>",
"فَكَذَّبَ وَعَصَىٰ A<۝>D<٢١>",
"ثُN<مَّ> أَQ<دۡ>بَR<رَ> یَسۡعَىٰ A<۝>D<٢٢>",
"فَحَشَR<رَ> فَنَادَىٰ A<۝>D<٢٣>",
"فَقَالَ أَنَا۠ R<رَ>بُّكُمُ ٱلۡأَعۡلَىٰ A<۝>D<٢٤>",
"فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلۡءَاخِR<رَ>ةِ وَX<ٱ>لۡأُولَىٰۤ A<۝>D<٢٥>",
"إِN<نَّ> فِی ذَT< ٰ>لِكَ لَعِQ<بۡ>R<رَ>ةX<ࣰ> لِّمَX<ن> N<یَ>خۡشَىٰۤ A<۝>D<٢٦>",
"ءَأَN<ن>تُمۡ أَشَدُّ خَلۡقًا أَمِ ٱX<ل>سَّمَW<اۤ>ءُۚ بَنَX<ى>T<ٰ>هَا A<۝>D<٢٧>",
"R<رَ>فَعَ سَمۡكَهَا فَسَوَّX<ى>T<ٰ>هَا A<۝>D<٢٨>",
"وَأَغۡطَشَ لَیۡلَهَا وَأَخۡR<رَ>جَ ضُحَX<ى>T<ٰ>هَا A<۝>D<٢٩>",
"وَX<ٱ>لۡأَR<رۡ>ضَ بَعۡدَ ذَT< ٰ>لِكَ دَحَX<ى>T<ٰ>هَاۤ A<۝>D<٣٠>",
"أَخۡR<رَ>جَ مِنۡهَا مَW<اۤ>ءَهَا وَمَR<رۡ>عَX<ى>T<ٰ>هَا A<۝>D<٣١>",
"وَX<ٱ>لۡجِبَالَ أَR<رۡ>سَX<ى>T<ٰ>هَا A<۝>D<٣٢>",
"مَتَT<ـٰ>عX<ࣰ>ا لَّكُمۡ وَلِأَنۡعَT<ـٰ>مِكُمۡ A<۝>D<٣٣>",
"فَإِذَا جَW<اۤ>ءَتِ ٱX<ل>طَّL<اۤ>N<مَّ>ةُ ٱلۡكُQ<بۡ>R<رَ>ىٰ A<۝>D<٣٤>",
"یَوۡمَ یَتَذَكَّR<رُ> ٱلۡإِN<ن>سَT<ـٰ>نُ مَا سَعَىٰ A<۝>D<٣٥>",
"وَبُرِّزَتِ ٱلۡجَحِیمُ لِمَX<ن> N<یَ>R<رَ>ىٰ A<۝>D<٣٦>",
"فَأَN<مَّ>ا مَN<ن> طَغَىٰ A<۝>D<٣٧>",
"وَءَاثَR<رَ> ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا A<۝>D<٣٨>",
"فَإِN<نَّ> ٱلۡجَحِیمَ هِیَ ٱلۡمَأۡوَىٰ A<۝>D<٣٩>",
"وَأَN<مَّ>ا مَنۡ خَافَ مَقَامَ R<رَ>بِّهِT<ۦ> وَنَهَى ٱX<ل>N<نَّ>فۡسَ عَنِ ٱلۡهَوَىٰ A<۝>D<٤٠>",
"فَإِN<نَّ> ٱلۡجَN<نَّ>ةَ هِیَ ٱلۡمَأۡوَىٰ A<۝>D<٤١>",
"یَسۡءَلُونَكَ عَنِ ٱX<ل>سَّاعَةِ أَیَّانَ مُR<رۡ>سَX<ى>T<ٰ>هَا A<۝>D<٤٢>",
"فِیمَ أَN<ن>تَ مِN<ن> ذِكۡR<رَ>X<ى>T<ٰ>هَاۤ A<۝>D<٤٣>",
"إِلَىٰ R<رَ>بِّكَ مُN<ن>تَهَX<ى>T<ٰ>هَاۤ A<۝>D<٤٤>",
"إِN<نَّ>مَW<اۤ> أَN<ن>تَ مُN<ن>ذِR<رُ> مَX<ن> N<یَ>خۡشَX<ى>T<ٰ>هَا A<۝>D<٤٥>",
"كَأَN<نَّ>هُمۡ یَوۡمَ یَR<رَ>وۡنَهَا لَمۡ یَلۡبَثُW<وۤ>X<ا۟> إِلَّا عَشِیَّةً أَوۡ ضُحَX<ى>T<ٰ>هَا A<۝>D<٤٦>",
"#۞ عَبَسَ وَتَوَلَّىٰۤ A<۝>D<١>",
"أَN<ن> جَW<اۤ>ءَهُ ٱلۡأَعۡمَىٰ A<۝>D<٢>",
"وَمَا یُQ<دۡ>رِیكَ لَعَلَّهُT<ۥ> یَزَّكَّىٰۤ A<۝>D<٣>",
"أَوۡ یَذَّكَّR<رُ> فَتَN<ن>فَعَهُ ٱX<ل>ذِّكۡR<رَ>ىٰۤ A<۝>D<٤>",
"أَN<مَّ>ا مَنِ ٱسۡتَغۡنَىٰ A<۝>D<٥>",
"فَأَN<ن>تَ لَهُT<ۥ> تَصَدَّىٰ A<۝>D<٦>",
"وَمَا عَلَیۡكَ أَلَّا یَزَّكَّىٰ A<۝>D<٧>",
"وَأَN<مَّ>ا مَN<ن> جَW<اۤ>ءَكَ یَسۡعَىٰ A<۝>D<٨>",
"وَهُوَ یَخۡشَىٰ A<۝>D<٩>",
"فَأَN<ن>تَ عَنۡهُ تَلَهَّىٰ A<۝>D<١٠>",
"كَلَّW<اۤ> إِN<نَّ>هَا تَذۡكِR<رَ>ةࣱ A<۝>D<١١>",
"فَمَN<ن> شَW<اۤ>ءَ ذَكَR<رَ>هُۥ A<۝>D<١٢>",
"فِی صُحُفX<ࣲ> N<مُّ>كَR<رَّ>مَةࣲ A<۝>D<١٣>",
"مَّR<رۡ>فُوعَةX<ࣲ> N<مُّ>طَهَّR<رَ>ةِۭ A<۝>D<١٤>",
"بِأَیۡدِی سَفَR<رَ>ةࣲ A<۝>D<١٥>",
"كِR<رَ>امِN<ۭ> بَR<رَ>R<رَ>ةࣲ A<۝>D<١٦>",
"قُتِلَ ٱلۡإِN<ن>سَT<ـٰ>نُ مَW<اۤ> أَكۡفَR<رَ>هُۥ A<۝>D<١٧>",
"مِنۡ أَیِّ شَیۡءٍ خَلَقَهُۥ A<۝>D<١٨>",
"مِن N<نُّ>Q<طۡ>فَةٍ خَلَقَهُT<ۥ> فَقَدَّR<رَ>هُۥ A<۝>D<١٩>",
"ثُN<مَّ> ٱX<ل>سَّبِیلَ یَسَّR<رَ>هُۥ A<۝>D<٢٠>",
"ثُN<مَّ> أَمَاتَهُT<ۥ> فَأَQ<قۡ>بَR<رَ>هُۥ A<۝>D<٢١>",
"ثُN<مَّ> إِذَا شَW<اۤ>ءَ أَN<ن>شَR<رَ>هُۥ A<۝>D<٢٢>",
"كَلَّا لَN<مَّ>ا یَQ<قۡ>ضِ مَW<اۤ> أَمَR<رَ>هُۥ A<۝>D<٢٣>",
"فَلۡیَN<ن>ظُرِ ٱلۡإِN<ن>سَT<ـٰ>نُ إِلَىٰ طَعَامِهِۦۤ A<۝>D<٢٤>",
"أَN<نَّ>ا صَبَQ<بۡ>نَا ٱلۡمَW<اۤ>ءَ صَبࣰّا A<۝>D<٢٥>",
"ثُN<مَّ> شَقَQ<قۡ>نَا ٱلۡأَR<رۡ>ضَ شَقࣰّا A<۝>D<٢٦>",
"فَأَX<ن>N<ۢ>بَتۡنَا فِیهَا حَبࣰّا A<۝>D<٢٧>",
"وَعِنَبX<ࣰ>ا N<وَ>قَضۡبࣰا A<۝>D<٢٨>",
"وَزَیۡتُونX<ࣰ>ا N<وَ>نَخۡلࣰا A<۝>D<٢٩>",
"وَحَدَW<اۤ>ئِقَ غُلۡبࣰا A<۝>D<٣٠>",
"وَفَT<ـٰ>كِهَةX<ࣰ> N<وَ>أَبࣰّا A<۝>D<٣١>",
"مَّتَT<ـٰ>عX<ࣰ>ا لَّكُمۡ وَلِأَنۡعَT<ـٰ>مِكُمۡ A<۝>D<٣٢>",
"فَإِذَا جَW<اۤ>ءَتِ ٱX<ل>صَّL<اۤ>خَّةُ A<۝>D<٣٣>",
"یَوۡمَ یَفِR<رُّ> ٱلۡمَR<رۡ>ءُ مِنۡ أَخِJ<ی>هِ A<۝>D<٣٤>",
"وَأُN<مِّ>هِT<ۦ> وَأَبِJ<ی>هِ A<۝>D<٣٥>",
"وَصَT<ـٰ>حِبَتِهِT<ۦ> وَبَنِJ<ی>هِ A<۝>D<٣٦>",
"لِكُلِّ ٱمۡرِئX<ࣲ> N<مِّ>نۡهُمۡ یَوۡمَئِذN<ࣲ> شَأۡنX<ࣱ> N<یُ>غۡنِJ<ی>هِ A<۝>D<٣٧>",
"وُجُوهX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> N<مُّ>سۡفِR<رَ>ةࣱ A<۝>D<٣٨>",
"ضَاحِكَةX<ࣱ> N<مُّ>سۡتَQ<بۡ>شِR<رَ>ةࣱ A<۝>D<٣٩>",
"وَوُجُوهX<ࣱ> N<یَ>وۡمَئِذٍ عَلَیۡهَا غَبَR<رَ>ةࣱ A<۝>D<٤٠>",
"تَR<رۡ>هَقُهَا قَتَR<رَ>ةٌ A<۝>D<٤١>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ هُمُ ٱلۡكَفَR<رَ>ةُ ٱلۡفَجَR<رَ>ةُ A<۝>D<٤٢>",
"#إِذَا ٱX<ل>شَّمۡسُ كُوِّR<رَ>تۡ A<۝>D<١>",
"وَإِذَا ٱX<ل>N<نُّ>جُومُ ٱN<ن>كَدَR<رَ>تۡ A<۝>D<٢>",
"وَإِذَا ٱلۡجِبَالُ سُیِّR<رَ>تۡ A<۝>D<٣>",
"وَإِذَا ٱلۡعِشَاR<رُ> عُطِّلَتۡ A<۝>D<٤>",
"وَإِذَا ٱلۡوُحُوشُ حُشِR<رَ>تۡ A<۝>D<٥>",
"وَإِذَا ٱلۡبِحَاR<رُ> سُجِّR<رَ>تۡ A<۝>D<٦>",
"وَإِذَا ٱX<ل>N<نُّ>فُوسُ زُوِّجَتۡ A<۝>D<٧>",
"وَإِذَا ٱلۡمَوۡءُT<ۥ>دَةُ سُئِلَتۡ A<۝>D<٨>",
"بِأَیِّ ذَX<ن>N<ۢ>بN<ࣲ> قُتِلَتۡ A<۝>D<٩>",
"وَإِذَا ٱX<ل>صُّحُفُ نُشِR<رَ>تۡ A<۝>D<١٠>",
"وَإِذَا ٱX<ل>سَّمَW<اۤ>ءُ كُشِطَتۡ A<۝>D<١١>",
"وَإِذَا ٱلۡجَحِیمُ سُعِّR<رَ>تۡ A<۝>D<١٢>",
"وَإِذَا ٱلۡجَN<نَّ>ةُ أُزۡلِفَتۡ A<۝>D<١٣>",
"عَلِمَتۡ نَفۡسX<ࣱ> N<مَّ>W<اۤ> أَحۡضَR<رَ>تۡ A<۝>D<١٤>",
"فَلَW<اۤ> أُQ<قۡ>سِمُ بِX<ٱ>لۡخُN<نَّ>سِ A<۝>D<١٥>",
"ٱلۡجَوَارِ ٱلۡكُN<نَّ>سِ A<۝>D<١٦>",
"وَX<ٱ>لَّیۡلِ إِذَا عَسۡعَسَ A<۝>D<١٧>",
"وَX<ٱ>X<ل>صُّQ<بۡ>حِ إِذَا تَنَفَّسَ A<۝>D<١٨>",
"إِN<نَّ>هُT<ۥ> لَقَوۡلُ R<رَ>سُولN<ࣲ> كَرِJ<ی>مࣲ A<۝>D<١٩>",
"ذِی قُوَّةٍ عِN<ن>دَ ذِی ٱلۡعَR<رۡ>شِ مَكِJ<ی>نࣲ A<۝>D<٢٠>",
"مُّطَاعN<ࣲ> ثَN<مَّ> أَمِJ<ی>نࣲ A<۝>D<٢١>",
"وَمَا صَاحِبُكُN<م> بِمَQ<جۡ>نُJ<و>نࣲ A<۝>D<٢٢>",
"وَلَقَQ<دۡ> R<رَ>ءَاهُ بِX<ٱ>لۡأُفُقِ ٱلۡمُبِJ<ی>نِ A<۝>D<٢٣>",
"وَمَا هُوَ عَلَى ٱلۡغَیۡبِ بِضَنِJ<ی>نࣲ A<۝>D<٢٤>",
"وَمَا هُوَ بِقَوۡلِ شَیۡطَT<ـٰ>نX<ࣲ> R<رَّ>جِJ<ی>مࣲ A<۝>D<٢٥>",
"فَأَیۡنَ تَذۡهَبُJ<و>نَ A<۝>D<٢٦>",
"إِنۡ هُوَ إِلَّا ذِكۡR<ر>X<ࣱ> لِّلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٢٧>",
"لِمَN<ن> شَW<اۤ>ءَ مِN<ن>كُمۡ أَX<ن> N<یَ>سۡتَقِJ<ی>مَ A<۝>D<٢٨>",
"وَمَا تَشَW<اۤ>ءُونَ إِلَّW<اۤ> أَX<ن> N<یَ>شَW<اۤ>ءَ ٱللَّهُ R<رَ>بُّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٢٩>",
"#۞ إِذَا ٱX<ل>سَّمَW<اۤ>ءُ ٱN<ن>فَطَR<رَ>تۡ A<۝>D<١>",
"وَإِذَا ٱلۡكَوَاكِبُ ٱN<ن>تَثَR<رَ>تۡ A<۝>D<٢>",
"وَإِذَا ٱلۡبِحَاR<رُ> فُجِّR<رَ>تۡ A<۝>D<٣>",
"وَإِذَا ٱلۡقُبُوR<رُ> بُعۡثِR<رَ>تۡ A<۝>D<٤>",
"عَلِمَتۡ نَفۡسX<ࣱ> N<مَّ>ا قَدَّمَتۡ وَأَخَّR<رَ>تۡ A<۝>D<٥>",
"یَW<ـٰۤ>أَیُّهَا ٱلۡإِN<ن>سَT<ـٰ>نُ مَا غَR<رَّ>كَ بِR<رَ>بِّكَ ٱلۡكَرِJ<ی>مِ A<۝>D<٦>",
"ٱلَّذِی خَلَقَكَ فَسَوَّX<ى>T<ٰ>كَ فَعَدَلَكَ A<۝>D<٧>",
"فِW<یۤ> أَیِّ صُوR<رَ>ةX<ࣲ> N<مَّ>ا شَW<اۤ>ءَ R<رَ>كَّبَكَ A<۝>D<٨>",
"كَلَّا بَلۡ تُكَذِّبُونَ بِX<ٱ>X<ل>دِّJ<ی>نِ A<۝>D<٩>",
"وَإِN<نَّ> عَلَیۡكُمۡ لَحَT<ـٰ>فِظِJ<ی>نَ A<۝>D<١٠>",
"كِR<رَ>امN<ࣰ>ا كَT<ـٰ>تِبِJ<ی>نَ A<۝>D<١١>",
"یَعۡلَمُونَ مَا تَفۡعَلُJ<و>نَ A<۝>D<١٢>",
"إِN<نَّ> ٱلۡأَQ<بۡ>R<رَ>اR<رَ> لَفِی نَعِJ<ی>مࣲ A<۝>D<١٣>",
"وَإِN<نَّ> ٱلۡفُجَّاR<رَ> لَفِی جَحِJ<ی>مࣲ A<۝>D<١٤>",
"یَصۡلَوۡنَهَا یَوۡمَ ٱX<ل>دِّJ<ی>نِ A<۝>D<١٥>",
"وَمَا هُمۡ عَنۡهَا بِغَW<اۤ>ئِبِJ<ی>نَ A<۝>D<١٦>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا یَوۡمُ ٱX<ل>دِّJ<ی>نِ A<۝>D<١٧>",
"ثُN<مَّ> مَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا یَوۡمُ ٱX<ل>دِّJ<ی>نِ A<۝>D<١٨>",
"یَوۡمَ لَا تَمۡلِكُ نَفۡسX<ࣱ> لِّنَفۡسN<ࣲ> شَیۡءX<ࣰ>اۖ N<وَ>X<ٱ>لۡأَمۡR<رُ> یَوۡمَئِذX<ࣲ> لِّلَّهِ A<۝>D<١٩>",
"#وَیۡلX<ࣱ> لِّلۡمُطَفِّفِJ<ی>نَ A<۝>D<١>",
"ٱلَّذِینَ إِذَا ٱكۡتَالُوX<ا۟> عَلَى ٱX<ل>N<نَّ>اسِ یَسۡتَوۡفُJ<و>نَ A<۝>D<٢>",
"وَإِذَا كَالُوهُمۡ أَو وَّزَنُوهُمۡ یُخۡسِR<رُ>J<و>نَ A<۝>D<٣>",
"أَلَا یَظُN<نُّ> أُX<و۟>لَW<ـٰۤ>ئِكَ أَN<نَّ>هُم N<مَّ>Q<بۡ>عُوثُJ<و>نَ A<۝>D<٤>",
"لِیَوۡمٍ عَظِJ<ی>مࣲ A<۝>D<٥>",
"یَوۡمَ یَقُومُ ٱX<ل>N<نَّ>اسُ لِR<رَ>بِّ ٱلۡعَT<ـٰ>لَمِJ<ی>نَ A<۝>D<٦>",
"كَلَّW<اۤ> إِN<نَّ> كِتَT<ـٰ>بَ ٱلۡفُجَّارِ لَفِی سِجِّJ<ی>نࣲ A<۝>D<٧>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا سِجِّJ<ی>نࣱ A<۝>D<٨>",
"كِتَT<ـٰ>بX<ࣱ> N<مَّ>R<رۡ>قُJ<و>مࣱ A<۝>D<٩>",
"وَیۡلX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> لِّلۡمُكَذِّبِJ<ی>نَ A<۝>D<١٠>",
"ٱلَّذِینَ یُكَذِّبُونَ بِیَوۡمِ ٱX<ل>دِّJ<ی>نِ A<۝>D<١١>",
"وَمَا یُكَذِّبُ بِهِW<ۦۤ> إِلَّا كُلُّ مُعۡتَدٍ أَثِJ<ی>مٍ A<۝>D<١٢>",
"إِذَا تُتۡلَىٰ عَلَیۡهِ ءَایَT<ـٰ>تُنَا قَالَ أَسَT<ـٰ>طِیR<رُ> ٱلۡأَوَّلِJ<ی>نَ A<۝>D<١٣>",
"كَلَّاۖ بَلۡۜ R<رَ>انَ عَلَىٰ قُلُوبِهِم N<مَّ>ا كَانُوX<ا۟> یَكۡسِبُJ<و>نَ A<۝>D<١٤>",
"كَلَّW<اۤ> إِN<نَّ>هُمۡ عَX<ن> R<رَّ>بِّهِمۡ یَوۡمَئِذX<ࣲ> لَّمَحۡجُوبُJ<و>نَ A<۝>D<١٥>",
"ثُN<مَّ> إِN<نَّ>هُمۡ لَصَالُوX<ا۟> ٱلۡجَحِJ<ی>مِ A<۝>D<١٦>",
"ثُN<مَّ> یُقَالُ هَT<ـٰ>ذَا ٱلَّذِی كُN<ن>تُN<م> بِهِT<ۦ> تُكَذِّبُJ<و>نَ A<۝>D<١٧>",
"كَلَّW<اۤ> إِN<نَّ> كِتَT<ـٰ>بَ ٱلۡأَQ<بۡ>R<رَ>ارِ لَفِی عِلِّیِّJ<ی>نَ A<۝>D<١٨>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا عِلِّیُّJ<و>نَ A<۝>D<١٩>",
"كِتَT<ـٰ>بX<ࣱ> N<مَّ>R<رۡ>قُJ<و>مࣱ A<۝>D<٢٠>",
"یَشۡهَدُهُ ٱلۡمُقَR<رَّ>بُJ<و>نَ A<۝>D<٢١>",
"إِN<نَّ> ٱلۡأَQ<بۡ>R<رَ>اR<رَ> لَفِی نَعِJ<ی>مٍ A<۝>D<٢٢>",
"عَلَى ٱلۡأَR<رَ>W<اۤ>ئِكِ یَN<ن>ظُR<رُ>J<و>نَ A<۝>D<٢٣>",
"تَعۡرِفُ فِی وُجُوهِهِمۡ نَضۡR<رَ>ةَ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<٢٤>",
"یُسۡقَوۡنَ مِX<ن> R<رَّ>حِیقX<ࣲ> N<مَّ>خۡتُJ<و>مٍ A<۝>D<٢٥>",
"خِتَT<ـٰ>مُهُT<ۥ> مِسۡكX<ࣱ>ۚ N<وَ>فِی ذَT< ٰ>لِكَ فَلۡیَتَنَافَسِ ٱلۡمُتَنَT<ـٰ>فِسُJ<و>نَ A<۝>D<٢٦>",
"وَمِزَاجُهُT<ۥ> مِN<ن> تَسۡنِJ<ی>مٍ A<۝>D<٢٧>",
"عَیۡنX<ࣰ>ا N<یَ>شۡR<رَ>بُ بِهَا ٱلۡمُقَR<رَّ>بُJ<و>نَ A<۝>D<٢٨>",
"إِN<نَّ> ٱلَّذِینَ أَQ<جۡ>R<رَ>مُوX<ا۟> كَانُوX<ا۟> مِنَ ٱلَّذِینَ ءَامَنُوX<ا۟> یَضۡحَكُJ<و>نَ A<۝>D<٢٩>",
"وَإِذَا مَR<رُّ>وX<ا۟> بِهِمۡ یَتَغَامَزُJ<و>نَ A<۝>D<٣٠>",
"وَإِذَا ٱN<ن>قَلَبُW<وۤ>X<ا۟> إِلَىW<ٰۤ> أَهۡلِهِمُ ٱN<ن>قَلَبُوX<ا۟> فَكِهِJ<ی>نَ A<۝>D<٣١>",
"وَإِذَا R<رَ>أَوۡهُمۡ قَالُW<وۤ>X<ا۟> إِN<نَّ> هَW<ـٰۤ>ؤُلَW<اۤ>ءِ لَضَL<اۤ>لُّJ<و>نَ A<۝>D<٣٢>",
"وَمَW<اۤ> أُR<رۡ>سِلُوX<ا۟> عَلَیۡهِمۡ حَT<ـٰ>فِظِJ<ی>نَ A<۝>D<٣٣>",
"فَX<ٱ>لۡیَوۡمَ ٱلَّذِینَ ءَامَنُوX<ا۟> مِنَ ٱلۡكُفَّارِ یَضۡحَكُJ<و>نَ A<۝>D<٣٤>",
"عَلَى ٱلۡأَR<رَ>W<اۤ>ئِكِ یَN<ن>ظُR<رُ>J<و>نَ A<۝>D<٣٥>",
"هَلۡ ثُوِّبَ ٱلۡكُفَّاR<رُ> مَا كَانُوX<ا۟> یَفۡعَلُJ<و>نَ A<۝>D<٣٦>",
"#۞ إِذَا ٱX<ل>سَّمَW<اۤ>ءُ ٱN<ن>شَقَّتۡ A<۝>D<١>",
"وَأَذِنَتۡ لِR<رَ>بِّهَا وَحُقَّتۡ A<۝>D<٢>",
"وَإِذَا ٱلۡأَR<رۡ>ضُ مُدَّتۡ A<۝>D<٣>",
"وَأَلۡقَتۡ مَا فِیهَا وَتَخَلَّتۡ A<۝>D<٤>",
"وَأَذِنَتۡ لِR<رَ>بِّهَا وَحُقَّتۡ A<۝>D<٥>",
"یَW<ـٰۤ>أَیُّهَا ٱلۡإِN<ن>سَT<ـٰ>نُ إِN<نَّ>كَ كَادِحٌ إِلَىٰ R<رَ>بِّكَ كَQ<دۡ>حN<ࣰ>ا فَمُلَT<ـٰ>قِJ<ی>هِ A<۝>D<٦>",
"فَأَN<مَّ>ا مَنۡ أُوتِیَ كِتَT<ـٰ>بَهُT<ۥ> بِیَمِینِهِۦ A<۝>D<٧>",
"فَسَوۡفَ یُحَاسَبُ حِسَابX<ࣰ>ا N<یَ>سِیR<ر>ࣰا A<۝>D<٨>",
"وَیَN<ن>قَلِبُ إِلَىW<ٰۤ> أَهۡلِهِT<ۦ> مَسۡR<رُ>وR<ر>ࣰا A<۝>D<٩>",
"وَأَN<مَّ>ا مَنۡ أُوتِیَ كِتَT<ـٰ>بَهُT<ۥ> وَR<رَ>W<اۤ>ءَ ظَهۡرِهِۦ A<۝>D<١٠>",
"فَسَوۡفَ یَQ<دۡ>عُوX<ا۟> ثُبُوR<ر>ࣰا A<۝>D<١١>",
"وَیَصۡلَىٰ سَعِیR<ر>ًا A<۝>D<١٢>",
"إِN<نَّ>هُT<ۥ> كَانَ فِW<یۤ> أَهۡلِهِT<ۦ> مَسۡR<رُ>وR<ر>ًا A<۝>D<١٣>",
"إِN<نَّ>هُT<ۥ> ظَN<نَّ> أَX<ن> لَّX<ن> N<یَ>حُJ<و>R<ر>َ A<۝>D<١٤>",
"بَلَىW<ٰۤ>ۚ إِN<نَّ> R<رَ>بَّهُT<ۥ> كَانَ بِهِT<ۦ> بَصِیR<ر>ࣰا A<۝>D<١٥>",
"فَلَW<اۤ> أُQ<قۡ>سِمُ بِX<ٱ>X<ل>شَّفَQ<ق>ِ A<۝>D<١٦>",
"وَX<ٱ>لَّیۡلِ وَمَا وَسَQ<ق>َ A<۝>D<١٧>",
"وَX<ٱ>لۡقَمَرِ إِذَا ٱتَّسَQ<ق>َ A<۝>D<١٨>",
"لَتَR<رۡ>كَبُN<نَّ> طَبَقًا عَN<ن> طَبَQ<ق>ࣲ A<۝>D<١٩>",
"فَمَا لَهُمۡ لَا یُؤۡمِنُJ<و>نَ A<۝>D<٢٠>",
"وَإِذَا قُرِئَ عَلَیۡهِمُ ٱلۡقُR<رۡ>ءَانُ لَا یَ̅سۡ̅جُ̅دُ̅J<و>̅نَ̅ A<۝>D<٢١> ۩",
"بَلِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> یُكَذِّبُJ<و>نَ A<۝>D<٢٢>",
"وَٱللَّهُ أَعۡلَمُ بِمَا یُوعُJ<و>نَ A<۝>D<٢٣>",
"فَبَشِّرۡهُN<م> بِعَذَابٍ أَلِJ<ی>مٍ A<۝>D<٢٤>",
"إِلَّا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَهُمۡ أَQ<جۡ>R<ر>ٌ غَیۡR<رُ> مَمۡنُJ<و>نِۭ A<۝>D<٢٥>",
"#وَX<ٱ>X<ل>سَّمَW<اۤ>ءِ ذَاتِ ٱلۡبُR<رُ>J<و>Q<ج>ِ A<۝>D<١>",
"وَX<ٱ>لۡیَوۡمِ ٱلۡمَوۡعُJ<و>Q<د>ِ A<۝>D<٢>",
"وَشَاهِدX<ࣲ> N<وَ>مَشۡهُJ<و>Q<د>ࣲ A<۝>D<٣>",
"قُتِلَ أَصۡحَT<ـٰ>بُ ٱلۡأُخۡدُJ<و>Q<د>ِ A<۝>D<٤>",
"ٱX<ل>N<نَّ>ارِ ذَاتِ ٱلۡوَقُJ<و>Q<د>ِ A<۝>D<٥>",
"إِذۡ هُمۡ عَلَیۡهَا قُعُJ<و>Q<د>ࣱ A<۝>D<٦>",
"وَهُمۡ عَلَىٰ مَا یَفۡعَلُونَ بِX<ٱ>لۡمُؤۡمِنِینَ شُهُJ<و>Q<د>ࣱ A<۝>D<٧>",
"وَمَا نَقَمُوX<ا۟> مِنۡهُمۡ إِلَّW<اۤ> أَX<ن> N<یُ>ؤۡمِنُوX<ا۟> بِٱللَّهِ ٱلۡعَزِیزِ ٱلۡحَمِJ<ی>Q<د>ِ A<۝>D<٨>",
"ٱلَّذِی لَهُT<ۥ> مُلۡكُ ٱX<ل>سَّمَT<ـٰ>وَT< ٰ>تِ وَX<ٱ>لۡأَR<رۡ>ضِۚ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءN<ࣲ> شَهِJ<ی>Q<د>ٌ A<۝>D<٩>",
"إِN<نَّ> ٱلَّذِینَ فَتَنُوX<ا۟> ٱلۡمُؤۡمِنِینَ وَX<ٱ>لۡمُؤۡمِنَT<ـٰ>تِ ثُN<مَّ> لَمۡ یَتُوبُوX<ا۟> فَلَهُمۡ عَذَابُ جَهَN<نَّ>مَ وَلَهُمۡ عَذَابُ ٱلۡحَرِJ<ی>Q<ق>ِ A<۝>D<١٠>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ لَهُمۡ جَN<نَّ>T<ـٰ>تN<ࣱ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ>ۚ ذَT< ٰ>لِكَ ٱلۡفَوۡزُ ٱلۡكَبِJ<ی>رُ A<۝>D<١١>",
"إِN<نَّ> بَQ<طۡ>شَ R<رَ>بِّكَ لَشَدِJ<ی>Q<د>ٌ A<۝>D<١٢>",
"إِN<نَّ>هُT<ۥ> هُوَ یُQ<بۡ>دِئُ وَیُعِJ<ی>Q<د>ُ A<۝>D<١٣>",
"وَهُوَ ٱلۡغَفُوR<رُ> ٱلۡوَدُJ<و>Q<د>ُ A<۝>D<١٤>",
"ذُو ٱلۡعَR<رۡ>شِ ٱلۡمَجِJ<ی>Q<د>ُ A<۝>D<١٥>",
"فَعَّالX<ࣱ> لِّمَا یُرِJ<ی>Q<د>ُ A<۝>D<١٦>",
"هَلۡ أَتَX<ى>T<ٰ>كَ حَدِیثُ ٱلۡجُنُJ<و>Q<د>ِ A<۝>D<١٧>",
"فِرۡعَوۡنَ وَثَمُJ<و>Q<د>َ A<۝>D<١٨>",
"بَلِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> فِی تَكۡذِJ<ی>Q<ب>ࣲ A<۝>D<١٩>",
"وَٱللَّهُ مِX<ن> N<وَ>R<رَ>W<اۤ>ئِهِم N<مُّ>حِJ<ی>Q<ط>ُۢ A<۝>D<٢٠>",
"بَلۡ هُوَ قُR<رۡ>ءَانX<ࣱ> N<مَّ>جِJ<ی>Q<د>ࣱ A<۝>D<٢١>",
"فِی لَوۡحX<ࣲ> N<مَّ>حۡفُJ<و>ظِۭ A<۝>D<٢٢>",
"#وَX<ٱ>X<ل>سَّمَW<اۤ>ءِ وَX<ٱ>X<ل>طَّارِQ<ق>ِ A<۝>D<١>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا ٱX<ل>طَّارِQ<ق>ُ A<۝>D<٢>",
"ٱX<ل>N<نَّ>Q<جۡ>مُ ٱX<ل>ثَّاقِQ<ب>ُ A<۝>D<٣>",
"إِN<ن> كُلُّ نَفۡسX<ࣲ> لَّN<مَّ>ا عَلَیۡهَا حَافِظࣱ A<۝>D<٤>",
"فَلۡیَN<ن>ظُرِ ٱلۡإِN<ن>سَT<ـٰ>نُ مِN<مَّ> خُلِQ<ق>َ A<۝>D<٥>",
"خُلِقَ مِX<ن> N<مَّ>W<اۤ>ءN<ࣲ> دَافِQ<ق>ࣲ A<۝>D<٦>",
"یَخۡR<رُ>جُ مِX<ن>N<ۢ> بَیۡنِ ٱX<ل>صُّلۡبِ وَX<ٱ>X<ل>تَّR<رَ>W<اۤ>ئِQ<ب>ِ A<۝>D<٧>",
"إِN<نَّ>هُT<ۥ> عَلَىٰ R<رَ>Q<جۡ>عِهِT<ۦ> لَقَادِرࣱ A<۝>D<٨>",
"یَوۡمَ تُQ<بۡ>لَى ٱX<ل>سَّR<رَ>W<اۤ>ئِرُ A<۝>D<٩>",
"فَمَا لَهُT<ۥ> مِN<ن> قُوَّةX<ࣲ> N<وَ>لَا نَاصِرࣲ A<۝>D<١٠>",
"وَX<ٱ>X<ل>سَّمَW<اۤ>ءِ ذَاتِ ٱX<ل>R<رَّ>Q<جۡ>عِ A<۝>D<١١>",
"وَX<ٱ>لۡأَR<رۡ>ضِ ذَاتِ ٱX<ل>صَّQ<دۡ>عِ A<۝>D<١٢>",
"إِN<نَّ>هُT<ۥ> لَقَوۡلN<ࣱ> فَصۡلࣱ A<۝>D<١٣>",
"وَمَا هُوَ بِX<ٱ>لۡهَزۡلِ A<۝>D<١٤>",
"إِN<نَّ>هُمۡ یَكِیدُونَ كَیۡدࣰا A<۝>D<١٥>",
"وَأَكِیدُ كَیۡدࣰا A<۝>D<١٦>",
"فَمَهِّلِ ٱلۡكَT<ـٰ>فِرِینَ أَمۡهِلۡهُمۡ R<رُ>وَیۡدَۢا A<۝>D<١٧>",
"#۞ سَبِّحِ ٱسۡمَ R<رَ>بِّكَ ٱلۡأَعۡلَى A<۝>D<١>",
"ٱلَّذِی خَلَقَ فَسَوَّىٰ A<۝>D<٢>",
"وَX<ٱ>لَّذِی قَدَّR<رَ> فَهَدَىٰ A<۝>D<٣>",
"وَX<ٱ>لَّذِW<یۤ> أَخۡR<رَ>جَ ٱلۡمَR<رۡ>عَىٰ A<۝>D<٤>",
"فَجَعَلَهُT<ۥ> غُثَW<اۤ>ءً أَحۡوَىٰ A<۝>D<٥>",
"سَنُQ<قۡ>رِئُكَ فَلَا تَN<ن>سَىٰۤ A<۝>D<٦>",
"إِلَّا مَا شَW<اۤ>ءَ ٱللَّهُۚ إِN<نَّ>هُT<ۥ> یَعۡلَمُ ٱلۡجَهۡR<رَ> وَمَا یَخۡفَىٰ A<۝>D<٧>",
"وَنُیَسِّR<رُ>كَ لِلۡیُسۡR<رَ>ىٰ A<۝>D<٨>",
"فَذَكِّرۡ إِن N<نَّ>فَعَتِ ٱX<ل>ذِّكۡR<رَ>ىٰ A<۝>D<٩>",
"سَیَذَّكَّR<رُ> مَX<ن> N<یَ>خۡشَىٰ A<۝>D<١٠>",
"وَیَتَجَN<نَّ>بُهَا ٱلۡأَشۡقَى A<۝>D<١١>",
"ٱلَّذِی یَصۡلَى ٱX<ل>N<نَّ>اR<رَ> ٱلۡكُQ<بۡ>R<رَ>ىٰ A<۝>D<١٢>",
"ثُN<مَّ> لَا یَمُوتُ فِیهَا وَلَا یَحۡیَىٰ A<۝>D<١٣>",
"قَQ<دۡ> أَفۡلَحَ مَN<ن> تَزَكَّىٰ A<۝>D<١٤>",
"وَذَكَR<رَ> ٱسۡمَ R<رَ>بِّهِT<ۦ> فَصَلَّىٰ A<۝>D<١٥>",
"بَلۡ تُؤۡثِR<رُ>ونَ ٱلۡحَیَX<و>T<ٰ>ةَ ٱX<ل>دُّنۡیَا A<۝>D<١٦>",
"وَX<ٱ>لۡءَاخِR<رَ>ةُ خَیۡR<ر>X<ࣱ> N<وَ>أَQ<بۡ>قَىٰۤ A<۝>D<١٧>",
"إِN<نَّ> هَT<ـٰ>ذَا لَفِی ٱX<ل>صُّحُفِ ٱلۡأُولَىٰ A<۝>D<١٨>",
"صُحُفِ إِQ<بۡ>R<رَ>T< ٰ>هِیمَ وَمُوسَىٰ A<۝>D<١٩>",
"#هَلۡ أَتَX<ى>T<ٰ>كَ حَدِیثُ ٱلۡغَT<ـٰ>شِیَةِ A<۝>D<١>",
"وُجُوهX<ࣱ> N<یَ>وۡمَئِذٍ خَT<ـٰ>شِعَةٌ A<۝>D<٢>",
"عَامِلَةX<ࣱ> N<نَّ>اصِبَةࣱ A<۝>D<٣>",
"تَصۡلَىٰ نَاR<ر>ًا حَامِیَةࣰ A<۝>D<٤>",
"تُسۡقَىٰ مِنۡ عَیۡنٍ ءَانِیَةࣲ A<۝>D<٥>",
"لَّیۡسَ لَهُمۡ طَعَامٌ إِلَّا مِN<ن> ضَرِJ<ی>عࣲ A<۝>D<٦>",
"لَّا یُسۡمِنُ وَلَا یُغۡنِی مِN<ن> جُJ<و>عࣲ A<۝>D<٧>",
"وُجُوهX<ࣱ> N<یَ>وۡمَئِذX<ࣲ> N<نَّ>اعِمَةࣱ A<۝>D<٨>",
"لِّسَعۡیِهَا R<رَ>اضِیَةࣱ A<۝>D<٩>",
"فِی جَN<نَّ>ةٍ عَالِیَةࣲ A<۝>D<١٠>",
"لَّا تَسۡمَعُ فِیهَا لَT<ـٰ>غِیَةࣰ A<۝>D<١١>",
"فِیهَا عَیۡنN<ࣱ> جَارِیَةࣱ A<۝>D<١٢>",
"فِیهَا سُR<رُ>R<ر>X<ࣱ> N<مَّ>R<رۡ>فُوعَةࣱ A<۝>D<١٣>",
"وَأَكۡوَابX<ࣱ> N<مَّ>وۡضُوعَةࣱ A<۝>D<١٤>",
"وَنَمَارِقُ مَصۡفُوفَةࣱ A<۝>D<١٥>",
"وَزَR<رَ>ابِیُّ مَQ<بۡ>ثُوثَةٌ A<۝>D<١٦>",
"أَفَلَا یَN<ن>ظُR<رُ>ونَ إِلَى ٱلۡإِبِلِ كَیۡفَ خُلِقَتۡ A<۝>D<١٧>",
"وَإِلَى ٱX<ل>سَّمَW<اۤ>ءِ كَیۡفَ R<رُ>فِعَتۡ A<۝>D<١٨>",
"وَإِلَى ٱلۡجِبَالِ كَیۡفَ نُصِبَتۡ A<۝>D<١٩>",
"وَإِلَى ٱلۡأَR<رۡ>ضِ كَیۡفَ سُطِحَتۡ A<۝>D<٢٠>",
"فَذَكِّرۡ إِN<نَّ>مَW<اۤ> أَN<ن>تَ مُذَكِّرࣱ A<۝>D<٢١>",
"لَّسۡتَ عَلَیۡهِN<م> بِمُصَۣیۡطِرٍ A<۝>D<٢٢>",
"إِلَّا مَN<ن> تَوَلَّىٰ وَكَفَR<ر>َ A<۝>D<٢٣>",
"فَیُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَR<ر>َ A<۝>D<٢٤>",
"إِN<نَّ> إِلَیۡنَW<اۤ> إِیَابَهُمۡ A<۝>D<٢٥>",
"ثُN<مَّ> إِN<نَّ> عَلَیۡنَا حِسَابَهُم A<۝>D<٢٦>",
"#وَX<ٱ>لۡفَQ<جۡ>R<ر>ِ A<۝>D<١>",
"وَلَیَالٍ عَشۡR<ر>ࣲ A<۝>D<٢>",
"وَX<ٱ>X<ل>شَّفۡعِ وَX<ٱ>لۡوَتۡR<ر>ِ A<۝>D<٣>",
"وَX<ٱ>لَّیۡلِ إِذَا یَسۡR<ر>ِ A<۝>D<٤>",
"هَلۡ فِی ذَT< ٰ>لِكَ قَسَمX<ࣱ> لِّذِی حِQ<جۡ>رٍ A<۝>D<٥>",
"أَلَمۡ تَR<رَ> كَیۡفَ فَعَلَ R<رَ>بُّكَ بِعَJ<ا>Q<د>ٍ A<۝>D<٦>",
"إِR<رَ>مَ ذَاتِ ٱلۡعِمَJ<ا>Q<د>ِ A<۝>D<٧>",
"ٱلَّتِی لَمۡ یُخۡلَQ<قۡ> مِثۡلُهَا فِی ٱلۡبِلَT<ـٰ>Q<د>ِ A<۝>D<٨>",
"وَثَمُودَ ٱلَّذِینَ جَابُوX<ا۟> ٱX<ل>صَّخۡR<رَ> بِX<ٱ>لۡوَJ<ا>Q<د>ِ A<۝>D<٩>",
"وَفِرۡعَوۡنَ ذِی ٱلۡأَوۡتَJ<ا>Q<د>ِ A<۝>D<١٠>",
"ٱلَّذِینَ طَغَوۡX<ا۟> فِی ٱلۡبِلَT<ـٰ>Q<د>ِ A<۝>D<١١>",
"فَأَكۡثَR<رُ>وX<ا۟> فِیهَا ٱلۡفَسَJ<ا>Q<د>َ A<۝>D<١٢>",
"فَصَبَّ عَلَیۡهِمۡ R<رَ>بُّكَ سَوۡطَ عَذَJ<ا>Q<ب>ٍ A<۝>D<١٣>",
"إِN<نَّ> R<رَ>بَّكَ لَبِX<ٱ>لۡمِرۡصَJ<ا>Q<د>ِ A<۝>D<١٤>",
"فَأَN<مَّ>ا ٱلۡإِN<ن>سَT<ـٰ>نُ إِذَا مَا ٱQ<بۡ>تَلَX<ى>T<ٰ>هُ R<رَ>بُّهُT<ۥ> فَأَكۡR<رَ>مَهُT<ۥ> وَنَعَّمَهُT<ۥ> فَیَقُولُ R<رَ>بِّW<یۤ> أَكۡR<رَ>مَنِ A<۝>D<١٥>",
"وَأَN<مَّ>W<اۤ> إِذَا مَا ٱQ<بۡ>تَلَX<ى>T<ٰ>هُ فَقَدَR<رَ> عَلَیۡهِ رِزۡقَهُT<ۥ> فَیَقُولُ R<رَ>بِّW<یۤ> أَهَT<ـٰ>نَنِ A<۝>D<١٦>",
"كَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡیَتِJ<ی>مَ A<۝>D<١٧>",
"وَلَا تَحَL<ـٰۤ>ضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِJ<ی>نِ A<۝>D<١٨>",
"وَتَأۡكُلُونَ ٱX<ل>تُّR<رَ>اثَ أَكۡلX<ࣰ>ا لَّN<مّ>ࣰا A<۝>D<١٩>",
"وَتُحِبُّونَ ٱلۡمَالَ حُبّN<ࣰ>ا جَN<مّ>ࣰا A<۝>D<٢٠>",
"كَلَّW<اۤ>ۖ إِذَا دُكَّتِ ٱلۡأَR<رۡ>ضُ دَكّN<ࣰ>ا دَكࣰّا A<۝>D<٢١>",
"وَجَW<اۤ>ءَ R<رَ>بُّكَ وَX<ٱ>لۡمَلَكُ صَفّN<ࣰ>ا صَفࣰّا A<۝>D<٢٢>",
"وَجِX<ا۟>W<یۤ>ءَ یَوۡمَئِذِN<ۭ> بِجَهَN<نَّ>مَۚ یَوۡمَئِذX<ࣲ> N<یَ>تَذَكَّR<رُ> ٱلۡإِN<ن>سَT<ـٰ>نُ وَأَN<نَّ>ىٰ لَهُ ٱX<ل>ذِّكۡR<رَ>ىٰ A<۝>D<٢٣>",
"یَقُولُ یَT<ـٰ>لَیۡتَنِی قَدَّمۡتُ لِحَیَاتِی A<۝>D<٢٤>",
"فَیَوۡمَئِذX<ࣲ> لَّا یُعَذِّبُ عَذَابَهُW<ۥۤ> أَحَQ<د>ࣱ A<۝>D<٢٥>",
"وَلَا یُوثِقُ وَثَاقَهُW<ۥۤ> أَحَQ<د>ࣱ A<۝>D<٢٦>",
"یَW<ـٰۤ>أَیَّتُهَا ٱX<ل>N<نَّ>فۡسُ ٱلۡمُQ<طۡ>مَئِN<نَّ>ةُ A<۝>D<٢٧>",
"ٱR<رۡ>جِعِW<یۤ> إِلَىٰ R<رَ>بِّكِ R<رَ>اضِیَةX<ࣰ> N<مَّ>R<رۡ>ضِیَّةࣰ A<۝>D<٢٨>",
"فَX<ٱ>Q<دۡ>خُلِی فِی عِبَT<ـٰ>دِی A<۝>D<٢٩>",
"وَX<ٱ>Q<دۡ>خُلِی جَN<نَّ>تِی A<۝>D<٣٠>",
"#۞ لَW<اۤ> أُQ<قۡ>سِمُ بِهَT<ـٰ>ذَا ٱلۡبَلَQ<د>ِ A<۝>D<١>",
"وَأَN<ن>تَ حِلُّN<ۢ> بِهَT<ـٰ>ذَا ٱلۡبَلَQ<د>ِ A<۝>D<٢>",
"وَوَالِدX<ࣲ> N<وَ>مَا وَلَQ<د>َ A<۝>D<٣>",
"لَقَQ<دۡ> خَلَQ<قۡ>نَا ٱلۡإِN<ن>سَT<ـٰ>نَ فِی كَبَQ<د>ٍ A<۝>D<٤>",
"أَیَحۡسَبُ أَX<ن> لَّX<ن> N<یَ>Q<قۡ>دِR<رَ> عَلَیۡهِ أَحَQ<د>ࣱ A<۝>D<٥>",
"یَقُولُ أَهۡلَكۡتُ مَالX<ࣰ>ا لُّبَدًا A<۝>D<٦>",
"أَیَحۡسَبُ أَX<ن> لَّمۡ یَR<رَ>هُW<ۥۤ> أَحَQ<د>ٌ A<۝>D<٧>",
"أَلَمۡ نَQ<جۡ>عَل لَّهُT<ۥ> عَیۡنَJ<یۡ>نِ A<۝>D<٨>",
"وَلِسَانX<ࣰ>ا N<وَ>شَفَتَJ<یۡ>نِ A<۝>D<٩>",
"وَهَدَیۡنَT<ـٰ>هُ ٱX<ل>N<نَّ>Q<جۡ>دَJ<یۡ>نِ A<۝>D<١٠>",
"فَلَا ٱQ<قۡ>تَحَمَ ٱلۡعَقَبَةَ A<۝>D<١١>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا ٱلۡعَقَبَةُ A<۝>D<١٢>",
"فَكُّ R<رَ>قَبَةٍ A<۝>D<١٣>",
"أَوۡ إِQ<طۡ>عَT<ـٰ>مN<ࣱ> فِی یَوۡمN<ࣲ> ذِی مَسۡغَبَةࣲ A<۝>D<١٤>",
"یَتِیمN<ࣰ>ا ذَا مَQ<قۡ>R<رَ>بَةٍ A<۝>D<١٥>",
"أَوۡ مِسۡكِینN<ࣰ>ا ذَا مَتۡR<رَ>بَةࣲ A<۝>D<١٦>",
"ثُN<مَّ> كَانَ مِنَ ٱلَّذِینَ ءَامَنُوX<ا۟> وَتَوَاصَوۡX<ا۟> بِX<ٱ>X<ل>صَّQ<بۡ>رِ وَتَوَاصَوۡX<ا۟> بِX<ٱ>لۡمَR<رۡ>حَمَةِ A<۝>D<١٧>",
"أُX<و۟>لَW<ـٰۤ>ئِكَ أَصۡحَT<ـٰ>بُ ٱلۡمَیۡمَنَةِ A<۝>D<١٨>",
"وَX<ٱ>لَّذِینَ كَفَR<رُ>وX<ا۟> بِءَایَT<ـٰ>تِنَا هُمۡ أَصۡحَT<ـٰ>بُ ٱلۡمَشۡءَمَةِ A<۝>D<١٩>",
"عَلَیۡهِمۡ نَاR<ر>X<ࣱ> N<مُّ>ؤۡصَدَةُۢ A<۝>D<٢٠>",
"#وَX<ٱ>X<ل>شَّمۡسِ وَضُحَX<ى>T<ٰ>هَا A<۝>D<١>",
"وَX<ٱ>لۡقَمَرِ إِذَا تَلَX<ى>T<ٰ>هَا A<۝>D<٢>",
"وَX<ٱ>X<ل>N<نَّ>هَارِ إِذَا جَلَّX<ى>T<ٰ>هَا A<۝>D<٣>",
"وَX<ٱ>لَّیۡلِ إِذَا یَغۡشَX<ى>T<ٰ>هَا A<۝>D<٤>",
"وَX<ٱ>X<ل>سَّمَW<اۤ>ءِ وَمَا بَنَX<ى>T<ٰ>هَا A<۝>D<٥>",
"وَX<ٱ>لۡأَR<رۡ>ضِ وَمَا طَحَX<ى>T<ٰ>هَا A<۝>D<٦>",
"وَنَفۡسX<ࣲ> N<وَ>مَا سَوَّX<ى>T<ٰ>هَا A<۝>D<٧>",
"فَأَلۡهَمَهَا فُجُوR<رَ>هَا وَتَQ<قۡ>وَX<ى>T<ٰ>هَا A<۝>D<٨>",
"قَQ<دۡ> أَفۡلَحَ مَN<ن> زَكَّX<ى>T<ٰ>هَا A<۝>D<٩>",
"وَقَQ<دۡ> خَابَ مَN<ن> دَسَّX<ى>T<ٰ>هَا A<۝>D<١٠>",
"كَذَّبَتۡ ثَمُودُ بِطَغۡوَX<ى>T<ٰ>هَاۤ A<۝>D<١١>",
"إِذِ ٱX<ن>N<ۢ>بَعَثَ أَشۡقَX<ى>T<ٰ>هَا A<۝>D<١٢>",
"فَقَالَ لَهُمۡ R<رَ>سُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُQ<قۡ>یَT<ـٰ>هَا A<۝>D<١٣>",
"فَكَذَّبُوهُ فَعَقَR<رُ>وهَا فَدَمۡدَمَ عَلَیۡهِمۡ R<رَ>بُّهُN<م> بِذَX<ن>N<ۢ>بِهِمۡ فَسَوَّX<ى>T<ٰ>هَا A<۝>D<١٤>",
"وَلَا یَخَافُ عُQ<قۡ>بَT<ـٰ>هَا A<۝>D<١٥>",
"#وَX<ٱ>لَّیۡلِ إِذَا یَغۡشَىٰ A<۝>D<١>",
"وَX<ٱ>X<ل>N<نَّ>هَارِ إِذَا تَجَلَّىٰ A<۝>D<٢>",
"وَمَا خَلَقَ ٱX<ل>ذَّكَR<رَ> وَX<ٱ>لۡأُN<ن>ثَىٰۤ A<۝>D<٣>",
"إِN<نَّ> سَعۡیَكُمۡ لَشَتَّىٰ A<۝>D<٤>",
"فَأَN<مَّ>ا مَنۡ أَعۡطَىٰ وَX<ٱ>تَّقَىٰ A<۝>D<٥>",
"وَصَدَّقَ بِX<ٱ>لۡحُسۡنَىٰ A<۝>D<٦>",
"فَسَنُیَسِّR<رُ>هُT<ۥ> لِلۡیُسۡR<رَ>ىٰ A<۝>D<٧>",
"وَأَN<مَّ>ا مَX<ن>N<ۢ> بَخِلَ وَX<ٱ>سۡتَغۡنَىٰ A<۝>D<٨>",
"وَكَذَّبَ بِX<ٱ>لۡحُسۡنَىٰ A<۝>D<٩>",
"فَسَنُیَسِّR<رُ>هُT<ۥ> لِلۡعُسۡR<رَ>ىٰ A<۝>D<١٠>",
"وَمَا یُغۡنِی عَنۡهُ مَالُهُW<ۥۤ> إِذَا تَR<رَ>دَّىٰۤ A<۝>D<١١>",
"إِN<نَّ> عَلَیۡنَا لَلۡهُدَىٰ A<۝>D<١٢>",
"وَإِN<نَّ> لَنَا لَلۡءَاخِR<رَ>ةَ وَX<ٱ>لۡأُولَىٰ A<۝>D<١٣>",
"فَأَN<ن>ذَR<رۡ>تُكُمۡ نَاR<ر>N<ࣰ>ا تَلَظَّىٰ A<۝>D<١٤>",
"لَا یَصۡلَX<ى>T<ٰ>هَW<اۤ> إِلَّا ٱلۡأَشۡقَى A<۝>D<١٥>",
"ٱلَّذِی كَذَّبَ وَتَوَلَّىٰ A<۝>D<١٦>",
"وَسَیُجَN<نَّ>بُهَا ٱلۡأَتۡقَى A<۝>D<١٧>",
"ٱلَّذِی یُؤۡتِی مَالَهُT<ۥ> یَتَزَكَّىٰ A<۝>D<١٨>",
"وَمَا لِأَحَدٍ عِN<ن>دَهُT<ۥ> مِن N<نِّ>عۡمَةN<ࣲ> تُQ<جۡ>زَىٰۤ A<۝>D<١٩>",
"إِلَّا ٱQ<بۡ>تِغَW<اۤ>ءَ وَQ<جۡ>هِ R<رَ>بِّهِ ٱلۡأَعۡلَىٰ A<۝>D<٢٠>",
"وَلَسَوۡفَ یَR<رۡ>ضَىٰ A<۝>D<٢١>",
"#وَX<ٱ>X<ل>ضُّحَىٰ A<۝>D<١>",
"وَX<ٱ>لَّیۡلِ إِذَا سَجَىٰ A<۝>D<٢>",
"مَا وَدَّعَكَ R<رَ>بُّكَ وَمَا قَلَىٰ A<۝>D<٣>",
"وَلَلۡءَاخِR<رَ>ةُ خَیۡR<ر>X<ࣱ> لَّكَ مِنَ ٱلۡأُولَىٰ A<۝>D<٤>",
"وَلَسَوۡفَ یُعۡطِیكَ R<رَ>بُّكَ فَتَR<رۡ>ضَىٰۤ A<۝>D<٥>",
"أَلَمۡ یَجِQ<دۡ>كَ یَتِیمN<ࣰ>ا فَءَاوَىٰ A<۝>D<٦>",
"وَوَجَدَكَ ضَL<اۤ>لّN<ࣰ>ا فَهَدَىٰ A<۝>D<٧>",
"وَوَجَدَكَ عَW<اۤ>ئِلN<ࣰ>ا فَأَغۡنَىٰ A<۝>D<٨>",
"فَأَN<مَّ>ا ٱلۡیَتِیمَ فَلَا تَQ<قۡ>هَR<رۡ> A<۝>D<٩>",
"وَأَN<مَّ>ا ٱX<ل>سَّW<اۤ>ئِلَ فَلَا تَنۡهَR<رۡ> A<۝>D<١٠>",
"وَأَN<مَّ>ا بِنِعۡمَةِ R<رَ>بِّكَ فَحَدِّثۡ A<۝>D<١١>",
"#۞ أَلَمۡ نَشۡR<رَ>حۡ لَكَ صَQ<دۡ>R<رَ>كَ A<۝>D<١>",
"وَوَضَعۡنَا عَN<ن>كَ وِزۡR<رَ>كَ A<۝>D<٢>",
"ٱلَّذِW<یۤ> أَN<ن>قَضَ ظَهۡR<رَ>كَ A<۝>D<٣>",
"وَR<رَ>فَعۡنَا لَكَ ذِكۡR<رَ>كَ A<۝>D<٤>",
"فَإِN<نَّ> مَعَ ٱلۡعُسۡرِ یُسۡR<ر>ًا A<۝>D<٥>",
"إِN<نَّ> مَعَ ٱلۡعُسۡرِ یُسۡR<ر>ࣰا A<۝>D<٦>",
"فَإِذَا فَR<رَ>غۡتَ فَX<ٱ>N<ن>صَQ<بۡ> A<۝>D<٧>",
"وَإِلَىٰ R<رَ>بِّكَ فَX<ٱ>R<رۡ>غَب A<۝>D<٨>",
"#وَX<ٱ>X<ل>تِّینِ وَX<ٱ>X<ل>زَّیۡتُJ<و>نِ A<۝>D<١>",
"وَطُورِ سِینِJ<ی>نَ A<۝>D<٢>",
"وَهَT<ـٰ>ذَا ٱلۡبَلَدِ ٱلۡأَمِJ<ی>نِ A<۝>D<٣>",
"لَقَQ<دۡ> خَلَQ<قۡ>نَا ٱلۡإِN<ن>سَT<ـٰ>نَ فِW<یۤ> أَحۡسَنِ تَQ<قۡ>وِJ<ی>مࣲ A<۝>D<٤>",
"ثُN<مَّ> R<رَ>دَQ<دۡ>نَT<ـٰ>هُ أَسۡفَلَ سَT<ـٰ>فِلِJ<ی>نَ A<۝>D<٥>",
"إِلَّا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ فَلَهُمۡ أَQ<جۡ>R<ر>ٌ غَیۡR<رُ> مَمۡنُJ<و>نࣲ A<۝>D<٦>",
"فَمَا یُكَذِّبُكَ بَعۡدُ بِX<ٱ>X<ل>دِّJ<ی>نِ A<۝>D<٧>",
"أَلَیۡسَ ٱللَّهُ بِأَحۡكَمِ ٱلۡحَT<ـٰ>كِمِJ<ی>نَ A<۝>D<٨>",
"#ٱQ<قۡ>R<رَ>أۡ بِX<ٱ>سۡمِ R<رَ>بِّكَ ٱلَّذِی خَلَQ<ق>َ A<۝>D<١>",
"خَلَقَ ٱلۡإِN<ن>سَT<ـٰ>نَ مِنۡ عَلَQ<ق>ٍ A<۝>D<٢>",
"ٱQ<قۡ>R<رَ>أۡ وَR<رَ>بُّكَ ٱلۡأَكۡR<رَ>مُ A<۝>D<٣>",
"ٱلَّذِی عَلَّمَ بِX<ٱ>لۡقَلَمِ A<۝>D<٤>",
"عَلَّمَ ٱلۡإِN<ن>سَT<ـٰ>نَ مَا لَمۡ یَعۡلَمۡ A<۝>D<٥>",
"كَلَّW<اۤ> إِN<نَّ> ٱلۡإِN<ن>سَT<ـٰ>نَ لَیَQ<طۡ>غَىٰۤ A<۝>D<٦>",
"أَX<ن> R<رَّ>ءَاهُ ٱسۡتَغۡنَىٰۤ A<۝>D<٧>",
"إِN<نَّ> إِلَىٰ R<رَ>بِّكَ ٱX<ل>R<رُّ>Q<جۡ>عَىٰۤ A<۝>D<٨>",
"أَR<رَ>ءَیۡتَ ٱلَّذِی یَنۡهَىٰ A<۝>D<٩>",
"عَQ<بۡ>دًا إِذَا صَلَّىٰۤ A<۝>D<١٠>",
"أَR<رَ>ءَیۡتَ إِN<ن> كَانَ عَلَى ٱلۡهُدَىٰۤ A<۝>D<١١>",
"أَوۡ أَمَR<رَ> بِX<ٱ>X<ل>تَّQ<قۡ>وَىٰۤ A<۝>D<١٢>",
"أَR<رَ>ءَیۡتَ إِN<ن> كَذَّبَ وَتَوَلَّىٰۤ A<۝>D<١٣>",
"أَلَمۡ یَعۡلَN<م> بِأَN<نَّ> ٱللَّهَ یَR<رَ>ىٰ A<۝>D<١٤>",
"كَلَّا لَئِX<ن> لَّمۡ یَN<ن>تَهِ لَنَسۡفَعَN<ۢ>ا بِX<ٱ>X<ل>N<نَّ>اصِیَةِ A<۝>D<١٥>",
"نَاصِیَةN<ࣲ> كَT<ـٰ>ذِبَةٍ خَاطِئَةࣲ A<۝>D<١٦>",
"فَلۡیَQ<دۡ>عُ نَادِیَهُۥ A<۝>D<١٧>",
"سَنَQ<دۡ>عُ ٱX<ل>زَّبَانِیَةَ A<۝>D<١٨>",
"كَلَّا لَا تُطِعۡهُ وَ̅X<ٱ>̅سۡ̅جُ̅Q<دۡ>̅ وَX<ٱ>Q<قۡ>تَرِب A<۝>D<١٩> ۩",
"#إِN<نَّ>W<اۤ> أَN<ن>زَلۡنَT<ـٰ>هُ فِی لَیۡلَةِ ٱلۡقَQ<دۡ>R<ر>ِ A<۝>D<١>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا لَیۡلَةُ ٱلۡقَQ<دۡ>R<ر>ِ A<۝>D<٢>",
"لَیۡلَةُ ٱلۡقَQ<دۡ>رِ خَیۡR<ر>X<ࣱ> N<مِّ>نۡ أَلۡفِ شَهۡR<ر>ࣲ A<۝>D<٣>",
"تَنَزَّلُ ٱلۡمَلَW<ـٰۤ>ئِكَةُ وَX<ٱ>X<ل>R<رُّ>وحُ فِیهَا بِإِذۡنِ R<رَ>بِّهِم N<مِّ>N<ن> كُلِّ أَمۡR<ر>ࣲ A<۝>D<٤>",
"سَلَT<ـٰ>مٌ هِیَ حَتَّىٰ مَQ<طۡ>لَعِ ٱلۡفَQ<جۡ>R<ر>ِ A<۝>D<٥>",
"#لَمۡ یَكُنِ ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِنۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ وَX<ٱ>لۡمُشۡرِكِینَ مُN<ن>فَكِّینَ حَتَّىٰ تَأۡتِیَهُمُ ٱلۡبَیِّنَةُ A<۝>D<١>",
"R<رَ>سُولX<ࣱ> N<مِّ>نَ ٱللَّهِ یَتۡلُوX<ا۟> صُحُفX<ࣰ>ا N<مُّ>طَهَّR<رَ>ةࣰ A<۝>D<٢>",
"فِیهَا كُتُبN<ࣱ> قَیِّمَةࣱ A<۝>D<٣>",
"وَمَا تَفَR<رَّ>قَ ٱلَّذِینَ أُوتُوX<ا۟> ٱلۡكِتَT<ـٰ>بَ إِلَّا مِX<ن>N<ۢ> بَعۡدِ مَا جَW<اۤ>ءَتۡهُمُ ٱلۡبَیِّنَةُ A<۝>D<٤>",
"وَمَW<اۤ> أُمِR<رُ>W<وۤ>X<ا۟> إِلَّا لِیَعۡبُدُوX<ا۟> ٱللَّهَ مُخۡلِصِینَ لَهُ ٱX<ل>دِّینَ حُنَفَW<اۤ>ءَ وَیُقِیمُوX<ا۟> ٱX<ل>صَّلَX<و>T<ٰ>ةَ وَیُؤۡتُوX<ا۟> ٱX<ل>زَّكَX<و>T<ٰ>ةَۚ وَذَT< ٰ>لِكَ دِینُ ٱلۡقَیِّمَةِ A<۝>D<٥>",
"إِN<نَّ> ٱلَّذِینَ كَفَR<رُ>وX<ا۟> مِنۡ أَهۡلِ ٱلۡكِتَT<ـٰ>بِ وَX<ٱ>لۡمُشۡرِكِینَ فِی نَارِ جَهَN<نَّ>مَ خَT<ـٰ>لِدِینَ فِیهَW<اۤ>ۚ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمۡ شَR<رُّ> ٱلۡبَرِیَّةِ A<۝>D<٦>",
"إِN<نَّ> ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ أُX<و۟>لَW<ـٰۤ>ئِكَ هُمۡ خَیۡR<رُ> ٱلۡبَرِیَّةِ A<۝>D<٧>",
"جَزَW<اۤ>ؤُهُمۡ عِN<ن>دَ R<رَ>بِّهِمۡ جَN<نَّ>T<ـٰ>تُ عَQ<دۡ>نN<ࣲ> تَQ<جۡ>رِی مِN<ن> تَحۡتِهَا ٱلۡأَنۡهَT<ـٰ>R<رُ> خَT<ـٰ>لِدِینَ فِیهَW<اۤ> أَبَدX<ࣰ>اۖ R<رَّ>ضِیَ ٱللَّهُ عَنۡهُمۡ وَR<رَ>ضُوX<ا۟> عَنۡهُۚ ذَT< ٰ>لِكَ لِمَنۡ خَشِیَ R<رَ>بَّهُۥ A<۝>D<٨>",
"#إِذَا زُلۡزِلَتِ ٱلۡأَR<رۡ>ضُ زِلۡزَالَهَا A<۝>D<١>",
"وَأَخۡR<رَ>جَتِ ٱلۡأَR<رۡ>ضُ أَثۡقَالَهَا A<۝>D<٢>",
"وَقَالَ ٱلۡإِN<ن>سَT<ـٰ>نُ مَا لَهَا A<۝>D<٣>",
"یَوۡمَئِذN<ࣲ> تُحَدِّثُ أَخۡبَاR<رَ>هَا A<۝>D<٤>",
"بِأَN<نَّ> R<رَ>بَّكَ أَوۡحَىٰ لَهَا A<۝>D<٥>",
"یَوۡمَئِذX<ࣲ> N<یَ>صۡدُR<رُ> ٱX<ل>N<نَّ>اسُ أَشۡتَاتX<ࣰ>ا لِّیُR<رَ>وۡX<ا۟> أَعۡمَT<ـٰ>لَهُمۡ A<۝>D<٦>",
"فَمَX<ن> N<یَ>عۡمَلۡ مِثۡقَالَ ذَR<رَّ>ةٍ خَیۡR<ر>X<ࣰ>ا N<یَ>R<رَ>هُۥ A<۝>D<٧>",
"وَمَX<ن> N<یَ>عۡمَلۡ مِثۡقَالَ ذَR<رَّ>ةN<ࣲ> شَR<رّ>X<ࣰ>ا N<یَ>R<رَ>هُۥ A<۝>D<٨>",
"#وَX<ٱ>لۡعَT<ـٰ>دِیَT<ـٰ>تِ ضَQ<بۡ>حࣰا A<۝>D<١>",
"فَX<ٱ>لۡمُورِیَT<ـٰ>تِ قَQ<دۡ>حࣰا A<۝>D<٢>",
"فَX<ٱ>لۡمُغِیR<رَ>T< ٰ>تِ صُQ<بۡ>حࣰا A<۝>D<٣>",
"فَأَثَR<رۡ>نَ بِهِT<ۦ> نَQ<قۡ>عࣰا A<۝>D<٤>",
"فَوَسَQ<طۡ>نَ بِهِT<ۦ> جَمۡعًا A<۝>D<٥>",
"إِN<نَّ> ٱلۡإِN<ن>سَT<ـٰ>نَ لِR<رَ>بِّهِT<ۦ> لَكَنُJ<و>Q<د>ࣱ A<۝>D<٦>",
"وَإِN<نَّ>هُT<ۥ> عَلَىٰ ذَT< ٰ>لِكَ لَشَهِJ<ی>Q<د>ࣱ A<۝>D<٧>",
"وَإِN<نَّ>هُT<ۥ> لِحُبِّ ٱلۡخَیۡرِ لَشَدِJ<ی>Q<د>ٌ A<۝>D<٨>",
"۞ أَفَلَا یَعۡلَمُ إِذَا بُعۡثِR<رَ> مَا فِی ٱلۡقُبُJ<و>R<ر>ِ A<۝>D<٩>",
"وَحُصِّلَ مَا فِی ٱX<ل>صُّدُJ<و>R<ر>ِ A<۝>D<١٠>",
"إِN<نَّ> R<رَ>بَّهُN<م> بِهِمۡ یَوۡمَئِذX<ࣲ> لَّخَبِJ<ی>رُۢ A<۝>D<١١>",
"#ٱلۡقَارِعَةُ A<۝>D<١>",
"مَا ٱلۡقَارِعَةُ A<۝>D<٢>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا ٱلۡقَارِعَةُ A<۝>D<٣>",
"یَوۡمَ یَكُونُ ٱX<ل>N<نَّ>اسُ كَX<ٱ>لۡفَR<رَ>اشِ ٱلۡمَQ<بۡ>ثُJ<و>ثِ A<۝>D<٤>",
"وَتَكُونُ ٱلۡجِبَالُ كَX<ٱ>لۡعِهۡنِ ٱلۡمَN<ن>فُJ<و>شِ A<۝>D<٥>",
"فَأَN<مَّ>ا مَN<ن> ثَقُلَتۡ مَوَT< ٰ>زِینُهُۥ A<۝>D<٦>",
"فَهُوَ فِی عِیشَةX<ࣲ> R<رَّ>اضِیَةࣲ A<۝>D<٧>",
"وَأَN<مَّ>ا مَنۡ خَفَّتۡ مَوَT< ٰ>زِینُهُۥ A<۝>D<٨>",
"فَأُN<مُّ>هُT<ۥ> هَاوِیَةࣱ A<۝>D<٩>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا هِیَهۡ A<۝>D<١٠>",
"نَاR<ر>ٌ حَامِیَةُۢ A<۝>D<١١>",
"#أَلۡهَX<ى>T<ٰ>كُمُ ٱX<ل>تَّكَاثُR<ر>ُ A<۝>D<١>",
"حَتَّىٰ زُR<رۡ>تُمُ ٱلۡمَقَابِرَ A<۝>D<٢>",
"كَلَّا سَوۡفَ تَعۡلَمُJ<و>نَ A<۝>D<٣>",
"ثُN<مَّ> كَلَّا سَوۡفَ تَعۡلَمُJ<و>نَ A<۝>D<٤>",
"كَلَّا لَوۡ تَعۡلَمُونَ عِلۡمَ ٱلۡیَقِJ<ی>نِ A<۝>D<٥>",
"لَتَR<رَ>وُN<نَّ> ٱلۡجَحِJ<ی>مَ A<۝>D<٦>",
"ثُN<مَّ> لَتَR<رَ>وُN<نَّ>هَا عَیۡنَ ٱلۡیَقِJ<ی>نِ A<۝>D<٧>",
"ثُN<مَّ> لَتُسۡءَلُN<نَّ> یَوۡمَئِذٍ عَنِ ٱX<ل>N<نَّ>عِJ<ی>مِ A<۝>D<٨>",
"#وَX<ٱ>لۡعَصۡR<ر>ِ A<۝>D<١>",
"إِN<نَّ> ٱلۡإِN<ن>سَT<ـٰ>نَ لَفِی خُسۡR<ر>ٍ A<۝>D<٢>",
"إِلَّا ٱلَّذِینَ ءَامَنُوX<ا۟> وَعَمِلُوX<ا۟> ٱX<ل>صَّT<ـٰ>لِحَT<ـٰ>تِ وَتَوَاصَوۡX<ا۟> بِX<ٱ>لۡحَقِّ وَتَوَاصَوۡX<ا۟> بِX<ٱ>X<ل>صَّQ<بۡ>R<ر>ِ A<۝>D<٣>",
"#وَیۡلX<ࣱ> لِّكُلِّ هُمَزَةX<ࣲ> لُّمَزَةٍ A<۝>D<١>",
"ٱلَّذِی جَمَعَ مَالX<ࣰ>ا N<وَ>عَدَّدَهُۥ A<۝>D<٢>",
"یَحۡسَبُ أَN<نَّ> مَالَهُW<ۥۤ> أَخۡلَدَهُۥ A<۝>D<٣>",
"كَلَّاۖ لَیُX<ن>N<ۢ>بَذَN<نَّ> فِی ٱلۡحُطَمَةِ A<۝>D<٤>",
"وَمَW<اۤ> أَQ<دۡ>R<رَ>X<ى>T<ٰ>كَ مَا ٱلۡحُطَمَةُ A<۝>D<٥>",
"نَاR<رُ> ٱللَّهِ ٱلۡمُوقَدَةُ A<۝>D<٦>",
"ٱلَّتِی تَطَّلِعُ عَلَى ٱلۡأَفۡءِدَةِ A<۝>D<٧>",
"إِN<نَّ>هَا عَلَیۡهِم N<مُّ>ؤۡصَدَةࣱ A<۝>D<٨>",
"فِی عَمَدX<ࣲ> N<مُّ>مَدَّدَةِۭ A<۝>D<٩>",
"#أَلَمۡ تَR<رَ> كَیۡفَ فَعَلَ R<رَ>بُّكَ بِأَصۡحَT<ـٰ>بِ ٱلۡفِJ<ی>لِ A<۝>D<١>",
"أَلَمۡ یَQ<جۡ>عَلۡ كَیۡدَهُمۡ فِی تَضۡلِJ<ی>لࣲ A<۝>D<٢>",
"وَأَR<رۡ>سَلَ عَلَیۡهِمۡ طَیۡR<ر>ًا أَبَابِJ<ی>لَ A<۝>D<٣>",
"تَR<رۡ>مِیهِN<م> بِحِجَاR<رَ>ةX<ࣲ> N<مِّ>N<ن> سِجِّJ<ی>لࣲ A<۝>D<٤>",
"فَجَعَلَهُمۡ كَعَصۡفX<ࣲ> N<مَّ>أۡكُJ<و>لِۭ A<۝>D<٥>",
"#لِإِیلَT<ـٰ>فِ قُR<رَ>J<یۡ>شٍ A<۝>D<١>",
"إِT<ۦ>لَT<ـٰ>فِهِمۡ رِحۡلَةَ ٱX<ل>شِّتَW<اۤ>ءِ وَX<ٱ>X<ل>صَّJ<یۡ>فِ A<۝>D<٢>",
"فَلۡیَعۡبُدُوX<ا۟> R<رَ>بَّ هَT<ـٰ>ذَا ٱلۡبَJ<یۡ>تِ A<۝>D<٣>",
"ٱلَّذِW<یۤ> أَQ<طۡ>عَمَهُم N<مِّ>N<ن> جُوعX<ࣲ> N<وَ>ءَامَنَهُم N<مِّ>نۡ خَJ<وۡ>فِۭ A<۝>D<٤>",
"#أَR<رَ>ءَیۡتَ ٱلَّذِی یُكَذِّبُ بِX<ٱ>X<ل>دِّJ<ی>نِ A<۝>D<١>",
"فَذَT< ٰ>لِكَ ٱلَّذِی یَدُعُّ ٱلۡیَتِJ<ی>مَ A<۝>D<٢>",
"وَلَا یَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِJ<ی>نِ A<۝>D<٣>",
"فَوَیۡلX<ࣱ> لِّلۡمُصَلِّJ<ی>نَ A<۝>D<٤>",
"ٱلَّذِینَ هُمۡ عَN<ن> صَلَاتِهِمۡ سَاهُJ<و>نَ A<۝>D<٥>",
"ٱلَّذِینَ هُمۡ یُR<رَ>W<اۤ>ءُJ<و>نَ A<۝>D<٦>",
"وَیَمۡنَعُونَ ٱلۡمَاعُJ<و>نَ A<۝>D<٧>",
"#إِN<نَّ>W<اۤ> أَعۡطَیۡنَT<ـٰ>كَ ٱلۡكَوۡثَR<ر>َ A<۝>D<١>",
"فَصَلِّ لِR<رَ>بِّكَ وَX<ٱ>نۡحَR<رۡ> A<۝>D<٢>",
"إِN<نَّ> شَانِئَكَ هُوَ ٱلۡأَQ<بۡ>تَR<ر>ُ A<۝>D<٣>",
"#قُلۡ یَW<ـٰۤ>أَیُّهَا ٱلۡكَT<ـٰ>فِR<رُ>J<و>نَ A<۝>D<١>",
"لَW<اۤ> أَعۡبُدُ مَا تَعۡبُدُJ<و>نَ A<۝>D<٢>",
"وَلَW<اۤ> أَN<ن>تُمۡ عَT<ـٰ>بِدُونَ مَW<اۤ> أَعۡبُQ<د>ُ A<۝>D<٣>",
"وَلَW<اۤ> أَنَا۠ عَابِدX<ࣱ> N<مَّ>ا عَبَX<د>تُّمۡ A<۝>D<٤>",
"وَلَW<اۤ> أَN<ن>تُمۡ عَT<ـٰ>بِدُونَ مَW<اۤ> أَعۡبُQ<د>ُ A<۝>D<٥>",
"لَكُمۡ دِینُكُمۡ وَلِیَ دِJ<ی>نِ A<۝>D<٦>",
"#إِذَا جَW<اۤ>ءَ نَصۡR<رُ> ٱللَّهِ وَX<ٱ>لۡفَتۡحُ A<۝>D<١>",
"وَR<رَ>أَیۡتَ ٱX<ل>N<نَّ>اسَ یَQ<دۡ>خُلُونَ فِی دِینِ ٱللَّهِ أَفۡوَاجࣰا A<۝>D<٢>",
"فَسَبِّحۡ بِحَمۡدِ R<رَ>بِّكَ وَX<ٱ>سۡتَغۡفِرۡهُۚ إِN<نَّ>هُT<ۥ> كَانَ تَوَّابَۢا A<۝>D<٣>",
"#تَبَّتۡ یَدَW<اۤ> أَبِی لَهَبX<ࣲ> N<وَ>تَQ<بّ>َ A<۝>D<١>",
"مَW<اۤ> أَغۡنَىٰ عَنۡهُ مَالُهُT<ۥ> وَمَا كَسَQ<ب>َ A<۝>D<٢>",
"سَیَصۡلَىٰ نَاR<ر>N<ࣰ>ا ذَاتَ لَهَQ<ب>ࣲ A<۝>D<٣>",
"وَX<ٱ>مۡR<رَ>أَتُهُT<ۥ> حَN<مَّ>الَةَ ٱلۡحَطَQ<ب>ِ A<۝>D<٤>",
"فِی جِیدِهَا حَQ<بۡ>لX<ࣱ> N<مِّ>X<ن> N<مَّ>سَQ<د>ِۭ A<۝>D<٥>",
"#قُلۡ هُوَ ٱللَّهُ أَحَQ<د>ٌ A<۝>D<١>",
"ٱللَّهُ ٱX<ل>صَّمَQ<د>ُ A<۝>D<٢>",
"لَمۡ یَلِQ<دۡ> وَلَمۡ یُولَQ<دۡ> A<۝>D<٣>",
"وَلَمۡ یَكُX<ن> لَّهُT<ۥ> كُفُوًا أَحَQ<د>ُۢ A<۝>D<٤>",
"#قُلۡ أَعُوذُ بِR<رَ>بِّ ٱلۡفَلَQ<ق>ِ A<۝>D<١>",
"مِN<ن> شَرِّ مَا خَلَQ<ق>َ A<۝>D<٢>",
"وَمِN<ن> شَرِّ غَاسِقٍ إِذَا وَقَQ<ب>َ A<۝>D<٣>",
"وَمِN<ن> شَرِّ ٱX<ل>N<نَّ>فَّT<ـٰ>ثَT<ـٰ>تِ فِی ٱلۡعُقَQ<د>ِ A<۝>D<٤>",
"وَمِN<ن> شَرِّ حَاسِدٍ إِذَا حَسَQ<د>َ A<۝>D<٥>",
"#قُلۡ أَعُوذُ بِR<رَ>بِّ ٱX<ل>N<نَّ>J<ا>سِ A<۝>D<١>",
"مَلِكِ ٱX<ل>N<نَّ>J<ا>سِ A<۝>D<٢>",
"إِلَT<ـٰ>هِ ٱX<ل>N<نَّ>J<ا>سِ A<۝>D<٣>",
"مِN<ن> شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَN<نَّ>J<ا>سِ A<۝>D<٤>",
"ٱلَّذِی یُوَسۡوِسُ فِی صُدُورِ ٱX<ل>N<نَّ>J<ا>سِ A<۝>D<٥>",
"مِنَ ٱلۡجِN<نَّ>ةِ وَX<ٱ>X<ل>N<نَّ>J<ا>سِ A<۝>D<٦>",
